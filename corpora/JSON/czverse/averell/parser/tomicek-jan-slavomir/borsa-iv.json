        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "„Pomoci bych mohla teď nemocné,",
                    "metrical_pattern": "1000100100",
                    "line_length": 10,
