                        },
                        {
                            "word_text": "chvěje"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Žena myslí – potom hlas vydá:",
                    "metrical_pattern": "101010110",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
