                        },
                        {
                            "word_text": "modrou"
                        },
                        {
                            "word_text": "dálí"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Až zvuky jeho dokonaly.",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 2,
                    "words": [
