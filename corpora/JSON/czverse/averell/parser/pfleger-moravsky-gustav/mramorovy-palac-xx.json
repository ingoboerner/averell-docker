                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Ve"
                        },
                        {
                            "word_text": "hvězdném"
                        },
                        {
                            "word_text": "ráje"
                        },
                        {
                            "word_text": "usmívání"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Jímž slasť se v srdci rozklene:",
                    "metrical_pattern": "00010100",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Jímž"
                        },
                        {
                            "word_text": "slasť"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "srdci"
                        },
                        {
                            "word_text": "rozklene"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Kdo v tváři, v oku rozlitou",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Kdo"
                        },
                        {
                            "word_text": "v"
