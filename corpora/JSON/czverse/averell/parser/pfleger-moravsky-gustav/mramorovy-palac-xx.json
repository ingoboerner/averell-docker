                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "komnatě"
                        },
                        {
                            "word_text": "své"
                        },
                        {
                            "word_text": "dlela"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Ve sladkých dumách zhloubena,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Ve"
                        },
                        {
                            "word_text": "sladkých"
                        },
                        {
                            "word_text": "dumách"
                        },
                        {
                            "word_text": "zhloubena"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "A sama děva nevěděla",
