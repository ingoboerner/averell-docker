            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Samotna v komnatě své dlela",
                    "metrical_pattern": "100100010",
                    "line_length": 9,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Samotna"
