                    "line_length": 9,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "sama"
                        },
                        {
                            "word_text": "děva"
                        },
                        {
                            "word_text": "nevěděla"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Jak naprosto je změněna.",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Jak"
                        },
                        {
                            "word_text": "naprosto"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "změněna"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Vše, jež v snách zří, je drahý milý;",
                    "metrical_pattern": "001101010",
                    "line_length": 9,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Vše"
                        },
                        {
                            "word_text": "jež"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "snách"
                        },
                        {
                            "word_text": "zří"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "drahý"
                        },
                        {
                            "word_text": "milý"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Myšlénka každá v život čilý",
                    "metrical_pattern": "100101010",
                    "line_length": 9,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Myšlénka"
                        },
                        {
                            "word_text": "každá"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "čilý"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Ze hloubi duše vzkříšená,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Ze"
                        },
                        {
                            "word_text": "hloubi"
                        },
                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "vzkříšená"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Juž v slastnou touhu svoje vání",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Juž"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "slastnou"
                        },
                        {
                            "word_text": "touhu"
                        },
                        {
                            "word_text": "svoje"
                        },
                        {
                            "word_text": "vání"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Lahodnomile měnila,",
                    "metrical_pattern": "10000100",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Lahodnomile"
                        },
                        {
                            "word_text": "měnila"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "A v sladkozdílném vzpomínání",
                    "metrical_pattern": "010001000",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "sladkozdílném"
                        },
                        {
                            "word_text": "vzpomínání"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "S jeho se duší snoubila.",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "S"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "duší"
                        },
                        {
                            "word_text": "snoubila"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Tu zaplál slunce východ právě",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "zaplál"
                        },
                        {
                            "word_text": "slunce"
                        },
                        {
                            "word_text": "východ"
                        },
                        {
                            "word_text": "právě"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "A v duhový déšť milohravě",
                    "metrical_pattern": "010011000",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "duhový"
                        },
                        {
                            "word_text": "déšť"
                        },
                        {
                            "word_text": "milohravě"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Svou clonu rozlil svitnoranní,",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Svou"
                        },
                        {
                            "word_text": "clonu"
                        },
                        {
                            "word_text": "rozlil"
                        },
                        {
                            "word_text": "svitnoranní"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "A milých růží rozkvětání",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "milých"
                        },
                        {
                            "word_text": "růží"
                        },
                        {
                            "word_text": "rozkvětání"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "V tvář bledou děvy nasypal,",
                    "metrical_pattern": "11010100",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "tvář"
                        },
                        {
                            "word_text": "bledou"
                        },
                        {
                            "word_text": "děvy"
                        },
                        {
                            "word_text": "nasypal"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Že odblesk její zorou plál.",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Že"
                        },
                        {
                            "word_text": "odblesk"
                        },
                        {
                            "word_text": "její"
                        },
                        {
                            "word_text": "zorou"
                        },
                        {
                            "word_text": "plál"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Kdo by v tom sladkém okamžení",
                    "metrical_pattern": "000101000",
                    "line_length": 9,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Kdo"
                        },
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "tom"
                        },
                        {
                            "word_text": "sladkém"
                        },
                        {
                            "word_text": "okamžení"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Byl nazřel v krásnou onu tvář,",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Byl"
                        },
                        {
                            "word_text": "nazřel"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "krásnou"
                        },
                        {
                            "word_text": "onu"
                        },
                        {
                            "word_text": "tvář"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "A viděl svaté zanícení",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "viděl"
                        },
                        {
                            "word_text": "svaté"
                        },
                        {
                            "word_text": "zanícení"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Kouzlící v líce žasnou zář;",
                    "metrical_pattern": "10010101",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Kouzlící"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "líce"
                        },
                        {
                            "word_text": "žasnou"
                        },
                        {
                            "word_text": "zář"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Kdo jejich tahy prochvívané",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Kdo"
                        },
                        {
                            "word_text": "jejich"
                        },
                        {
                            "word_text": "tahy"
                        },
                        {
                            "word_text": "prochvívané"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Milostnou touhou v nyvém hrání;",
                    "metrical_pattern": "100101010",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Milostnou"
                        },
                        {
                            "word_text": "touhou"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "nyvém"
                        },
                        {
                            "word_text": "hrání"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Kdo uzřel oko rozkochané",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Kdo"
                        },
                        {
                            "word_text": "uzřel"
                        },
                        {
                            "word_text": "oko"
                        },
                        {
                            "word_text": "rozkochané"
                        }
                    ]
                },
