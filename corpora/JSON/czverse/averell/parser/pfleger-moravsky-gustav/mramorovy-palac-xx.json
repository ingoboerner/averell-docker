                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "děvy"
                        },
                        {
                            "word_text": "nejmilostnější"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Zas v bytosť sladce přenešenou,",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 17,
                    "words": [
