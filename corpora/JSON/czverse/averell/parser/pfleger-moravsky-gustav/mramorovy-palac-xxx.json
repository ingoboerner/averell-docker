    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "A hrabě Alfons?... Bledý stál",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "hrabě"
                        },
                        {
                            "word_text": "Alfons"
                        },
                        {
                            "word_text": "Bledý"
                        },
                        {
                            "word_text": "stál"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "U sloupův bílých na tarasu.",
                    "metrical_pattern": "100101000",
                    "line_length": 9,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "U"
                        },
                        {
                            "word_text": "sloupův"
                        },
                        {
                            "word_text": "bílých"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "tarasu"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Levicí v hrozném bolu rval",
                    "metrical_pattern": "10010101",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Levicí"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "hrozném"
                        },
                        {
                            "word_text": "bolu"
                        },
                        {
                            "word_text": "rval"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "To siné stříbro svojich vlasů.",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "To"
                        },
                        {
                            "word_text": "siné"
                        },
                        {
                            "word_text": "stříbro"
                        },
                        {
                            "word_text": "svojich"
                        },
                        {
                            "word_text": "vlasů"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "A pravice ta v drhavých",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "pravice"
                        },
                        {
                            "word_text": "ta"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "drhavých"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Se třásla křečech svíravých,",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "třásla"
                        },
                        {
                            "word_text": "křečech"
                        },
                        {
                            "word_text": "svíravých"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Jak ze sebe by zdrvělá;",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Jak"
                        },
                        {
                            "word_text": "ze"
                        },
                        {
                            "word_text": "sebe"
                        },
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "zdrvělá"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "A tiskem pevným držela",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "tiskem"
                        },
                        {
                            "word_text": "pevným"
                        },
                        {
                            "word_text": "držela"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Doposuď smrtonosnou zbraň,",
                    "metrical_pattern": "10010001",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Doposuď"
                        },
                        {
                            "word_text": "smrtonosnou"
                        },
                        {
                            "word_text": "zbraň"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Jež přes jezera sivou pláň",
                    "metrical_pattern": "11000101",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Jež"
                        },
                        {
                            "word_text": "přes"
                        },
                        {
                            "word_text": "jezera"
                        },
                        {
                            "word_text": "sivou"
                        },
                        {
                            "word_text": "pláň"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Hrot smrti vrhla krutým pádem",
                    "metrical_pattern": "110101010",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Hrot"
                        },
                        {
                            "word_text": "smrti"
                        },
                        {
                            "word_text": "vrhla"
                        },
                        {
                            "word_text": "krutým"
                        },
                        {
                            "word_text": "pádem"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Ach! v ňader sladkých kolíbání.",
                    "metrical_pattern": "110101000",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Ach"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "ňader"
                        },
                        {
                            "word_text": "sladkých"
                        },
                        {
                            "word_text": "kolíbání"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Ta ňadra, ježto k zulíbání",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Ta"
                        },
                        {
                            "word_text": "ňadra"
                        },
                        {
                            "word_text": "ježto"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "zulíbání"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Tím překynoucím, měkkým skladem",
                    "metrical_pattern": "010001010",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Tím"
                        },
                        {
                            "word_text": "překynoucím"
                        },
                        {
                            "word_text": "měkkým"
                        },
                        {
                            "word_text": "skladem"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Tak znyle něhou sladkou zvala:",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Tak"
                        },
                        {
                            "word_text": "znyle"
                        },
                        {
                            "word_text": "něhou"
                        },
                        {
                            "word_text": "sladkou"
                        },
                        {
                            "word_text": "zvala"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Ta ňadra více nedýchala...",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Ta"
                        },
                        {
                            "word_text": "ňadra"
                        },
                        {
                            "word_text": "více"
                        },
                        {
                            "word_text": "nedýchala"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "A otec sám ten skonal čin;",
                    "metrical_pattern": "01000101",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "otec"
                        },
                        {
                            "word_text": "sám"
                        },
                        {
                            "word_text": "ten"
                        },
                        {
                            "word_text": "skonal"
                        },
                        {
                            "word_text": "čin"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Ač ruka tamto nemířila.",
                    "metrical_pattern": "010101000",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Ač"
                        },
                        {
                            "word_text": "ruka"
                        },
                        {
                            "word_text": "tamto"
                        },
                        {
                            "word_text": "nemířila"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Však kdyby cíl dostihla byla,",
                    "metrical_pattern": "010110010",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Však"
                        },
                        {
                            "word_text": "kdyby"
                        },
                        {
                            "word_text": "cíl"
                        },
                        {
                            "word_text": "dostihla"
                        },
                        {
                            "word_text": "byla"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Po němž se ruky chýlil kyn:",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Po"
                        },
                        {
                            "word_text": "němž"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "ruky"
                        },
                        {
                            "word_text": "chýlil"
                        },
                        {
                            "word_text": "kyn"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Dvě duše byl by jediný,",
                    "metrical_pattern": "11000100",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Dvě"
                        },
                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "byl"
                        },
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "jediný"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Ve hněvu ráz ten zločinný",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Ve"
                        },
                        {
                            "word_text": "hněvu"
                        },
                        {
                            "word_text": "ráz"
                        },
                        {
                            "word_text": "ten"
                        },
                        {
                            "word_text": "zločinný"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Vyloudil z dvou milostných těl;",
                    "metrical_pattern": "10011001",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Vyloudil"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "dvou"
                        },
                        {
                            "word_text": "milostných"
                        },
                        {
                            "word_text": "těl"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Neb v milci život děvy tkvěl...",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Neb"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "milci"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "děvy"
                        },
                        {
                            "word_text": "tkvěl"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Když stráží ze snův probuzen",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Když"
                        },
                        {
                            "word_text": "stráží"
                        },
                        {
                            "word_text": "ze"
                        },
                        {
                            "word_text": "snův"
                        },
                        {
                            "word_text": "probuzen"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Skrz dcery kobku vyběh’ ven;",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Skrz"
                        },
                        {
                            "word_text": "dcery"
                        },
                        {
                            "word_text": "kobku"
                        },
                        {
                            "word_text": "vyběh"
                        },
                        {
                            "word_text": "ven"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Po ní svůj žaslý vrhav hled,",
                    "metrical_pattern": "10010101",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Po"
                        },
                        {
                            "word_text": "ní"
                        },
                        {
                            "word_text": "svůj"
                        },
                        {
                            "word_text": "žaslý"
                        },
                        {
                            "word_text": "vrhav"
                        },
                        {
                            "word_text": "hled"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Tu neuzřel, leč zmatný sled,",
                    "metrical_pattern": "01000101",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "neuzřel"
                        },
                        {
                            "word_text": "leč"
                        },
                        {
                            "word_text": "zmatný"
                        },
                        {
                            "word_text": "sled"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Jejž útěk její zůstavil,",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Jejž"
                        },
                        {
                            "word_text": "útěk"
                        },
                        {
                            "word_text": "její"
                        },
                        {
                            "word_text": "zůstavil"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Ha! útěk s oním milánem,",
                    "metrical_pattern": "11010100",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Ha"
                        },
                        {
                            "word_text": "útěk"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "oním"
                        },
                        {
                            "word_text": "milánem"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "(Tak komorné žas vypravil)",
                    "metrical_pattern": "01001100",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Tak"
                        },
                        {
                            "word_text": "komorné"
                        },
                        {
                            "word_text": "žas"
                        },
                        {
                            "word_text": "vypravil"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Jejž stíhal v vzteku skrývaném:",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Jejž"
                        },
                        {
                            "word_text": "stíhal"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "vzteku"
                        },
                        {
                            "word_text": "skrývaném"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Tu kletbou stížil prchlou děvu",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "kletbou"
                        },
                        {
                            "word_text": "stížil"
                        },
                        {
                            "word_text": "prchlou"
                        },
                        {
                            "word_text": "děvu"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "A v nezkroceně divém hněvu",
                    "metrical_pattern": "010001010",
                    "line_length": 9,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "nezkroceně"
                        },
                        {
                            "word_text": "divém"
                        },
                        {
                            "word_text": "hněvu"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Chtěl, by zplatila smrtí hřích,",
                    "metrical_pattern": "10100101",
                    "line_length": 8,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "Chtěl"
                        },
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "zplatila"
                        },
                        {
                            "word_text": "smrtí"
                        },
                        {
                            "word_text": "hřích"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Že směla povinností svých",
                    "metrical_pattern": "01010000",
                    "line_length": 8,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "Že"
                        },
                        {
                            "word_text": "směla"
                        },
                        {
                            "word_text": "povinností"
                        },
                        {
                            "word_text": "svých"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "K starému otci zapomníti",
                    "metrical_pattern": "100101000",
                    "line_length": 9,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "K"
                        },
                        {
                            "word_text": "starému"
                        },
                        {
                            "word_text": "otci"
                        },
                        {
                            "word_text": "zapomníti"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "A přezradně jej opustiti...",
                    "metrical_pattern": "010001000",
                    "line_length": 9,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "přezradně"
                        },
                        {
                            "word_text": "jej"
                        },
                        {
                            "word_text": "opustiti"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "A teď, když strašnou náhodou",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 20,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "teď"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "strašnou"
                        },
                        {
                            "word_text": "náhodou"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Zavraždiv vlastní dceru svou,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 20,
                    "words": [
                        {
                            "word_text": "Zavraždiv"
                        },
                        {
                            "word_text": "vlastní"
                        },
                        {
                            "word_text": "dceru"
                        },
                        {
                            "word_text": "svou"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "To, co chtěl v hněvu, vykonal:",
                    "metrical_pattern": "00110100",
                    "line_length": 8,
                    "rhyme": 21,
                    "words": [
                        {
                            "word_text": "To"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "chtěl"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "hněvu"
                        },
                        {
                            "word_text": "vykonal"
                        }
                    ]
                },
                {
                    "line_number": 42,
                    "line_text": "Teď šílil... plakal... srdce rval...",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 21,
                    "words": [
                        {
                            "word_text": "Teď"
                        },
                        {
                            "word_text": "šílil"
                        },
                        {
                            "word_text": "plakal"
                        },
                        {
                            "word_text": "srdce"
                        },
                        {
                            "word_text": "rval"
                        }
                    ]
                },
                {
                    "line_number": 43,
                    "line_text": "Vše darmo; víc k novému žití",
                    "metrical_pattern": "010010010",
                    "line_length": 9,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Vše"
