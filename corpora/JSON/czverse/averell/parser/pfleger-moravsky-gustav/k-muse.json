                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "bohův"
                        },
                        {
                            "word_text": "potomky"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "V děsné zbroji búrou užásal?",
                    "metrical_pattern": "101010100",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "děsné"
