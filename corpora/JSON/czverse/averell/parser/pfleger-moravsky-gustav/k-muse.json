                    "line_number": 19,
                    "line_text": "A jehožto obrvou zatřásl se svět",
                    "metrical_pattern": "010010010001",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "jehožto"
                        },
                        {
