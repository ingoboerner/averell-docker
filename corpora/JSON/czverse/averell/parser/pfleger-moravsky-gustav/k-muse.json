                        {
                            "word_text": "zbroji"
                        },
                        {
                            "word_text": "búrou"
                        },
                        {
                            "word_text": "užásal"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Neb na tisíce mužných vojínův",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neb"
                        },
                        {
                            "word_text": "na"
