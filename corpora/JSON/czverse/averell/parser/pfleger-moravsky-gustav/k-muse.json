                        },
                        {
                            "word_text": "zavítal"
                        },
                        {
                            "word_text": "odměnný"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Vítězi, jenž jako hromu rána",
                    "metrical_pattern": "1000101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vítězi"
                        },
                        {
                            "word_text": "jenž"
                        },
