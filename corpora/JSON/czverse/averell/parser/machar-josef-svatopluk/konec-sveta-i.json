                        {
                            "word_text": "statisíce"
                        },
                        {
                            "word_text": "nocí"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "teď uvidí, že opustila náhle",
                    "metrical_pattern": "01000100010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
