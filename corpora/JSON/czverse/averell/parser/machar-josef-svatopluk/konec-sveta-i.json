                    "line_number": 9,
                    "line_text": "A myšlenek a plánů dobrodruzi",
                    "metrical_pattern": "01000101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "myšlenek"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "plánů"
                        },
                        {
                            "word_text": "dobrodruzi"
                        }
                    ]
                },
