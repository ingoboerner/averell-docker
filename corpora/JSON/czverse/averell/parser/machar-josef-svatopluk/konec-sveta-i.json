                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "mu"
                        },
                        {
                            "word_text": "bude"
                        },
                        {
                            "word_text": "malým"
                        },
                        {
                            "word_text": "Také"
                        },
                        {
                            "word_text": "chladnout"
                        },
                        {
                            "word_text": "začne"
                        }
                    ]
                },
