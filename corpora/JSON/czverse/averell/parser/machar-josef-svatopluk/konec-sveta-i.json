                        {
                            "word_text": "lidstvu"
                        },
                        {
                            "word_text": "tomu"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Vzdor válkám krvavým i nekrvavým",
                    "metrical_pattern": "01010001000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vzdor"
                        },
                        {
