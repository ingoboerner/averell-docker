                        },
                        {
                            "word_text": "Komise"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "sejde"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "a Yankeové dají miliony,",
                    "metrical_pattern": "01000101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "Yankeové"
                        },
                        {
                            "word_text": "dají"
                        },
                        {
                            "word_text": "miliony"
                        }
