                        {
                            "word_text": "vybásnil"
                        },
                        {
                            "word_text": "nám"
                        },
                        {
                            "word_text": "kdysi"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "a jak jsme čtli to v blahých dětských letech –",
                    "metrical_pattern": "10010101010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "jsme"
