                        {
                            "word_text": "dětských"
                        },
                        {
                            "word_text": "letech"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "je vidno, že i v příštích lidstvo časech",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
