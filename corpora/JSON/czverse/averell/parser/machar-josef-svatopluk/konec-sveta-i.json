                    "line_number": 10,
                    "line_text": "vykřiknou hlasně: K čemu máme hvězdy?",
                    "metrical_pattern": "10010101010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "vykřiknou"
                        },
                        {
                            "word_text": "hlasně"
                        },
                        {
                            "word_text": "K"
