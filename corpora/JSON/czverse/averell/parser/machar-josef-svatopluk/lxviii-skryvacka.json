                },
                {
                    "line_number": 25,
                    "line_text": "i uzlík nesou, plný něčeho",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "uzlík"
                        },
                        {
                            "word_text": "nesou"
                        },
                        {
