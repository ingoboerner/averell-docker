                },
                {
                    "line_number": 21,
                    "line_text": "jen vídáš, jak tam dvířky vklouzají,",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "vídáš"
