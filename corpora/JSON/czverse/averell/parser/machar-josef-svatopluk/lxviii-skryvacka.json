                    "line_number": 23,
                    "line_text": "když vyjdou pak, jim pěkně plane zrak",
                    "metrical_pattern": "0100010101",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "vyjdou"
