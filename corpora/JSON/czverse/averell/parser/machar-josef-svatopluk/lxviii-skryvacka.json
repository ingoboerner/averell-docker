                        {
                            "word_text": "tím"
                        },
                        {
                            "word_text": "obrázkem"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "a hledej, kde je „blaho veřejné“!",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "hledej"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "je"
                        },
                        {
