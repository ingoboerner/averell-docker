                        },
                        {
                            "word_text": "ono"
                        },
                        {
                            "word_text": "břemeno"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "tím horlivěji přinášejí je –",
                    "metrical_pattern": "0100010000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "tím"
                        },
                        {
                            "word_text": "horlivěji"
                        },
                        {
                            "word_text": "přinášejí"
                        },
                        {
                            "word_text": "je"
                        }
                    ]
                },
