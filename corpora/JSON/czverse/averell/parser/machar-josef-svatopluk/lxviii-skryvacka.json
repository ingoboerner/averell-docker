{
    "poem_title": "LXVIII. SKRÝVAČKA",
    "author": "Machar, Josef Svatopluk",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Hle, znáte všichni ony skrývačky,",
                    "metrical_pattern": "1101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hle"
                        },
                        {
                            "word_text": "znáte"
                        },
                        {
                            "word_text": "všichni"
                        },
                        {
                            "word_text": "ony"
                        },
                        {
                            "word_text": "skrývačky"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "jež zábavou jsou dětské bystrosti:",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jež"
                        },
                        {
                            "word_text": "zábavou"
                        },
                        {
                            "word_text": "jsou"
                        },
                        {
                            "word_text": "dětské"
                        },
                        {
                            "word_text": "bystrosti"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "je třeba úhor, mezi kameny",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "třeba"
                        },
                        {
                            "word_text": "úhor"
                        },
                        {
                            "word_text": "mezi"
                        },
                        {
                            "word_text": "kameny"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "a keříky se pase stádo ovcí",
                    "metrical_pattern": "01000101010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "keříky"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "pase"
                        },
                        {
                            "word_text": "stádo"
                        },
                        {
                            "word_text": "ovcí"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "a otázka zní: kde tu pastýř je?",
                    "metrical_pattern": "0100110100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "otázka"
                        },
                        {
                            "word_text": "zní"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "pastýř"
                        },
                        {
                            "word_text": "je"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "A kroutíš-li a točíš obrázkem,",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "kroutíš"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "točíš"
                        },
                        {
                            "word_text": "obrázkem"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "hle, pastýř se ti náhle objeví,",
                    "metrical_pattern": "1100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "hle"
                        },
                        {
                            "word_text": "pastýř"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "náhle"
                        },
                        {
                            "word_text": "objeví"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "je zkroucen, skrčen mezi kameny,",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "zkroucen"
                        },
                        {
                            "word_text": "skrčen"
                        },
                        {
                            "word_text": "mezi"
                        },
                        {
                            "word_text": "kameny"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "nestvůrný jaksi, ale dítě má",
                    "metrical_pattern": "1001010101",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "nestvůrný"
                        },
                        {
                            "word_text": "jaksi"
                        },
                        {
                            "word_text": "ale"
                        },
                        {
