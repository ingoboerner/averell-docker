                    "line_number": 24,
                    "line_text": "a kapsy jejich jsou jak nabity",
                    "metrical_pattern": "0101000100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "kapsy"
                        },
                        {
                            "word_text": "jejich"
