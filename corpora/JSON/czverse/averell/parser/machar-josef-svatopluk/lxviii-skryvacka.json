                    "line_number": 32,
                    "line_text": "tak, čtenáři můj, toč tím obrázkem",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "čtenáři"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "toč"
