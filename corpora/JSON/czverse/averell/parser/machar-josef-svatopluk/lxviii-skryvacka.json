                    "line_number": 12,
                    "line_text": "pro dorostlého, malá skrývačka:",
                    "metrical_pattern": "1000010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "pro"
                        },
                        {
                            "word_text": "dorostlého"
                        },
                        {
                            "word_text": "malá"
                        },
                        {
                            "word_text": "skrývačka"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "zde najdi, kde je „blaho veřejné“!",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "zde"
                        },
                        {
                            "word_text": "najdi"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "blaho"
                        },
                        {
                            "word_text": "veřejné"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Jsi občanem a poplatníkem státu,",
                    "metrical_pattern": "01000100010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jsi"
                        },
                        {
                            "word_text": "občanem"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "poplatníkem"
                        },
                        {
                            "word_text": "státu"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "(jenž důmyslně ždímá kapsy tvé),",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jenž"
                        },
                        {
                            "word_text": "důmyslně"
                        },
                        {
                            "word_text": "ždímá"
                        },
                        {
                            "word_text": "kapsy"
                        },
                        {
                            "word_text": "tvé"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "zde vidíš tedy jeden pěkný sad,",
                    "metrical_pattern": "0101010101",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "zde"
                        },
                        {
                            "word_text": "vidíš"
                        },
                        {
                            "word_text": "tedy"
                        },
                        {
                            "word_text": "jeden"
                        },
                        {
                            "word_text": "pěkný"
                        },
                        {
                            "word_text": "sad"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "zdí vysokou je hrazen, hlídán strážci,",
                    "metrical_pattern": "11000101010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "zdí"
                        },
                        {
                            "word_text": "vysokou"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "hrazen"
                        },
                        {
                            "word_text": "hlídán"
                        },
                        {
                            "word_text": "strážci"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "jež paragrafy, soudy, četníky,",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jež"
                        },
                        {
                            "word_text": "paragrafy"
                        },
                        {
                            "word_text": "soudy"
                        },
                        {
                            "word_text": "četníky"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "se zovou takto řečí života;",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "zovou"
                        },
                        {
                            "word_text": "takto"
                        },
                        {
                            "word_text": "řečí"
                        },
                        {
                            "word_text": "života"
                        }
