                        },
                        {
                            "word_text": "žití"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "pak vládne, žije. Tak jsou mezi nimi",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "pak"
                        },
                        {
                            "word_text": "vládne"
                        },
                        {
                            "word_text": "žije"
                        },
                        {
                            "word_text": "Tak"
                        },
                        {
                            "word_text": "jsou"
                        },
                        {
                            "word_text": "mezi"
                        },
                        {
                            "word_text": "nimi"
                        }
                    ]
                },
