                        {
                            "word_text": "Myšlenky"
                        },
                        {
                            "word_text": "hle"
                        },
                        {
                            "word_text": "Pravdy"
                        },
                        {
                            "word_text": "Theorie"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Je člověk vytvoří, a ona ihned",
                    "metrical_pattern": "01010001010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
