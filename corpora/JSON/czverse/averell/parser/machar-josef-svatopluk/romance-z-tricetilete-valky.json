                    "metrical_pattern": "1001001010",
                    "line_length": 10,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Fridland"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "nad"
