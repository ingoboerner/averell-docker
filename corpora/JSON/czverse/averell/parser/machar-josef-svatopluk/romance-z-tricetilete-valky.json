                    "line_text": "po světě jak ptáci poletovat.",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "světě"
                        },
