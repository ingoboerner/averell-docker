        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "„Máš-li ty takovou matičku zlou,",
                    "metrical_pattern": "1001001001",
                    "line_length": 10,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Máš"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "ty"
                        },
                        {
                            "word_text": "takovou"
                        },
                        {
                            "word_text": "matičku"
                        },
                        {
                            "word_text": "zlou"
                        }
                    ]
                },
