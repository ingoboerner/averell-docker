            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Fridland je kavalír a nás má rád,",
                    "metrical_pattern": "1001001011",
                    "line_length": 10,
                    "rhyme": 11,
                    "words": [
