                    "line_text": "po bílé silnici pojedeme.",
                    "metrical_pattern": "1001001000",
                    "line_length": 10,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "bílé"
                        },
                        {
                            "word_text": "silnici"
                        },
                        {
                            "word_text": "pojedeme"
                        }
                    ]
                }
            ],
            "stanza_text": "Pojď, sedni na koně, děvčátko mé,\npo bílé silnici pojedeme."
        },
