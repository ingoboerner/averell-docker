            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Budem u Fridlanda vojákovat,",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Budem"
