                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "tam"
                        },
                        {
                            "word_text": "zhyne"
                        },
                        {
                            "word_text": "má"
                        },
                        {
                            "word_text": "hlava"
                        },
                        {
                            "word_text": "mladá"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "vezmeš si, děvčátko, kamaráda.",
                    "metrical_pattern": "1001001000",
                    "line_length": 10,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "vezmeš"
                        },
                        {
