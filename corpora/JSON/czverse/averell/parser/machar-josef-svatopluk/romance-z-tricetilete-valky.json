        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "„Kdybych ti věneček měla dáti,",
                    "metrical_pattern": "1001001010",
                    "line_length": 10,
                    "rhyme": 7,
                    "words": [
                        {
