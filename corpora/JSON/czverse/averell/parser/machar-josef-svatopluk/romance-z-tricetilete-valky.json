            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Pojedem, panenko, k Fridlandovi,",
                    "metrical_pattern": "1001001000",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
