                    "line_number": 20,
                    "line_text": "že vlastní řeč mu jde jen neurčito.",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "že"
                        },
