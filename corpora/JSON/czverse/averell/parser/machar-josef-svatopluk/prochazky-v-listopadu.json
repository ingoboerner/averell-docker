            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Dnes myslím: dvacet roků? Za nic!",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Dnes"
                        },
