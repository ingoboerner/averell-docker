                    "metrical_pattern": "11010100",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "den"
                        },
                        {
                            "word_text": "zašlý"
                        },
                        {
                            "word_text": "stínů"
                        },
                        {
                            "word_text": "nevrhá"
                        }
