                        },
                        {
                            "word_text": "síni"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Jaké bolesti hrozí mu, neví.",
                    "metrical_pattern": "1010010010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jaké"
                        },
                        {
                            "word_text": "bolesti"
                        },
                        {
                            "word_text": "hrozí"
