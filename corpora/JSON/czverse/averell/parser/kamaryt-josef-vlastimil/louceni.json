                        },
                        {
                            "word_text": "vlast"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Válce rek a pěvec míru Čech jest.",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Válce"
                        },
                        {
