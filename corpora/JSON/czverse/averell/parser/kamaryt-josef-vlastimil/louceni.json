        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 14,
                    "line_text": "Lzeli nám se v rozkošech baviti,",
                    "metrical_pattern": "1000100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lzeli"
                        },
