                    "line_length": 6,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ať"
                        },
                        {
                            "word_text": "nezemdlí"
                        },
                        {
                            "word_text": "parnem"
                        }
                    ]
                }
