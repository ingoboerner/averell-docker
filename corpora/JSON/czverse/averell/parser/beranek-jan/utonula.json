                        },
                        {
                            "word_text": "Utopená"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Joj, toť Eva Salajčena!",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Joj"
                        },
                        {
                            "word_text": "toť"
                        },
                        {
                            "word_text": "Eva"
                        },
                        {
                            "word_text": "Salajčena"
                        }
                    ]
                }
