                        },
                        {
                            "word_text": "zaplakalo"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "vždyť ji každé děcko znalo.",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "vždyť"
                        },
                        {
                            "word_text": "ji"
                        },
                        {
