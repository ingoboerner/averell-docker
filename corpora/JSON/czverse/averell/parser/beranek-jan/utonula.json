    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Nechtěla jí voda v tůni,",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
