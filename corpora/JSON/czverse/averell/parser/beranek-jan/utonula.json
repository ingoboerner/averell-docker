        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "běda, kdo v nich světlo shasil:",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "běda"
                        },
                        {
                            "word_text": "kdo"
