                        },
                        {
                            "word_text": "tady"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "zůstal sám jen biró mladý.",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "zůstal"
                        },
                        {
                            "word_text": "sám"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "biró"
                        },
                        {
                            "word_text": "mladý"
                        }
                    ]
