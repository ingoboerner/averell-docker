        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Ach, to krásné čelo bílé!",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
