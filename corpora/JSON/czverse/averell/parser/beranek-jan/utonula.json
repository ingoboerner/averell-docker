                        },
                        {
                            "word_text": "Senice"
                        },
                        {
                            "word_text": "páni"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "podepsali dobrozdání:",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "podepsali"
                        },
                        {
                            "word_text": "dobrozdání"
                        }
                    ]
                }
            ],
            "stanza_text": "Přišli ze Senice páni,\npodepsali dobrozdání:"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": ",než by děcku život dala,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
