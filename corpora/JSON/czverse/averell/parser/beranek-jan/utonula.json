                        {
                            "word_text": "světlo"
                        },
                        {
                            "word_text": "shasil"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "vrah – on útěkem se spasil!",
                    "metrical_pattern": "10100010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "vrah"
                        },
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "útěkem"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "spasil"
                        }
                    ]
                }
            ],
            "stanza_text": "běda, kdo v nich světlo shasil:\nvrah – on útěkem se spasil!"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Za ním, za ním na vše strany",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 6,
