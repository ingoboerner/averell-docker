            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Každá jako ohník malý,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 17,
                    "words": [
