                        },
                        {
                            "word_text": "vše"
                        },
                        {
                            "word_text": "strany"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "tu je kůň již osedlaný,",
                    "metrical_pattern": "00101000",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "kůň"
                        },
                        {
                            "word_text": "již"
                        },
                        {
                            "word_text": "osedlaný"
                        }
                    ]
                }
            ],
            "stanza_text": "Za ním, za ním na vše strany\ntu je kůň již osedlaný,"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "a tu, hle, i provaz lněný",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "hle"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "provaz"
                        },
                        {
                            "word_text": "lněný"
                        }
                    ]
                },
                {
                    "line_number": 14,
