            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Žhou jej v prsty pravé ruky:",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 18,
                    "words": [
