                    "line_text": "ale pálí, jeho pálí.",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "ale"
                        },
                        {
                            "word_text": "pálí"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "pálí"
                        }
                    ]
                }
            ],
            "stanza_text": "Každá jako ohník malý,\nale pálí, jeho pálí."
        },
