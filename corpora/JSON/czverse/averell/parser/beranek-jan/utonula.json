                        },
                        {
                            "word_text": "nikdo"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "lidí"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "on, hle, s hrůzou tady vidí:",
                    "metrical_pattern": "01101010",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "hle"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "hrůzou"
                        },
                        {
                            "word_text": "tady"
                        },
                        {
                            "word_text": "vidí"
                        }
                    ]
                }
            ],
            "stanza_text": "Čeho nezřel nikdo z lidí,\non, hle, s hrůzou tady vidí:"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Na labutím krku děvy",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Na"
                        },
                        {
                            "word_text": "labutím"
                        },
                        {
                            "word_text": "krku"
                        },
                        {
                            "word_text": "děvy"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "bleďounkých pět skvrn se jeví.",
                    "metrical_pattern": "10011010",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "bleďounkých"
                        },
                        {
                            "word_text": "pět"
                        },
                        {
                            "word_text": "skvrn"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jeví"
                        }
                    ]
                }
            ],
            "stanza_text": "Na labutím krku děvy\nbleďounkých pět skvrn se jeví."
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Rumění se, červenají...",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Rumění"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "červenají"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "už jak žhavé uhly plají.",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "už"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "žhavé"
                        },
                        {
