                },
                {
                    "line_number": 2,
                    "line_text": "vynesla ji na výsluní.",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "vynesla"
                        },
                        {
