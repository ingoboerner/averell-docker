                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Jiná vnaditi je musí spokojenost.",
                    "metrical_pattern": "101000101000",
                    "line_length": 12,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Jiná"
                        },
                        {
                            "word_text": "vnaditi"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "musí"
                        },
                        {
                            "word_text": "spokojenost"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Půlku lidstva svazkem kouzedlným",
                    "metrical_pattern": "1010101000",
                    "line_length": 10,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Půlku"
                        },
                        {
                            "word_text": "lidstva"
                        },
                        {
                            "word_text": "svazkem"
                        },
                        {
                            "word_text": "kouzedlným"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Ramenama obejmula láska;",
