                        {
                            "word_text": "zlato"
                        },
                        {
                            "word_text": "trží"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Co tak v blahu tento chumel drží?",
                    "metrical_pattern": "0010101010",
                    "line_length": 10,
                    "rhyme": 5,
                    "words": [
