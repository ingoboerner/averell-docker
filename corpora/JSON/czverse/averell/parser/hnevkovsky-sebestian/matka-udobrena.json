        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Přišel syn k své matce; byla paní hrdá,",
                    "metrical_pattern": "101010101010",
                    "line_length": 12,
                    "rhyme": 1,
                    "words": [
