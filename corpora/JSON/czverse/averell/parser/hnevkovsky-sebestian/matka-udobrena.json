                        },
                        {
                            "word_text": "obnoží"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Máti považ! kdybys mohla v tajnu koho",
                    "metrical_pattern": "101010101010",
                    "line_length": 12,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Máti"
                        },
                        {
                            "word_text": "považ"
                        },
                        {
                            "word_text": "kdybys"
