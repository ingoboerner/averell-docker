                        },
                        {
                            "word_text": "ušlechtilé"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Byla dobrotivá, k neslušnostem tvrdá:",
                    "metrical_pattern": "101000100010",
                    "line_length": 12,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Byla"
                        },
                        {
                            "word_text": "dobrotivá"
                        },
                        {
                            "word_text": "k"
                        },
                        {
