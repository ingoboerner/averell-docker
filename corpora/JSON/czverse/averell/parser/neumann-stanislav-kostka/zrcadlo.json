                    "line_text": "je dceři své, by příliš neplakala...",
                    "metrical_pattern": "01000101000",
                    "line_length": 11,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "dceři"
                        },
                        {
                            "word_text": "své"
                        },
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "příliš"
                        },
                        {
                            "word_text": "neplakala"
