                    "line_number": 19,
                    "line_text": "V malé chvíli počnou znít",
                    "metrical_pattern": "1010101",
                    "line_length": 7,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "malé"
                        },
                        {
                            "word_text": "chvíli"
                        },
                        {
                            "word_text": "počnou"
                        },
                        {
                            "word_text": "znít"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Také ptactva milé zpěvy.",
                    "metrical_pattern": "10101010",
