                        },
                        {
                            "word_text": "cest"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Snad že kouzlem oněch hlásků,",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
