                        {
                            "word_text": "zpěvný"
                        },
                        {
                            "word_text": "háj"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "sad"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Své chorální sladké tóny.",
                    "metrical_pattern": "01001010",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Své"
