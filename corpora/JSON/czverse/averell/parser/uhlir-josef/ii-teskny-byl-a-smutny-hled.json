                        },
                        {
                            "word_text": "svit"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Na nebi se slunce jeví;",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Na"
                        },
                        {
                            "word_text": "nebi"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "slunce"
                        },
                        {
                            "word_text": "jeví"
                        }
                    ]
                },
