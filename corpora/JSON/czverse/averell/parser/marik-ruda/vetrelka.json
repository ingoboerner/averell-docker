                        },
                        {
                            "word_text": "chorobnému"
                        },
                        {
                            "word_text": "děcku"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "jsem pohádkou Ti dával těchu",
                    "metrical_pattern": "010001010",
                    "line_length": 9,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "pohádkou"
                        },
                        {
                            "word_text": "Ti"
                        },
                        {
                            "word_text": "dával"
                        },
                        {
                            "word_text": "těchu"
                        }
                    ]
                }
            ],
            "stanza_text": "a naslouchal jsem Tvému dechu.\nJak matka chorobnému děcku\njsem pohádkou Ti dával těchu"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "sám dobře cítě tíhu všecku. –",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 3,
                    "words": [
                        {
