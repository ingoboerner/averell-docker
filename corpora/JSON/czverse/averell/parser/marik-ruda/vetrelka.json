                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "jak oheň jich pozvolna hasne.",
                    "metrical_pattern": "010010010",
                    "line_length": 9,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "oheň"
                        },
                        {
