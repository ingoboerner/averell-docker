                    "line_number": 17,
                    "line_text": "by z nemoci se’s uzdravila –",
                    "metrical_pattern": "010001000",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "nemoci"
                        },
                        {
                            "word_text": "ses"
                        },
                        {
                            "word_text": "uzdravila"
                        }
                    ]
                },
