{
    "poem_title": "VETŘELKA.",
    "author": "Mařík, Ruda",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Já tehdy dlel u lůžka Tvého,",
                    "metrical_pattern": "010110010",
