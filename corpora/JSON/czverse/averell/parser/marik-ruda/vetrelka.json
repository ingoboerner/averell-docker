                        },
                        {
                            "word_text": "všecka"
                        },
                        {
                            "word_text": "síla"
                        }
                    ]
                }
            ],
            "stanza_text": "a naděj v srdce teplo vlila.\nJá líbal Tvoje ručky krásné,\nv nichž ochabla již všecka síla –"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "v Tvé oči zřel jsem, dřív tak jasné,",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "Tvé"
                        },
                        {
                            "word_text": "oči"
                        },
                        {
                            "word_text": "zřel"
                        },
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "dřív"
                        },
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "jasné"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "(však nyní horečnatě bílé,)",
                    "metrical_pattern": "010100010",
                    "line_length": 9,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "však"
                        },
                        {
                            "word_text": "nyní"
                        },
                        {
                            "word_text": "horečnatě"
                        },
                        {
