                        {
                            "word_text": "zvuky"
                        },
                        {
                            "word_text": "hlásku"
                        },
                        {
                            "word_text": "mně"
                        },
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "milé"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "již zanikly, je bolest tíží...",
                    "metrical_pattern": "010001010",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "již"
                        },
                        {
                            "word_text": "zanikly"
                        },
                        {
                            "word_text": "je"
