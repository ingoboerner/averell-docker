                            "word_text": "a"
                        },
                        {
                            "word_text": "naslouchal"
                        },
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "Tvému"
                        },
                        {
                            "word_text": "dechu"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Jak matka chorobnému děcku",
                    "metrical_pattern": "010100010",
                    "line_length": 9,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Jak"
                        },
                        {
