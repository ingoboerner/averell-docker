                        {
                            "word_text": "cítě"
                        },
                        {
                            "word_text": "tíhu"
                        },
                        {
                            "word_text": "všecku"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Jas slunka plnil pokoj září,",
                    "metrical_pattern": "110101010",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Jas"
                        },
                        {
                            "word_text": "slunka"
