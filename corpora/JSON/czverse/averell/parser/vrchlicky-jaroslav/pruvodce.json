                        },
                        {
                            "word_text": "bystřicí"
                        },
                        {
                            "word_text": "sklene"
                        },
                        {
                            "word_text": "rozkypělou"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "A moře tu; on, jenž plášť strhne s ramen",
                    "metrical_pattern": "01000011010",
                    "line_length": 11,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "moře"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "jenž"
                        },
                        {
                            "word_text": "plášť"
                        },
                        {
                            "word_text": "strhne"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "ramen"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "a nad skráněmi červánkový plamen",
                    "metrical_pattern": "01000100010",
                    "line_length": 11,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "skráněmi"
                        },
                        {
                            "word_text": "červánkový"
                        },
                        {
                            "word_text": "plamen"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "mne jako Fausta nese nad vodami.",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "Fausta"
                        },
                        {
                            "word_text": "nese"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "vodami"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Noc byla; kdysi lesem šli jsme sami,",
                    "metrical_pattern": "11010101010",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Noc"
                        },
                        {
                            "word_text": "byla"
                        },
                        {
                            "word_text": "kdysi"
                        },
                        {
                            "word_text": "lesem"
                        },
                        {
                            "word_text": "šli"
                        },
                        {
                            "word_text": "jsme"
                        },
                        {
                            "word_text": "sami"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "laň s družinou se pásla u mýtiny,",
                    "metrical_pattern": "11000101000",
                    "line_length": 11,
                    "rhyme": 8,
                    "words": [
                        {
