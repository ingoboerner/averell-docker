                            "word_text": "a"
                        },
                        {
                            "word_text": "duši"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "písní"
                        },
                        {
                            "word_text": "medem"
                        },
                        {
                            "word_text": "sytím"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Bůh tvého vnitra, s tebou plám i cítím;",
                    "metrical_pattern": "11010101010",
                    "line_length": 11,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Bůh"
                        },
                        {
                            "word_text": "tvého"
                        },
                        {
                            "word_text": "vnitra"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "tebou"
                        },
                        {
                            "word_text": "plám"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "cítím"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "z tvé duše proto jen jsem ustoup’ zcela,",
                    "metrical_pattern": "01010001010",
                    "line_length": 11,
                    "rhyme": 16,
                    "words": [
                        {
