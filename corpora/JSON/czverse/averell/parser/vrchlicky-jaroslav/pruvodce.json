{
    "poem_title": "PRŮVODCE.",
    "author": "Vrchlický, Jaroslav",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Kam dojdu, nevím – ale vím, že spěje",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
                    "rhyme": 1,
                    "words": [
