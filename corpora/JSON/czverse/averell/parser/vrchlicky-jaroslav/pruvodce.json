                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "zbudí"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "však před básníkem kráčím strmou drahou,",
                    "metrical_pattern": "01000101010",
                    "line_length": 11,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "však"
                        },
                        {
                            "word_text": "před"
                        },
                        {
                            "word_text": "básníkem"
                        },
                        {
                            "word_text": "kráčím"
                        },
                        {
                            "word_text": "strmou"
                        },
                        {
                            "word_text": "drahou"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "jak mlhy sloup; rty jeho rosy vlahou",
                    "metrical_pattern": "01011101010",
                    "line_length": 11,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "mlhy"
                        },
                        {
                            "word_text": "sloup"
                        },
                        {
                            "word_text": "rty"
                        },
                        {
                            "word_text": "jeho"
