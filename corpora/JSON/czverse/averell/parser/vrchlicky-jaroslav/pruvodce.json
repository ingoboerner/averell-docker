                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "stín"
                        },
                        {
                            "word_text": "stromů"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "„Což neznáš mne? Spím v každé lidské hrudi;",
                    "metrical_pattern": "11001101010",
                    "line_length": 11,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Což"
                        },
                        {
                            "word_text": "neznáš"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "Spím"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "každé"
                        },
                        {
                            "word_text": "lidské"
                        },
                        {
                            "word_text": "hrudi"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "kdo hyne bídou, modlitbou mne zbudí,",
                    "metrical_pattern": "01010100010",
                    "line_length": 11,
                    "rhyme": 13,
                    "words": [
                        {
