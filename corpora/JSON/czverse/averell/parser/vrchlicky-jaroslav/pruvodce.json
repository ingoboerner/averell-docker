                        },
                        {
                            "word_text": "hůř"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "mém"
                        },
                        {
                            "word_text": "srdci"
                        },
                        {
                            "word_text": "běda"
                        },
                        {
                            "word_text": "není"
                        },
                        {
                            "word_text": "boha"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Tak pusto tam, jak na daleké pláni",
                    "metrical_pattern": "01000100010",
                    "line_length": 11,
                    "rhyme": 11,
                    "words": [
