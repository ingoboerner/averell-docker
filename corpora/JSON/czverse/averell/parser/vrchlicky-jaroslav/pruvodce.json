                            "word_text": "do"
                        },
                        {
                            "word_text": "vlhké"
                        },
                        {
                            "word_text": "trávy"
                        },
                        {
                            "word_text": "mdlá"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "vplítá"
                        },
                        {
                            "word_text": "noha"
                        }
                    ]
                },
