                        },
                        {
                            "word_text": "ale"
                        },
                        {
                            "word_text": "vím"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "spěje"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "duch přede mnou. Svit jeho obličeje",
                    "metrical_pattern": "11001101000",
                    "line_length": 11,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "duch"
                        },
                        {
                            "word_text": "přede"
                        },
                        {
                            "word_text": "mnou"
                        },
                        {
                            "word_text": "Svit"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "obličeje"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "mi v nocích svítí na neznámou dráhu.",
                    "metrical_pattern": "01010100010",
                    "line_length": 11,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "nocích"
                        },
                        {
                            "word_text": "svítí"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "neznámou"
                        },
                        {
                            "word_text": "dráhu"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Zda dojdu za ním ku domova prahu,",
                    "metrical_pattern": "01010100010",
                    "line_length": 11,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Zda"
                        },
                        {
                            "word_text": "dojdu"
                        },
                        {
                            "word_text": "za"
                        },
                        {
                            "word_text": "ním"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "domova"
                        },
                        {
                            "word_text": "prahu"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "já neptám se; má duše jemu věří.",
                    "metrical_pattern": "01000101010",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "já"
                        },
                        {
                            "word_text": "neptám"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "má"
                        },
                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "jemu"
                        },
                        {
                            "word_text": "věří"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Před námi propasť – on to, jenž ji změří,",
                    "metrical_pattern": "10010001010",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Před"
                        },
                        {
                            "word_text": "námi"
                        },
                        {
                            "word_text": "propasť"
                        },
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "jenž"
                        },
                        {
                            "word_text": "ji"
                        },
                        {
                            "word_text": "změří"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "já, který přejdu bez úrazu nohy.",
                    "metrical_pattern": "01010100010",
                    "line_length": 11,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "já"
                        },
                        {
                            "word_text": "který"
                        },
                        {
                            "word_text": "přejdu"
                        },
                        {
                            "word_text": "bez"
                        },
                        {
                            "word_text": "úrazu"
                        },
                        {
                            "word_text": "nohy"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "A lesem jdeme; sdrcený kmen mnohý",
                    "metrical_pattern": "01010100110",
                    "line_length": 11,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "lesem"
                        },
                        {
                            "word_text": "jdeme"
                        },
                        {
                            "word_text": "sdrcený"
                        },
                        {
                            "word_text": "kmen"
                        },
                        {
                            "word_text": "mnohý"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "nám trčí v cestu, on jej rukou smělou",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "nám"
                        },
                        {
                            "word_text": "trčí"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "cestu"
                        },
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "jej"
                        },
                        {
                            "word_text": "rukou"
                        },
                        {
                            "word_text": "smělou"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "v most nad bystřicí sklene rozkypělou.",
                    "metrical_pattern": "11000101000",
                    "line_length": 11,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "most"
                        },
                        {
