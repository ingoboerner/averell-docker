{
    "poem_title": "ŽALM Z BABYLONU.",
    "author": "Dostál-Lutinov, Karel",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Nuž zazpívej nám, otrokyně krásná!",
