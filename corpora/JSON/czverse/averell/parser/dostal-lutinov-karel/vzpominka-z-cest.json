        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Tu proti nám loď výletní",
                    "metrical_pattern": "01001100",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
