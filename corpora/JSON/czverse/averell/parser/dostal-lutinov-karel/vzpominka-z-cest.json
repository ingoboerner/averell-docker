        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "My velký svět – tam drobný lid -",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
