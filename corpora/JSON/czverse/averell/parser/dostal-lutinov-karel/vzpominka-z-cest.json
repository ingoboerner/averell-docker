        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Fadessa v tváři – hořký ret",
                    "metrical_pattern": "10010101",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Fadessa"
                        },
                        {
