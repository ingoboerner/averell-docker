        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Koho ten pustý příkrov chová?",
                    "metrical_pattern": "100101010",
                    "line_length": 9,
