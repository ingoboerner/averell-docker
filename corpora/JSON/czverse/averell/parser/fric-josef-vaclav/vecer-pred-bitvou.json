                    "metrical_pattern": "00100100",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "Morana"
                        },
                        {
                            "word_text": "zahude"
                        }
                    ]
