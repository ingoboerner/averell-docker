                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "derou"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "těžkou hlavou, duši žerou.",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "těžkou"
                        },
                        {
                            "word_text": "hlavou"
                        },
                        {
                            "word_text": "duši"
                        },
                        {
                            "word_text": "žerou"
                        }
                    ]
                }
