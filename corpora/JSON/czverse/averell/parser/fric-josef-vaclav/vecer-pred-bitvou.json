                        },
                        {
                            "word_text": "odcházel"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "někdo mne tam vyprovázel.",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "někdo"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "tam"
                        },
                        {
                            "word_text": "vyprovázel"
                        }
                    ]
                }
            ],
            "stanza_text": "Když jsem z domova odcházel,\nněkdo mne tam vyprovázel."
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Děva má mne provázela,",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Děva"
                        },
                        {
                            "word_text": "má"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "provázela"
                        }
                    ]
