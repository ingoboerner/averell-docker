                        },
                        {
                            "word_text": "tichou"
                        },
                        {
                            "word_text": "nocí"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "budu-liž pak plakat moci?",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "budu"
                        },
                        {
                            "word_text": "liž"
                        },
                        {
                            "word_text": "pak"
                        },
                        {
                            "word_text": "plakat"
                        },
                        {
                            "word_text": "moci"
                        }
                    ]
                }
            ],
            "stanza_text": "Až poplyne tichou nocí,\nbudu-liž pak plakat moci?"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Když mne smrtná kule schvátí,",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Když"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "smrtná"
                        },
                        {
                            "word_text": "kule"
                        },
                        {
                            "word_text": "schvátí"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "nebudu smět zaplakati.",
                    "metrical_pattern": "10011000",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "nebudu"
                        },
                        {
                            "word_text": "smět"
                        },
                        {
                            "word_text": "zaplakati"
                        }
                    ]
                }
            ],
            "stanza_text": "Když mne smrtná kule schvátí,\nnebudu smět zaplakati."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Když mi palaš protne hlavu,",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Když"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "palaš"
                        },
                        {
                            "word_text": "protne"
                        },
                        {
                            "word_text": "hlavu"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "nebudu víc myslit v stavu.",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "nebudu"
                        },
                        {
                            "word_text": "víc"
                        },
                        {
                            "word_text": "myslit"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "stavu"
                        }
                    ]
