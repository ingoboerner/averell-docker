                        {
                            "word_text": "ty"
                        },
                        {
                            "word_text": "máti"
                        },
                        {
                            "word_text": "moje"
                        },
                        {
                            "word_text": "milá"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "cos mne s bolem porodila,",
                    "metrical_pattern": "00101000",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "cos"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "bolem"
                        },
                        {
                            "word_text": "porodila"
                        }
                    ]
                }
            ],
            "stanza_text": "A ty, máti moje milá,\ncos mne s bolem porodila,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "S bolem jsi mne odkojila,",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "S"
