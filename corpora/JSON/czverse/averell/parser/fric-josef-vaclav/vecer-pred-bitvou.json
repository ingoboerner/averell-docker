                {
                    "line_number": 2,
                    "line_text": "až zas měsíc pozasvítí?",
                    "metrical_pattern": "00101000",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "až"
                        },
                        {
