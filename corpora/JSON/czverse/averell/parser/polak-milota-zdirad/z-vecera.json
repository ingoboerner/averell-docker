                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Lína"
                        },
                        {
                            "word_text": "ještě"
                        },
                        {
                            "word_text": "nepřichází"
                        }
                    ]
                }
