                        },
                        {
                            "word_text": "se"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "S oblibováním?",
                    "metrical_pattern": "10000",
                    "line_length": 5,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "S"
                        },
                        {
                            "word_text": "oblibováním"
                        }
                    ]
