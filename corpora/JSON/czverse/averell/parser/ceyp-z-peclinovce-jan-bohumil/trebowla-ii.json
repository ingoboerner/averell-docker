        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "„Neplačte!“ dí vůdce temným hlasem,",
                    "metrical_pattern": "1001101010",
                    "line_length": 10,
                    "rhyme": null,
