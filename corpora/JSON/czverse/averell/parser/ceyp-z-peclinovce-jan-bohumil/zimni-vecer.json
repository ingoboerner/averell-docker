{
    "poem_title": "Zimní večer.",
    "author": "Ceyp z Peclinovce, Jan Bohumil",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "„Dědoušek, náš dědoušek přichází!“",
                    "metrical_pattern": "1000100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Dědoušek"
                        },
                        {
                            "word_text": "náš"
                        },
                        {
                            "word_text": "dědoušek"
                        },
                        {
                            "word_text": "přichází"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Volá mládež podál dveří,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Volá"
                        },
                        {
                            "word_text": "mládež"
                        },
                        {
                            "word_text": "podál"
                        },
                        {
                            "word_text": "dveří"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "A již harfeníka obstupují.",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "již"
                        },
                        {
                            "word_text": "harfeníka"
                        },
                        {
                            "word_text": "obstupují"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Hospodyně dere peří,",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Hospodyně"
                        },
                        {
                            "word_text": "dere"
                        },
                        {
                            "word_text": "peří"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Po dni hospodář si odpočívá.",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Po"
                        },
                        {
                            "word_text": "dni"
                        },
                        {
                            "word_text": "hospodář"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "odpočívá"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Kraj se zmítá v chumelici.",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Kraj"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "zmítá"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "chumelici"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "„Pojď sem, dobrá duše, tu si sedni,",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pojď"
                        },
                        {
                            "word_text": "sem"
                        },
                        {
                            "word_text": "dobrá"
                        },
                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "sedni"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "A pěj nám zde ve světnici!",
                    "metrical_pattern": "01001000",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "pěj"
                        },
                        {
                            "word_text": "nám"
                        },
                        {
                            "word_text": "zde"
                        },
                        {
                            "word_text": "ve"
                        },
                        {
                            "word_text": "světnici"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Rád ti tatíček dá nocleh, někdo",
                    "metrical_pattern": "1010011010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Rád"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "tatíček"
                        },
                        {
                            "word_text": "dá"
                        },
                        {
                            "word_text": "nocleh"
                        },
                        {
                            "word_text": "někdo"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Z našich pro pivo ti skočí,",
                    "metrical_pattern": "10100010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Z"
                        },
                        {
                            "word_text": "našich"
                        },
                        {
                            "word_text": "pro"
                        },
                        {
                            "word_text": "pivo"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "skočí"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Jen nám zazpívej a zůstaň u nás!“",
                    "metrical_pattern": "0010001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jen"
                        },
                        {
                            "word_text": "nám"
                        },
                        {
                            "word_text": "zazpívej"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "zůstaň"
                        },
                        {
                            "word_text": "u"
                        },
                        {
                            "word_text": "nás"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Tu harfeník dále kročí:",
                    "metrical_pattern": "01001010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "harfeník"
                        },
                        {
                            "word_text": "dále"
                        },
                        {
                            "word_text": "kročí"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "„Dobrý večer pohostinní lidé!",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Dobrý"
                        },
                        {
                            "word_text": "večer"
                        },
                        {
                            "word_text": "pohostinní"
                        },
                        {
                            "word_text": "lidé"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Neveselo v kraji nyní.",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Neveselo"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "kraji"
                        },
                        {
                            "word_text": "nyní"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Vůkol ledná poušť jen prostírá se;",
                    "metrical_pattern": "1010101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vůkol"
                        },
                        {
                            "word_text": "ledná"
                        },
                        {
                            "word_text": "poušť"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "prostírá"
                        },
                        {
                            "word_text": "se"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Stromy kryje sníh a jíní,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Stromy"
                        },
                        {
                            "word_text": "kryje"
                        },
                        {
                            "word_text": "sníh"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "jíní"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Mlčí mrazně utuhlý potůček,",
                    "metrical_pattern": "1010100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Mlčí"
                        },
                        {
                            "word_text": "mrazně"
                        },
                        {
                            "word_text": "utuhlý"
                        },
                        {
                            "word_text": "potůček"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Sad nehostí zvěře ptačí,",
                    "metrical_pattern": "11001010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Sad"
                        },
                        {
                            "word_text": "nehostí"
                        },
                        {
                            "word_text": "zvěře"
                        },
                        {
                            "word_text": "ptačí"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "V šírém poli nespatříš človíčka,",
                    "metrical_pattern": "1010100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "šírém"
                        },
                        {
                            "word_text": "poli"
                        },
                        {
                            "word_text": "nespatříš"
                        },
                        {
                            "word_text": "človíčka"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Obloha se děsně mračí.",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Obloha"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "děsně"
                        },
                        {
                            "word_text": "mračí"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Mílo nyní dlíti v teplé jistbě.",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Mílo"
                        },
                        {
                            "word_text": "nyní"
                        },
                        {
                            "word_text": "dlíti"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "teplé"
                        },
                        {
                            "word_text": "jistbě"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Budiž, tedy zanotuji!",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Budiž"
                        },
                        {
                            "word_text": "tedy"
                        },
                        {
                            "word_text": "zanotuji"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Jako májový déšť svadlou pažiť,",
                    "metrical_pattern": "1010011010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jako"
                        },
                        {
                            "word_text": "májový"
                        },
                        {
                            "word_text": "déšť"
                        },
                        {
                            "word_text": "svadlou"
                        },
                        {
                            "word_text": "pažiť"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Tak mne písně zotavují;",
                    "metrical_pattern": "00101000",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Tak"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "písně"
                        },
                        {
                            "word_text": "zotavují"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Budiž, povím vám vše, co jsem slyšel,",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Budiž"
                        },
                        {
                            "word_text": "povím"
                        },
                        {
                            "word_text": "vám"
                        },
                        {
                            "word_text": "vše"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "slyšel"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Co se dálo během času,“",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Co"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "dálo"
                        },
                        {
                            "word_text": "během"
                        },
                        {
                            "word_text": "času"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Vece kmet, a u krbu se hostí.",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vece"
                        },
                        {
                            "word_text": "kmet"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "u"
                        },
                        {
                            "word_text": "krbu"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "hostí"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Odrostkové tonou v jasu,",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Odrostkové"
                        },
                        {
                            "word_text": "tonou"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "jasu"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Zůstavují večerní své dílo,",
                    "metrical_pattern": "1000100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zůstavují"
                        },
                        {
                            "word_text": "večerní"
                        },
                        {
                            "word_text": "své"
                        },
                        {
                            "word_text": "dílo"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "A lnou k milenému druhu.",
                    "metrical_pattern": "01100010",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "lnou"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "milenému"
                        },
                        {
                            "word_text": "druhu"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Tu harfeník po stroji svém sáhne,",
                    "metrical_pattern": "0100100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "harfeník"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "stroji"
                        },
                        {
                            "word_text": "svém"
                        },
                        {
                            "word_text": "sáhne"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Zladí jej a zpívá v kruhu.",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Zladí"
                        },
                        {
                            "word_text": "jej"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "zpívá"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "kruhu"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Hospodyně časem draní staví,",
                    "metrical_pattern": "1000101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hospodyně"
                        },
                        {
                            "word_text": "časem"
                        },
                        {
                            "word_text": "draní"
                        },
                        {
                            "word_text": "staví"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "A na zpěv si pozor dává;",
                    "metrical_pattern": "01001010",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "zpěv"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "pozor"
                        },
                        {
                            "word_text": "dává"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Chvílemi i hospodář naslouchá,",
                    "metrical_pattern": "1000100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Chvílemi"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "hospodář"
                        },
                        {
                            "word_text": "naslouchá"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "A potichu z lože vstává.",
                    "metrical_pattern": "01001010",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "potichu"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "lože"
                        },
                        {
                            "word_text": "vstává"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Lakotně zpěv ssají odrostkové,",
                    "metrical_pattern": "1001101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lakotně"
                        },
                        {
                            "word_text": "zpěv"
                        },
                        {
                            "word_text": "ssají"
                        },
                        {
                            "word_text": "odrostkové"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Žijí jenom svému sluchu,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Žijí"
                        },
                        {
                            "word_text": "jenom"
                        },
                        {
                            "word_text": "svému"
                        },
                        {
                            "word_text": "sluchu"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "Pohromadě táboří se podál",
                    "metrical_pattern": "1000100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pohromadě"
                        },
                        {
                            "word_text": "táboří"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "podál"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Pěvce, a plesají v duchu.",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Pěvce"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "plesají"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "duchu"
                        }
                    ]
