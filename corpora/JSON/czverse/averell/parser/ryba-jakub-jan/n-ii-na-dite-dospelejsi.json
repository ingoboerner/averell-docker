            "stanza_text": "Pospěšte sem bratříčkové,\nPojďte kemně sestřičky!\nZazpívejte zpěváčkové\nLibě u mé rakvičky!"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Poslední můj přibyteček",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Poslední"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "přibyteček"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Vonným kvítím skrášlete –",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Vonným"
                        },
                        {
                            "word_text": "kvítím"
                        },
                        {
                            "word_text": "skrášlete"
                        }
