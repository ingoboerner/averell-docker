                    "line_number": 28,
                    "line_text": "To vám věrně vinšuji.",
                    "metrical_pattern": "0010100",
                    "line_length": 7,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "To"
                        },
                        {
                            "word_text": "vám"
                        },
                        {
                            "word_text": "věrně"
                        },
                        {
                            "word_text": "vinšuji"
                        }
                    ]
                }
