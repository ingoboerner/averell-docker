    "poem_title": "N. II. Na Dítě dospělejší.",
    "author": "Ryba, Jakub Jan",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Pospěšte sem bratříčkové,",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
