                        },
                        {
                            "word_text": "byl"
                        },
                        {
                            "word_text": "dosud"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "bude"
                        },
                        {
                            "word_text": "těžký"
                        },
                        {
                            "word_text": "a"
