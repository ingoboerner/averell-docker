{
    "poem_title": "Zříceniny Zvíkovské.1 (III.)",
    "author": "Koubek, Jan Pravoslav",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "„Nesvornost – (tak tajné runy znié)",
                    "metrical_pattern": "100010100",
                    "line_length": 9,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Nesvornost"
                        },
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "tajné"
                        },
                        {
                            "word_text": "runy"
                        },
                        {
                            "word_text": "znié"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "A neláska k svojim pobratřencóm,",
                    "metrical_pattern": "0100101000",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "neláska"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "svojim"
                        },
                        {
                            "word_text": "pobratřencóm"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Ba i odščepenstvo kmenovnié,",
                    "metrical_pattern": "001000100",
                    "line_length": 9,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Ba"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "odščepenstvo"
                        },
                        {
                            "word_text": "kmenovnié"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Oddanost že slepá k cuzozemcóm,",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Oddanost"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "slepá"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "cuzozemcóm"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Svár, co kališnické rozžže války,",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Svár"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "kališnické"
                        },
                        {
                            "word_text": "rozžže"
                        },
                        {
                            "word_text": "války"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Až i po vítězstvě dvúvěkém",
                    "metrical_pattern": "001000100",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Až"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "vítězstvě"
                        },
                        {
                            "word_text": "dvúvěkém"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "S královým sie skonči útěkem,",
                    "metrical_pattern": "100010100",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "S"
                        },
                        {
                            "word_text": "královým"
                        },
                        {
                            "word_text": "sie"
                        },
                        {
                            "word_text": "skonči"
                        },
                        {
                            "word_text": "útěkem"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "A hle národ klesnu do zahálky,",
                    "metrical_pattern": "0110101000",
                    "line_length": 10,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "hle"
                        },
                        {
                            "word_text": "národ"
                        },
                        {
                            "word_text": "klesnu"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "zahálky"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Nevázanost jedněch, spiatost druhých,",
                    "metrical_pattern": "1000101010",
                    "line_length": 10,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Nevázanost"
                        },
                        {
                            "word_text": "jedněch"
                        },
                        {
                            "word_text": "spiatost"
                        },
                        {
                            "word_text": "druhých"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "A předsudkóv vláda vítězná,",
                    "metrical_pattern": "010010100",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "předsudkóv"
                        },
                        {
                            "word_text": "vláda"
                        },
                        {
                            "word_text": "vítězná"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Jejéž ruka hrubě železná",
                    "metrical_pattern": "101010100",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Jejéž"
                        },
                        {
                            "word_text": "ruka"
                        },
                        {
                            "word_text": "hrubě"
                        },
                        {
                            "word_text": "železná"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Spiáše všecky stavy do pút tuhých:",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Spiáše"
                        },
                        {
                            "word_text": "všecky"
                        },
                        {
                            "word_text": "stavy"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "pút"
                        },
                        {
                            "word_text": "tuhých"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Toť sú byly viny vašich dědóv;",
                    "metrical_pattern": "0010101010",
                    "line_length": 10,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Toť"
                        },
                        {
                            "word_text": "sú"
                        },
                        {
                            "word_text": "byly"
                        },
                        {
                            "word_text": "viny"
                        },
                        {
                            "word_text": "vašich"
                        },
                        {
                            "word_text": "dědóv"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Chraňte sie jich co dědičných vředóv,",
                    "metrical_pattern": "1000010010",
                    "line_length": 10,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Chraňte"
                        },
                        {
                            "word_text": "sie"
                        },
                        {
                            "word_text": "jich"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "dědičných"
                        },
                        {
                            "word_text": "vředóv"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Medle držte se jich stkvúcích cností",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Medle"
                        },
                        {
                            "word_text": "držte"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jich"
                        },
                        {
                            "word_text": "stkvúcích"
                        },
                        {
                            "word_text": "cností"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "A svobodni súce od jich bludóv",
                    "metrical_pattern": "0100101010",
                    "line_length": 10,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "svobodni"
                        },
                        {
                            "word_text": "súce"
                        },
                        {
                            "word_text": "od"
                        },
                        {
                            "word_text": "jich"
                        },
                        {
                            "word_text": "bludóv"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Dojdete dřév tejto velikosti,",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Dojdete"
                        },
                        {
                            "word_text": "dřév"
                        },
                        {
                            "word_text": "tejto"
                        },
                        {
                            "word_text": "velikosti"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Jež nám určena jest od osudóv.",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Jež"
                        },
                        {
                            "word_text": "nám"
                        },
                        {
                            "word_text": "určena"
                        },
                        {
                            "word_text": "jest"
                        },
                        {
                            "word_text": "od"
                        },
                        {
                            "word_text": "osudóv"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Přede všiém svornosti buďte pilni,",
                    "metrical_pattern": "1001001010",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Přede"
                        },
                        {
                            "word_text": "všiém"
                        },
                        {
                            "word_text": "svornosti"
                        },
                        {
                            "word_text": "buďte"
                        },
                        {
                            "word_text": "pilni"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Nebo mysli shodné v obci svorné5",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Nebo"
                        },
                        {
                            "word_text": "mysli"
                        },
                        {
                            "word_text": "shodné"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "obci"
                        },
                        {
                            "word_text": "svorné5"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Svazkové sú srdec nerozdílní,",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Svazkové"
                        },
                        {
                            "word_text": "sú"
                        },
                        {
                            "word_text": "srdec"
                        },
                        {
                            "word_text": "nerozdílní"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Pevnější než hradby nerozborné,",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Pevnější"
                        },
                        {
                            "word_text": "než"
                        },
                        {
                            "word_text": "hradby"
                        },
                        {
                            "word_text": "nerozborné"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Než Zvěkovské věže věkozdorné!",
                    "metrical_pattern": "0100101000",
                    "line_length": 10,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Než"
                        },
                        {
                            "word_text": "Zvěkovské"
                        },
                        {
                            "word_text": "věže"
                        },
                        {
                            "word_text": "věkozdorné"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Láska k vlasti, ta bez chtiéče ziska,",
                    "metrical_pattern": "1010010010",
                    "line_length": 10,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Láska"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "vlasti"
                        },
                        {
                            "word_text": "ta"
                        },
                        {
                            "word_text": "bez"
                        },
                        {
                            "word_text": "chtiéče"
                        },
                        {
                            "word_text": "ziska"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "A národstvié múdré, bez nátiska",
                    "metrical_pattern": "0100101000",
                    "line_length": 10,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "národstvié"
                        },
                        {
                            "word_text": "múdré"
                        },
                        {
                            "word_text": "bez"
                        },
                        {
                            "word_text": "nátiska"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Cuzie, jinotvárnéj národnosti,",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Cuzie"
                        },
                        {
                            "word_text": "jinotvárnéj"
                        },
                        {
                            "word_text": "národnosti"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Buďtež neodstupné od vás cnosti.",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Buďtež"
                        },
                        {
                            "word_text": "neodstupné"
                        },
                        {
                            "word_text": "od"
                        },
                        {
                            "word_text": "vás"
                        },
                        {
                            "word_text": "cnosti"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Jedni hrdost, druzí podlost sprostú",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Jedni"
                        },
                        {
                            "word_text": "hrdost"
                        },
                        {
                            "word_text": "druzí"
                        },
                        {
                            "word_text": "podlost"
                        },
                        {
                            "word_text": "sprostú"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Ze svých srdec rázem vyžeňte,",
                    "metrical_pattern": "101010100",
                    "line_length": 9,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Ze"
                        },
                        {
                            "word_text": "svých"
                        },
                        {
                            "word_text": "srdec"
                        },
                        {
                            "word_text": "rázem"
                        },
                        {
                            "word_text": "vyžeňte"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "V národniú jú hrdost proměňte,",
                    "metrical_pattern": "100110100",
                    "line_length": 9,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "národniú"
                        },
                        {
                            "word_text": "jú"
                        },
                        {
                            "word_text": "hrdost"
                        },
                        {
                            "word_text": "proměňte"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "A uzřiéte kakie žně vám vzrostú:",
                    "metrical_pattern": "0100101010",
                    "line_length": 10,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "uzřiéte"
                        },
                        {
                            "word_text": "kakie"
                        },
                        {
                            "word_text": "žně"
                        },
                        {
                            "word_text": "vám"
                        },
                        {
                            "word_text": "vzrostú"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Neb ta čacká hrdost národnié,",
                    "metrical_pattern": "0010101000",
                    "line_length": 10,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Neb"
                        },
                        {
                            "word_text": "ta"
                        },
                        {
                            "word_text": "čacká"
                        },
                        {
                            "word_text": "hrdost"
                        },
                        {
                            "word_text": "národnié"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Osvětú když bude podněcena,",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "Osvětú"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "bude"
                        },
                        {
                            "word_text": "podněcena"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Vám vlastenskú roli zúrodnié,",
                    "metrical_pattern": "0100101000",
                    "line_length": 10,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Vám"
                        },
                        {
                            "word_text": "vlastenskú"
                        },
                        {
                            "word_text": "roli"
                        },
                        {
                            "word_text": "zúrodnié"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Že se vaše setba zazelená,",
                    "metrical_pattern": "0010101000",
                    "line_length": 10,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "Že"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "vaše"
                        },
                        {
                            "word_text": "setba"
                        },
                        {
                            "word_text": "zazelená"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "I že miésto pustej, prázdnej řasy",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "I"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "miésto"
                        },
                        {
                            "word_text": "pustej"
                        },
                        {
                            "word_text": "prázdnej"
                        },
                        {
                            "word_text": "řasy"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Ponese vám juž plznějšie klasy,",
                    "metrical_pattern": "1000010010",
                    "line_length": 10,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "Ponese"
                        },
                        {
                            "word_text": "vám"
                        },
                        {
                            "word_text": "juž"
                        },
                        {
                            "word_text": "plznějšie"
                        },
                        {
                            "word_text": "klasy"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Juž i polských květóv do věncóv,",
                    "metrical_pattern": "001010100",
                    "line_length": 9,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Juž"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "polských"
                        },
                        {
                            "word_text": "květóv"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "věncóv"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "By sie jimi mužie ozdobili,",
                    "metrical_pattern": "0010101000",
                    "line_length": 10,
                    "rhyme": 20,
                    "words": [
                        {
                            "word_text": "By"
                        },
                        {
                            "word_text": "sie"
                        },
                        {
                            "word_text": "jimi"
                        },
                        {
                            "word_text": "mužie"
                        },
                        {
                            "word_text": "ozdobili"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Již by věru dostojnější byli",
                    "metrical_pattern": "0010100010",
                    "line_length": 10,
                    "rhyme": 20,
                    "words": [
                        {
                            "word_text": "Již"
                        },
                        {
                            "word_text": "by"
                        },
                        {
                            "word_text": "věru"
                        },
                        {
                            "word_text": "dostojnější"
                        },
                        {
                            "word_text": "byli"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "Jmene jinej trsti vlastencóv!",
                    "metrical_pattern": "101010100",
                    "line_length": 9,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Jmene"
                        },
                        {
                            "word_text": "jinej"
