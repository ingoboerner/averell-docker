                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "rozveselí"
                        },
                        {
                            "word_text": "já"
                        },
                        {
                            "word_text": "pak"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "povídati"
                        },
                        {
                            "word_text": "začnu"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Jak švarné moje vlasť dívčiny v lůně chová;",
                    "metrical_pattern": "0101001001010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
