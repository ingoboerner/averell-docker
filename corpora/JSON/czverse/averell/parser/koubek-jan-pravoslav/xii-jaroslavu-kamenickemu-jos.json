                        },
                        {
                            "word_text": "ústa"
                        },
                        {
                            "word_text": "zavřeš"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Lháře pronikneš okem pravdoskumným, kdyby ráčil",
                    "metrical_pattern": "101001010001010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lháře"
                        },
                        {
