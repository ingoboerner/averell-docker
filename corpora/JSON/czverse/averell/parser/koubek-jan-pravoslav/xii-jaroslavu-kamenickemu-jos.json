                        {
                            "word_text": "sklenicí"
                        },
                        {
                            "word_text": "hlubokou"
                        },
                        {
                            "word_text": "budeme"
                        },
                        {
                            "word_text": "blaho"
                        },
                        {
                            "word_text": "píti"
                        },
                        {
                            "word_text": "panenské"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Zdrávo budiž české děvče! volám hlasitě;",
                    "metrical_pattern": "1010101010100",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
