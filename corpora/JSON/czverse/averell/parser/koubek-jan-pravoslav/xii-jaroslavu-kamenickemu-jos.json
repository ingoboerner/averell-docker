                        },
                        {
                            "word_text": "vlasť"
                        },
                        {
                            "word_text": "dívčiny"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "lůně"
                        },
                        {
                            "word_text": "chová"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Ty znovu rozhněvaná sladkou ztrestáš mě hubinkou,",
                    "metrical_pattern": "010100010100100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
                        },
                        {
                            "word_text": "znovu"
                        },
                        {
                            "word_text": "rozhněvaná"
                        },
                        {
                            "word_text": "sladkou"
                        },
                        {
