                        },
                        {
                            "word_text": "nalej"
                        },
                        {
                            "word_text": "Zdráv"
                        },
                        {
                            "word_text": "buď"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "Josefoviči"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Věhlasem otcovským dědici zásobený,",
                    "metrical_pattern": "1001001001000",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Věhlasem"
                        },
                        {
                            "word_text": "otcovským"
                        },
                        {
                            "word_text": "dědici"
                        },
                        {
                            "word_text": "zásobený"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Ty mne chudého k štědrosti vzýváš jsa boháčem,",
                    "metrical_pattern": "00100100100100",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "chudého"
                        },
                        {
                            "word_text": "k"
