{
    "poem_title": "XII. Jaroslavu Kamenickému, Jos.Jungmannovi a Jaroslavu Langerovi,",
    "author": "Koubek, Jan Pravoslav",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Dnes do propasti časů minulých pohřbí se jeden rok.",
                    "metrical_pattern": "0100010100100101",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Dnes"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "propasti"
                        },
                        {
                            "word_text": "časů"
                        },
                        {
                            "word_text": "minulých"
                        },
                        {
                            "word_text": "pohřbí"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jeden"
                        },
                        {
                            "word_text": "rok"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Hej zatatranského spěšně, Hrihori9, nalej!",
                    "metrical_pattern": "1100001010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hej"
                        },
                        {
                            "word_text": "zatatranského"
                        },
                        {
                            "word_text": "spěšně"
                        },
                        {
                            "word_text": "Hrihori9"
                        },
                        {
                            "word_text": "nalej"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Já budu nad Dněstrem blaho píti Čechů krajanů svých,",
                    "metrical_pattern": "0101001010101000",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Já"
                        },
                        {
                            "word_text": "budu"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "Dněstrem"
                        },
                        {
                            "word_text": "blaho"
                        },
                        {
                            "word_text": "píti"
                        },
                        {
                            "word_text": "Čechů"
                        },
                        {
                            "word_text": "krajanů"
                        },
                        {
                            "word_text": "svých"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Hlásati pochvalu jich, kroutě si vousy mokré. –",
                    "metrical_pattern": "10010001001010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hlásati"
                        },
                        {
                            "word_text": "pochvalu"
                        },
                        {
                            "word_text": "jich"
                        },
                        {
                            "word_text": "kroutě"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "vousy"
                        },
                        {
                            "word_text": "mokré"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Jest první koflík pro Jaroslava prostolibého,",
                    "metrical_pattern": "010101000010000",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jest"
                        },
                        {
                            "word_text": "první"
                        },
                        {
                            "word_text": "koflík"
                        },
                        {
                            "word_text": "pro"
                        },
                        {
                            "word_text": "Jaroslava"
                        },
                        {
                            "word_text": "prostolibého"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Ten, jenž národní národu písně pěje. –",
                    "metrical_pattern": "001001001010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ten"
                        },
                        {
                            "word_text": "jenž"
                        },
                        {
                            "word_text": "národní"
                        },
                        {
                            "word_text": "národu"
                        },
                        {
                            "word_text": "písně"
                        },
                        {
                            "word_text": "pěje"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Vrchovatě mně nalej! Zdráv buď, můj Josefoviči!",
                    "metrical_pattern": "100001010010000",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
