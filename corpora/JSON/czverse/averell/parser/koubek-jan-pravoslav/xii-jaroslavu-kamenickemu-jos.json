                        },
                        {
                            "word_text": "Kopřiváriuse"
                        },
                        {
                            "word_text": "zdráv"
                        },
                        {
                            "word_text": "buď"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "dobře"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "měj"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Proč se tak usmíváš potutelně, Rusínko milostná?",
                    "metrical_pattern": "0001001000100100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Proč"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "usmíváš"
                        },
                        {
                            "word_text": "potutelně"
                        },
                        {
                            "word_text": "Rusínko"
                        },
                        {
                            "word_text": "milostná"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Pojď na slovíčko tu dál, černobrivaja10 děvo!",
                    "metrical_pattern": "11000001000010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
