                        },
                        {
                            "word_text": "doplyne"
                        },
                        {
                            "word_text": "rok"
                        },
                        {
                            "word_text": "připrav"
                        },
                        {
                            "word_text": "čaje"
                        },
                        {
                            "word_text": "nápoj"
                        },
                        {
                            "word_text": "horoucí"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Až se přiblíží půlnoci čas čekaný,",
                    "metrical_pattern": "001001001100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Až"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "přiblíží"
                        },
                        {
                            "word_text": "půlnoci"
                        },
                        {
                            "word_text": "čas"
                        },
                        {
                            "word_text": "čekaný"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Vluď se do mého bytu, vždyť nám třeba vítati hosta",
                    "metrical_pattern": "1010010101010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vluď"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "mého"
                        },
                        {
                            "word_text": "bytu"
                        },
