            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Kde, kde dítky otce svého máte,",
                    "metrical_pattern": "0010101010",
                    "line_length": 10,
                    "rhyme": 13,
                    "words": [
