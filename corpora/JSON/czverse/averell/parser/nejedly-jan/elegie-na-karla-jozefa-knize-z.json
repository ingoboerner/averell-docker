                        {
                            "word_text": "jsy"
                        },
                        {
                            "word_text": "volají"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Hořem opuštění tisýcové",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Hořem"
                        },
                        {
                            "word_text": "opuštění"
                        },
                        {
                            "word_text": "tisýcové"
                        }
                    ]
                },
                {
                    "line_number": 38,
