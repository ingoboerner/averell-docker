                    "metrical_pattern": "100010100",
                    "line_length": 9,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Vyrážeti"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "žravé"
                        },
                        {
                            "word_text": "žalosti"
                        }
                    ]
                },
