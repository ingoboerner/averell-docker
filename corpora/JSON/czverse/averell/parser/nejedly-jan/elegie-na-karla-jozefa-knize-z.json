                        {
                            "word_text": "kvítek"
                        },
                        {
                            "word_text": "svadnul"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Kvěltež ho, o věrní Čechové!",
                    "metrical_pattern": "100100100",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Kvěltež"
