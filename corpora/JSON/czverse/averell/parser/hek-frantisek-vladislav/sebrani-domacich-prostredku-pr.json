            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Hojný stateček zděď, byť tisíce vynesl skrovně;",
                    "metrical_pattern": "101001010010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
