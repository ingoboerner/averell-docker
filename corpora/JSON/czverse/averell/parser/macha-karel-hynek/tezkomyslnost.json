            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "A proč teskním? – že nevzešlo kvítí,",
                    "metrical_pattern": "1010010010",
                    "line_length": 10,
                    "rhyme": 9,
                    "words": [
