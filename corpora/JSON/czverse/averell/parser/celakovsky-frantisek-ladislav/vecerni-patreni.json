                        {
                            "word_text": "připomínajecího"
                        },
                        {
                            "word_text": "klekání"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Z proutí šum vrbového tichý vyniká u potůčku,",
                    "metrical_pattern": "1011000101001000",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
