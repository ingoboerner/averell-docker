                        },
                        {
                            "word_text": "pahorku"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Tihne krokem, veselou si klučík vzadu zpěvkuje píseň.",
                    "metrical_pattern": "10101000101010010",
                    "line_length": 17,
                    "rhyme": null,
