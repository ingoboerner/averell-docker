                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "miláčku"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Už nám na věky se rozejíti;",
                    "metrical_pattern": "0010001000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Už"
