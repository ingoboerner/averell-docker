                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "moje"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Ty družko nelom ruce bílé!",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
