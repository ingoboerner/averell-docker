                        },
                        {
                            "word_text": "břehu"
                        },
                        {
                            "word_text": "vyroubím"
                        },
                        {
                            "word_text": "rychlou"
                        },
                        {
                            "word_text": "loďku"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "K ostrovu na ní se doplavíme;",
                    "metrical_pattern": "1001001000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "K"
                        },
                        {
                            "word_text": "ostrovu"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "ní"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "doplavíme"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Na ostrovu vystavíme město,",
                    "metrical_pattern": "1000100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Na"
