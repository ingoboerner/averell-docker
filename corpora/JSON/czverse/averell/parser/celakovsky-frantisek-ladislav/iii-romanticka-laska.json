                        {
                            "word_text": "kraji"
                        },
                        {
                            "word_text": "mezi"
                        },
                        {
                            "word_text": "horami"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Proteká jezero velké velmi,",
                    "metrical_pattern": "1001001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
