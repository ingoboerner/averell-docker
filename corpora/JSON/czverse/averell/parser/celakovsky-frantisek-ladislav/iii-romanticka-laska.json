                        },
                        {
                            "word_text": "přebývati"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Až do věku, až do smrti naší. –",
                    "metrical_pattern": "0100010010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Až"
                        },
                        {
                            "word_text": "do"
                        },
                        {
