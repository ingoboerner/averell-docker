                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kam"
                        },
                        {
                            "word_text": "kvítečky"
                        },
                        {
                            "word_text": "červené"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "modré"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Kdež tam otce, kde matičku najdu,",
                    "metrical_pattern": "0010010010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kdež"
                        },
                        {
                            "word_text": "tam"
                        },
                        {
                            "word_text": "otce"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "matičku"
                        },
                        {
                            "word_text": "najdu"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Kde sestřičky, mé milé družičky? –",
                    "metrical_pattern": "0100010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
