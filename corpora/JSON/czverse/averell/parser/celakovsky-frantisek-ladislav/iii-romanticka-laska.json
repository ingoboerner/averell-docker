                        },
                        {
                            "word_text": "odpověděl"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Ty neplač, neplač, duše moje!",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
                        },
                        {
                            "word_text": "neplač"
