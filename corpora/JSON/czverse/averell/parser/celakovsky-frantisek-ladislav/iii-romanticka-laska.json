                        },
                        {
                            "word_text": "promluvila"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ty s bohem buď, s bohem, druhu milý!",
                    "metrical_pattern": "0100101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
