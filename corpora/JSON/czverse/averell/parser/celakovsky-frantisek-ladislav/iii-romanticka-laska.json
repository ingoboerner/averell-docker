                        {
                            "word_text": "věky"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "rozejíti"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Otec, matka tebe nemilují,",
                    "metrical_pattern": "1010101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Otec"
                        },
                        {
