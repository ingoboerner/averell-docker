                        },
                        {
                            "word_text": "sestřičky"
                        },
                        {
                            "word_text": "mé"
                        },
                        {
                            "word_text": "milé"
                        },
                        {
                            "word_text": "družičky"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "„Všude najdeš zas, milá, zahrádku,",
                    "metrical_pattern": "1010010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Všude"
                        },
                        {
                            "word_text": "najdeš"
                        },
                        {
                            "word_text": "zas"
                        },
                        {
                            "word_text": "milá"
                        },
                        {
                            "word_text": "zahrádku"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Kam pohlédneš, modráčky vyrostou,",
                    "metrical_pattern": "0100100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kam"
                        },
                        {
                            "word_text": "pohlédneš"
                        },
                        {
                            "word_text": "modráčky"
                        },
                        {
                            "word_text": "vyrostou"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "Kde umyješ líce, růže zkvetou;",
                    "metrical_pattern": "0100101010",
                    "line_length": 10,
                    "rhyme": null,
