                        },
                        {
                            "word_text": "vesničku"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "V té budeme v lásce přebývati",
                    "metrical_pattern": "0100101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "té"
