                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ach"
                        },
                        {
                            "word_text": "kam"
                        },
                        {
                            "word_text": "poděje"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "má"
                        },
                        {
                            "word_text": "zahrádka"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Kam kvítečky, červené a modré?",
