                    "line_text": "Máš z toho pěknou čest!“",
                    "metrical_pattern": "110101",
                    "line_length": 6,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Máš"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "toho"
                        },
                        {
                            "word_text": "pěknou"
                        },
                        {
                            "word_text": "čest"
                        }
