            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Ty mne viníš, že proti snaživým věku bojuji cílům,",
                    "metrical_pattern": "00100101001010010",
                    "line_length": 17,
                    "rhyme": null,
                    "words": [
