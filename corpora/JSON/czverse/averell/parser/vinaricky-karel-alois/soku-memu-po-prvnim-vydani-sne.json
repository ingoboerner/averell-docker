                        {
                            "word_text": "prolévali"
                        },
                        {
                            "word_text": "krev"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Křivdu činíš mi, jiný mé bájce přičítaje záměr;",
                    "metrical_pattern": "1010010010100010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Křivdu"
                        },
                        {
                            "word_text": "činíš"
