                        },
                        {
                            "word_text": "biješ"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Znám věku cíl, tvůj cíl neznám, mým pravda je cílem;",
                    "metrical_pattern": "11010110010010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
