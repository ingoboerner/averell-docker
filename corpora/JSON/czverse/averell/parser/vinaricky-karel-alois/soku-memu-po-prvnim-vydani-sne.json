                        },
                        {
                            "word_text": "světlo"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Tvé bludné jen nad výpary bahna kmitá.",
                    "metrical_pattern": "010010001010",
                    "line_length": 12,
                    "rhyme": null,
