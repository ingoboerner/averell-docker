                        },
                        {
                            "word_text": "ubíháš"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "odpor nejvyšší lidstva idéi kladeš.",
                    "metrical_pattern": "101001010010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "odpor"
                        },
                        {
