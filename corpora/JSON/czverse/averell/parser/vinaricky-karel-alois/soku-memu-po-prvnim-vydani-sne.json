                        },
                        {
                            "word_text": "bájkou"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "ni Lva divým řváním, ni tlukotem slavičím.",
                    "metrical_pattern": "0110100100100",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
