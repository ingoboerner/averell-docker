{
    "poem_title": "Hlaholy zvířat.",
    "author": "Vinařický, Karel Alois",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Strašné je lvů řvání,",
                    "metrical_pattern": "100110",
                    "line_length": 6,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Strašné"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "lvů"
                        },
                        {
                            "word_text": "řvání"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Kanců divokých rochání.",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Kanců"
                        },
                        {
                            "word_text": "divokých"
                        },
                        {
                            "word_text": "rochání"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Voli bučí – krávy mučí.",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Voli"
                        },
                        {
                            "word_text": "bučí"
                        },
                        {
                            "word_text": "krávy"
                        },
                        {
                            "word_text": "mučí"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Jelen v říjnu říje,",
                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Jelen"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "říjnu"
                        },
                        {
                            "word_text": "říje"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Hladový vlk vyje.",
                    "metrical_pattern": "100110",
                    "line_length": 6,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Hladový"
                        },
                        {
                            "word_text": "vlk"
                        },
                        {
                            "word_text": "vyje"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Nozdrama kůň bujný frká,",
                    "metrical_pattern": "10011010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Nozdrama"
                        },
                        {
                            "word_text": "kůň"
                        },
                        {
                            "word_text": "bujný"
                        },
                        {
                            "word_text": "frká"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Krotký holub vrká,",
                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Krotký"
                        },
                        {
                            "word_text": "holub"
                        },
                        {
                            "word_text": "vrká"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Divoký po lese houká,",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Divoký"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "lese"
                        },
                        {
                            "word_text": "houká"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Kočka vrčí, mňouká.",
                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Kočka"
                        },
                        {
                            "word_text": "vrčí"
                        },
                        {
                            "word_text": "mňouká"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Hříbě vesele řičí,",
                    "metrical_pattern": "1010010",
                    "line_length": 7,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Hříbě"
                        },
                        {
                            "word_text": "vesele"
                        },
                        {
                            "word_text": "řičí"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Husa jáje a sičí.",
                    "metrical_pattern": "1010010",
                    "line_length": 7,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Husa"
                        },
                        {
                            "word_text": "jáje"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "sičí"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Oslíček hyká, kvíčala kviká.",
                    "metrical_pattern": "1001010010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Oslíček"
                        },
                        {
                            "word_text": "hyká"
                        },
                        {
                            "word_text": "kvíčala"
                        },
                        {
                            "word_text": "kviká"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Opice kvikoce, pták malý štěboce.",
                    "metrical_pattern": "100100110100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Opice"
                        },
                        {
                            "word_text": "kvikoce"
                        },
                        {
                            "word_text": "pták"
                        },
                        {
                            "word_text": "malý"
                        },
                        {
                            "word_text": "štěboce"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Zajíc vříská, ovce bečí,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Zajíc"
                        },
                        {
                            "word_text": "vříská"
                        },
                        {
                            "word_text": "ovce"
                        },
                        {
                            "word_text": "bečí"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Koza bříská, meká, břečí.",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Koza"
                        },
                        {
                            "word_text": "bříská"
                        },
                        {
                            "word_text": "meká"
                        },
                        {
                            "word_text": "břečí"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Kůzle mekotá, kozel brekotá,",
                    "metrical_pattern": "1010010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kůzle"
                        },
                        {
                            "word_text": "mekotá"
                        },
                        {
                            "word_text": "kozel"
                        },
                        {
                            "word_text": "brekotá"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Myška piští, prase kručí;",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Myška"
                        },
                        {
                            "word_text": "piští"
                        },
                        {
                            "word_text": "prase"
                        },
                        {
                            "word_text": "kručí"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Pes na cizí vrčí, kňučí,",
                    "metrical_pattern": "11001010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Pes"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "cizí"
                        },
                        {
                            "word_text": "vrčí"
                        },
                        {
                            "word_text": "kňučí"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Zimou v noci vyje a skučí,",
                    "metrical_pattern": "101010010",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Zimou"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "noci"
                        },
                        {
                            "word_text": "vyje"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "skučí"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Silně hafá, štěká,",
                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Silně"
                        },
                        {
                            "word_text": "hafá"
                        },
                        {
                            "word_text": "štěká"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Uvázán se vzteká.",
                    "metrical_pattern": "100010",
                    "line_length": 6,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Uvázán"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "vzteká"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Liška blekoce, skolí,",
                    "metrical_pattern": "1010010",
                    "line_length": 7,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Liška"
                        },
                        {
                            "word_text": "blekoce"
                        },
                        {
                            "word_text": "skolí"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Vlašťovice šveholí.",
                    "metrical_pattern": "1000100",
                    "line_length": 7,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Vlašťovice"
                        },
                        {
                            "word_text": "šveholí"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Kohout kokrhá, v noci pěje,",
                    "metrical_pattern": "101001010",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Kohout"
                        },
                        {
                            "word_text": "kokrhá"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "noci"
                        },
                        {
                            "word_text": "pěje"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Krocan hudruje – hrdlička cukruje,",
                    "metrical_pattern": "10100100100",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Krocan"
                        },
                        {
                            "word_text": "hudruje"
                        },
                        {
                            "word_text": "hrdlička"
                        },
                        {
                            "word_text": "cukruje"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "A v lese ráda se směje.",
                    "metrical_pattern": "01010010",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "lese"
                        },
                        {
                            "word_text": "ráda"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "směje"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Slepice po dvoře, kdáče a krakoře,",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Slepice"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "dvoře"
                        },
                        {
                            "word_text": "kdáče"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "krakoře"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Kvočna kvoká – křepelka ťoká,",
                    "metrical_pattern": "101010010",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kvočna"
                        },
                        {
                            "word_text": "kvoká"
                        },
                        {
                            "word_text": "křepelka"
                        },
                        {
                            "word_text": "ťoká"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Tence kuřátko típá,",
                    "metrical_pattern": "1010010",
                    "line_length": 7,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Tence"
                        },
                        {
                            "word_text": "kuřátko"
                        },
                        {
                            "word_text": "típá"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Hádě siptí a sípá.",
                    "metrical_pattern": "1010010",
                    "line_length": 7,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Hádě"
                        },
                        {
                            "word_text": "siptí"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "sípá"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Jeřáb hejhá – houser kejhá,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jeřáb"
                        },
                        {
                            "word_text": "hejhá"
                        },
                        {
                            "word_text": "houser"
                        },
                        {
                            "word_text": "kejhá"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Kachna káchá – kačer knáchá.",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kachna"
                        },
                        {
                            "word_text": "káchá"
                        },
                        {
                            "word_text": "kačer"
                        },
                        {
                            "word_text": "knáchá"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Bílý čáp klepoce – černý klekoce,",
                    "metrical_pattern": "10110010100",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Bílý"
                        },
                        {
                            "word_text": "čáp"
                        },
                        {
                            "word_text": "klepoce"
                        },
                        {
                            "word_text": "černý"
                        },
                        {
                            "word_text": "klekoce"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Vrána kráká – kavka kváká,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vrána"
                        },
                        {
                            "word_text": "kráká"
                        },
                        {
                            "word_text": "kavka"
                        },
                        {
                            "word_text": "kváká"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Krkavec krká – kobylka cvrká,",
                    "metrical_pattern": "1001010010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Krkavec"
                        },
                        {
                            "word_text": "krká"
                        },
                        {
                            "word_text": "kobylka"
                        },
                        {
                            "word_text": "cvrká"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Kosi hvízdají, – pěnkavy pinkají,",
                    "metrical_pattern": "10100100100",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kosi"
                        },
                        {
                            "word_text": "hvízdají"
                        },
                        {
                            "word_text": "pěnkavy"
                        },
                        {
                            "word_text": "pinkají"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Datlík tekotá – straka rapotá,",
                    "metrical_pattern": "1010010100",
                    "line_length": 10,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Datlík"
                        },
                        {
                            "word_text": "tekotá"
                        },
                        {
                            "word_text": "straka"
                        },
                        {
                            "word_text": "rapotá"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Vrabec švihlá, štilipí,",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Vrabec"
                        },
                        {
                            "word_text": "švihlá"
                        },
                        {
                            "word_text": "štilipí"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "Kanárek v kleci filípí.",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Kanárek"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "kleci"
                        },
                        {
                            "word_text": "filípí"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Slavík tluče, klokotá,",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Slavík"
                        },
                        {
                            "word_text": "tluče"
                        },
                        {
                            "word_text": "klokotá"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "Žába škvoře, křehotá,",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Žába"
                        },
                        {
                            "word_text": "škvoře"
                        },
                        {
                            "word_text": "křehotá"
                        }
                    ]
                },
                {
                    "line_number": 42,
                    "line_text": "Z louže kuňká, do vody žbluňká.",
                    "metrical_pattern": "101010010",
                    "line_length": 9,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Z"
                        },
                        {
                            "word_text": "louže"
                        },
                        {
                            "word_text": "kuňká"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "vody"
                        },
                        {
                            "word_text": "žbluňká"
                        }
                    ]
                },
                {
                    "line_number": 43,
                    "line_text": "Dedek dudá – žezulice kuká,",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Dedek"
                        },
                        {
                            "word_text": "dudá"
                        },
                        {
                            "word_text": "žezulice"
                        },
                        {
                            "word_text": "kuká"
                        }
                    ]
                },
                {
                    "line_number": 44,
                    "line_text": "Vejr v noci huhlá – sova buká.",
                    "metrical_pattern": "110101010",
                    "line_length": 9,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Vejr"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "noci"
                        },
                        {
                            "word_text": "huhlá"
                        },
                        {
                            "word_text": "sova"
                        },
                        {
                            "word_text": "buká"
                        }
                    ]
                },
                {
                    "line_number": 45,
                    "line_text": "Včela bzučí – čmelák vučí,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Včela"
                        },
                        {
                            "word_text": "bzučí"
                        },
                        {
                            "word_text": "čmelák"
                        },
                        {
                            "word_text": "vučí"
                        }
                    ]
                },
                {
                    "line_number": 46,
                    "line_text": "Komár zvínčí – muška bzí,",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Komár"
                        },
                        {
                            "word_text": "zvínčí"
                        },
                        {
                            "word_text": "muška"
                        },
                        {
                            "word_text": "bzí"
                        }
                    ]
                },
                {
                    "line_number": 47,
                    "line_text": "Brumbál z temna zabručí.",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 15,
                    "words": [
