        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Zakroužím oknem. Kdo by mne chyt’?",
                    "metrical_pattern": "100101000",
                    "line_length": 9,
