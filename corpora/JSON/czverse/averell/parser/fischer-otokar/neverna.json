            ],
            "stanza_text": "Nese mne vítr a lehounká krev.\nZrcátko, pověz a pravdu zjev:"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Sotva se půdy dotkne mé tělo.",
                    "metrical_pattern": "1001010010",
                    "line_length": 10,
                    "rhyme": 4,
                    "words": [
