                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Duše bude žíti!“",
                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Duše"
                        },
                        {
