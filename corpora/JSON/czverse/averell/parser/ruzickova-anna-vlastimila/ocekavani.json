                        },
                        {
                            "word_text": "láskou"
                        },
                        {
                            "word_text": "vroucí"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Hledat začne všude.",
                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Hledat"
                        },
                        {
                            "word_text": "začne"
                        },
                        {
                            "word_text": "všude"
                        }
                    ]
