            ],
            "stanza_text": "Tu zas kroky jeho\nDomnívá se slýchat;\nVidouc však se v klamu,\nTakto počne vzdychat:"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "„Proč jest mi tak bolno,",
                    "metrical_pattern": "000010",
                    "line_length": 6,
                    "rhyme": null,
                    "words": [
