                        {
                            "word_text": "vlastních"
                        },
                        {
                            "word_text": "prsou"
                        },
                        {
                            "word_text": "plane"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Se světla, vlaha tamo mně dostane;",
                    "metrical_pattern": "10010100100",
                    "line_length": 11,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "světla"
                        },
                        {
