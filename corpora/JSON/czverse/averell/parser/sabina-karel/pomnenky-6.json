                        },
                        {
                            "word_text": "jediné"
                        },
                        {
                            "word_text": "zabývání"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Samoten stojím v chrámu šíropustém;",
                    "metrical_pattern": "10010101000",
                    "line_length": 11,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Samoten"
                        },
                        {
                            "word_text": "stojím"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "chrámu"
                        },
                        {
                            "word_text": "šíropustém"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Jedinká lampa klenutím rozlívá",
                    "metrical_pattern": "10010100100",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Jedinká"
                        },
                        {
                            "word_text": "lampa"
                        },
                        {
                            "word_text": "klenutím"
                        },
                        {
                            "word_text": "rozlívá"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Nejisté světlo; hrůzojevným šustem",
                    "metrical_pattern": "10010100010",
                    "line_length": 11,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Nejisté"
                        },
                        {
                            "word_text": "světlo"
                        },
                        {
                            "word_text": "hrůzojevným"
                        },
                        {
                            "word_text": "šustem"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Zde onde stín z mrtvé mně doby kývá.",
                    "metrical_pattern": "01011001010",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Zde"
                        },
                        {
                            "word_text": "onde"
                        },
                        {
                            "word_text": "stín"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "mrtvé"
                        },
                        {
                            "word_text": "mně"
                        },
                        {
                            "word_text": "doby"
                        },
                        {
                            "word_text": "kývá"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Jest-li to přízrak, či se pravda jeví",
                    "metrical_pattern": "00010101010",
                    "line_length": 11,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Jest"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "přízrak"
                        },
                        {
                            "word_text": "či"
