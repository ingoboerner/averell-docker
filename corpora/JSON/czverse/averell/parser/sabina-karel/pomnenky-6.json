                        },
                        {
                            "word_text": "zapomenouc"
                        },
                        {
                            "word_text": "přestálé"
                        },
                        {
                            "word_text": "již"
                        },
                        {
                            "word_text": "hoře"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Baví vlastními výtvory se svými.",
                    "metrical_pattern": "10100100100",
                    "line_length": 11,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Baví"
                        },
                        {
                            "word_text": "vlastními"
                        },
                        {
                            "word_text": "výtvory"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "svými"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Sama jen sobě věčně zůstavena",
                    "metrical_pattern": "10010101000",
                    "line_length": 11,
                    "rhyme": 11,
                    "words": [
