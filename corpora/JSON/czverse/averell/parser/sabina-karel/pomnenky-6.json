                        },
                        {
                            "word_text": "hru"
                        },
                        {
                            "word_text": "ostává"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Však ticho; obraz nade vše milostný",
                    "metrical_pattern": "01010100100",
                    "line_length": 11,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Však"
                        },
                        {
                            "word_text": "ticho"
