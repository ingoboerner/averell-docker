                        },
                        {
                            "word_text": "kdy"
                        },
                        {
                            "word_text": "zašumí"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Viz ptactva sbor jej s hlukem oblétati;",
                    "metrical_pattern": "11010101000",
                    "line_length": 11,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Viz"
                        },
                        {
                            "word_text": "ptactva"
                        },
                        {
                            "word_text": "sbor"
                        },
                        {
