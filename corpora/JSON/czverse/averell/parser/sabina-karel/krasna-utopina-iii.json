                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "kdo"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jich"
                        },
                        {
                            "word_text": "v"
                        },
                        {
