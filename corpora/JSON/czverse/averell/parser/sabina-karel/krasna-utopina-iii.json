                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Tam"
                        },
                        {
                            "word_text": "muž"
                        },
                        {
                            "word_text": "slavné"
                        },
                        {
                            "word_text": "výmluvnosti"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Nadějným je praktikantem",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Nadějným"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "praktikantem"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Ten – onen vysoko slouží,",
                    "metrical_pattern": "01010010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Ten"
                        },
                        {
                            "word_text": "onen"
                        },
                        {
                            "word_text": "vysoko"
                        },
                        {
                            "word_text": "slouží"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Ten zas bude jenerálem,",
                    "metrical_pattern": "00101000",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
