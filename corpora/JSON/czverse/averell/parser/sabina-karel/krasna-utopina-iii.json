                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Články"
                        },
                        {
                            "word_text": "slavně"
                        },
                        {
                            "word_text": "odměněné"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Studenti a kavalíři,",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Studenti"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "kavalíři"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Umělci a oficíři,",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
