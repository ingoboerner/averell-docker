                            "word_text": "Milujíce"
                        },
                        {
                            "word_text": "vás"
                        },
                        {
                            "word_text": "potají"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Odložíte-li tu skvrnu,",
                    "metrical_pattern": "10000010",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Odložíte"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "skvrnu"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Nedopustí na vás hanu!",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Nedopustí"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "vás"
                        },
                        {
                            "word_text": "hanu"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "Jenom pomněte, že módy",
                    "metrical_pattern": "10100010",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Jenom"
                        },
                        {
                            "word_text": "pomněte"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "módy"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Panují teď zcela jiné,",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Panují"
                        },
                        {
                            "word_text": "teď"
                        },
                        {
                            "word_text": "zcela"
                        },
                        {
                            "word_text": "jiné"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "A kdo se jich v davu mine,",
