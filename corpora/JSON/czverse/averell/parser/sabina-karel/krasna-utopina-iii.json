                        {
                            "word_text": "nemódní"
                        },
                        {
                            "word_text": "karkulici"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Jeden z pánů těch má panství,",
                    "metrical_pattern": "10100110",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jeden"
