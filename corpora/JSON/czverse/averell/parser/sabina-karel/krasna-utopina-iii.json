                        },
                        {
                            "word_text": "módních"
                        },
                        {
                            "word_text": "mladých"
                        },
                        {
                            "word_text": "pánů"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "O vaši se lásku snaží,",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "O"
                        },
                        {
                            "word_text": "vaši"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "lásku"
                        },
                        {
                            "word_text": "snaží"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Tisíce obstárlých srdcí",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tisíce"
                        },
                        {
                            "word_text": "obstárlých"
                        },
                        {
                            "word_text": "srdcí"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Po úsměvu vašem baží;",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Po"
                        },
                        {
                            "word_text": "úsměvu"
                        },
                        {
                            "word_text": "vašem"
                        },
                        {
                            "word_text": "baží"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Jen nebuďte tak ukrutná",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jen"
                        },
                        {
                            "word_text": "nebuďte"
                        },
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "ukrutná"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "A dejte si přece říci,",
                    "metrical_pattern": "01001010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
