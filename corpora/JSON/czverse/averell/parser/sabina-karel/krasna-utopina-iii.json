                            "word_text": "Ten"
                        },
                        {
                            "word_text": "zas"
                        },
                        {
                            "word_text": "bude"
                        },
                        {
                            "word_text": "jenerálem"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Pro vás-li se neusouží.",
                    "metrical_pattern": "10001000",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Pro"
                        },
                        {
                            "word_text": "vás"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "neusouží"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Tamhle ten zas verše píše",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tamhle"
                        },
                        {
                            "word_text": "ten"
                        },
                        {
                            "word_text": "zas"
                        },
                        {
                            "word_text": "verše"
                        },
                        {
                            "word_text": "píše"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Velmi draze oceněné,",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Velmi"
                        },
                        {
                            "word_text": "draze"
                        },
                        {
                            "word_text": "oceněné"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Onen druhý zas do novin",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Onen"
                        },
                        {
                            "word_text": "druhý"
                        },
                        {
                            "word_text": "zas"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "novin"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Články slavně odměněné; –",
