                        },
                        {
                            "word_text": "nesnáze"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Na sta módních mladých pánů",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
