                        {
                            "word_text": "nenoste"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "ulici"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Tu šerednou karkulici!",
                    "metrical_pattern": "01001000",
