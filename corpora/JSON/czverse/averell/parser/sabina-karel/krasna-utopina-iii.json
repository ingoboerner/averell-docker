                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tretka"
                        },
                        {
                            "word_text": "věru"
                        },
                        {
                            "word_text": "nepatrná"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Odstranila by nesnáze.",
