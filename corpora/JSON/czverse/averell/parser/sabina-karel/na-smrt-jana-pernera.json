                        },
                        {
                            "word_text": "kmen"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Jitřenu ranila – nadějný zašel den;",
                    "metrical_pattern": "100100100101",
                    "line_length": 12,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Jitřenu"
                        },
                        {
