        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Po den dcera stůně: druhý zmírá,",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": 12,
                    "words": [
