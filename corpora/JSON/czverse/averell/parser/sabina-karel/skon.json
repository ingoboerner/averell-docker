            ],
            "stanza_text": "Proč jsi smutna, proč se nezasměješ,\nProč víc písně čilé nezapěješ?"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "„Ach, matičko, jak být smutna nemám,",
                    "metrical_pattern": "1100101010",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Ach"
                        },
                        {
                            "word_text": "matičko"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "být"
                        },
                        {
                            "word_text": "smutna"
                        },
                        {
                            "word_text": "nemám"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Srdci svému marně pokoj hledám.“",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Srdci"
                        },
                        {
