                        },
                        {
                            "word_text": "mě"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "milý"
                        },
                        {
                            "word_text": "vyprovodil"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Zahlídl tu jinou, s ní se vodil;",
                    "metrical_pattern": "1000100010",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Zahlídl"
                        },
                        {
