            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Až tam mně můj milý věrnost choval,",
                    "metrical_pattern": "0000101010",
                    "line_length": 10,
                    "rhyme": 11,
                    "words": [
