                            "word_text": "Zabřesklo"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "horách"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "dolinách"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "A nový život nastal po Čechách. –",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "nový"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "nastal"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "Čechách"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "A v řadě skvělých ducha bohatýrů,",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "řadě"
                        },
                        {
                            "word_text": "skvělých"
                        },
                        {
                            "word_text": "ducha"
                        },
                        {
                            "word_text": "bohatýrů"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Již prapor pokroku rozvinuli,",
                    "metrical_pattern": "0101001000",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Již"
                        },
                        {
                            "word_text": "prapor"
                        },
                        {
                            "word_text": "pokroku"
                        },
                        {
                            "word_text": "rozvinuli"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Tu v zápalu se hrnouce v zvučnou lýru,",
                    "metrical_pattern": "010001001010",
                    "line_length": 12,
                    "rhyme": 9,
                    "words": [
