                        {
                            "word_text": "pěvce"
                        },
                        {
                            "word_text": "vzpomínají"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Jenž je láskou opěval,",
                    "metrical_pattern": "0010100",
                    "line_length": 7,
                    "rhyme": 14,
                    "words": [
