    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Buď oslaven, památný den!",
                    "metrical_pattern": "01001001",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Buď"
                        },
                        {
