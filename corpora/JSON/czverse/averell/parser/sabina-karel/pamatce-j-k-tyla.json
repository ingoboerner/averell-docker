                        {
                            "word_text": "slovem"
                        },
                        {
                            "word_text": "zápasíce"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "prudkém"
                        },
                        {
                            "word_text": "víru"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Pro vlast a národ žili, hynuli,",
                    "metrical_pattern": "1001010100",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
