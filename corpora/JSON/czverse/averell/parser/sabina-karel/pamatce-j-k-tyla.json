                        {
                            "word_text": "lásky"
                        },
                        {
                            "word_text": "slast"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "V snách beznadějných národ pohřížený",
                    "metrical_pattern": "11000101000",
                    "line_length": 11,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "snách"
                        },
                        {
                            "word_text": "beznadějných"
                        },
                        {
                            "word_text": "národ"
                        },
                        {
                            "word_text": "pohřížený"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Východu hledal z bludiště tmavého,",
                    "metrical_pattern": "10010100100",
                    "line_length": 11,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Východu"
                        },
                        {
                            "word_text": "hledal"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "bludiště"
                        },
                        {
                            "word_text": "tmavého"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Od světa neznán, sobě odcizený",
                    "metrical_pattern": "10010101000",
                    "line_length": 11,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Od"
                        },
                        {
                            "word_text": "světa"
                        },
                        {
