                        },
                        {
                            "word_text": "sobě"
                        },
                        {
                            "word_text": "odcizený"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Trudně se vlékal po kraji hrobu svého. –",
                    "metrical_pattern": "100101001010",
                    "line_length": 12,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Trudně"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "vlékal"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "kraji"
                        },
                        {
                            "word_text": "hrobu"
                        },
                        {
                            "word_text": "svého"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Tu jako andělové v soudný den",
                    "metrical_pattern": "0101000101",
                    "line_length": 10,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "andělové"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "soudný"
                        },
                        {
                            "word_text": "den"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Vyvstali mužové v pochmúrné noci",
                    "metrical_pattern": "10010010010",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Vyvstali"
                        },
                        {
                            "word_text": "mužové"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "pochmúrné"
                        },
                        {
                            "word_text": "noci"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "A světlem ducha, slova čaromocí",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "světlem"
                        },
                        {
                            "word_text": "ducha"
                        },
                        {
                            "word_text": "slova"
                        },
                        {
                            "word_text": "čaromocí"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Vzbudili národ, zaplašili sen.",
                    "metrical_pattern": "1001010001",
                    "line_length": 10,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Vzbudili"
                        },
                        {
                            "word_text": "národ"
                        },
                        {
                            "word_text": "zaplašili"
                        },
                        {
                            "word_text": "sen"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Zabřesklo na horách i v dolinách",
                    "metrical_pattern": "1001000100",
                    "line_length": 10,
                    "rhyme": 8,
                    "words": [
