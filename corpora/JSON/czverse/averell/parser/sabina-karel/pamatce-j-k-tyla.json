                        {
                            "word_text": "skromná"
                        },
                        {
                            "word_text": "ta"
                        },
                        {
                            "word_text": "budova"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Z níž povznesla se duše básníkova,",
                    "metrical_pattern": "01000101000",
                    "line_length": 11,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Z"
                        },
                        {
                            "word_text": "níž"
                        },
                        {
                            "word_text": "povznesla"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "básníkova"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "By drahnou ozdobila Čechů vlast!",
                    "metrical_pattern": "0101000101",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "By"
                        },
                        {
                            "word_text": "drahnou"
                        },
                        {
                            "word_text": "ozdobila"
                        },
                        {
                            "word_text": "Čechů"
                        },
                        {
                            "word_text": "vlast"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Zde vznikla jiskra v pochodeň vzplanoucí,",
                    "metrical_pattern": "01010100100",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Zde"
                        },
                        {
                            "word_text": "vznikla"
                        },
                        {
                            "word_text": "jiskra"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "pochodeň"
                        },
                        {
                            "word_text": "vzplanoucí"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Jež světlem svým budila národ mroucí",
                    "metrical_pattern": "01001001010",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Jež"
                        },
                        {
                            "word_text": "světlem"
                        },
                        {
                            "word_text": "svým"
                        },
                        {
                            "word_text": "budila"
                        },
                        {
                            "word_text": "národ"
                        },
                        {
                            "word_text": "mroucí"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "A hlubokou proniknouc jeho strast,",
                    "metrical_pattern": "0100100101",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "hlubokou"
                        },
                        {
                            "word_text": "proniknouc"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "strast"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "V bolestné rány jeho, v srdce žhoucí",
                    "metrical_pattern": "10010101010",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "bolestné"
                        },
                        {
                            "word_text": "rány"
                        },
                        {
                            "word_text": "jeho"
