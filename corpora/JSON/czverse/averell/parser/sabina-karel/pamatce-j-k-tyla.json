                        },
                        {
                            "word_text": "památný"
                        },
                        {
                            "word_text": "den"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Buď oslavena skromná ta budova,",
                    "metrical_pattern": "01000100100",
                    "line_length": 11,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Buď"
                        },
                        {
                            "word_text": "oslavena"
