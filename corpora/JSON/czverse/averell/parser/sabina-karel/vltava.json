            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Ubohá sestro, marnéť tvé hledání!",
                    "metrical_pattern": "10010100100",
                    "line_length": 11,
                    "rhyme": 9,
