            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Osiřen ukryl se v klín matky přírody;",
                    "metrical_pattern": "100100110100",
                    "line_length": 12,
                    "rhyme": 11,
