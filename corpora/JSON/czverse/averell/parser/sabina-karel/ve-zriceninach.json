        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Blažený mudrci! Ty věčnost zkoumaje",
                    "metrical_pattern": "100100010100",
                    "line_length": 12,
                    "rhyme": 7,
                    "words": [
