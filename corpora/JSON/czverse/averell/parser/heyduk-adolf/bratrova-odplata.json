                    "line_number": 12,
                    "line_text": "ty jsi byl té druhé sluhou!",
                    "metrical_pattern": "00001010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "ty"
                        },
                        {
                            "word_text": "jsi"
                        },
                        {
                            "word_text": "byl"
                        },
                        {
                            "word_text": "té"
                        },
                        {
                            "word_text": "druhé"
                        },
                        {
                            "word_text": "sluhou"
                        }
                    ]
                }
            ],
            "stanza_text": "jednu v srdci, na rtech druhou,\nty jsi byl té druhé sluhou!"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Přišla matka, chtěla radu,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Přišla"
                        },
                        {
