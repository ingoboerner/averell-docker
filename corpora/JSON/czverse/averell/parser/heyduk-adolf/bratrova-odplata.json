{
    "poem_title": "Bratrova odplata.",
    "author": "Heyduk, Adolf",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
