        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Běda, bratře, sílu tratím,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Běda"
