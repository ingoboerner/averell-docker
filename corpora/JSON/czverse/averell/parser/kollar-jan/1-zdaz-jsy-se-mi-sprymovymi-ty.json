                        },
                        {
                            "word_text": "přírodě"
                        },
                        {
                            "word_text": "mrtvé"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Jakž ten může živým jmouti milenku cytem?",
                    "metrical_pattern": "0010101010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jakž"
                        },
                        {
                            "word_text": "ten"
