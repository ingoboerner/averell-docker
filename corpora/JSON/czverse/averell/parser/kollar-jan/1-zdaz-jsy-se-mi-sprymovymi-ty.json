                    "metrical_pattern": "1010100010010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Láska"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "libuje"
                        },
                        {
                            "word_text": "kdo"
                        },
                        {
                            "word_text": "kochá"
