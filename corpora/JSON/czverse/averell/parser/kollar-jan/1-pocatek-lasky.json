{
    "poem_title": "1. Počátek lásky.",
    "author": "Kollár, Jan",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Zdaž si se mi šprymnými ty sám posměšky nerouhal,",
                    "metrical_pattern": "100010000100100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zdaž"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "šprymnými"
                        },
                        {
                            "word_text": "ty"
                        },
                        {
                            "word_text": "sám"
                        },
                        {
                            "word_text": "posměšky"
                        },
                        {
                            "word_text": "nerouhal"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Sotvy že já milovat, příteli, někdy budu?",
                    "metrical_pattern": "10001001001010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Sotvy"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "já"
                        },
                        {
                            "word_text": "milovat"
                        },
                        {
                            "word_text": "příteli"
                        },
                        {
                            "word_text": "někdy"
                        },
                        {
                            "word_text": "budu"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Ty řka jen Umce hovíš, krásného spolku utíkáš,",
                    "metrical_pattern": "010101010010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
                        },
                        {
                            "word_text": "řka"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "Umce"
                        },
                        {
                            "word_text": "hovíš"
                        },
                        {
                            "word_text": "krásného"
                        },
                        {
                            "word_text": "spolku"
                        },
                        {
                            "word_text": "utíkáš"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Tě mhla, tě růže, tě háj, ne svět a dívče těší.",
                    "metrical_pattern": "00010010101010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tě"
                        },
                        {
                            "word_text": "mhla"
                        },
                        {
                            "word_text": "tě"
                        },
                        {
                            "word_text": "růže"
                        },
                        {
                            "word_text": "tě"
                        },
                        {
                            "word_text": "háj"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "svět"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "dívče"
                        },
                        {
                            "word_text": "těší"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Láska život libuje, kdo kochá se v přírodě mertvé,",
                    "metrical_pattern": "1010100010010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Láska"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "libuje"
                        },
                        {
                            "word_text": "kdo"
                        },
                        {
                            "word_text": "kochá"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "přírodě"
                        },
                        {
                            "word_text": "mertvé"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Jakž ten může živým jmouti milenku citem?",
                    "metrical_pattern": "0010101010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jakž"
                        },
                        {
                            "word_text": "ten"
                        },
                        {
                            "word_text": "může"
                        },
                        {
                            "word_text": "živým"
                        },
                        {
                            "word_text": "jmouti"
                        },
                        {
                            "word_text": "milenku"
                        },
                        {
                            "word_text": "citem"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Bezpečný klamavé netušil jsem nástrahy; volné,",
                    "metrical_pattern": "100100100010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Bezpečný"
                        },
                        {
                            "word_text": "klamavé"
                        },
                        {
                            "word_text": "netušil"
                        },
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "nástrahy"
                        },
                        {
                            "word_text": "volné"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Kamžkoli jen táhlo, průduchy sardci daje.",
                    "metrical_pattern": "1000101001010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kamžkoli"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "táhlo"
                        },
                        {
                            "word_text": "průduchy"
                        },
                        {
                            "word_text": "sardci"
                        },
                        {
                            "word_text": "daje"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Než juž pozdě želím; však tys vina věštče falešný,",
                    "metrical_pattern": "001010001010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Než"
                        },
                        {
                            "word_text": "juž"
                        },
                        {
                            "word_text": "pozdě"
                        },
                        {
                            "word_text": "želím"
                        },
                        {
                            "word_text": "však"
                        },
                        {
                            "word_text": "tys"
                        },
                        {
                            "word_text": "vina"
                        },
                        {
                            "word_text": "věštče"
                        },
                        {
                            "word_text": "falešný"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Blah! kdo cizého časem nedbati soudu umí.",
                    "metrical_pattern": "10100101001010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Blah"
                        },
                        {
                            "word_text": "kdo"
                        },
                        {
                            "word_text": "cizého"
                        },
                        {
                            "word_text": "časem"
                        },
                        {
                            "word_text": "nedbati"
                        },
                        {
                            "word_text": "soudu"
                        },
                        {
                            "word_text": "umí"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Když spanilou po lukách oko mé se kochávalo krásou,",
                    "metrical_pattern": "01001001000100010",
                    "line_length": 17,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Když"
                        },
                        {
                            "word_text": "spanilou"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "lukách"
                        },
                        {
                            "word_text": "oko"
                        },
                        {
                            "word_text": "mé"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "kochávalo"
                        },
                        {
                            "word_text": "krásou"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Myslel sem, že jiné ve světě krásy není.",
                    "metrical_pattern": "1000101001010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Myslel"
                        },
                        {
                            "word_text": "sem"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "jiné"
                        },
                        {
                            "word_text": "ve"
                        },
                        {
                            "word_text": "světě"
                        },
                        {
                            "word_text": "krásy"
                        },
                        {
                            "word_text": "není"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Rád jsem tvému věřil v sladkém želi, příteli, žertu,",
                    "metrical_pattern": "101010101010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Rád"
                        },
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "tvému"
                        },
                        {
                            "word_text": "věřil"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "sladkém"
                        },
                        {
                            "word_text": "želi"
                        },
                        {
                            "word_text": "příteli"
                        },
                        {
                            "word_text": "žertu"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Jistli si tvých čárů, nech z něho pravda bude.",
                    "metrical_pattern": "1000101101010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jistli"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "tvých"
                        },
                        {
                            "word_text": "čárů"
                        },
                        {
                            "word_text": "nech"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "něho"
                        },
                        {
                            "word_text": "pravda"
                        },
                        {
                            "word_text": "bude"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Zhás řeřavou, jenž ňádra moří má, žáhu milosti,",
                    "metrical_pattern": "010001010110100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zhás"
                        },
                        {
                            "word_text": "řeřavou"
                        },
                        {
                            "word_text": "jenž"
                        },
                        {
                            "word_text": "ňádra"
                        },
                        {
                            "word_text": "moří"
                        },
                        {
                            "word_text": "má"
                        },
                        {
                            "word_text": "žáhu"
                        },
                        {
                            "word_text": "milosti"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Pak ránu, hluboké ostny vytáhna, zahoj.",
                    "metrical_pattern": "0101001010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pak"
                        },
                        {
                            "word_text": "ránu"
                        },
                        {
                            "word_text": "hluboké"
                        },
                        {
                            "word_text": "ostny"
                        },
                        {
                            "word_text": "vytáhna"
                        },
                        {
                            "word_text": "zahoj"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Neb čím déle tiché v ututlání dřímaly jiskry,",
                    "metrical_pattern": "001010100010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neb"
                        },
                        {
                            "word_text": "čím"
                        },
                        {
                            "word_text": "déle"
                        },
                        {
                            "word_text": "tiché"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "ututlání"
                        },
                        {
                            "word_text": "dřímaly"
                        },
                        {
                            "word_text": "jiskry"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Tím se nyní po celém hůř těle rozšiřují.",
                    "metrical_pattern": "00101000101000",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tím"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "nyní"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "celém"
                        },
                        {
                            "word_text": "hůř"
                        },
                        {
                            "word_text": "těle"
                        },
                        {
                            "word_text": "rozšiřují"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Veď mne zpátkem opět k ztraceným, duše poklidu, cestám,",
                    "metrical_pattern": "1010101001010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Veď"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "zpátkem"
                        },
                        {
                            "word_text": "opět"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "ztraceným"
                        },
                        {
                            "word_text": "duše"
                        },
                        {
                            "word_text": "poklidu"
                        },
                        {
                            "word_text": "cestám"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Tam kde ani svodný nezněje lásky ohlas.",
                    "metrical_pattern": "0010101001010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tam"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "ani"
                        },
                        {
                            "word_text": "svodný"
                        },
                        {
                            "word_text": "nezněje"
                        },
                        {
                            "word_text": "lásky"
                        },
                        {
                            "word_text": "ohlas"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Neb kde se jen verhnu, zmařené kam smysly obrátím,",
                    "metrical_pattern": "000010100010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neb"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "verhnu"
                        },
                        {
                            "word_text": "zmařené"
                        },
                        {
                            "word_text": "kam"
                        },
                        {
                            "word_text": "smysly"
                        },
                        {
                            "word_text": "obrátím"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Tu sto hned jazyky všecko mi lásku volá.",
                    "metrical_pattern": "0101001001010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "sto"
                        },
                        {
                            "word_text": "hned"
                        },
                        {
                            "word_text": "jazyky"
                        },
                        {
                            "word_text": "všecko"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "lásku"
                        },
                        {
                            "word_text": "volá"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Buď jarní v temné zpěv houšti slavíka poslouchám,",
                    "metrical_pattern": "01010110100100",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Buď"
                        },
                        {
                            "word_text": "jarní"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "temné"
                        },
                        {
                            "word_text": "zpěv"
                        },
                        {
                            "word_text": "houšti"
                        },
                        {
                            "word_text": "slavíka"
                        },
                        {
                            "word_text": "poslouchám"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Neb v zelené jak se trávě potůčky hrají;",
                    "metrical_pattern": "0100001010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neb"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "zelené"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "trávě"
                        },
                        {
                            "word_text": "potůčky"
                        },
                        {
                            "word_text": "hrají"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Buď dolinou jda patřím, vítav v tichu háje jitřenku,",
                    "metrical_pattern": "0100110101010100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Buď"
                        },
                        {
                            "word_text": "dolinou"
                        },
                        {
                            "word_text": "jda"
                        },
                        {
                            "word_text": "patřím"
                        },
                        {
                            "word_text": "vítav"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "tichu"
                        },
                        {
                            "word_text": "háje"
                        },
                        {
                            "word_text": "jitřenku"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Jak květinám sladké kradne kořisti zefyr.",
                    "metrical_pattern": "0100101010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jak"
                        },
                        {
                            "word_text": "květinám"
                        },
                        {
                            "word_text": "sladké"
                        },
                        {
                            "word_text": "kradne"
                        },
                        {
                            "word_text": "kořisti"
                        },
                        {
                            "word_text": "zefyr"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Lásku slavík štebetá, chřestem pěje lásku potůček,",
                    "metrical_pattern": "1010100101010100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lásku"
                        },
                        {
                            "word_text": "slavík"
                        },
                        {
                            "word_text": "štebetá"
                        },
                        {
                            "word_text": "chřestem"
                        },
                        {
                            "word_text": "pěje"
                        },
                        {
                            "word_text": "lásku"
                        },
                        {
                            "word_text": "potůček"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Touž les, touž kvítí, touž v ucho šepce větřík.",
                    "metrical_pattern": "010100101010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Touž"
                        },
                        {
                            "word_text": "les"
                        },
                        {
                            "word_text": "touž"
                        },
                        {
                            "word_text": "kvítí"
                        },
                        {
                            "word_text": "touž"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "ucho"
                        },
                        {
                            "word_text": "šepce"
                        },
                        {
                            "word_text": "větřík"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Dobře se měj ztracený blažené tedy ráji mladosti,",
                    "metrical_pattern": "10011001001010100",
                    "line_length": 17,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Dobře"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "měj"
                        },
                        {
                            "word_text": "ztracený"
                        },
                        {
                            "word_text": "blažené"
                        },
                        {
                            "word_text": "tedy"
                        },
                        {
                            "word_text": "ráji"
                        },
                        {
                            "word_text": "mladosti"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Už do jiného mě hlas anjela mého volá.",
                    "metrical_pattern": "01000011001010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Už"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "jiného"
                        },
                        {
                            "word_text": "mě"
                        },
                        {
                            "word_text": "hlas"
                        },
                        {
                            "word_text": "anjela"
                        },
                        {
                            "word_text": "mého"
