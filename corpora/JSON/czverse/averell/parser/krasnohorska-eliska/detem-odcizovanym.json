        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Řeč od úst jim bere, klíč zlatý to všech",
                    "metrical_pattern": "11001011000",
                    "line_length": 11,
                    "rhyme": 17,
