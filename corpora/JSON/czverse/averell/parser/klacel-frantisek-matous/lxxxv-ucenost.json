            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Však ten kdo špatné hlíně nepoddaný",
                    "metrical_pattern": "00010101000",
                    "line_length": 11,
