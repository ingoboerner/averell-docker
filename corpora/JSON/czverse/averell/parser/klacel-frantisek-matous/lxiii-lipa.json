                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "nevyzáblé"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ač si lezou houfy po něm",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ač"
                        },
                        {
