                    "line_number": 37,
                    "line_text": "Smutno jest se přiznať,",
                    "metrical_pattern": "100010",
                    "line_length": 6,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Smutno"
                        },
