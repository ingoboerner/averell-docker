                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Že"
                        },
                        {
                            "word_text": "vyplní"
                        },
                        {
                            "word_text": "což"
                        },
                        {
                            "word_text": "kázáno"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Jest osudem celkovládným,",
