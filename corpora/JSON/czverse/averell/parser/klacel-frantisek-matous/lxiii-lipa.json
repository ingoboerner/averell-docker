                        {
                            "word_text": "někde"
                        },
                        {
                            "word_text": "suchá"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Pošlá nepříznivým časem,",
                    "metrical_pattern": "10100010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pošlá"
                        },
                        {
                            "word_text": "nepříznivým"
                        },
                        {
                            "word_text": "časem"
                        }
                    ]
                },
