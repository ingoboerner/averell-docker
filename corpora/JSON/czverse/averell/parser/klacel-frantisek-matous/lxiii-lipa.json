                        {
                            "word_text": "osudem"
                        },
                        {
                            "word_text": "celkovládným"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Od té chvíle, jak semínko",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
