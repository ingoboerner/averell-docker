                        },
                        {
                            "word_text": "okovábné"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "A na koncích nesčíselný",
                    "metrical_pattern": "01001000",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
