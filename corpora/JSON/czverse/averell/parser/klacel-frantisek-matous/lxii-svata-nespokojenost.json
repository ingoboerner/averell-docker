                    "line_text": "Němou si zalíbiv skutečnosť,",
                    "metrical_pattern": "100100100",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Němou"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "zalíbiv"
                        },
                        {
                            "word_text": "skutečnosť"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Utlapanou minulosti stezku;",
                    "metrical_pattern": "1000100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
