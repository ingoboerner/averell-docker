                        },
                        {
                            "word_text": "sílu"
                        },
                        {
                            "word_text": "jen"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Ulež se tam na čas čekaje,",
                    "metrical_pattern": "100010100",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ulež"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "tam"
                        },
                        {
                            "word_text": "na"
                        },
