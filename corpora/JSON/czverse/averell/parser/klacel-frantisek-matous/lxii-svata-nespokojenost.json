            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Bleskem přibíť svým má ku Blaníku tě",
                    "metrical_pattern": "10100110000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
