                    "line_number": 20,
                    "line_text": "Jej jalovosť doby lícoměrné.",
                    "metrical_pattern": "0100101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jej"
