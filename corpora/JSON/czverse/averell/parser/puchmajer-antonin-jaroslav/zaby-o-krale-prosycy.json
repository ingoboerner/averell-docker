                        },
                        {
                            "word_text": "dne"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "nocy"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Vřeštěly Žáby k Kralomocy.",
                    "metrical_pattern": "100101000",
                    "line_length": 9,
