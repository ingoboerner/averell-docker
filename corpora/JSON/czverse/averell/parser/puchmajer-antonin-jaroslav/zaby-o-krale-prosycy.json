                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "Kralomocy"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Smál se Perún: ale kdo chce kam,",
                    "metrical_pattern": "101010010",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Smál"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "Perún"
                        },
