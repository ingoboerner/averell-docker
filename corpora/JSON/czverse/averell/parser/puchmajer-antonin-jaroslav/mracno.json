                        },
                        {
                            "word_text": "krupobití"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "O jakéms neslyšel za svého živobytí.",
                    "metrical_pattern": "1001001001000",
                    "line_length": 13,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "O"
                        },
                        {
                            "word_text": "jakéms"
                        },
