                        {
                            "word_text": "zemi"
                        },
                        {
                            "word_text": "zavlaží"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "A oseníčko oblaží.",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 9,
