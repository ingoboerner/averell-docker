                        {
                            "word_text": "Což"
                        },
                        {
                            "word_text": "nevidíš"
                        },
                        {
                            "word_text": "tam"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "tím"
                        },
                        {
                            "word_text": "lesem"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "To černé hrozné mračno? věru,",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "To"
                        },
                        {
                            "word_text": "černé"
                        },
                        {
                            "word_text": "hrozné"
                        },
                        {
                            "word_text": "mračno"
                        },
                        {
                            "word_text": "věru"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Já za znamení zkázy největší je beru.",
                    "metrical_pattern": "0100010100010",
                    "line_length": 13,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Já"
                        },
                        {
                            "word_text": "za"
                        },
                        {
