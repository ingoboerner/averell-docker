                        },
                        {
                            "word_text": "bojoviště"
                        },
                        {
                            "word_text": "střícy"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "A za trubače byl, ten jemuž žlutý nos,",
                    "metrical_pattern": "010000010101",
                    "line_length": 12,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
