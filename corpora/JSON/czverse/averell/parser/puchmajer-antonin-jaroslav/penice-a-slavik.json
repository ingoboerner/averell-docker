                        },
                        {
                            "word_text": "boji"
                        },
                        {
                            "word_text": "příhodný"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Se jim vykáže plac, od kání svobodný.",
                    "metrical_pattern": "101001100100",
                    "line_length": 12,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "jim"
                        },
                        {
                            "word_text": "vykáže"
                        },
                        {
                            "word_text": "plac"
                        },
                        {
                            "word_text": "od"
                        },
                        {
                            "word_text": "kání"
                        },
                        {
                            "word_text": "svobodný"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Vpřed v tichém udolí",
                    "metrical_pattern": "010100",
                    "line_length": 6,
                    "rhyme": 5,
                    "words": [
