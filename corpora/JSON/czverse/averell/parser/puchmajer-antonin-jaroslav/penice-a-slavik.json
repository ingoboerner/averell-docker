                    "line_number": 14,
                    "line_text": "Podál se rozhostila po větvičkách háje.",
                    "metrical_pattern": "1001000100010",
                    "line_length": 13,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Podál"
                        },
                        {
                            "word_text": "se"
