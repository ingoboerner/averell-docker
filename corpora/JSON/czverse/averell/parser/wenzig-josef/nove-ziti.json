            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Tak já tebou teď po trapné pouti",
                    "metrical_pattern": "0010010010",
                    "line_length": 10,
                    "rhyme": 8,
                    "words": [
