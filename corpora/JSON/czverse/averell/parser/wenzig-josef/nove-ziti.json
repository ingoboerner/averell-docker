                    "line_number": 21,
                    "line_text": "I vedoucí k ladné krajině",
                    "metrical_pattern": "010010100",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "I"
                        },
                        {
                            "word_text": "vedoucí"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "ladné"
                        },
                        {
                            "word_text": "krajině"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Pěšinu – tož hned ji nastupuje.",
                    "metrical_pattern": "1000001000",
                    "line_length": 10,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Pěšinu"
                        },
