                    "line_text": "V roucho tmy se úval obálí,",
                    "metrical_pattern": "101010100",
                    "line_length": 9,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "roucho"
                        },
                        {
                            "word_text": "tmy"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "úval"
                        },
                        {
                            "word_text": "obálí"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Kde pastýřů zněly píšťaly:",
                    "metrical_pattern": "010010100",
                    "line_length": 9,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Kde"
                        },
                        {
                            "word_text": "pastýřů"
                        },
                        {
                            "word_text": "zněly"
                        },
                        {
                            "word_text": "píšťaly"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Vábí mě i dosud žalostnění.",
                    "metrical_pattern": "1000101000",
                    "line_length": 10,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Vábí"
                        },
                        {
                            "word_text": "mě"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "dosud"
                        },
                        {
                            "word_text": "žalostnění"
                        }
                    ]
                }
