                    "line_number": 16,
                    "line_text": "Bloudivší po lesní pustině,",
                    "metrical_pattern": "100100100",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Bloudivší"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "lesní"
                        },
                        {
                            "word_text": "pustině"
                        }
                    ]
                },
