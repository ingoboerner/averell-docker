                        },
                        {
                            "word_text": "výšině"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Kteráž oku rozhled poskytuje!",
                    "metrical_pattern": "1010101000",
                    "line_length": 10,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Kteráž"
                        },
                        {
                            "word_text": "oku"
                        },
                        {
                            "word_text": "rozhled"
                        },
                        {
                            "word_text": "poskytuje"
                        }
                    ]
