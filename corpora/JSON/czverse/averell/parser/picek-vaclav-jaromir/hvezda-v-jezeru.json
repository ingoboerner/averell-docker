        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "„Aj matinko, co to spadlo?",
                    "metrical_pattern": "11001010",
                    "line_length": 8,
