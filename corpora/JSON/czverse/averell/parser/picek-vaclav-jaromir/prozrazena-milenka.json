        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Proč se bráníš, že tě líbám?",
                    "metrical_pattern": "00101010",
                    "line_length": 8,
