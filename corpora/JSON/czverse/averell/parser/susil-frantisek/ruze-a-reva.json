                    "metrical_pattern": "101010",
                    "line_length": 6,
                    "rhyme": 22,
                    "words": [
                        {
                            "word_text": "Mocné"
                        },
                        {
                            "word_text": "tvojí"
                        },
                        {
                            "word_text": "berle"
                        }
                    ]
                }
            ],
            "stanza_text": "Což já láskou hořím\nK tobě květin perle!\nCož se nice kořím\nMocné tvojí berle!"
        },
