        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "„Královna ta růže",
                    "metrical_pattern": "100010",
                    "line_length": 6,
                    "rhyme": 7,
                    "words": [
