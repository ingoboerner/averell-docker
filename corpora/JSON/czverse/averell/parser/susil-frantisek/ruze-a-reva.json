                    "metrical_pattern": "001010",
                    "line_length": 6,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Co"
                        },
                        {
                            "word_text": "jí"
                        },
                        {
                            "word_text": "srdce"
                        },
                        {
                            "word_text": "káže"
                        }
                    ]
                }
