                    "line_number": 24,
                    "line_text": "Radostí se z toho!",
                    "metrical_pattern": "100010",
                    "line_length": 6,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Radostí"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "toho"
                        }
                    ]
