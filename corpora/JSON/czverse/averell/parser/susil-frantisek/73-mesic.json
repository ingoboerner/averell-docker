                        },
                        {
                            "word_text": "našel"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Výše a výše pořád vyvstává množstvo odevšad",
                    "metrical_pattern": "100101010010100",
                    "line_length": 15,
                    "rhyme": null,
