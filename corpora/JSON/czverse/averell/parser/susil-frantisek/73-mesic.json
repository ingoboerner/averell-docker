                        {
                            "word_text": "množstvo"
                        },
                        {
                            "word_text": "odevšad"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Stínův a vráží jak tlupa na mne vrahů.",
                    "metrical_pattern": "100100101010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
