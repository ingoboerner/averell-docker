                        },
                        {
                            "word_text": "slunka"
                        },
                        {
                            "word_text": "roní"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Svět vůkol se tají, kvítí oči líně zavírá,",
                    "metrical_pattern": "110010101010100",
                    "line_length": 15,
                    "rhyme": null,
