                        },
                        {
                            "word_text": "vrata"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Noc tu o zem jako o spící klade závoj o dítě,",
                    "metrical_pattern": "1010101001010100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Noc"
                        },
