                        {
                            "word_text": "zapuď"
                        },
                        {
                            "word_text": "nepřátely"
                        },
                        {
                            "word_text": "tyhle"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "A sjednej souzvuk v přírodě k tužbě mojí.",
                    "metrical_pattern": "010101001010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
