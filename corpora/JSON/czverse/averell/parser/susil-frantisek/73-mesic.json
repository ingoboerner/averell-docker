            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tuhle stojím a čekám, až měsíc mi vyjde,",
                    "metrical_pattern": "1010010010010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
