                        {
                            "word_text": "kvítí"
                        },
                        {
                            "word_text": "ubudí"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "V tvého těla orudí?",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "V"
