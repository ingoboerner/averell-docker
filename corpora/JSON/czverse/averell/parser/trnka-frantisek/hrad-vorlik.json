            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Haj právo nevinnosti, právo ctnosti,",
                    "metrical_pattern": "01010001010",
                    "line_length": 11,
                    "rhyme": 12,
