            ],
            "stanza_text": "V tom místě pustém snad i syna svého\nKmet čestný k lásce České měl,\nA z srdce vlasti věrně oddaného\nTak rozhorleně k němu děl:"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "„Můj synu! staniž se svých předků hodným,",
                    "metrical_pattern": "01010001010",
                    "line_length": 11,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Můj"
                        },
                        {
                            "word_text": "synu"
                        },
                        {
                            "word_text": "staniž"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "svých"
                        },
                        {
                            "word_text": "předků"
                        },
                        {
                            "word_text": "hodným"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Buď ctnosti skalou, zastaň vlast;",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Buď"
                        },
                        {
                            "word_text": "ctnosti"
                        },
                        {
                            "word_text": "skalou"
                        },
                        {
                            "word_text": "zastaň"
                        },
                        {
                            "word_text": "vlast"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Své mysli neroď dáti činům svodným,",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Své"
                        },
                        {
                            "word_text": "mysli"
                        },
                        {
                            "word_text": "neroď"
                        },
                        {
                            "word_text": "dáti"
                        },
                        {
                            "word_text": "činům"
                        },
                        {
                            "word_text": "svodným"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "A mužně zmáhej každou strast.",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "mužně"
                        },
                        {
                            "word_text": "zmáhej"
                        },
                        {
                            "word_text": "každou"
                        },
                        {
                            "word_text": "strast"
                        }
