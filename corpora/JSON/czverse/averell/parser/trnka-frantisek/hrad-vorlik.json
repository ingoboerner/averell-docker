        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "V tom místě pustém snad i syna svého",
                    "metrical_pattern": "01010001010",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
