                        },
                        {
                            "word_text": "České"
                        },
                        {
                            "word_text": "měl"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "A z srdce vlasti věrně oddaného",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
