                        {
                            "word_text": "rušitelům"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Od Vlasti zvracuj nehodu!",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Od"
                        },
                        {
                            "word_text": "Vlasti"
                        },
                        {
                            "word_text": "zvracuj"
                        },
                        {
                            "word_text": "nehodu"
                        }
                    ]
                }
