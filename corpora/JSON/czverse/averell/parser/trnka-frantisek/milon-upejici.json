{
    "poem_title": "Miloň upějící.",
    "author": "Trnka, František",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Proč mne dívenko milá! nevidíš ach! ráda na pastvě,",
                    "metrical_pattern": "0010010100110100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Proč"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "dívenko"
                        },
                        {
                            "word_text": "milá"
                        },
                        {
                            "word_text": "nevidíš"
                        },
                        {
                            "word_text": "ach"
                        },
                        {
                            "word_text": "ráda"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "pastvě"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Přiblížím-li se já k tobě? a proč, když se tě táži:",
                    "metrical_pattern": "100000101010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Přiblížím"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "já"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "tobě"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "proč"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "tě"
                        },
                        {
                            "word_text": "táži"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Uzřelalis mé po skalinách tu se toulati jehně?",
                    "metrical_pattern": "1000010000010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Uzřelalis"
                        },
                        {
                            "word_text": "mé"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "skalinách"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "toulati"
                        },
                        {
                            "word_text": "jehně"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Ukrutná ty mlčíš, usedá moje srdce žalostí.",
                    "metrical_pattern": "1000101001010100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ukrutná"
                        },
                        {
                            "word_text": "ty"
                        },
                        {
                            "word_text": "mlčíš"
                        },
                        {
                            "word_text": "usedá"
                        },
                        {
                            "word_text": "moje"
                        },
                        {
                            "word_text": "srdce"
                        },
                        {
                            "word_text": "žalostí"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Dobře mi ach! rozumíš, kdy se táži na jehně pozašlé.",
                    "metrical_pattern": "10011001010100100",
                    "line_length": 17,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Dobře"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "ach"
                        },
                        {
                            "word_text": "rozumíš"
                        },
                        {
                            "word_text": "kdy"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "táži"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "jehně"
                        },
                        {
                            "word_text": "pozašlé"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Brav můj krotký jest, a nerad z oulehle vybíhá;",
                    "metrical_pattern": "10100010100100",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Brav"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "krotký"
                        },
                        {
                            "word_text": "jest"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "nerad"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "oulehle"
                        },
                        {
                            "word_text": "vybíhá"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Toť jen lásky vinou, a vinou tvou, Rodko milostná!",
                    "metrical_pattern": "001010010010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Toť"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "lásky"
                        },
                        {
                            "word_text": "vinou"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "vinou"
                        },
                        {
                            "word_text": "tvou"
                        },
                        {
                            "word_text": "Rodko"
                        },
                        {
                            "word_text": "milostná"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ty krásnás na pahorku milá, an slunce vychází,",
                    "metrical_pattern": "010100010010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
                        },
                        {
                            "word_text": "krásnás"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "pahorku"
                        },
                        {
                            "word_text": "milá"
                        },
                        {
                            "word_text": "an"
                        },
                        {
                            "word_text": "slunce"
                        },
                        {
                            "word_text": "vychází"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Děvčino modrooká! v bílém co květnice rouše.",
                    "metrical_pattern": "100100010010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Děvčino"
                        },
                        {
                            "word_text": "modrooká"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "bílém"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "květnice"
                        },
                        {
                            "word_text": "rouše"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Mé touží tobě srdce, okem si ho podjala lásky;",
                    "metrical_pattern": "0101010100010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Mé"
                        },
                        {
                            "word_text": "touží"
                        },
                        {
                            "word_text": "tobě"
                        },
                        {
                            "word_text": "srdce"
                        },
                        {
                            "word_text": "okem"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "ho"
                        },
                        {
                            "word_text": "podjala"
                        },
                        {
                            "word_text": "lásky"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Proč věrného se mě vzdaluješ, mne i málo si všímáš?",
                    "metrical_pattern": "0100001001010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Proč"
                        },
                        {
                            "word_text": "věrného"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mě"
                        },
                        {
                            "word_text": "vzdaluješ"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "málo"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "všímáš"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "O, jak rád tebe mám, i raděj než včelka medunku.",
                    "metrical_pattern": "101101010010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "O"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "rád"
                        },
                        {
                            "word_text": "tebe"
                        },
                        {
                            "word_text": "mám"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "raděj"
                        },
                        {
                            "word_text": "než"
                        },
                        {
                            "word_text": "včelka"
                        },
                        {
                            "word_text": "medunku"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Na břehu mám ti salaš vrbovou, a zahrádku malinkou,",
                    "metrical_pattern": "10010101000100100",
                    "line_length": 17,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Na"
                        },
                        {
                            "word_text": "břehu"
                        },
                        {
                            "word_text": "mám"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "salaš"
                        },
                        {
                            "word_text": "vrbovou"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "zahrádku"
                        },
                        {
                            "word_text": "malinkou"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Kdež ve stínu libém snídám i za parna spočívám;",
                    "metrical_pattern": "010010100100100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kdež"
                        },
                        {
                            "word_text": "ve"
                        },
                        {
                            "word_text": "stínu"
                        },
                        {
                            "word_text": "libém"
                        },
                        {
                            "word_text": "snídám"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "za"
                        },
                        {
                            "word_text": "parna"
                        },
                        {
                            "word_text": "spočívám"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Sladké mlíko co stred čisté v nádobce ořešné;",
                    "metrical_pattern": "10100010100100",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Sladké"
                        },
                        {
                            "word_text": "mlíko"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "stred"
                        },
                        {
                            "word_text": "čisté"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "nádobce"
                        },
                        {
                            "word_text": "ořešné"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Třešně i červenavé co tvé ustínka hebounké.",
                    "metrical_pattern": "100100000100100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Třešně"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "červenavé"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "tvé"
                        },
                        {
                            "word_text": "ustínka"
                        },
                        {
                            "word_text": "hebounké"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Pěkně na rákosovou s pěti dírkami píšťalu hlasnou",
                    "metrical_pattern": "10100001010010010",
                    "line_length": 17,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pěkně"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "rákosovou"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "pěti"
                        },
                        {
                            "word_text": "dírkami"
                        },
                        {
                            "word_text": "píšťalu"
                        },
                        {
                            "word_text": "hlasnou"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Hráti umím, a zpívati vhod, pastevkyně sličná!",
                    "metrical_pattern": "101001000100010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hráti"
                        },
                        {
                            "word_text": "umím"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "zpívati"
                        },
                        {
                            "word_text": "vhod"
                        },
                        {
                            "word_text": "pastevkyně"
                        },
                        {
                            "word_text": "sličná"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Lib-liž tento uchyst tobě můj, miluješli mě věrně,",
                    "metrical_pattern": "1010101001000010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lib"
                        },
                        {
                            "word_text": "liž"
                        },
                        {
                            "word_text": "tento"
                        },
                        {
                            "word_text": "uchyst"
                        },
                        {
                            "word_text": "tobě"
                        },
                        {
                            "word_text": "můj"
                        },
                        {
                            "word_text": "miluješli"
                        },
                        {
                            "word_text": "mě"
                        },
                        {
                            "word_text": "věrně"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Jestli jenom se upejpáš, stádce v jedno sežeňme.",
                    "metrical_pattern": "101001001010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jestli"
                        },
                        {
                            "word_text": "jenom"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "upejpáš"
                        },
                        {
                            "word_text": "stádce"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "jedno"
                        },
                        {
                            "word_text": "sežeňme"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Se mnou, děvče zlaté! v jedné chyžince obývej,",
                    "metrical_pattern": "10101010100100",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "mnou"
                        },
                        {
                            "word_text": "děvče"
                        },
                        {
                            "word_text": "zlaté"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "jedné"
                        },
                        {
                            "word_text": "chyžince"
                        },
                        {
                            "word_text": "obývej"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Vemenaté se mnou doj ovce a jehňata hlídej;",
                    "metrical_pattern": "100010110010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vemenaté"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mnou"
                        },
                        {
                            "word_text": "doj"
                        },
                        {
                            "word_text": "ovce"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "jehňata"
                        },
                        {
                            "word_text": "hlídej"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Se mnou, ach! jakové snažení nás v dílo zanítí:",
                    "metrical_pattern": "101100100010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "mnou"
                        },
                        {
                            "word_text": "ach"
                        },
                        {
                            "word_text": "jakové"
                        },
                        {
                            "word_text": "snažení"
                        },
                        {
                            "word_text": "nás"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "dílo"
                        },
                        {
                            "word_text": "zanítí"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Stroj mi večeř chutnou, kdykoliv se z louky navrátím.",
                    "metrical_pattern": "101010100010100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Stroj"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "večeř"
                        },
                        {
                            "word_text": "chutnou"
                        },
                        {
                            "word_text": "kdykoliv"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "louky"
                        },
                        {
                            "word_text": "navrátím"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Jak mi z rukou ladných jídlo vše tvých bude chutnat! –",
                    "metrical_pattern": "00101010001010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jak"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "rukou"
                        },
                        {
                            "word_text": "ladných"
                        },
                        {
                            "word_text": "jídlo"
                        },
                        {
                            "word_text": "vše"
                        },
                        {
                            "word_text": "tvých"
                        },
                        {
                            "word_text": "bude"
                        },
                        {
                            "word_text": "chutnat"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Však co nyní sobě nesmyslný jen na hru zamejšlím?",
                    "metrical_pattern": "0010101000010100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Však"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "nyní"
                        },
                        {
                            "word_text": "sobě"
                        },
                        {
                            "word_text": "nesmyslný"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "hru"
                        },
                        {
                            "word_text": "zamejšlím"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Chciť veselit se z lásky, či vím, co za růže přináší?",
                    "metrical_pattern": "1100010010100100",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Chciť"
                        },
                        {
                            "word_text": "veselit"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "lásky"
                        },
                        {
                            "word_text": "či"
                        },
                        {
                            "word_text": "vím"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "za"
                        },
                        {
                            "word_text": "růže"
                        },
                        {
                            "word_text": "přináší"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Kam nerozum tě zavádí tvůj, ovčíři nehodný!",
                    "metrical_pattern": "010001000100100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Kam"
                        },
                        {
                            "word_text": "nerozum"
                        },
                        {
                            "word_text": "tě"
                        },
                        {
                            "word_text": "zavádí"
                        },
                        {
                            "word_text": "tvůj"
                        },
                        {
                            "word_text": "ovčíři"
                        },
                        {
                            "word_text": "nehodný"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Práce zanedbáváš plzné, marnosti shovíváš:",
