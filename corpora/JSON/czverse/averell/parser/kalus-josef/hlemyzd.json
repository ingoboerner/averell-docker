                        },
                        {
                            "word_text": "budku"
                        },
                        {
                            "word_text": "zlatí"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "„Chacha! Platí, bloudku, platí!",
                    "metrical_pattern": "10101010",
