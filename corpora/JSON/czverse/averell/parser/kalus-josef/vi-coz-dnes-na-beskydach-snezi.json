        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "K mateřské jej vinou hrudi.",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
