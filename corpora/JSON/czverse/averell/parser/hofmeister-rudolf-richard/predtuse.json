                    "line_number": 11,
                    "line_text": "kdy do srdce se kradí",
                    "metrical_pattern": "0100010",
                    "line_length": 7,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "kdy"
                        },
                        {
