                {
                    "line_number": 20,
                    "line_text": "Šavle, potřebuje tebe polská otčina.",
                    "metrical_pattern": "1010001010100",
                    "line_length": 13,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Šavle"
