            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "„Tam pospícháš k bratrům drahým, krásné slunce denní,",
                    "metrical_pattern": "01001010101010",
                    "line_length": 14,
                    "rhyme": 4,
