        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "„Nechť nebe i země se třese,",
                    "metrical_pattern": "010010010",
                    "line_length": 9,
