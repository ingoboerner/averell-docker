            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Obrazy ty Polák zírá v chrámu Páně –",
                    "metrical_pattern": "100010101010",
                    "line_length": 12,
