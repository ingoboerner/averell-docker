            ],
            "stanza_text": "Plná luna druhdy Polska – teď jak měsíc nový!\nProto nevýslovně bolno Polákovi:"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Od černého moře až tam k baltickému",
                    "metrical_pattern": "100010001000",
                    "line_length": 12,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Od"
                        },
                        {
                            "word_text": "černého"
                        },
                        {
