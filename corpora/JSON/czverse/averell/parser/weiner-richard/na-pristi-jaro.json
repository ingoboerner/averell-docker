            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Jednoho rána se probudím s úsměvem ze sna.",
                    "metrical_pattern": "10010010010010",
                    "line_length": 14,
                    "rhyme": 1,
                    "words": [
