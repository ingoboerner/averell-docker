                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "hledím"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "na holé smutné stromy, do ulic plných sněhu!",
                    "metrical_pattern": "10010101001010",
                    "line_length": 14,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "holé"
                        },
                        {
                            "word_text": "smutné"
                        },
                        {
                            "word_text": "stromy"
