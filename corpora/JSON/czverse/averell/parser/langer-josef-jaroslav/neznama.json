                    "line_number": 31,
                    "line_text": "Plápolaloť mé tu srdce",
                    "metrical_pattern": "10000010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Plápolaloť"
                        },
                        {
                            "word_text": "mé"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "srdce"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Horoucí.",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Horoucí"
                        }
                    ]
                }
            ],
            "stanza_text": "V útrobě mi vznikla jiskra\nLásky nesmírné – nedlouho –\nPlápolaloť mé tu srdce\nHoroucí."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Pojmu tu bílou ručičku",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pojmu"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "bílou"
                        },
                        {
                            "word_text": "ručičku"
                        }
                    ]
                },
