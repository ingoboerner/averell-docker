{
    "poem_title": "Neznámá.",
    "author": "Langer, Josef Jaroslav",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Spánek lahodný zaletí",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Spánek"
                        },
                        {
                            "word_text": "lahodný"
                        },
                        {
                            "word_text": "zaletí"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Ke mně; ranní sen blažil mne",
                    "metrical_pattern": "10101100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ke"
                        },
                        {
                            "word_text": "mně"
                        },
                        {
                            "word_text": "ranní"
                        },
                        {
                            "word_text": "sen"
                        },
                        {
                            "word_text": "blažil"
                        },
                        {
                            "word_text": "mne"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Obrazotvorný; kratičký –",
                    "metrical_pattern": "10000100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Obrazotvorný"
                        },
                        {
                            "word_text": "kratičký"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Libý předc!",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Libý"
                        },
                        {
                            "word_text": "předc"
                        }
                    ]
                }
            ],
            "stanza_text": "Spánek lahodný zaletí\nKe mně; ranní sen blažil mne\nObrazotvorný; kratičký –\nLibý předc!"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Zdálo se mi, své varytko",
                    "metrical_pattern": "10000100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zdálo"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "své"
                        },
                        {
                            "word_text": "varytko"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Tiskna na prs, že jsem sladce",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tiskna"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "prs"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "jsem"
                        },
                        {
                            "word_text": "sladce"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Zpíval; o čem? jak to vezdy –",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zpíval"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "čem"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "vezdy"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "O lásce.",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "O"
                        },
                        {
                            "word_text": "lásce"
                        }
                    ]
                }
            ],
            "stanza_text": "Zdálo se mi, své varytko\nTiskna na prs, že jsem sladce\nZpíval; o čem? jak to vezdy –\nO lásce."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "V tom se poblíží divenka",
                    "metrical_pattern": "00100100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "tom"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "poblíží"
                        },
                        {
                            "word_text": "divenka"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Ke mně; její líce růžné,",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ke"
                        },
                        {
                            "word_text": "mně"
                        },
                        {
                            "word_text": "její"
                        },
                        {
                            "word_text": "líce"
                        },
                        {
                            "word_text": "růžné"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Čílko bělé; vlásky hebké",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Čílko"
                        },
                        {
                            "word_text": "bělé"
                        },
                        {
                            "word_text": "vlásky"
                        },
                        {
                            "word_text": "hebké"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Vlají přes",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vlají"
                        },
                        {
                            "word_text": "přes"
                        }
                    ]
                }
            ],
            "stanza_text": "V tom se poblíží divenka\nKe mně; její líce růžné,\nČílko bělé; vlásky hebké\nVlají přes"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Nádra labutí; očinka",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Nádra"
                        },
                        {
                            "word_text": "labutí"
                        },
                        {
                            "word_text": "očinka"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Jí modravá; ústka se rdí",
                    "metrical_pattern": "11001001",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jí"
                        },
                        {
                            "word_text": "modravá"
                        },
                        {
                            "word_text": "ústka"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "rdí"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "V červenu pěstovaném",
                    "metrical_pattern": "1001000",
                    "line_length": 7,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "červenu"
                        },
                        {
                            "word_text": "pěstovaném"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Nyjící.",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Nyjící"
                        }
                    ]
                }
            ],
            "stanza_text": "Nádra labutí; očinka\nJí modravá; ústka se rdí\nV červenu pěstovaném\nNyjící."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Mysle, že anděl s nebeska",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Mysle"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "anděl"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "nebeska"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Ke mně tu kráčí – mlčel jsem,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ke"
                        },
                        {
                            "word_text": "mně"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "kráčí"
                        },
                        {
                            "word_text": "mlčel"
                        },
                        {
                            "word_text": "jsem"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Hrůza posvátná neboť mne",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hrůza"
                        },
                        {
                            "word_text": "posvátná"
                        },
                        {
                            "word_text": "neboť"
                        },
                        {
                            "word_text": "mne"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Obešla.",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Obešla"
                        }
                    ]
                }
            ],
            "stanza_text": "Mysle, že anděl s nebeska\nKe mně tu kráčí – mlčel jsem,\nHrůza posvátná neboť mne\nObešla."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Postava sličná popojde",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Postava"
                        },
                        {
                            "word_text": "sličná"
                        },
                        {
                            "word_text": "popojde"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Dále, a modré očinko",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Dále"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "modré"
                        },
                        {
                            "word_text": "očinko"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Na mne se dívá; jak milo",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Na"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "dívá"
                        },
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "milo"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Se dívá!",
                    "metrical_pattern": "010",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "dívá"
                        }
                    ]
                }
            ],
            "stanza_text": "Postava sličná popojde\nDále, a modré očinko\nNa mne se dívá; jak milo\nSe dívá!"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Tu položím své varytko",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "položím"
                        },
                        {
                            "word_text": "své"
                        },
                        {
                            "word_text": "varytko"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "A jmu se zírat u krásu –",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "jmu"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "zírat"
                        },
                        {
                            "word_text": "u"
                        },
                        {
                            "word_text": "krásu"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Však je mi neznáma dívka",
                    "metrical_pattern": "00010010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Však"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "neznáma"
                        },
                        {
                            "word_text": "dívka"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Milostná.",
                    "metrical_pattern": "100",
                    "line_length": 3,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Milostná"
                        }
                    ]
                }
            ],
            "stanza_text": "Tu položím své varytko\nA jmu se zírat u krásu –\nVšak je mi neznáma dívka\nMilostná."
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "V útrobě mi vznikla jiskra",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "útrobě"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "vznikla"
                        },
                        {
                            "word_text": "jiskra"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Lásky nesmírné – nedlouho –",
                    "metrical_pattern": "10100100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lásky"
                        },
                        {
                            "word_text": "nesmírné"
                        },
                        {
                            "word_text": "nedlouho"
                        }
                    ]
                },
