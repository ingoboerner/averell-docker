{
    "poem_title": "Zem se usmívá, nebe mladne; božská",
    "author": "Langer, Josef Jaroslav",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Rákose hovorný, a ty vodo,",
                    "metrical_pattern": "1001001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
