                        {
                            "word_text": "černé"
                        },
                        {
                            "word_text": "díry"
                        },
                        {
                            "word_text": "bystro"
                        },
                        {
                            "word_text": "proti"
                        },
                        {
                            "word_text": "němu"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Vyletělo sedmero sov černých.",
                    "metrical_pattern": "1000100110",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vyletělo"
                        },
                        {
                            "word_text": "sedmero"
                        },
                        {
                            "word_text": "sov"
                        },
                        {
                            "word_text": "černých"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "„Hoj, ty june, june zlatovlasý!",
                    "metrical_pattern": "1010101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
