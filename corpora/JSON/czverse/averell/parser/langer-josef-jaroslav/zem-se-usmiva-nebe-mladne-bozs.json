                        {
                            "word_text": "hory"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "doly"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "nevěstě"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "A když přijde ku záborské skále,",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
