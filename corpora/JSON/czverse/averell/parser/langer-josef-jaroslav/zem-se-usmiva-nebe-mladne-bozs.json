                        {
                            "word_text": "černých"
                        },
                        {
                            "word_text": "lesóv"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "„Vrať se, vrať se, june zlatovlasý,",
                    "metrical_pattern": "1010101000",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vrať"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "vrať"
                        },
