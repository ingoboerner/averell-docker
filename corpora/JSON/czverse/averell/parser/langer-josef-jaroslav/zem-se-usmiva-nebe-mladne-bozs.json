                        },
                        {
                            "word_text": "moje"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Lekne se tu Kalvod hrůzou jatý.",
                    "metrical_pattern": "1000101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Lekne"
