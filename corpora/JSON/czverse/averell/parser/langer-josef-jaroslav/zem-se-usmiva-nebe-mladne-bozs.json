                        {
                            "word_text": "soumraku"
                        },
                        {
                            "word_text": "večerním"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Aj, není to sokol, ani orel,",
                    "metrical_pattern": "1100101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Aj"
                        },
                        {
                            "word_text": "není"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "sokol"
                        },
                        {
                            "word_text": "ani"
                        },
                        {
                            "word_text": "orel"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ale Kalvod je to, rybák mladý;",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ale"
                        },
                        {
                            "word_text": "Kalvod"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "rybák"
                        },
                        {
                            "word_text": "mladý"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Běží přes hory a doly prudce,",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
