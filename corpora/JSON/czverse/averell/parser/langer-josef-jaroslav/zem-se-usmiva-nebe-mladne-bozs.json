                            "word_text": "Nechoď"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "noci"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "lukách"
                        },
                        {
                            "word_text": "polabských"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Ať nezavedou tě bludná světla!“",
                    "metrical_pattern": "0100001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ať"
                        },
                        {
                            "word_text": "nezavedou"
                        },
                        {
                            "word_text": "tě"
                        },
                        {
                            "word_text": "bludná"
                        },
                        {
                            "word_text": "světla"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Ale neposlechnul bujný Kalvod,",
                    "metrical_pattern": "1010001010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ale"
                        },
                        {
                            "word_text": "neposlechnul"
                        },
                        {
                            "word_text": "bujný"
                        },
                        {
                            "word_text": "Kalvod"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Co mu nočních sov hučelo sedum;",
                    "metrical_pattern": "0010110010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
