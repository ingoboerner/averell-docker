                        },
                        {
                            "word_text": "dubu"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Zašumělo jemu husté větví:",
                    "metrical_pattern": "1000101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zašumělo"
                        },
                        {
