                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "přijde"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "záborské"
                        },
                        {
                            "word_text": "skále"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Z černé díry bystro proti němu",
                    "metrical_pattern": "1010101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Z"
