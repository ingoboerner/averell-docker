                            "word_text": "a"
                        },
                        {
                            "word_text": "ty"
                        },
                        {
                            "word_text": "vodo"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Vodo šumná! – utichnětež nyní,",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vodo"
                        },
                        {
                            "word_text": "šumná"
                        },
                        {
                            "word_text": "utichnětež"
                        },
                        {
                            "word_text": "nyní"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Neboť zpívám o nesmírné lásce! –",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neboť"
                        },
                        {
                            "word_text": "zpívám"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "nesmírné"
                        },
                        {
                            "word_text": "lásce"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Co to letí ze tmavého lesa?",
                    "metrical_pattern": "0010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Co"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "letí"
                        },
                        {
                            "word_text": "ze"
                        },
                        {
                            "word_text": "tmavého"
                        },
                        {
                            "word_text": "lesa"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Zelený to sokol? čili orel,",
                    "metrical_pattern": "1000101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zelený"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "sokol"
                        },
                        {
                            "word_text": "čili"
                        },
                        {
                            "word_text": "orel"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Černý orel v soumraku večerním?",
                    "metrical_pattern": "1010100100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Černý"
                        },
                        {
                            "word_text": "orel"
                        },
                        {
                            "word_text": "v"
