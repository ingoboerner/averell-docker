                        },
                        {
                            "word_text": "nočních"
                        },
                        {
                            "word_text": "sov"
                        },
                        {
                            "word_text": "hučelo"
                        },
                        {
                            "word_text": "sedum"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Běží lesinou jelenem prudkým,",
                    "metrical_pattern": "1010010010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Běží"
                        },
                        {
                            "word_text": "lesinou"
                        },
                        {
                            "word_text": "jelenem"
                        },
                        {
                            "word_text": "prudkým"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Šumným větrem přes vysoké hory,",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Šumným"
                        },
                        {
                            "word_text": "větrem"
                        },
                        {
                            "word_text": "přes"
                        },
                        {
                            "word_text": "vysoké"
                        },
                        {
                            "word_text": "hory"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "A vodou se jako ryba brodí. –",
                    "metrical_pattern": "0100101010",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
