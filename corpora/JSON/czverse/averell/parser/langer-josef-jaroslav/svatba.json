    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Z Krkonošských hor se Labe",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Z"
                        },
                        {
                            "word_text": "Krkonošských"
                        },
                        {
                            "word_text": "hor"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "Labe"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Vážně vyvalilo,",
                    "metrical_pattern": "101000",
                    "line_length": 6,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Vážně"
                        },
                        {
                            "word_text": "vyvalilo"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Trudně sobě dráhu dlabe",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Trudně"
                        },
                        {
                            "word_text": "sobě"
                        },
                        {
                            "word_text": "dráhu"
                        },
                        {
                            "word_text": "dlabe"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Až ku prorockému dubu,",
                    "metrical_pattern": "01000010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Až"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "prorockému"
                        },
                        {
                            "word_text": "dubu"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Jenž se nehrozí věčnosti zubu.",
                    "metrical_pattern": "0010010010",
                    "line_length": 10,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Jenž"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "nehrozí"
                        },
                        {
                            "word_text": "věčnosti"
                        },
                        {
                            "word_text": "zubu"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Aj, tu se mu božstvo vyjevilo.",
                    "metrical_pattern": "1100101000",
                    "line_length": 10,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Aj"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mu"
                        },
                        {
                            "word_text": "božstvo"
                        },
                        {
                            "word_text": "vyjevilo"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "„Řeko, stůj!",
                    "metrical_pattern": "101",
                    "line_length": 3,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Řeko"
                        },
                        {
                            "word_text": "stůj"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Proč točíš tak na pravo tok svůj?“",
                    "metrical_pattern": "010010010",
                    "line_length": 9,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Proč"
                        },
                        {
                            "word_text": "točíš"
                        },
                        {
                            "word_text": "tak"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "pravo"
                        },
                        {
                            "word_text": "tok"
                        },
                        {
                            "word_text": "svůj"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Tak se ptalo.",
                    "metrical_pattern": "0010",
                    "line_length": 4,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Tak"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "ptalo"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Labe v odpověd mu dalo:",
                    "metrical_pattern": "10100010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Labe"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "odpověd"
                        },
                        {
                            "word_text": "mu"
                        },
                        {
                            "word_text": "dalo"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "„Abych se žádným se nepotkalo.“",
                    "metrical_pattern": "1010001000",
                    "line_length": 10,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Abych"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "žádným"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "nepotkalo"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "To když řeknulo,",
                    "metrical_pattern": "00100",
                    "line_length": 5,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "To"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "řeknulo"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Šumotem pěnným se zedmulo.",
                    "metrical_pattern": "100100100",
                    "line_length": 9,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Šumotem"
                        },
                        {
                            "word_text": "pěnným"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "zedmulo"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "V tom od východu",
                    "metrical_pattern": "01000",
                    "line_length": 5,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "tom"
                        },
                        {
                            "word_text": "od"
                        },
                        {
                            "word_text": "východu"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Jilovatém po schodu",
                    "metrical_pattern": "1000100",
                    "line_length": 7,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Jilovatém"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "schodu"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Přiletěla bystrá Orlice;",
                    "metrical_pattern": "100010100",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Přiletěla"
                        },
                        {
                            "word_text": "bystrá"
                        },
                        {
                            "word_text": "Orlice"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "A když Labe uhlídala,",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "Labe"
                        },
                        {
                            "word_text": "uhlídala"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Dívčím studem na levo se dala.",
                    "metrical_pattern": "1010100010",
                    "line_length": 10,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "Dívčím"
                        },
                        {
                            "word_text": "studem"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "levo"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "dala"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "„Řeko, stůj!",
                    "metrical_pattern": "101",
                    "line_length": 3,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Řeko"
                        },
                        {
                            "word_text": "stůj"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Proč vracíš tok svůj?",
                    "metrical_pattern": "01010",
                    "line_length": 5,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Proč"
                        },
                        {
                            "word_text": "vracíš"
                        },
                        {
                            "word_text": "tok"
                        },
                        {
                            "word_text": "svůj"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Sem se viň, zde ženich tvůj!",
                    "metrical_pattern": "0010100",
                    "line_length": 7,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Sem"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "viň"
                        },
                        {
                            "word_text": "zde"
                        },
                        {
                            "word_text": "ženich"
                        },
                        {
                            "word_text": "tvůj"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Růžolící Labe",
                    "metrical_pattern": "100010",
                    "line_length": 6,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Růžolící"
                        },
                        {
                            "word_text": "Labe"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Cestu sobě k své nevěstě dlabe.",
                    "metrical_pattern": "1010010010",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Cestu"
                        },
                        {
                            "word_text": "sobě"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "své"
                        },
                        {
                            "word_text": "nevěstě"
                        },
                        {
                            "word_text": "dlabe"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Tys jeho, on budiž tvůj!“",
                    "metrical_pattern": "0100100",
                    "line_length": 7,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "Tys"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "budiž"
                        },
                        {
                            "word_text": "tvůj"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Co když mladé Labe slyšelo,",
                    "metrical_pattern": "001010100",
                    "line_length": 9,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Co"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "mladé"
                        },
                        {
                            "word_text": "Labe"
                        },
                        {
                            "word_text": "slyšelo"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Hned studem se zardělo –",
                    "metrical_pattern": "0100100",
                    "line_length": 7,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "Hned"
                        },
                        {
                            "word_text": "studem"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "zardělo"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Neboť se jemu zalíbila",
                    "metrical_pattern": "100101000",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neboť"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jemu"
                        },
                        {
                            "word_text": "zalíbila"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Nelíbaná posud mladice,",
                    "metrical_pattern": "100010100",
                    "line_length": 9,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Nelíbaná"
                        },
                        {
                            "word_text": "posud"
                        },
                        {
                            "word_text": "mladice"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Lepá Orlice;",
                    "metrical_pattern": "10100",
                    "line_length": 5,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "Lepá"
                        },
                        {
                            "word_text": "Orlice"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "A když slunce zapadalo,",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "slunce"
                        },
                        {
                            "word_text": "zapadalo"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Tito proudové se spojili,",
                    "metrical_pattern": "101000100",
                    "line_length": 9,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Tito"
                        },
                        {
                            "word_text": "proudové"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "spojili"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Barvu jen, ne jestotu svou změnili,",
                    "metrical_pattern": "10001000100",
                    "line_length": 11,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Barvu"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "jestotu"
                        },
                        {
                            "word_text": "svou"
                        },
                        {
                            "word_text": "změnili"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Potom hlučnou svatbu strojili.",
                    "metrical_pattern": "101010100",
                    "line_length": 9,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "Potom"
                        },
                        {
                            "word_text": "hlučnou"
                        },
                        {
                            "word_text": "svatbu"
                        },
                        {
                            "word_text": "strojili"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Všecko ku kvasu se hnalo",
                    "metrical_pattern": "10100010",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Všecko"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "kvasu"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "hnalo"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Z naší vlasti zdálené: –",
                    "metrical_pattern": "1010100",
                    "line_length": 7,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Z"
                        },
                        {
                            "word_text": "naší"
                        },
                        {
                            "word_text": "vlasti"
                        },
                        {
                            "word_text": "zdálené"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Všecko Češi jen a Slované! –",
                    "metrical_pattern": "101000100",
                    "line_length": 9,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "Všecko"
                        },
                        {
                            "word_text": "Češi"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "Slované"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Já tam také byl,",
                    "metrical_pattern": "00100",
                    "line_length": 5,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Já"
                        },
                        {
                            "word_text": "tam"
                        },
                        {
                            "word_text": "také"
                        },
                        {
                            "word_text": "byl"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "S nimi jed’ a s nimi pil;",
                    "metrical_pattern": "1000101",
                    "line_length": 7,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "S"
                        },
                        {
                            "word_text": "nimi"
                        },
                        {
                            "word_text": "jed"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "nimi"
                        },
                        {
                            "word_text": "pil"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "A proč bych se za to styděl?",
                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "proč"
                        },
                        {
                            "word_text": "bych"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "za"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "styděl"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Vždyť to každý viděl,",
                    "metrical_pattern": "001010",
                    "line_length": 6,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "Vždyť"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "každý"
                        },
                        {
                            "word_text": "viděl"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "Že mne – pozvali. –",
                    "metrical_pattern": "00100",
                    "line_length": 5,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "Že"
                        },
                        {
                            "word_text": "mne"
                        },
                        {
                            "word_text": "pozvali"
                        }
                    ]
                },
                {
                    "line_number": 42,
                    "line_text": "Co mně ale nejdivnější bylo,",
                    "metrical_pattern": "0010100010",
                    "line_length": 10,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Co"
                        },
                        {
                            "word_text": "mně"
                        },
                        {
                            "word_text": "ale"
                        },
                        {
                            "word_text": "nejdivnější"
                        },
                        {
                            "word_text": "bylo"
                        }
                    ]
                },
                {
                    "line_number": 43,
                    "line_text": "Jest, že když se hovořilo –",
                    "metrical_pattern": "01001000",
                    "line_length": 8,
                    "rhyme": 19,
                    "words": [
                        {
                            "word_text": "Jest"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "hovořilo"
                        }
                    ]
                },
                {
                    "line_number": 44,
                    "line_text": "Ač se zdálo, jakoby se neznali –",
                    "metrical_pattern": "00101000100",
                    "line_length": 11,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "Ač"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "zdálo"
                        },
                        {
                            "word_text": "jakoby"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "neznali"
                        }
                    ]
                },
                {
                    "line_number": 45,
                    "line_text": "Všickni sobě rozuměli;",
                    "metrical_pattern": "10101000",
                    "line_length": 8,
                    "rhyme": 20,
                    "words": [
                        {
                            "word_text": "Všickni"
                        },
                        {
                            "word_text": "sobě"
                        },
                        {
                            "word_text": "rozuměli"
                        }
                    ]
                },
                {
                    "line_number": 46,
                    "line_text": "A předc jako tyto řeky",
                    "metrical_pattern": "10101010",
                    "line_length": 8,
                    "rhyme": 21,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "předc"
                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "tyto"
