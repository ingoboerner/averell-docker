    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Co klopíte vaše týmě,",
                    "metrical_pattern": "01001010",
                    "line_length": 8,
