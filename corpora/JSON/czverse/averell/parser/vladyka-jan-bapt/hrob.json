                    "line_number": 28,
                    "line_text": "Těm služíme co porobné,",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Těm"
                        },
                        {
                            "word_text": "služíme"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "porobné"
                        }
                    ]
                },
