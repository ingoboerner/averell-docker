                        },
                        {
                            "word_text": "blyští"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Jdouce nad ní. –",
                    "metrical_pattern": "1010",
                    "line_length": 4,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jdouce"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "ní"
                        }
                    ]
                }
