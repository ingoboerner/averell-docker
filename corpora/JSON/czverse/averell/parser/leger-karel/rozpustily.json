{
    "poem_title": "ROZPUSTILÝ.",
    "author": "Leger, Karel",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Minula noční hodina,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "Minula"
                        },
                        {
                            "word_text": "noční"
                        },
                        {
                            "word_text": "hodina"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "ze sna se budí dědina.",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 1,
                    "words": [
                        {
                            "word_text": "ze"
                        },
                        {
                            "word_text": "sna"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "budí"
                        },
                        {
                            "word_text": "dědina"
                        }
                    ]
                }
            ],
            "stanza_text": "Minula noční hodina,\nze sna se budí dědina."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Slunéčko vyšlo nad hory,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Slunéčko"
                        },
                        {
                            "word_text": "vyšlo"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "hory"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Anička naše z komory.",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "Anička"
                        },
                        {
                            "word_text": "naše"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "komory"
                        }
                    ]
                }
            ],
            "stanza_text": "Slunéčko vyšlo nad hory,\nAnička naše z komory."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Vyběhla na dvůr lehýnce,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Vyběhla"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "dvůr"
                        },
                        {
                            "word_text": "lehýnce"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "v jediné byla sukýnce,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "jediné"
                        },
                        {
                            "word_text": "byla"
                        },
                        {
                            "word_text": "sukýnce"
                        }
                    ]
                }
            ],
            "stanza_text": "Vyběhla na dvůr lehýnce,\nv jediné byla sukýnce,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "košilku měla jako sníh",
                    "metrical_pattern": "10010101",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "košilku"
                        },
                        {
                            "word_text": "měla"
                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "sníh"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "a vlasy ve dvou pletencích.",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "vlasy"
                        },
                        {
                            "word_text": "ve"
                        },
                        {
                            "word_text": "dvou"
                        },
                        {
                            "word_text": "pletencích"
                        }
                    ]
                }
            ],
            "stanza_text": "košilku měla jako sníh\na vlasy ve dvou pletencích."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Tvářičky svěží po spánku",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Tvářičky"
                        },
                        {
                            "word_text": "svěží"
                        },
                        {
                            "word_text": "po"
                        },
                        {
                            "word_text": "spánku"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "s odleskem ranních červánků.",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "s"
                        },
                        {
                            "word_text": "odleskem"
                        },
                        {
                            "word_text": "ranních"
                        },
                        {
                            "word_text": "červánků"
                        }
                    ]
                }
            ],
            "stanza_text": "Tvářičky svěží po spánku\ns odleskem ranních červánků."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "A smála se – proč? – Kdož to ví,",
                    "metrical_pattern": "01000101",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "smála"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "proč"
                        },
                        {
                            "word_text": "Kdož"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "ví"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Nu, byl to diblík hotový.",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Nu"
                        },
                        {
                            "word_text": "byl"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "diblík"
                        },
                        {
                            "word_text": "hotový"
                        }
                    ]
                }
            ],
            "stanza_text": "A smála se – proč? – Kdož to ví,\nNu, byl to diblík hotový."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "K studánce sběhla roubené,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "K"
                        },
                        {
                            "word_text": "studánce"
                        },
                        {
                            "word_text": "sběhla"
                        },
                        {
                            "word_text": "roubené"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "nabrala vody studené,",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "nabrala"
                        },
                        {
                            "word_text": "vody"
                        },
                        {
                            "word_text": "studené"
                        }
                    ]
                }
            ],
            "stanza_text": "K studánce sběhla roubené,\nnabrala vody studené,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "A myje tváře jako květ",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "myje"
                        },
                        {
                            "word_text": "tváře"
                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "květ"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "a hrdlo, ručky nad loket.",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "hrdlo"
                        },
                        {
                            "word_text": "ručky"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "loket"
                        }
                    ]
                }
            ],
            "stanza_text": "A myje tváře jako květ\na hrdlo, ručky nad loket."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "A smáčí vlásky bohaté –",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "smáčí"
                        },
                        {
                            "word_text": "vlásky"
                        },
                        {
                            "word_text": "bohaté"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "hm, cizí hled ji nezmate.",
                    "metrical_pattern": "11010100",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "hm"
                        },
                        {
                            "word_text": "cizí"
                        },
                        {
                            "word_text": "hled"
                        },
                        {
                            "word_text": "ji"
                        },
                        {
                            "word_text": "nezmate"
                        }
                    ]
                }
            ],
            "stanza_text": "A smáčí vlásky bohaté –\nhm, cizí hled ji nezmate."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Své vlásky smáčí havraní –",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Své"
                        },
                        {
                            "word_text": "vlásky"
                        },
                        {
                            "word_text": "smáčí"
                        },
                        {
                            "word_text": "havraní"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "a Jeník hledí z ústraní.",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "Jeník"
                        },
                        {
                            "word_text": "hledí"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "ústraní"
                        }
                    ]
                }
            ],
            "stanza_text": "Své vlásky smáčí havraní –\na Jeník hledí z ústraní."
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "A hledí, oči otvírá",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "hledí"
                        },
                        {
                            "word_text": "oči"
                        },
                        {
                            "word_text": "otvírá"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "a láskou skorem umírá.",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 11,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "láskou"
                        },
                        {
                            "word_text": "skorem"
                        },
                        {
                            "word_text": "umírá"
                        }
                    ]
                }
            ],
            "stanza_text": "A hledí, oči otvírá\na láskou skorem umírá."
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "A stojí jako v půdu vryt –",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "stojí"
                        },
                        {
                            "word_text": "jako"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "půdu"
                        },
                        {
                            "word_text": "vryt"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "má – nemá k ní se odvážit?",
                    "metrical_pattern": "11000100",
                    "line_length": 8,
                    "rhyme": 12,
                    "words": [
                        {
                            "word_text": "má"
                        },
                        {
                            "word_text": "nemá"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "ní"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "odvážit"
                        }
                    ]
                }
            ],
            "stanza_text": "A stojí jako v půdu vryt –\nmá – nemá k ní se odvážit?"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Ta krása málo střežená,",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "Ta"
                        },
                        {
                            "word_text": "krása"
                        },
                        {
                            "word_text": "málo"
                        },
                        {
                            "word_text": "střežená"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "ta bílá šíj a ramena!",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 13,
                    "words": [
                        {
                            "word_text": "ta"
                        },
                        {
                            "word_text": "bílá"
                        },
                        {
                            "word_text": "šíj"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "ramena"
                        }
                    ]
                }
            ],
            "stanza_text": "Ta krása málo střežená,\nta bílá šíj a ramena!"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "On věděl dobře, že to hřích,",
                    "metrical_pattern": "01010101",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "On"
                        },
                        {
                            "word_text": "věděl"
                        },
                        {
                            "word_text": "dobře"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "hřích"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "však vězel v sítích ďáblových.",
                    "metrical_pattern": "01010100",
                    "line_length": 8,
                    "rhyme": 14,
                    "words": [
                        {
                            "word_text": "však"
                        },
                        {
                            "word_text": "vězel"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "sítích"
                        },
                        {
                            "word_text": "ďáblových"
                        }
                    ]
                }
            ],
            "stanza_text": "On věděl dobře, že to hřích,\nvšak vězel v sítích ďáblových."
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "A skočil k ní a v náruč jal",
                    "metrical_pattern": "01000101",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "skočil"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "ní"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "náruč"
                        },
                        {
                            "word_text": "jal"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "a celičkou ji zulíbal!",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 15,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "celičkou"
                        },
                        {
                            "word_text": "ji"
                        },
                        {
                            "word_text": "zulíbal"
                        }
                    ]
                }
            ],
            "stanza_text": "A skočil k ní a v náruč jal\na celičkou ji zulíbal!"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Vykřikla dívka ubohá:",
                    "metrical_pattern": "10010100",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Vykřikla"
                        },
                        {
                            "word_text": "dívka"
                        },
                        {
                            "word_text": "ubohá"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "„Ach, nečiň toho, pro Boha!",
                    "metrical_pattern": "11010100",
                    "line_length": 8,
                    "rhyme": 16,
                    "words": [
                        {
                            "word_text": "Ach"
                        },
                        {
                            "word_text": "nečiň"
                        },
                        {
                            "word_text": "toho"
                        },
                        {
                            "word_text": "pro"
                        },
                        {
                            "word_text": "Boha"
                        }
                    ]
                }
            ],
            "stanza_text": "Vykřikla dívka ubohá:\n„Ach, nečiň toho, pro Boha!"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Teď aspoň ne, vždyť vidíš sám,",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "Teď"
                        },
                        {
                            "word_text": "aspoň"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "vždyť"
                        },
                        {
                            "word_text": "vidíš"
                        },
                        {
                            "word_text": "sám"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "jak celičká se červenám!“ –",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 17,
                    "words": [
                        {
                            "word_text": "jak"
                        },
                        {
                            "word_text": "celičká"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "červenám"
                        }
                    ]
                }
            ],
            "stanza_text": "Teď aspoň ne, vždyť vidíš sám,\njak celičká se červenám!“ –"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Však neodbytným Jeník byl",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "Však"
                        },
                        {
                            "word_text": "neodbytným"
                        },
                        {
                            "word_text": "Jeník"
                        },
                        {
                            "word_text": "byl"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "a líbal jen a chlácholil:",
                    "metrical_pattern": "01000100",
                    "line_length": 8,
                    "rhyme": 18,
                    "words": [
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "líbal"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "chlácholil"
                        }
                    ]
                }
            ],
            "stanza_text": "Však neodbytným Jeník byl\na líbal jen a chlácholil:"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
