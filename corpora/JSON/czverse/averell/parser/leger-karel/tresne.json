{
    "poem_title": "TŘEŠNĚ.",
    "author": "Leger, Karel",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Poslouchej, dívčinko mladá,",
