            ],
            "stanza_text": "Snad přijde druhý i třetí –\nale čas úprkem letí."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Pel mládí odvane spěšně,",
                    "metrical_pattern": "11010010",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
