                        },
                        {
                            "word_text": "druhý"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "třetí"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "ale čas úprkem letí.",
                    "metrical_pattern": "10110010",
                    "line_length": 8,
                    "rhyme": 8,
                    "words": [
                        {
                            "word_text": "ale"
                        },
                        {
                            "word_text": "čas"
                        },
                        {
                            "word_text": "úprkem"
                        },
                        {
                            "word_text": "letí"
                        }
                    ]
