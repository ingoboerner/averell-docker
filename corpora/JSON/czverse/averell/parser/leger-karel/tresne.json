                        },
                        {
                            "word_text": "chlapci"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "snědí tě, třešničko, vrabci!",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "snědí"
                        },
                        {
