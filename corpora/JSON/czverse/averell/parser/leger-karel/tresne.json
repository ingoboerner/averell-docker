                    "line_number": 18,
                    "line_text": "vyschne a sestárne třešně.",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 9,
                    "words": [
                        {
                            "word_text": "vyschne"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "sestárne"
                        },
                        {
                            "word_text": "třešně"
                        }
                    ]
                }
            ],
            "stanza_text": "Pel mládí odvane spěšně,\nvyschne a sestárne třešně."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Se smíchem půjdou kol chlapci –",
                    "metrical_pattern": "10010110",
                    "line_length": 8,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "smíchem"
                        },
                        {
                            "word_text": "půjdou"
                        },
                        {
