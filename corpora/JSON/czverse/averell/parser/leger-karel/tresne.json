            ],
            "stanza_text": "Poslouchej, dívčinko mladá,\nkvět stromů zvolna už padá."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "A jaro odletí spěšně,",
                    "metrical_pattern": "01010010",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "jaro"
                        },
                        {
                            "word_text": "odletí"
                        },
                        {
                            "word_text": "spěšně"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "na větvi zardí se třešně.",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 2,
                    "words": [
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "větvi"
                        },
                        {
                            "word_text": "zardí"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "třešně"
                        }
                    ]
                }
            ],
            "stanza_text": "A jaro odletí spěšně,\nna větvi zardí se třešně."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Ovoce sladké a rudé!",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Ovoce"
                        },
                        {
                            "word_text": "sladké"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "rudé"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Čí tahle třešnička bude?",
                    "metrical_pattern": "01010010",
                    "line_length": 8,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Čí"
                        },
                        {
                            "word_text": "tahle"
                        },
                        {
                            "word_text": "třešnička"
                        },
                        {
                            "word_text": "bude"
                        }
                    ]
                }
            ],
            "stanza_text": "Ovoce sladké a rudé!\nČí tahle třešnička bude?"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "A přijde mládenec, ptá se:",
                    "metrical_pattern": "01010010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "přijde"
                        },
                        {
                            "word_text": "mládenec"
                        },
                        {
                            "word_text": "ptá"
                        },
                        {
                            "word_text": "se"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "„Pro koho’s uzrála v kráse?",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Pro"
                        },
                        {
                            "word_text": "kohos"
                        },
                        {
                            "word_text": "uzrála"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "kráse"
                        }
                    ]
                }
            ],
            "stanza_text": "A přijde mládenec, ptá se:\n„Pro koho’s uzrála v kráse?"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Sládneš-li, třešničko, pro mě?“ –",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Sládneš"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "třešničko"
                        },
                        {
                            "word_text": "pro"
                        },
                        {
                            "word_text": "mě"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Nezhrdni, přikývni skromně!",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Nezhrdni"
                        },
                        {
                            "word_text": "přikývni"
                        },
                        {
                            "word_text": "skromně"
                        }
                    ]
                }
            ],
            "stanza_text": "Sládneš-li, třešničko, pro mě?“ –\nNezhrdni, přikývni skromně!"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Nenech ho trápit se mukou,",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "Nenech"
                        },
                        {
                            "word_text": "ho"
                        },
                        {
                            "word_text": "trápit"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mukou"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "padni hned do jeho rukou!",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 6,
                    "words": [
                        {
                            "word_text": "padni"
                        },
                        {
                            "word_text": "hned"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "rukou"
                        }
                    ]
                }
            ],
            "stanza_text": "Nenech ho trápit se mukou,\npadni hned do jeho rukou!"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Nechtěj se z pýchy ho zříkat,",
                    "metrical_pattern": "10010010",
                    "line_length": 8,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Nechtěj"
                        },
                        {
