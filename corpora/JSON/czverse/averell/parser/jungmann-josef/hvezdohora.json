                        },
                        {
                            "word_text": "porostem"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Hostinec úpravný, stál za starodávna Donínů 1)",
                    "metrical_pattern": "100100110000100",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
