                            "word_text": "S"
                        },
                        {
                            "word_text": "volností"
                        },
                        {
                            "word_text": "čistý"
                        },
                        {
                            "word_text": "kde"
                        },
                        {
                            "word_text": "vzduch"
                        },
                        {
                            "word_text": "jímž"
                        },
                        {
                            "word_text": "lehko"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "dýchá"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Nechceš býti Varům rovnána, ni Teplici valné,",
                    "metrical_pattern": "101010100010010",
                    "line_length": 15,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Nechceš"
                        },
                        {
                            "word_text": "býti"
                        },
                        {
                            "word_text": "Varům"
                        },
                        {
                            "word_text": "rovnána"
                        },
                        {
                            "word_text": "ni"
                        },
                        {
                            "word_text": "Teplici"
                        },
                        {
                            "word_text": "valné"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Kde klokotá věčným topená voda přírody ohněm:",
                    "metrical_pattern": "0100101001010010",
                    "line_length": 16,
                    "rhyme": null,
                    "words": [
