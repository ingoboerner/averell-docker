            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Hvězdohoro sličná, líbezný ústupe městským",
                    "metrical_pattern": "10001010010010",
                    "line_length": 14,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hvězdohoro"
