                        {
                            "word_text": "starodávna"
                        },
                        {
                            "word_text": "Donínů"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Poutimilovných hrad, s vznešeným jejž sobě Jaroslav",
                    "metrical_pattern": "100001100010100",
                    "line_length": 15,
