            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Jsou naše nivy. V půdě naše zdatnost.",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
                    "rhyme": 9,
                    "words": [
