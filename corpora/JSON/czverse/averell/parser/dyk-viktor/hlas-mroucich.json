            "stanza_text": "Proč nenecháte ještě chvíli\nhledati nás,\nkdyž nic už oči neomýlí?"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "My bychom jistě nyní našli.",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "My"
                        },
                        {
                            "word_text": "bychom"
                        },
                        {
                            "word_text": "jistě"
                        },
                        {
                            "word_text": "nyní"
                        },
                        {
                            "word_text": "našli"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Jen býti čas!",
                    "metrical_pattern": "0101",
