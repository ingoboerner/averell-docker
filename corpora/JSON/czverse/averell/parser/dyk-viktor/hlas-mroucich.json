                        },
                        {
                            "word_text": "žil"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "my"
                        },
                        {
                            "word_text": "tím"
                        },
                        {
                            "word_text": "mdlíme"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "V okamžik ten",
                    "metrical_pattern": "1000",
                    "line_length": 4,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "okamžik"
                        },
                        {
                            "word_text": "ten"
                        }
