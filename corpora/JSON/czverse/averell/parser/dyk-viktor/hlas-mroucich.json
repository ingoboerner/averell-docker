                    "line_number": 38,
                    "line_text": "poslyšte nás!",
                    "metrical_pattern": "1000",
                    "line_length": 4,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "poslyšte"
                        },
                        {
                            "word_text": "nás"
                        }
                    ]
                },
                {
