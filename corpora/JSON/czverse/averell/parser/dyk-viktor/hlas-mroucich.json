        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "A hle, teď soumrak náhle padá,",
                    "metrical_pattern": "010101010",
                    "line_length": 9,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "hle"
                        },
                        {
                            "word_text": "teď"
                        },
                        {
                            "word_text": "soumrak"
                        },
                        {
                            "word_text": "náhle"
                        },
                        {
                            "word_text": "padá"
                        }
                    ]
