                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "bílém"
                        },
                        {
                            "word_text": "moři"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Za vrchem vrch se do té mlhy noří,",
                    "metrical_pattern": "10010101010",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "Za"
                        },
                        {
                            "word_text": "vrchem"
                        },
                        {
                            "word_text": "vrch"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "do"
                        },
                        {
                            "word_text": "té"
                        },
                        {
                            "word_text": "mlhy"
                        },
                        {
                            "word_text": "noří"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "jak do podušek. Hory usnou němé.",
                    "metrical_pattern": "01000101010",
                    "line_length": 11,
                    "rhyme": 8,
                    "words": [
