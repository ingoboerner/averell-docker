                            "word_text": "Náš"
                        },
                        {
                            "word_text": "rozmar"
                        },
                        {
                            "word_text": "obraz"
                        },
                        {
                            "word_text": "zvláštní"
                        },
                        {
                            "word_text": "nekalí"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "A vítr vrhá bílé chuchvaly",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": 10,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "vítr"
                        },
                        {
                            "word_text": "vrhá"
                        },
                        {
                            "word_text": "bílé"
                        },
                        {
                            "word_text": "chuchvaly"
                        }
