                        {
                            "word_text": "hřeben"
                        },
                        {
                            "word_text": "až"
                        },
                        {
                            "word_text": "sem"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Co na tom? Výše nad tím chumlem jdem’.",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "Co"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "tom"
                        },
                        {
                            "word_text": "Výše"
                        },
                        {
                            "word_text": "nad"
                        },
                        {
                            "word_text": "tím"
                        },
                        {
                            "word_text": "chumlem"
                        },
                        {
                            "word_text": "jdem"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Zatopit mohou pouze údolí;",
                    "metrical_pattern": "1001010100",
                    "line_length": 10,
                    "rhyme": 4,
                    "words": [
                        {
                            "word_text": "Zatopit"
                        },
                        {
                            "word_text": "mohou"
                        },
                        {
                            "word_text": "pouze"
                        },
                        {
                            "word_text": "údolí"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "co dole bolí, výše nebolí.",
                    "metrical_pattern": "0101010100",
                    "line_length": 10,
