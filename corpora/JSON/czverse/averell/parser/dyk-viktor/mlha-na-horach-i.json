            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Dvoračky zvolna mizely už dole.",
                    "metrical_pattern": "10010100010",
                    "line_length": 11,
                    "rhyme": 1,
