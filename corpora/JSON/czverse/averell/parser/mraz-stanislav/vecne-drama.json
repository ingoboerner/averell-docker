                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Tak"
                        },
                        {
                            "word_text": "byla"
                        },
                        {
                            "word_text": "sanquinická"
                        },
                        {
                            "word_text": "příliš"
                        },
                        {
                            "word_text": "Já"
                        },
                        {
                            "word_text": "to"
                        },
                        {
                            "word_text": "zřel"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "On beránčí zas měl kýs temperament,",
                    "metrical_pattern": "01000101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "On"
                        },
                        {
                            "word_text": "beránčí"
                        },
                        {
                            "word_text": "zas"
                        },
                        {
                            "word_text": "měl"
                        },
                        {
                            "word_text": "kýs"
                        },
                        {
                            "word_text": "temperament"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "směs krve jaké ve formaci muže.",
                    "metrical_pattern": "11010100010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "směs"
                        },
                        {
                            "word_text": "krve"
                        },
                        {
                            "word_text": "jaké"
                        },
                        {
                            "word_text": "ve"
                        },
                        {
                            "word_text": "formaci"
                        },
                        {
                            "word_text": "muže"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Věděla, paní bude, vyhrazená. Diky",
                    "metrical_pattern": "1001010100010",
                    "line_length": 13,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Věděla"
                        },
                        {
                            "word_text": "paní"
                        },
                        {
                            "word_text": "bude"
                        },
                        {
                            "word_text": "vyhrazená"
                        },
                        {
                            "word_text": "Diky"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "že váha citlivá je, triomphuje v sled,",
                    "metrical_pattern": "010100010001",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "váha"
                        },
                        {
                            "word_text": "citlivá"
                        },
