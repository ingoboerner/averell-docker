                    "line_number": 18,
                    "line_text": "A mnila, při polibku náhodném",
                    "metrical_pattern": "0101000100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "mnila"
                        },
                        {
                            "word_text": "při"
                        },
                        {
                            "word_text": "polibku"
                        },
                        {
                            "word_text": "náhodném"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "cizím, že neshasíná. Klání v sklonu",
                    "metrical_pattern": "10010001010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "cizím"
                        },
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "neshasíná"
                        },
                        {
                            "word_text": "Klání"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "sklonu"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "že turnaje pohlaví jest, že štít",
                    "metrical_pattern": "0100100001",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "turnaje"
                        },
                        {
                            "word_text": "pohlaví"
