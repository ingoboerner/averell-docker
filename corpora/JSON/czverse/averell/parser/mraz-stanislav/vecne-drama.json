                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "sled"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "když značí ženy celé odevzdání.",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "když"
                        },
                        {
                            "word_text": "značí"
                        },
                        {
                            "word_text": "ženy"
                        },
                        {
                            "word_text": "celé"
                        },
                        {
                            "word_text": "odevzdání"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Jak věštka usoudila, prozírala,",
                    "metrical_pattern": "01010001000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jak"
                        },
                        {
                            "word_text": "věštka"
                        },
                        {
                            "word_text": "usoudila"
                        },
                        {
                            "word_text": "prozírala"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "že něha nevykypí, nevykvasí,",
                    "metrical_pattern": "01010001000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "něha"
                        },
                        {
                            "word_text": "nevykypí"
                        },
                        {
                            "word_text": "nevykvasí"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "že jest to látka věčně botnající,",
                    "metrical_pattern": "00010101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "že"
                        },
                        {
                            "word_text": "jest"
                        },
                        {
                            "word_text": "to"
