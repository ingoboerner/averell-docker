                },
                {
                    "line_number": 21,
                    "line_text": "jeho je v dvé a kopí rozbito.",
                    "metrical_pattern": "1001010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jeho"
                        },
                        {
                            "word_text": "je"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "dvé"
                        },
                        {
                            "word_text": "a"
