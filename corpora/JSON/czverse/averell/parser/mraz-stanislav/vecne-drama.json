                },
                {
                    "line_number": 29,
                    "line_text": "jest, na mih odkládá jen fikci,",
                    "metrical_pattern": "010100010",
                    "line_length": 9,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jest"
                        },
                        {
                            "word_text": "na"
                        },
                        {
                            "word_text": "mih"
                        },
                        {
                            "word_text": "odkládá"
                        },
                        {
                            "word_text": "jen"
                        },
                        {
                            "word_text": "fikci"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "zdráhání maskou zhrdá, abstinence.",
