            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "A napřed juž se jemu vysmívala",
                    "metrical_pattern": "01000101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "napřed"
                        },
                        {
                            "word_text": "juž"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "jemu"
                        },
                        {
                            "word_text": "vysmívala"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "a její smích byl pošklebný a hrdý.",
                    "metrical_pattern": "01010100010",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
