                        },
                        {
                            "word_text": "abstinence"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Druh vždy leč zbude v vrchol podnícený.",
                    "metrical_pattern": "10010101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Druh"
                        },
                        {
                            "word_text": "vždy"
                        },
                        {
                            "word_text": "leč"
                        },
                        {
                            "word_text": "zbude"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "vrchol"
                        },
                        {
                            "word_text": "podnícený"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Ó bez sklonu je Éra komedie,",
                    "metrical_pattern": "11000101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ó"
                        },
                        {
                            "word_text": "bez"
                        },
                        {
                            "word_text": "sklonu"
