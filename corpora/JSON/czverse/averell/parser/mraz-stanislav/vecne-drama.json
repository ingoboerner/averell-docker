                    "line_text": "záhadná, měkká v dotek, poddajná,",
                    "metrical_pattern": "1001010100",
                    "line_length": 10,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "záhadná"
                        },
                        {
                            "word_text": "měkká"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "dotek"
                        },
                        {
                            "word_text": "poddajná"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "jež vzrůstá leda a se nezmenšuje.",
                    "metrical_pattern": "01010101000",
                    "line_length": 11,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "jež"
                        },
                        {
                            "word_text": "vzrůstá"
                        },
                        {
                            "word_text": "leda"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "nezmenšuje"
                        }
                    ]
                },
