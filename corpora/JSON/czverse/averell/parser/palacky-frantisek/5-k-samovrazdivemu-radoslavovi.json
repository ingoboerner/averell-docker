{
    "poem_title": "5. K samovraždivému Radoslavovi.",
    "author": "Palacký, František",
    "manually_checked": true,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Stůj, o vrať vztažený pošvě, Radoslave,",
                    "metrical_pattern": "110100101000",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Stůj"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "vrať"
                        },
                        {
                            "word_text": "vztažený"
                        },
                        {
                            "word_text": "pošvě"
                        },
                        {
                            "word_text": "Radoslave"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Meč! o zdaž nevidíš? Nesmyslný! tobě",
                    "metrical_pattern": "110100100010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Meč"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "zdaž"
                        },
                        {
                            "word_text": "nevidíš"
                        },
                        {
                            "word_text": "Nesmyslný"
                        },
                        {
                            "word_text": "tobě"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Střílí z tváři bledé hydry Megeřiny.",
                    "metrical_pattern": "101010101000",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Střílí"
                        },
                        {
                            "word_text": "z"
                        },
                        {
                            "word_text": "tváři"
                        },
                        {
                            "word_text": "bledé"
                        },
                        {
                            "word_text": "hydry"
                        },
                        {
                            "word_text": "Megeřiny"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Stůj! jest k živobytí brána odevřená",
                    "metrical_pattern": "101000101000",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Stůj"
                        },
                        {
                            "word_text": "jest"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "živobytí"
                        },
                        {
                            "word_text": "brána"
                        },
                        {
                            "word_text": "odevřená"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Ještě; zhoubu volá zoufale nesmělec.",
                    "metrical_pattern": "101010100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ještě"
                        },
                        {
                            "word_text": "zhoubu"
                        },
                        {
                            "word_text": "volá"
                        },
                        {
                            "word_text": "zoufale"
                        },
                        {
                            "word_text": "nesmělec"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Ukroť bouři prsou; ku břehu nádějí",
                    "metrical_pattern": "101010100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ukroť"
                        },
                        {
                            "word_text": "bouři"
                        },
                        {
                            "word_text": "prsou"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "břehu"
                        },
                        {
                            "word_text": "nádějí"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Jasných roztřepenou bárku poveď směle.",
                    "metrical_pattern": "101000101010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jasných"
                        },
                        {
                            "word_text": "roztřepenou"
                        },
                        {
                            "word_text": "bárku"
                        },
                        {
                            "word_text": "poveď"
                        },
                        {
                            "word_text": "směle"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Po zrádných skalinách, v rozbroji vichrovém",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Po"
                        },
                        {
                            "word_text": "zrádných"
                        },
                        {
                            "word_text": "skalinách"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "rozbroji"
                        },
                        {
                            "word_text": "vichrovém"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Živlů rozzlobených mocně, co bleskové",
                    "metrical_pattern": "101000100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Živlů"
                        },
                        {
                            "word_text": "rozzlobených"
                        },
                        {
                            "word_text": "mocně"
                        },
                        {
                            "word_text": "co"
                        },
                        {
                            "word_text": "bleskové"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Bouřkou, ať zeměsyn portu se doplaví –",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Bouřkou"
                        },
                        {
                            "word_text": "ať"
                        },
                        {
                            "word_text": "zeměsyn"
                        },
                        {
                            "word_text": "portu"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "doplaví"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Nejvyšší poručil. Bratře polož braně!",
                    "metrical_pattern": "100100101010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Nejvyšší"
                        },
                        {
                            "word_text": "poručil"
                        },
                        {
                            "word_text": "Bratře"
                        },
                        {
                            "word_text": "polož"
                        },
                        {
                            "word_text": "braně"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Odbojným se čelem rouhati nesluší",
                    "metrical_pattern": "100010100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Odbojným"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "čelem"
                        },
                        {
                            "word_text": "rouhati"
                        },
                        {
                            "word_text": "nesluší"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Strážícýmu život andělu! Jest! tobě",
                    "metrical_pattern": "100010100010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Strážícýmu"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "andělu"
                        },
                        {
                            "word_text": "Jest"
                        },
                        {
                            "word_text": "tobě"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Hořký píti kalich osudové dali.",
                    "metrical_pattern": "101010100010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Hořký"
                        },
                        {
                            "word_text": "píti"
                        },
                        {
                            "word_text": "kalich"
                        },
                        {
                            "word_text": "osudové"
                        },
                        {
                            "word_text": "dali"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Neštastný! ku prsům zrádce sy ještěra,",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Neštastný"
                        },
                        {
                            "word_text": "ku"
                        },
                        {
                            "word_text": "prsům"
                        },
                        {
                            "word_text": "zrádce"
                        },
                        {
                            "word_text": "sy"
                        },
                        {
                            "word_text": "ještěra"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Zmámen, vroucně vinul; on střely jízlivé",
                    "metrical_pattern": "101010010100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zmámen"
                        },
                        {
                            "word_text": "vroucně"
                        },
                        {
                            "word_text": "vinul"
                        },
                        {
                            "word_text": "on"
                        },
                        {
                            "word_text": "střely"
                        },
                        {
                            "word_text": "jízlivé"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "V oupřimné ti zaryv srdce, nechal tebe.",
                    "metrical_pattern": "100010101010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "oupřimné"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "zaryv"
                        },
                        {
                            "word_text": "srdce"
                        },
                        {
                            "word_text": "nechal"
                        },
                        {
                            "word_text": "tebe"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Ty zplýváš ve slzách? Přej sobě odlevy",
                    "metrical_pattern": "010100110100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ty"
                        },
                        {
                            "word_text": "zplýváš"
                        },
                        {
                            "word_text": "ve"
                        },
                        {
                            "word_text": "slzách"
                        },
                        {
                            "word_text": "Přej"
                        },
                        {
                            "word_text": "sobě"
                        },
                        {
                            "word_text": "odlevy"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Léčícý: hoře tvé v krůpě se roztaje. –",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Léčícý"
                        },
                        {
                            "word_text": "hoře"
                        },
                        {
                            "word_text": "tvé"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "krůpě"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "roztaje"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Ach tvé srdce čilé, duch spanilou hoře",
                    "metrical_pattern": "101010110010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Ach"
                        },
                        {
                            "word_text": "tvé"
                        },
                        {
                            "word_text": "srdce"
                        },
                        {
                            "word_text": "čilé"
                        },
                        {
                            "word_text": "duch"
                        },
                        {
                            "word_text": "spanilou"
                        },
                        {
                            "word_text": "hoře"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Vroucností, sliboval v usvitě růžovém",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vroucností"
                        },
                        {
                            "word_text": "sliboval"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "usvitě"
                        },
                        {
                            "word_text": "růžovém"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Jasné plápoly dnů: v háji svatém hrdě",
                    "metrical_pattern": "101001101010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jasné"
                        },
                        {
                            "word_text": "plápoly"
                        },
                        {
                            "word_text": "dnů"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "háji"
                        },
                        {
                            "word_text": "svatém"
                        },
                        {
                            "word_text": "hrdě"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Zachvíval vysoký náděje praporec.",
                    "metrical_pattern": "100100100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Zachvíval"
                        },
                        {
                            "word_text": "vysoký"
                        },
                        {
                            "word_text": "náděje"
                        },
                        {
                            "word_text": "praporec"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Vstaň; osměl se nový ohnivě cýtiti",
                    "metrical_pattern": "110010100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vstaň"
                        },
                        {
                            "word_text": "osměl"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "nový"
                        },
                        {
                            "word_text": "ohnivě"
                        },
                        {
                            "word_text": "cýtiti"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "V jarní sýle život; aj vznešená věje",
                    "metrical_pattern": "101010110010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "V"
                        },
                        {
                            "word_text": "jarní"
                        },
                        {
                            "word_text": "sýle"
                        },
                        {
                            "word_text": "život"
                        },
                        {
                            "word_text": "aj"
                        },
                        {
                            "word_text": "vznešená"
                        },
                        {
                            "word_text": "věje"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Palma v posvěceném háji, a vítěze",
                    "metrical_pattern": "101000100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Palma"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "posvěceném"
                        },
                        {
                            "word_text": "háji"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "vítěze"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "K nesmrtnosti volá, k ochraně vlasti své.",
                    "metrical_pattern": "100010100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "K"
                        },
                        {
