                        },
                        {
                            "word_text": "volá"
                        },
                        {
                            "word_text": "k"
                        },
                        {
                            "word_text": "ochraně"
                        },
                        {
                            "word_text": "vlasti"
                        },
                        {
                            "word_text": "své"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "O třikrát blažený ten, komu plápolem",
                    "metrical_pattern": "100100010100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "O"
                        },
                        {
                            "word_text": "třikrát"
                        },
                        {
                            "word_text": "blažený"
                        },
                        {
                            "word_text": "ten"
                        },
                        {
                            "word_text": "komu"
                        },
                        {
                            "word_text": "plápolem"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Božským ňádra se dmou, genyové čelo",
                    "metrical_pattern": "101001100010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Božským"
                        },
                        {
                            "word_text": "ňádra"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "dmou"
                        },
                        {
                            "word_text": "genyové"
                        },
                        {
                            "word_text": "čelo"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Vlídní obletují v kůru milostivém.",
                    "metrical_pattern": "101000101000",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Vlídní"
                        },
                        {
                            "word_text": "obletují"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "kůru"
                        },
                        {
                            "word_text": "milostivém"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Se mnou,*) Příteli, poď! Tam u památky té",
                    "metrical_pattern": "101001010000",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "mnou"
                        },
                        {
                            "word_text": "Příteli"
                        },
                        {
                            "word_text": "poď"
                        },
                        {
                            "word_text": "Tam"
                        },
                        {
                            "word_text": "u"
                        },
                        {
                            "word_text": "památky"
                        },
                        {
                            "word_text": "té"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Jasné slávy otců, v obrošeném luze,",
                    "metrical_pattern": "101010100010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Jasné"
                        },
                        {
                            "word_text": "slávy"
                        },
                        {
                            "word_text": "otců"
                        },
                        {
                            "word_text": "v"
                        },
                        {
                            "word_text": "obrošeném"
                        },
                        {
                            "word_text": "luze"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Poustennícy, nové začneme spolčení.",
                    "metrical_pattern": "100010100100",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Poustennícy"
                        },
                        {
                            "word_text": "nové"
                        },
                        {
                            "word_text": "začneme"
                        },
                        {
                            "word_text": "spolčení"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Blyštícý duhu zříš? Bouři litostivý",
                    "metrical_pattern": "100101101000",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Blyštícý"
                        },
                        {
                            "word_text": "duhu"
                        },
                        {
                            "word_text": "zříš"
                        },
                        {
                            "word_text": "Bouři"
                        },
                        {
                            "word_text": "litostivý"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Pán ráčiv potišit, laskavě nám kyne.",
                    "metrical_pattern": "110100100010",
                    "line_length": 12,
                    "rhyme": null,
                    "words": [
                        {
                            "word_text": "Pán"
                        },
                        {
                            "word_text": "ráčiv"
                        },
                        {
                            "word_text": "potišit"
                        },
                        {
                            "word_text": "laskavě"
                        },
                        {
                            "word_text": "nám"
                        },
                        {
