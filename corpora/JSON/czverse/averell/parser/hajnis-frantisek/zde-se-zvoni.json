                    "metrical_pattern": "10001010",
                    "line_length": 8,
                    "rhyme": 5,
                    "words": [
                        {
                            "word_text": "Po"
                        },
                        {
                            "word_text": "nedlouhém"
                        },
                        {
                            "word_text": "na"
                        },
                        {
