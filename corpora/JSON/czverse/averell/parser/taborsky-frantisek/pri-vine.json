                        },
                        {
                            "word_text": "zvykneš"
                        },
                        {
                            "word_text": "nosit"
                        },
                        {
                            "word_text": "hlavu"
                        },
                        {
                            "word_text": "vždycky"
                        },
                        {
                            "word_text": "zpříma"
                        }
                    ]
