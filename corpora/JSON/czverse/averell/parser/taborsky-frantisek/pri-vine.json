                        },
                        {
                            "word_text": "lítosti"
                        },
                        {
                            "word_text": "vzteku"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "tu hořkost zapít, zvolat místo breku",
                    "metrical_pattern": "01010101010",
                    "line_length": 11,
                    "rhyme": 7,
                    "words": [
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "hořkost"
                        },
                        {
