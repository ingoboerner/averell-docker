                    "line_number": 7,
                    "line_text": "životem celým, celou svojí duší.",
                    "metrical_pattern": "10010101010",
                    "line_length": 11,
                    "rhyme": 3,
                    "words": [
                        {
                            "word_text": "životem"
                        },
