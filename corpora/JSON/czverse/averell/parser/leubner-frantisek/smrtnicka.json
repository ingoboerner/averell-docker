        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Král u bran sedí, u ráje,",
                    "metrical_pattern": "11010100",
                    "line_length": 8,
