            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "„Rci, nedal's nikdy příčiny,",
                    "metrical_pattern": "11010100",
                    "line_length": 8,
                    "rhyme": 12,
