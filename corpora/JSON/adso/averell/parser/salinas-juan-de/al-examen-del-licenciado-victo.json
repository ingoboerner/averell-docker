{
    "poem_title": "Al examen del licenciado Victoria ",
    "corpus": "adso",
    "manually_checked": false,
    "author": "Salinas,  Juan de",
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 1,
                    "n": "1",
                    "line_text": "Cual caballo ligero y animoso,",
                    "metrical_pattern": "+-+--+---+-"
                },
                {
                    "line_number": 2,
                    "n": "2",
                    "line_text": "que apenas la afilada espuela siente,",
                    "metrical_pattern": "-+---+-+-+-"
                },
                {
                    "line_number": 3,
                    "n": "3",
                    "line_text": "cuando es ya menester que diestramente",
                    "metrical_pattern": "-++--+-+-+-"
                },
                {
                    "line_number": 4,
                    "n": "4",
                    "line_text": "la rienda enfrene el curso presuroso,",
                    "metrical_pattern": "-+-+-+---+-"
                }
            ],
            "stanza_text": "Cual caballo ligero y animoso,\nque apenas la afilada espuela siente,\ncuando es ya menester que diestramente\nla rienda enfrene el curso presuroso,"
        },
        {
            "stanza_number": 2,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 5,
                    "n": "5",
                    "line_text": "que volviendo del término glorioso",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 6,
                    "n": "6",
                    "line_text": "del rico palio, entre la alegre gente",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": 7,
                    "n": "7",
                    "line_text": "se huella y encamina la alta frente",
                    "metrical_pattern": "-+---+-+-+-"
                },
                {
                    "line_number": 8,
                    "n": "8",
                    "line_text": "al son del instrumento belicoso,",
                    "metrical_pattern": "-+---+---+-"
                }
            ],
            "stanza_text": "que volviendo del término glorioso\ndel rico palio, entre la alegre gente\nse huella y encamina la alta frente\nal son del instrumento belicoso,"
        },
        {
            "stanza_number": 3,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 9,
                    "n": "9",
                    "line_text": "y adornada de plumas la testera",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 10,
                    "n": "10",
                    "line_text": "mira al vistoso premio, y va lozano",
                    "metrical_pattern": "+--+-+-+-+-"
                },
                {
                    "line_number": 11,
                    "n": "11",
                    "line_text": "dejando mil envidias de sus glorias,",
                    "metrical_pattern": "-+-+-+---+-"
                }
            ],
            "stanza_text": "y adornada de plumas la testera\nmira al vistoso premio, y va lozano\ndejando mil envidias de sus glorias,"
        },
        {
            "stanza_number": 4,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 12,
                    "n": "12",
                    "line_text": "tal tú, Victoria, en la veloz carrera",
                    "metrical_pattern": "++-+---+-+-"
                },
                {
                    "line_number": 13,
                    "n": "13",
                    "line_text": "ganando el palio, tornas hoy ufano",
                    "metrical_pattern": "-+-+-+-+-+-"
                },
                {
                    "line_number": 14,
                    "n": "14",
                    "line_text": "añadiendo victoria a tus victorias.",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "tal tú, Victoria, en la veloz carrera\nganando el palio, tornas hoy ufano\nañadiendo victoria a tus victorias."
        }
    ]
}