{
    "poem_title": "- LIII - ",
    "corpus": "adso",
    "manually_checked": true,
    "author": "Gutierre de Cetina",
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 1,
                    "n": "1",
                    "line_text": "No por el cielo ver correr estrellas,",
                    "metrical_pattern": "+--+-+-+-+-"
                },
                {
                    "line_number": 2,
                    "n": "2",
                    "line_text": "ni por tranquilo mar navíos cargados,",
                    "metrical_pattern": "---+-+-+-+-"
                },
                {
                    "line_number": 3,
                    "n": "3",
                    "line_text": "ni en plaza tornear hombres armados,",
                    "metrical_pattern": "-+---++--+-"
                },
                {
                    "line_number": 4,
                    "n": "4",
                    "line_text": "ni a caza en bosque ver ninfas muy bellas;",
                    "metrical_pattern": "-+-+-++-++-"
                }
            ],
            "stanza_text": "No por el cielo ver correr estrellas,\nni por tranquilo mar navíos cargados,\nni en plaza tornear hombres armados,\nni a caza en bosque ver ninfas muy bellas;"
        },
        {
            "stanza_number": 2,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 5,
                    "n": "5",
                    "line_text": "ni en gran oscuridad volar estrellas,",
                    "metrical_pattern": "-+---+-+-+-"
                },
                {
                    "line_number": 6,
                    "n": "6",
                    "line_text": "ni llenos por abril de flor los prados,",
                    "metrical_pattern": "-+---+-+-+-"
                },
                {
                    "line_number": 7,
                    "n": "7",
                    "line_text": "ni galanes en sala aderezados,",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 8,
                    "n": "8",
                    "line_text": "ni en cabello bailar tiernas doncellas;",
                    "metrical_pattern": "--+--++--+-"
                }
            ],
            "stanza_text": "ni en gran oscuridad volar estrellas,\nni llenos por abril de flor los prados,\nni galanes en sala aderezados,\nni en cabello bailar tiernas doncellas;"
        },
        {
            "stanza_number": 3,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 9,
                    "n": "9",
                    "line_text": "no el sol en el nacer de un claro día,",
                    "metrical_pattern": "++---+++-+-"
                },
                {
                    "line_number": 10,
                    "n": "10",
                    "line_text": "ni árboles de flor y fruta llenos,",
                    "metrical_pattern": "-+---+-+-+-"
                },
                {
                    "line_number": 11,
                    "n": "11",
                    "line_text": "ni fuego sobre nieve helada y fría;",
                    "metrical_pattern": "-+---+-+-+-"
                }
            ],
            "stanza_text": "no el sol en el nacer de un claro día,\nni árboles de flor y fruta llenos,\nni fuego sobre nieve helada y fría;"
        },
        {
            "stanza_number": 4,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 12,
                    "n": "12",
                    "line_text": "ni todo cuanto hay más ni cuanto hay menos",
                    "metrical_pattern": "-+--++--++-"
                },
                {
                    "line_number": 13,
                    "n": "13",
                    "line_text": "de hermoso en el mundo, igualaría",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 14,
                    "n": "14",
                    "line_text": "vuestro dulce mirar, ojos serenos.",
                    "metrical_pattern": "--+--++--+-"
                }
            ],
            "stanza_text": "ni todo cuanto hay más ni cuanto hay menos\nde hermoso en el mundo, igualaría\nvuestro dulce mirar, ojos serenos."
        }
    ]
}