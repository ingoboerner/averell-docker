{
    "poem_title": "- CXLIII - ",
    "corpus": "adso",
    "manually_checked": true,
    "author": "Argensola",
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 1,
                    "n": "1",
                    "line_text": "Pródiga de nariz, de ojos avara,",
                    "metrical_pattern": "+----++--+-"
                },
                {
                    "line_number": 2,
                    "n": "2",
                    "line_text": "espaciosa de boca, angosta en frente,",
                    "metrical_pattern": "--+--+-+-+-"
                },
                {
                    "line_number": 3,
                    "n": "3",
                    "line_text": "mejillas de cuaresma penitente,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 4,
                    "n": "4",
                    "line_text": "y barba que en pirámide repara;",
                    "metrical_pattern": "-+---+---+-"
                }
            ],
            "stanza_text": "Pródiga de nariz, de ojos avara,\nespaciosa de boca, angosta en frente,\nmejillas de cuaresma penitente,\ny barba que en pirámide repara;"
        },
        {
            "stanza_number": 2,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 5,
                    "n": "5",
                    "line_text": "bosque do el tiempo con los años ara,",
                    "metrical_pattern": "+--+---+-+-"
                },
                {
                    "line_number": 6,
                    "n": "6",
                    "line_text": "encubierto a la luz del rojo oriente;",
                    "metrical_pattern": "--+--+-+-+-"
                },
                {
                    "line_number": 7,
                    "n": "7",
                    "line_text": "fértil mina de pez que eternamente",
                    "metrical_pattern": "+-+--+-+-+-"
                },
                {
                    "line_number": 8,
                    "n": "8",
                    "line_text": "destila en cada poro un alquitara;",
                    "metrical_pattern": "-+-+-++--+-"
                }
            ],
            "stanza_text": "bosque do el tiempo con los años ara,\nencubierto a la luz del rojo oriente;\nfértil mina de pez que eternamente\ndestila en cada poro un alquitara;"
        },
        {
            "stanza_number": 3,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 9,
                    "n": "9",
                    "line_text": "vientre de odre, pecho de amazona,",
                    "metrical_pattern": "+--+-+---+-"
                },
                {
                    "line_number": 10,
                    "n": "10",
                    "line_text": "cuello de tina, brazos de cordeles,",
                    "metrical_pattern": "+--+-+---+-"
                },
                {
                    "line_number": 11,
                    "n": "11",
                    "line_text": "y en piernas de raíces pies de pato;",
                    "metrical_pattern": "-+---+-+-+-"
                }
            ],
            "stanza_text": "vientre de odre, pecho de amazona,\ncuello de tina, brazos de cordeles,\ny en piernas de raíces pies de pato;"
        },
        {
            "stanza_number": 4,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 12,
                    "n": "12",
                    "line_text": "es dibujada al vivo en líneas fieles,",
                    "metrical_pattern": "+--+-+-+-+-"
                },
                {
                    "line_number": 13,
                    "n": "13",
                    "line_text": "monseñor, la magnífica persona",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 14,
                    "n": "14",
                    "line_text": "di quella che vi piace in bel ritrato.",
                    "metrical_pattern": "-+--++++-+-"
                }
            ],
            "stanza_text": "es dibujada al vivo en líneas fieles,\nmonseñor, la magnífica persona\ndi quella che vi piace in bel ritrato."
        }
    ]
}