{
    "poem_title": "- CLXXXVI - ",
    "corpus": "adso",
    "manually_checked": true,
    "author": "Juan de Tassis y Peralta",
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 1,
                    "n": "1",
                    "line_text": "Cual matutina lumbre soberano",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": 2,
                    "n": "2",
                    "line_text": "esplendor concediendo es centelleante,",
                    "metrical_pattern": "--+--++--+-"
                },
                {
                    "line_number": 3,
                    "n": "3",
                    "line_text": "cual despuntar se vio deidad amante",
                    "metrical_pattern": "---+-+-+-+-"
                },
                {
                    "line_number": 4,
                    "n": "4",
                    "line_text": "de la fecunda sal del oceano;",
                    "metrical_pattern": "---+-+---+-"
                }
            ],
            "stanza_text": "Cual matutina lumbre soberano\nesplendor concediendo es centelleante,\ncual despuntar se vio deidad amante\nde la fecunda sal del oceano;"
        },
        {
            "stanza_number": 2,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 5,
                    "n": "5",
                    "line_text": "cual virgen rosa que en jardín temprano",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": 6,
                    "n": "6",
                    "line_text": "de verde cárcel se soltó fragante,",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": 7,
                    "n": "7",
                    "line_text": "fovente al parto céfiro espirante,",
                    "metrical_pattern": "-+-+-+---+-"
                },
                {
                    "line_number": 8,
                    "n": "8",
                    "line_text": "de los grávidos senos del verano;",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "cual virgen rosa que en jardín temprano\nde verde cárcel se soltó fragante,\nfovente al parto céfiro espirante,\nde los grávidos senos del verano;"
        },
        {
            "stanza_number": 3,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 9,
                    "n": "9",
                    "line_text": "tal Fénix nueva en sus flamantes plumas",
                    "metrical_pattern": "++-+---+-+-"
                },
                {
                    "line_number": 10,
                    "n": "10",
                    "line_text": "le desmintió crepúsculos al día",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": 11,
                    "n": "11",
                    "line_text": "que formó sol de viva hermosura,",
                    "metrical_pattern": "--++-+---+-"
                }
            ],
            "stanza_text": "tal Fénix nueva en sus flamantes plumas\nle desmintió crepúsculos al día\nque formó sol de viva hermosura,"
        },
        {
            "stanza_number": 4,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 12,
                    "n": "12",
                    "line_text": "beldad originando las espumas",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 13,
                    "n": "13",
                    "line_text": "de piélagos de gracia a la luz mía",
                    "metrical_pattern": "-+---+--++-"
                },
                {
                    "line_number": 14,
                    "n": "14",
                    "line_text": "imperceptible siempre, siempre pura.",
                    "metrical_pattern": "---+-+-+-+-"
                }
            ],
            "stanza_text": "beldad originando las espumas\nde piélagos de gracia a la luz mía\nimperceptible siempre, siempre pura."
        }
    ]
}