{
    "poem_title": "- VI - ",
    "corpus": "adso",
    "manually_checked": false,
    "author": "Lope de Vega",
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 1,
                    "n": "1",
                    "line_text": "Al sepulcro de amor, que contra el filo",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 2,
                    "n": "2",
                    "line_text": "del tiempo hizo Artemisia vivir claro,",
                    "metrical_pattern": "-++--+--++-"
                },
                {
                    "line_number": 3,
                    "n": "3",
                    "line_text": "a la torre bellísima de Faro,",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 4,
                    "n": "4",
                    "line_text": "un tiempo de las naves luz y asilo;",
                    "metrical_pattern": "++---+-+-+-"
                }
            ],
            "stanza_text": "Al sepulcro de amor, que contra el filo\ndel tiempo hizo Artemisia vivir claro,\na la torre bellísima de Faro,\nun tiempo de las naves luz y asilo;"
        },
        {
            "stanza_number": 2,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 5,
                    "n": "5",
                    "line_text": "al templo Efesio de famoso estilo,",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": 6,
                    "n": "6",
                    "line_text": "al Coloso del sol, único y raro,",
                    "metrical_pattern": "--+--++--+-"
                },
                {
                    "line_number": 7,
                    "n": "7",
                    "line_text": "al muro de Semíramis reparo,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 8,
                    "n": "8",
                    "line_text": "y a las altas Pirámides del Nilo;",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "al templo Efesio de famoso estilo,\nal Coloso del sol, único y raro,\nal muro de Semíramis reparo,\ny a las altas Pirámides del Nilo;"
        },
        {
            "stanza_number": 3,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 9,
                    "n": "9",
                    "line_text": "en fin, a los milagros inauditos,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 10,
                    "n": "10",
                    "line_text": "a Júpiter Olímpica, y al templo,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 11,
                    "n": "11",
                    "line_text": "Pirámides, Coloso y Mauseolo;",
                    "metrical_pattern": "-+---+---+-"
                }
            ],
            "stanza_text": "en fin, a los milagros inauditos,\na Júpiter Olímpica, y al templo,\nPirámides, Coloso y Mauseolo;"
        },
        {
            "stanza_number": 4,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 12,
                    "n": "12",
                    "line_text": "y a cuantos hoy el mundo tiene escritos,",
                    "metrical_pattern": "-+-+-+-+-+-"
                },
                {
                    "line_number": 13,
                    "n": "13",
                    "line_text": "en fama vence de mi fe el ejemplo,",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": 14,
                    "n": "14",
                    "line_text": "que es mayor maravilla mi amor solo.",
                    "metrical_pattern": "+-+--+--++-"
                }
            ],
            "stanza_text": "y a cuantos hoy el mundo tiene escritos,\nen fama vence de mi fe el ejemplo,\nque es mayor maravilla mi amor solo."
        }
    ]
}