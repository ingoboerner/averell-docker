{
    "poem_title": "Al mismo Góngora ",
    "corpus": "adso",
    "manually_checked": false,
    "author": "Quevedo",
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 1,
                    "n": "1",
                    "line_text": "Sulquivagante, pretensor de Estolo,",
                    "metrical_pattern": "---+---+-+-"
                },
                {
                    "line_number": 2,
                    "n": "2",
                    "line_text": "pues que lo expuesto al Noto solificas",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": 3,
                    "n": "3",
                    "line_text": "y obtusas speluncas comunicas,",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 4,
                    "n": "4",
                    "line_text": "despecho de las musas a ti solo,",
                    "metrical_pattern": "-+---+--++-"
                }
            ],
            "stanza_text": "Sulquivagante, pretensor de Estolo,\npues que lo expuesto al Noto solificas\ny obtusas speluncas comunicas,\ndespecho de las musas a ti solo,"
        },
        {
            "stanza_number": 2,
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": 5,
                    "n": "5",
                    "line_text": "huye, no carpa, de tu Dafne Apolo",
                    "metrical_pattern": "+-++---+-+-"
                },
                {
                    "line_number": 6,
                    "n": "6",
                    "line_text": "surculos slabros de teretes picas,",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": 7,
                    "n": "7",
                    "line_text": "porque con tus perversos damnificas",
                    "metrical_pattern": "-----+---+-"
                },
                {
                    "line_number": 8,
                    "n": "8",
                    "line_text": "los institutos de tu sacro Tolo.",
                    "metrical_pattern": "---+---+-+-"
                }
            ],
            "stanza_text": "huye, no carpa, de tu Dafne Apolo\nsurculos slabros de teretes picas,\nporque con tus perversos damnificas\nlos institutos de tu sacro Tolo."
        },
        {
            "stanza_number": 3,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 9,
                    "n": "9",
                    "line_text": "Has acabado aliundo su Parnaso;",
                    "metrical_pattern": "+--+-+---+-"
                },
                {
                    "line_number": 10,
                    "n": "10",
                    "line_text": "adulteras la casta poesía,",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": 11,
                    "n": "11",
                    "line_text": "ventilas blandos, niños inquïetas,",
                    "metrical_pattern": "-+-+-+---+-"
                }
            ],
            "stanza_text": "Has acabado aliundo su Parnaso;\nadulteras la casta poesía,\nventilas blandos, niños inquïetas,"
        },
        {
            "stanza_number": 4,
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": 12,
                    "n": "12",
                    "line_text": "parco, cerúleo, veterano vaso:",
                    "metrical_pattern": "+--+---+-+-"
                },
                {
                    "line_number": 13,
                    "n": "13",
                    "line_text": "piáculos perpetra su porfía,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": 14,
                    "n": "14",
                    "line_text": "estuprando neotéricos poetas.",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "parco, cerúleo, veterano vaso:\npiáculos perpetra su porfía,\nestuprando neotéricos poetas."
        }
    ]
}