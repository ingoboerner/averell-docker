{
    "poem_title": "Antífonas",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Araújo Figueredo",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Encheram-se as velas brancas",
                    "line_length": "7"
                },
                {
                    "line_number": 2,
                    "line_text": "De rajadas as mais francas.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Encheram-se as velas brancas\nDe rajadas as mais francas."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Partiram, numa enfiada,",
                    "line_length": "7"
                },
                {
                    "line_number": 4,
                    "line_text": "As canoas da enseada.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Partiram, numa enfiada,\nAs canoas da enseada."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Foram-se todas às vagas,",
                    "line_length": "7"
                },
                {
                    "line_number": 6,
                    "line_text": "Sem tristezas aziagas.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Foram-se todas às vagas,\nSem tristezas aziagas."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Sem negros pressentimentos",
                    "line_length": "7"
                },
                {
                    "line_number": 8,
                    "line_text": "De virem medonhos ventos.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Sem negros pressentimentos\nDe virem medonhos ventos."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "É que o sol todo floria,",
                    "line_length": "7"
                },
                {
                    "line_number": 10,
                    "line_text": "De rosas e pedraria",
                    "line_length": "7"
                }
            ],
            "stanza_text": "É que o sol todo floria,\nDe rosas e pedraria"
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "A verde encosta dos montes,",
                    "line_length": "7"
                },
                {
                    "line_number": 12,
                    "line_text": "E afagava os horizontes.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "A verde encosta dos montes,\nE afagava os horizontes."
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Sobre o alto Cambirela",
                    "line_length": "7"
                },
                {
                    "line_number": 14,
                    "line_text": "Havia luz amarela.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Sobre o alto Cambirela\nHavia luz amarela."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "E havia nas suas fraldas",
                    "line_length": "7"
                },
                {
                    "line_number": 16,
                    "line_text": "Liquescentes esmeraldas;",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E havia nas suas fraldas\nLiquescentes esmeraldas;"
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "E nas suas cachoeiras,",
                    "line_length": "7"
                },
                {
                    "line_number": 18,
                    "line_text": "Claridades de poncheiras",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E nas suas cachoeiras,\nClaridades de poncheiras"
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "E muita luz de berilos",
                    "line_length": "7"
                },
                {
                    "line_number": 20,
                    "line_text": "Pelos seus rios tranquilos.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E muita luz de berilos\nPelos seus rios tranquilos."
        },
        {
            "stanza_type": null,
            "stanza_number": 11,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "E além, fulgurantes listas",
                    "line_length": "7"
                },
                {
                    "line_number": 22,
                    "line_text": "De safiras e ametistas!",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E além, fulgurantes listas\nDe safiras e ametistas!"
        },
        {
            "stanza_type": null,
            "stanza_number": 12,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "E sobre as folhas do mangue",
                    "line_length": "7"
                },
                {
                    "line_number": 24,
                    "line_text": "Derrames de vivo sangue.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E sobre as folhas do mangue\nDerrames de vivo sangue."
        },
        {
            "stanza_type": null,
            "stanza_number": 13,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "E sobre a Ilha das Vinhas",
                    "line_length": "7"
                },
                {
                    "line_number": 26,
                    "line_text": "Gaivotas e andorinhas.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E sobre a Ilha das Vinhas\nGaivotas e andorinhas."
        },
        {
            "stanza_type": null,
            "stanza_number": 14,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "E um forte perfume de erva,",
                    "line_length": "7"
                },
                {
                    "line_number": 28,
                    "line_text": "Que as fibras do peito enerva,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E um forte perfume de erva,\nQue as fibras do peito enerva,"
        },
        {
            "stanza_type": null,
            "stanza_number": 15,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Derramava-se por tudo,",
                    "line_length": "7"
                },
                {
                    "line_number": 30,
                    "line_text": "Como em mantos de veludo.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Derramava-se por tudo,\nComo em mantos de veludo."
        },
        {
            "stanza_type": null,
            "stanza_number": 16,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "As Tipitingas sorriam",
                    "line_length": "7"
                },
                {
                    "line_number": 32,
                    "line_text": "Às ondinas que se abriam...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "As Tipitingas sorriam\nÀs ondinas que se abriam..."
        },
        {
            "stanza_type": null,
            "stanza_number": 17,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Ia-lhes um sorriso brando",
                    "line_length": "7"
                },
                {
                    "line_number": 34,
                    "line_text": "Divinamente embalando...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Ia-lhes um sorriso brando\nDivinamente embalando..."
        },
        {
            "stanza_type": null,
            "stanza_number": 18,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "E de lado a lado, as casas",
                    "line_length": "7"
                },
                {
                    "line_number": 36,
                    "line_text": "Eram brancas como as asas",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E de lado a lado, as casas\nEram brancas como as asas"
        },
        {
            "stanza_type": null,
            "stanza_number": 19,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Das garças, quando paradas",
                    "line_length": "7"
                },
                {
                    "line_number": 38,
                    "line_text": "Pelas praias perfumadas.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Das garças, quando paradas\nPelas praias perfumadas."
        },
        {
            "stanza_type": null,
            "stanza_number": 20,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "E agora a noite é um manto",
                    "line_length": "7"
                },
                {
                    "line_number": 40,
                    "line_text": "De veludilho o mais santo.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E agora a noite é um manto\nDe veludilho o mais santo."
        },
        {
            "stanza_type": null,
            "stanza_number": 21,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Desceu crivada de estrelas",
                    "line_length": "7"
                },
                {
                    "line_number": 42,
                    "line_text": "Que a gente se alegra ao vê-las.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Desceu crivada de estrelas\nQue a gente se alegra ao vê-las."
        },
        {
            "stanza_type": null,
            "stanza_number": 22,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Como o teu manto, Senhora",
                    "line_length": "7"
                },
                {
                    "line_number": 44,
                    "line_text": "De prata a noite se enflora,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Como o teu manto, Senhora\nDe prata a noite se enflora,"
        },
        {
            "stanza_type": null,
            "stanza_number": 23,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Tal qual teu manto sagrado",
                    "line_length": "7"
                },
                {
                    "line_number": 46,
                    "line_text": "Todo o céu é salpicado",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Tal qual teu manto sagrado\nTodo o céu é salpicado"
        },
        {
            "stanza_type": null,
            "stanza_number": 24,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "De encantadores pórfiros:",
                    "line_length": "7"
                },
                {
                    "line_number": 48,
                    "line_text": "— Urnas dos nossos suspiros;",
                    "line_length": "7"
                }
            ],
            "stanza_text": "De encantadores pórfiros:\n— Urnas dos nossos suspiros;"
        },
        {
            "stanza_type": null,
            "stanza_number": 25,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Segredos das nossas ânsias",
                    "line_length": "7"
                },
                {
                    "line_number": 50,
                    "line_text": "Que se vão pelas distâncias.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Segredos das nossas ânsias\nQue se vão pelas distâncias."
        },
        {
            "stanza_type": null,
            "stanza_number": 26,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Em procura de refúgios,",
                    "line_length": "7"
                },
                {
                    "line_number": 52,
                    "line_text": "Bem distantes dos efúgios;",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Em procura de refúgios,\nBem distantes dos efúgios;"
        },
        {
            "stanza_type": null,
            "stanza_number": 27,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Bem distantes dos insanos",
                    "line_length": "7"
                },
                {
                    "line_number": 54,
                    "line_text": "Tormentos e desenganos,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Bem distantes dos insanos\nTormentos e desenganos,"
        },
        {
            "stanza_type": null,
            "stanza_number": 28,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Dos quais o mundo se alastra",
                    "line_length": "7"
                },
                {
                    "line_number": 56,
                    "line_text": "Numa tortura de adastra.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Dos quais o mundo se alastra\nNuma tortura de adastra."
        },
        {
            "stanza_type": null,
            "stanza_number": 29,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "E como desceu a noite",
                    "line_length": "7"
                },
                {
                    "line_number": 58,
                    "line_text": "Sem um gemido de açoite,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E como desceu a noite\nSem um gemido de açoite,"
        },
        {
            "stanza_type": null,
            "stanza_number": 30,
            "lines": [
                {
                    "line_number": 59,
                    "line_text": "Do vento no mar luzindo,",
                    "line_length": "7"
                },
                {
                    "line_number": 60,
                    "line_text": "No mar que se vê tão lindo!",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Do vento no mar luzindo,\nNo mar que se vê tão lindo!"
        },
        {
            "stanza_type": null,
            "stanza_number": 31,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Permite, Virgem Maria,",
                    "line_length": "7"
                },
                {
                    "line_number": 62,
                    "line_text": "Que não venha à calmaria",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Permite, Virgem Maria,\nQue não venha à calmaria"
        },
        {
            "stanza_type": null,
            "stanza_number": 32,
            "lines": [
                {
                    "line_number": 63,
                    "line_text": "Das ondas um vento solto,",
                    "line_length": "7"
                },
                {
                    "line_number": 64,
                    "line_text": "E o mar se torne revolto;",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Das ondas um vento solto,\nE o mar se torne revolto;"
        },
        {
            "stanza_type": null,
            "stanza_number": 33,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Ou se torne o mar bem como",
                    "line_length": "7"
                },
                {
                    "line_number": 66,
                    "line_text": "Um leão cheio de assomo...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Ou se torne o mar bem como\nUm leão cheio de assomo..."
        },
        {
            "stanza_type": null,
            "stanza_number": 34,
            "lines": [
                {
                    "line_number": 67,
                    "line_text": "Que o mar se conserve ameno",
                    "line_length": "7"
                },
                {
                    "line_number": 68,
                    "line_text": "Como o olhar do Nazareno.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Que o mar se conserve ameno\nComo o olhar do Nazareno."
        },
        {
            "stanza_type": null,
            "stanza_number": 35,
            "lines": [
                {
                    "line_number": 69,
                    "line_text": "E por esses arredores",
                    "line_length": "7"
                },
                {
                    "line_number": 70,
                    "line_text": "Fiquem bem os pescadores,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E por esses arredores\nFiquem bem os pescadores,"
        },
        {
            "stanza_type": null,
            "stanza_number": 36,
            "lines": [
                {
                    "line_number": 71,
                    "line_text": "Lançando ao mar as braçadas",
                    "line_length": "7"
                },
                {
                    "line_number": 72,
                    "line_text": "Das suas redes talhadas;",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Lançando ao mar as braçadas\nDas suas redes talhadas;"
        },
        {
            "stanza_type": null,
            "stanza_number": 37,
            "lines": [
                {
                    "line_number": 73,
                    "line_text": "E cantem, junto às canoas,",
                    "line_length": "7"
                },
                {
                    "line_number": 74,
                    "line_text": "Seus ditirambos e loas,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E cantem, junto às canoas,\nSeus ditirambos e loas,"
        },
        {
            "stanza_type": null,
            "stanza_number": 38,
            "lines": [
                {
                    "line_number": 75,
                    "line_text": "Que são, à luz das estrelas,",
                    "line_length": "7"
                },
                {
                    "line_number": 76,
                    "line_text": "As antífonas mais belas...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Que são, à luz das estrelas,\nAs antífonas mais belas..."
        },
        {
            "stanza_type": null,
            "stanza_number": 39,
            "lines": [
                {
                    "line_number": 77,
                    "line_text": "E que tu, Virgem Maria,",
                    "line_length": "7"
                },
                {
                    "line_number": 78,
                    "line_text": "Plena de graça e alegria,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E que tu, Virgem Maria,\nPlena de graça e alegria,"
        },
        {
            "stanza_type": null,
            "stanza_number": 40,
            "lines": [
                {
                    "line_number": 79,
                    "line_text": "Possas ouvi-los cantar,",
                    "line_length": "7"
                },
                {
                    "line_number": 80,
                    "line_text": "Do largo espaço sem fim,",
                    "line_length": "7"
                },
                {
                    "line_number": 81,
                    "line_text": "Onde vives a viajar",
                    "line_length": "7"
                },
                {
                    "line_number": 82,
                    "line_text": "No teu barco de marfim.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Possas ouvi-los cantar,\nDo largo espaço sem fim,\nOnde vives a viajar\nNo teu barco de marfim."
        }
    ]
}