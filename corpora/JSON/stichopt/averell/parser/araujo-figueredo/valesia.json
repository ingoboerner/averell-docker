{
    "poem_title": "Valésia",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Araújo Figueredo",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Valésia, lembrei-me agora",
                    "line_length": "7"
                },
                {
                    "line_number": 2,
                    "line_text": "De orar à Nossa Senhora",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Valésia, lembrei-me agora\nDe orar à Nossa Senhora"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Pelo que tanto padeces...",
                    "line_length": "7"
                },
                {
                    "line_number": 4,
                    "line_text": "E não há nada como as preces",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Pelo que tanto padeces...\nE não há nada como as preces"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Que a gente faz de joelhos.",
                    "line_length": "7"
                },
                {
                    "line_number": 6,
                    "line_text": "Ao receber os conselhos",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Que a gente faz de joelhos.\nAo receber os conselhos"
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Da própria alma enternecida",
                    "line_length": "7"
                },
                {
                    "line_number": 8,
                    "line_text": "Pelos tormentos da vida.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Da própria alma enternecida\nPelos tormentos da vida."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "As preces da alma se evolam,",
                    "line_length": "7"
                },
                {
                    "line_number": 10,
                    "line_text": "E são fluidos que consolam.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "As preces da alma se evolam,\nE são fluidos que consolam."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Minhas preces são perfumes",
                    "line_length": "7"
                },
                {
                    "line_number": 12,
                    "line_text": "Que se transformam em lumes.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Minhas preces são perfumes\nQue se transformam em lumes."
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Minhas preces são tão belas",
                    "line_length": "7"
                },
                {
                    "line_number": 14,
                    "line_text": "Como a prata das estrelas.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Minhas preces são tão belas\nComo a prata das estrelas."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "São óleos, as minhas preces;",
                    "line_length": "7"
                },
                {
                    "line_number": 16,
                    "line_text": "E tu que bem as mereces...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São óleos, as minhas preces;\nE tu que bem as mereces..."
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Óleos de unção, óleos bentos",
                    "line_length": "7"
                },
                {
                    "line_number": 18,
                    "line_text": "Por todos os Sacramentos.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Óleos de unção, óleos bentos\nPor todos os Sacramentos."
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "São asas amparadoras;",
                    "line_length": "7"
                },
                {
                    "line_number": 20,
                    "line_text": "E, por certo, guiadoras...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São asas amparadoras;\nE, por certo, guiadoras..."
        },
        {
            "stanza_type": null,
            "stanza_number": 11,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Da tua alma pelo espaço",
                    "line_length": "7"
                },
                {
                    "line_number": 22,
                    "line_text": "Ao qual ergues o teu braço;",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Da tua alma pelo espaço\nAo qual ergues o teu braço;"
        },
        {
            "stanza_type": null,
            "stanza_number": 12,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Ao qual, aflita suplicas",
                    "line_length": "7"
                },
                {
                    "line_number": 24,
                    "line_text": "Amparo, e amparada ficas.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Ao qual, aflita suplicas\nAmparo, e amparada ficas."
        },
        {
            "stanza_type": null,
            "stanza_number": 13,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "São clarões pelos caminhos",
                    "line_length": "7"
                },
                {
                    "line_number": 26,
                    "line_text": "Atapetados de espinhos.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São clarões pelos caminhos\nAtapetados de espinhos."
        },
        {
            "stanza_type": null,
            "stanza_number": 14,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Por onde passas descalça",
                    "line_length": "7"
                },
                {
                    "line_number": 28,
                    "line_text": "Sob a luz que te realça.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Por onde passas descalça\nSob a luz que te realça."
        },
        {
            "stanza_type": null,
            "stanza_number": 15,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "São uns leves lenços brancos",
                    "line_length": "7"
                },
                {
                    "line_number": 30,
                    "line_text": "Para te enxugar os prantos.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São uns leves lenços brancos\nPara te enxugar os prantos."
        },
        {
            "stanza_type": null,
            "stanza_number": 16,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "São os meus próprios olhares",
                    "line_length": "7"
                },
                {
                    "line_number": 32,
                    "line_text": "Aliviando os teus pesares.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São os meus próprios olhares\nAliviando os teus pesares."
        },
        {
            "stanza_type": null,
            "stanza_number": 17,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "São meus braços, e meu peito,",
                    "line_length": "7"
                },
                {
                    "line_number": 34,
                    "line_text": "Servindo aos teus, desse jeito...",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São meus braços, e meu peito,\nServindo aos teus, desse jeito..."
        },
        {
            "stanza_type": null,
            "stanza_number": 18,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "São meus lábios e meus beijos",
                    "line_length": "7"
                },
                {
                    "line_number": 36,
                    "line_text": "Na taça dos teus desejos.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São meus lábios e meus beijos\nNa taça dos teus desejos."
        },
        {
            "stanza_type": null,
            "stanza_number": 19,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "São grãos de trigo dourado,",
                    "line_length": "7"
                },
                {
                    "line_number": 38,
                    "line_text": "Germinando com cuidado.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "São grãos de trigo dourado,\nGerminando com cuidado."
        },
        {
            "stanza_type": null,
            "stanza_number": 20,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "As minhas preces são águas",
                    "line_length": "7"
                },
                {
                    "line_number": 40,
                    "line_text": "Ao encontro das tuas mágoas",
                    "line_length": "7"
                }
            ],
            "stanza_text": "As minhas preces são águas\nAo encontro das tuas mágoas"
        },
        {
            "stanza_type": null,
            "stanza_number": 21,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "E são remédios de flores",
                    "line_length": "7"
                },
                {
                    "line_number": 42,
                    "line_text": "Ao encontro das tuas dores.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "E são remédios de flores\nAo encontro das tuas dores."
        },
        {
            "stanza_type": null,
            "stanza_number": 22,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Quando de mim estás longe,",
                    "line_length": "7"
                },
                {
                    "line_number": 44,
                    "line_text": "Oro mais do que um monge.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Quando de mim estás longe,\nOro mais do que um monge."
        },
        {
            "stanza_type": null,
            "stanza_number": 23,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Ergo aos céus as mãos em palma.",
                    "line_length": "7"
                },
                {
                    "line_number": 46,
                    "line_text": "E me ponho a orar com calma,",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Ergo aos céus as mãos em palma.\nE me ponho a orar com calma,"
        },
        {
            "stanza_type": null,
            "stanza_number": 24,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "Porque sei quanto padeces...",
                    "line_length": "7"
                },
                {
                    "line_number": 48,
                    "line_text": "E não há nada como as preces",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Porque sei quanto padeces...\nE não há nada como as preces"
        },
        {
            "stanza_type": null,
            "stanza_number": 25,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Erguidas a qualquer hora.",
                    "line_length": "7"
                },
                {
                    "line_number": 50,
                    "line_text": "À Virgem Nossa Senhora",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Erguidas a qualquer hora.\nÀ Virgem Nossa Senhora"
        },
        {
            "stanza_type": null,
            "stanza_number": 26,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Que muito mais que os seus filhos,",
                    "line_length": "7"
                },
                {
                    "line_number": 52,
                    "line_text": "Sabe quais lhes são os trilhos",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Que muito mais que os seus filhos,\nSabe quais lhes são os trilhos"
        },
        {
            "stanza_type": null,
            "stanza_number": 27,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Por onde passam, coitados!",
                    "line_length": "7"
                },
                {
                    "line_number": 54,
                    "line_text": "Quase sempre fatigados.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Por onde passam, coitados!\nQuase sempre fatigados."
        },
        {
            "stanza_type": null,
            "stanza_number": 28,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Como se aos ombros levassem",
                    "line_length": "7"
                },
                {
                    "line_number": 56,
                    "line_text": "Muitas cruzes que pesassem",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Como se aos ombros levassem\nMuitas cruzes que pesassem"
        },
        {
            "stanza_type": null,
            "stanza_number": 29,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Mais do que os montões de areia",
                    "line_length": "7"
                },
                {
                    "line_number": 58,
                    "line_text": "Das praias da luz cheia.",
                    "line_length": "7"
                }
            ],
            "stanza_text": "Mais do que os montões de areia\nDas praias da luz cheia."
        }
    ]
}