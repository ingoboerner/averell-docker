{
    "poem_title": "TROCADILHO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Deixando a lei no tinteiro,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "todo o direito transtorna",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "o juiz quando é bigorna",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "sob a pressão do malheiro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Deixando a lei no tinteiro,\ntodo o direito transtorna\no juiz quando é bigorna\nsob a pressão do malheiro."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Se escolher sentidos latos",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "contra o réu não se consente,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "pra condenar o inocente",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "só o faria Pilatos.",
                    "line_length": null
                }
            ],
            "stanza_text": "Se escolher sentidos latos\ncontra o réu não se consente,\npra condenar o inocente\nsó o faria Pilatos."
        }
    ]
}