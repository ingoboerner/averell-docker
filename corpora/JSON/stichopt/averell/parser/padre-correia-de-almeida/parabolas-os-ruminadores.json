{
    "poem_title": "PARÁBOLAS, OS RUMINADORES",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Algum animal,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Como seja o boi,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Engole o alimento,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "E depois remói.",
                    "line_length": null
                }
            ],
            "stanza_text": "Algum animal,\nComo seja o boi,\nEngole o alimento,\nE depois remói."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Assim faz o crítico",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "De tática fina,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Muita palha engole,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "E depois rumina.",
                    "line_length": null
                }
            ],
            "stanza_text": "Assim faz o crítico\nDe tática fina,\nMuita palha engole,\nE depois rumina."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Não tem sucedido",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Por uma vez só",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Ao crítico e ao boi",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Comerem timbó.",
                    "line_length": null
                }
            ],
            "stanza_text": "Não tem sucedido\nPor uma vez só\nAo crítico e ao boi\nComerem timbó."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Algum livro mau",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Para quem o lê,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "É como o timbó",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "Que o boi não vê.",
                    "line_length": null
                }
            ],
            "stanza_text": "Algum livro mau\nPara quem o lê,\nÉ como o timbó\nQue o boi não vê."
        }
    ]
}