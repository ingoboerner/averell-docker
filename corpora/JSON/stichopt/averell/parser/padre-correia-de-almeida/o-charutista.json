{
    "poem_title": "O CHARUTISTA",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Até à vista",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "De gente limpa",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "O charutista",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Vai, e se chimpa.",
                    "line_length": null
                }
            ],
            "stanza_text": "Até à vista\nDe gente limpa\nO charutista\nVai, e se chimpa."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "É na presença",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "De gente honesta",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Pior doença,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Causa molesta.",
                    "line_length": null
                }
            ],
            "stanza_text": "É na presença\nDe gente honesta\nPior doença,\nCausa molesta."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "O homem polido,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Por mais que faça,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Vê-se aturdido",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Pela fumaça.",
                    "line_length": null
                }
            ],
            "stanza_text": "O homem polido,\nPor mais que faça,\nVê-se aturdido\nPela fumaça."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Inquieto e triste",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Escarra e tosse,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "Porém persiste",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "O outro na posse.",
                    "line_length": null
                }
            ],
            "stanza_text": "Inquieto e triste\nEscarra e tosse,\nPorém persiste\nO outro na posse."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "E só pergunta",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "A toda a roda",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "Que aí está junta,",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "Se os incomoda?",
                    "line_length": null
                }
            ],
            "stanza_text": "E só pergunta\nA toda a roda\nQue aí está junta,\nSe os incomoda?"
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "“Ora essa é boa!",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "Eis a resposta,",
                    "line_length": null
                },
                {
                    "line_number": 23,
                    "line_text": "Sua pessoa",
                    "line_length": null
                },
                {
                    "line_number": 24,
                    "line_text": "Não nos desgosta!”",
                    "line_length": null
                }
            ],
            "stanza_text": "“Ora essa é boa!\nEis a resposta,\nSua pessoa\nNão nos desgosta!”"
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "E o malcriado",
                    "line_length": null
                },
                {
                    "line_number": 26,
                    "line_text": "Não avalia",
                    "line_length": null
                },
                {
                    "line_number": 27,
                    "line_text": "Que é tolerado",
                    "line_length": null
                },
                {
                    "line_number": 28,
                    "line_text": "Por cortesia.",
                    "line_length": null
                }
            ],
            "stanza_text": "E o malcriado\nNão avalia\nQue é tolerado\nPor cortesia."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Ele asqueroso",
                    "line_length": null
                },
                {
                    "line_number": 30,
                    "line_text": "Mau cheiro exala,",
                    "line_length": null
                },
                {
                    "line_number": 31,
                    "line_text": "E exclui o gozo",
                    "line_length": null
                },
                {
                    "line_number": 32,
                    "line_text": "De qualquer sala.",
                    "line_length": null
                }
            ],
            "stanza_text": "Ele asqueroso\nMau cheiro exala,\nE exclui o gozo\nDe qualquer sala."
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "A quem ele ama",
                    "line_length": null
                },
                {
                    "line_number": 34,
                    "line_text": "Produz enjoo,",
                    "line_length": null
                },
                {
                    "line_number": 35,
                    "line_text": "Da pobre dama",
                    "line_length": null
                },
                {
                    "line_number": 36,
                    "line_text": "Eu me condoo.",
                    "line_length": null
                }
            ],
            "stanza_text": "A quem ele ama\nProduz enjoo,\nDa pobre dama\nEu me condoo."
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Por gosto impuro",
                    "line_length": null
                },
                {
                    "line_number": 38,
                    "line_text": "Queima fedores,",
                    "line_length": null
                },
                {
                    "line_number": 39,
                    "line_text": "E, vil monturo,",
                    "line_length": null
                },
                {
                    "line_number": 40,
                    "line_text": "Requesta amores.",
                    "line_length": null
                }
            ],
            "stanza_text": "Por gosto impuro\nQueima fedores,\nE, vil monturo,\nRequesta amores."
        },
        {
            "stanza_type": null,
            "stanza_number": 11,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "A moça linda",
                    "line_length": null
                },
                {
                    "line_number": 42,
                    "line_text": "Amar não pode",
                    "line_length": null
                },
                {
                    "line_number": 43,
                    "line_text": "A um tal que ainda",
                    "line_length": null
                },
                {
                    "line_number": 44,
                    "line_text": "Torce o bigode,",
                    "line_length": null
                }
            ],
            "stanza_text": "A moça linda\nAmar não pode\nA um tal que ainda\nTorce o bigode,"
        },
        {
            "stanza_type": null,
            "stanza_number": 12,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Sem recordar-se",
                    "line_length": null
                },
                {
                    "line_number": 46,
                    "line_text": "De que na coma",
                    "line_length": null
                },
                {
                    "line_number": 47,
                    "line_text": "Sabe aninhar-se",
                    "line_length": null
                },
                {
                    "line_number": 48,
                    "line_text": "O ruim aroma.",
                    "line_length": null
                }
            ],
            "stanza_text": "Sem recordar-se\nDe que na coma\nSabe aninhar-se\nO ruim aroma."
        },
        {
            "stanza_type": null,
            "stanza_number": 13,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Se a uma bela",
                    "line_length": null
                },
                {
                    "line_number": 50,
                    "line_text": "Carícias pede,",
                    "line_length": null
                },
                {
                    "line_number": 51,
                    "line_text": "Merece dela",
                    "line_length": null
                },
                {
                    "line_number": 52,
                    "line_text": "Quem tanto fede?",
                    "line_length": null
                }
            ],
            "stanza_text": "Se a uma bela\nCarícias pede,\nMerece dela\nQuem tanto fede?"
        },
        {
            "stanza_type": null,
            "stanza_number": 14,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Ela não sofre",
                    "line_length": null
                },
                {
                    "line_number": 54,
                    "line_text": "Este basbaque",
                    "line_length": null
                },
                {
                    "line_number": 55,
                    "line_text": "Sem muito enxofre",
                    "line_length": null
                },
                {
                    "line_number": 56,
                    "line_text": "Ou Labarraque.",
                    "line_length": null
                }
            ],
            "stanza_text": "Ela não sofre\nEste basbaque\nSem muito enxofre\nOu Labarraque."
        }
    ]
}