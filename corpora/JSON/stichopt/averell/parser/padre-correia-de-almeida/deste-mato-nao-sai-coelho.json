{
    "poem_title": "DESTE MATO NÃO SAI COELHO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Egoísta financeiro",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "suas patacas poupava,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "e, se não dava dinheiro,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "mui justas desculpas dava.",
                    "line_length": null
                }
            ],
            "stanza_text": "Egoísta financeiro\nsuas patacas poupava,\ne, se não dava dinheiro,\nmui justas desculpas dava."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Os diferentes pedintes,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "de bacia de Irmandade,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "eram de uma escusa ouvintes",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "ungida de urbanidade.",
                    "line_length": null
                }
            ],
            "stanza_text": "Os diferentes pedintes,\nde bacia de Irmandade,\neram de uma escusa ouvintes\nungida de urbanidade."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "— Pra cera de São Maurício!",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "— Que nos corrija do vício!",
                    "line_length": null
                }
            ],
            "stanza_text": "— Pra cera de São Maurício!\n— Que nos corrija do vício!"
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "— Esmola pra Santo Antônio!",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "— Que nos livre do demônio!",
                    "line_length": null
                }
            ],
            "stanza_text": "— Esmola pra Santo Antônio!\n— Que nos livre do demônio!"
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "— Senhora da Boa Morte!",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "— Ela nos dê boa sorte!",
                    "line_length": null
                }
            ],
            "stanza_text": "— Senhora da Boa Morte!\n— Ela nos dê boa sorte!"
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "— A Senhora do Rosário!",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "— Já dei para Santo Hilário!",
                    "line_length": null
                }
            ],
            "stanza_text": "— A Senhora do Rosário!\n— Já dei para Santo Hilário!"
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "— O São Gonçalo Garcia!",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "— Procure cá outro dia!",
                    "line_length": null
                }
            ],
            "stanza_text": "— O São Gonçalo Garcia!\n— Procure cá outro dia!"
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "— As almas santas benditas!",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "— As rendas são pequenitas!",
                    "line_length": null
                }
            ],
            "stanza_text": "— As almas santas benditas!\n— As rendas são pequenitas!"
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "— A Senhora das Mercês!",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "— Ficará para outra vez!",
                    "line_length": null
                }
            ],
            "stanza_text": "— A Senhora das Mercês!\n— Ficará para outra vez!"
        }
    ]
}