{
    "poem_title": "PARÁBOLA, A PORCA E OS LEITÕES",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Quem os fatos aprofunda",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Lhes acha comparações!",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Tem-se visto a porca imunda",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Matar os próprios leitões.",
                    "line_length": null
                }
            ],
            "stanza_text": "Quem os fatos aprofunda\nLhes acha comparações!\nTem-se visto a porca imunda\nMatar os próprios leitões."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Análogo ou parecido",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Este caso triste achei",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "À prática de um partido",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Que faz e viola uma lei.",
                    "line_length": null
                }
            ],
            "stanza_text": "Análogo ou parecido\nEste caso triste achei\nÀ prática de um partido\nQue faz e viola uma lei."
        }
    ]
}