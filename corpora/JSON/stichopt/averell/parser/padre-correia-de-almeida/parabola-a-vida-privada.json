{
    "poem_title": "PARÁBOLA, A VIDA PRIVADA",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tem gosto o mais esquisito",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Quem mexe alheia miséria,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "E supõe muito bonito",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Recrear-se em tal matéria.",
                    "line_length": null
                }
            ],
            "stanza_text": "Tem gosto o mais esquisito\nQuem mexe alheia miséria,\nE supõe muito bonito\nRecrear-se em tal matéria."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Da parábola no espelho",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Se espreite quem de tal gosta,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "E verá o escaravelho",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "A revolver-se na bosta.",
                    "line_length": null
                }
            ],
            "stanza_text": "Da parábola no espelho\nSe espreite quem de tal gosta,\nE verá o escaravelho\nA revolver-se na bosta."
        }
    ]
}