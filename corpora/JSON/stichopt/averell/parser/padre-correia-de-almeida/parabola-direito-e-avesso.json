{
    "poem_title": "PARÁBOLA, DIREITO E AVESSO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Da casimira",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Vendo-se a teia,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "De um lado admira,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Mas do outro é feia.",
                    "line_length": null
                }
            ],
            "stanza_text": "Da casimira\nVendo-se a teia,\nDe um lado admira,\nMas do outro é feia."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Se isto respeita,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "E cabe ao pano,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Também se ajeita",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Ao ente humano.",
                    "line_length": null
                }
            ],
            "stanza_text": "Se isto respeita,\nE cabe ao pano,\nTambém se ajeita\nAo ente humano."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Um, que aí vedes,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Jurisconsulto",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Fura paredes",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Só por indulto.",
                    "line_length": null
                }
            ],
            "stanza_text": "Um, que aí vedes,\nJurisconsulto\nFura paredes\nSó por indulto."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Aquele passa",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Por ser fidalgo,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "Porque na caça",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "Usa de um galgo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Aquele passa\nPor ser fidalgo,\nPorque na caça\nUsa de um galgo."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Eis desse nobre",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "A fidalguia,",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "No mais se cobre",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "De vilania.",
                    "line_length": null
                }
            ],
            "stanza_text": "Eis desse nobre\nA fidalguia,\nNo mais se cobre\nDe vilania."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Este, modelo",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "De sã virtude,",
                    "line_length": null
                },
                {
                    "line_number": 23,
                    "line_text": "Por mais podê-lo,",
                    "line_length": null
                },
                {
                    "line_number": 24,
                    "line_text": "Mais nos ilude.",
                    "line_length": null
                }
            ],
            "stanza_text": "Este, modelo\nDe sã virtude,\nPor mais podê-lo,\nMais nos ilude."
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Estoutro goza",
                    "line_length": null
                },
                {
                    "line_number": 26,
                    "line_text": "De sábio a fama,",
                    "line_length": null
                },
                {
                    "line_number": 27,
                    "line_text": "Dá-nos babosa,",
                    "line_length": null
                },
                {
                    "line_number": 28,
                    "line_text": "Dorme na cama.",
                    "line_length": null
                }
            ],
            "stanza_text": "Estoutro goza\nDe sábio a fama,\nDá-nos babosa,\nDorme na cama."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Vate que escreve",
                    "line_length": null
                },
                {
                    "line_number": 30,
                    "line_text": "Só de improviso,",
                    "line_length": null
                },
                {
                    "line_number": 31,
                    "line_text": "Já nos ter deve",
                    "line_length": null
                },
                {
                    "line_number": 32,
                    "line_text": "De sobreaviso.",
                    "line_length": null
                }
            ],
            "stanza_text": "Vate que escreve\nSó de improviso,\nJá nos ter deve\nDe sobreaviso."
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Há na ciência",
                    "line_length": null
                },
                {
                    "line_number": 34,
                    "line_text": "Da criatura",
                    "line_length": null
                },
                {
                    "line_number": 35,
                    "line_text": "Muita aparência,",
                    "line_length": null
                },
                {
                    "line_number": 36,
                    "line_text": "Muita impostura.",
                    "line_length": null
                }
            ],
            "stanza_text": "Há na ciência\nDa criatura\nMuita aparência,\nMuita impostura."
        }
    ]
}