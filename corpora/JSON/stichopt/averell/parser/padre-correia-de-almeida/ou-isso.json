{
    "poem_title": "OU ISSO!",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Liso inventor anuncia",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "em repetidos cartazes",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "um segredo que traria",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "resultados eficazes.",
                    "line_length": null
                }
            ],
            "stanza_text": "Liso inventor anuncia\nem repetidos cartazes\num segredo que traria\nresultados eficazes."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Pela sincera linguagem",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "tu bem convencido julgas",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "que são de imensa vantagem",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "os tais pós de matar pulgas.",
                    "line_length": null
                }
            ],
            "stanza_text": "Pela sincera linguagem\ntu bem convencido julgas\nque são de imensa vantagem\nos tais pós de matar pulgas."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Há chusmas de compradores,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "pois é o segredo armadilha",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "que apanha grossos valores,",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "qual nova salsaparrilha.",
                    "line_length": null
                }
            ],
            "stanza_text": "Há chusmas de compradores,\npois é o segredo armadilha\nque apanha grossos valores,\nqual nova salsaparrilha."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "O chale, a saia, a camisa,",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "a colcha, o lençol, a esteira,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "em suma, tudo precisa",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "da mortífera poeira.",
                    "line_length": null
                }
            ],
            "stanza_text": "O chale, a saia, a camisa,\na colcha, o lençol, a esteira,\nem suma, tudo precisa\nda mortífera poeira."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Nenhum trabalho se poupa,",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "e o mundo inteiro se ocupa",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "em ver se extingue na roupa",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "a importuna sangue-chupa.",
                    "line_length": null
                }
            ],
            "stanza_text": "Nenhum trabalho se poupa,\ne o mundo inteiro se ocupa\nem ver se extingue na roupa\na importuna sangue-chupa."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Mas o bichinho teimoso",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "vai mordendo e continua",
                    "line_length": null
                },
                {
                    "line_number": 23,
                    "line_text": "na antiga posse, e no gozo",
                    "line_length": null
                },
                {
                    "line_number": 24,
                    "line_text": "da pele vestida ou nua.",
                    "line_length": null
                }
            ],
            "stanza_text": "Mas o bichinho teimoso\nvai mordendo e continua\nna antiga posse, e no gozo\nda pele vestida ou nua."
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "E por mais que Ana Delfina",
                    "line_length": null
                },
                {
                    "line_number": 26,
                    "line_text": "o pescoço e os braço s cubra,",
                    "line_length": null
                },
                {
                    "line_number": 27,
                    "line_text": "deixa ver na cútis fina",
                    "line_length": null
                },
                {
                    "line_number": 28,
                    "line_text": "muita e muita pinta rubra.",
                    "line_length": null
                }
            ],
            "stanza_text": "E por mais que Ana Delfina\no pescoço e os braço s cubra,\ndeixa ver na cútis fina\nmuita e muita pinta rubra."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Se presumível já fora",
                    "line_length": null
                },
                {
                    "line_number": 30,
                    "line_text": "que sempre estaria em voga,",
                    "line_length": null
                },
                {
                    "line_number": 31,
                    "line_text": "a invenção destruidora",
                    "line_length": null
                },
                {
                    "line_number": 32,
                    "line_text": "parece que deu em droga!",
                    "line_length": null
                }
            ],
            "stanza_text": "Se presumível já fora\nque sempre estaria em voga,\na invenção destruidora\nparece que deu em droga!"
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Comprador mais exigente",
                    "line_length": null
                },
                {
                    "line_number": 34,
                    "line_text": "ao droguista participa",
                    "line_length": null
                },
                {
                    "line_number": 35,
                    "line_text": "que, apesar dos pós, a gente",
                    "line_length": null
                },
                {
                    "line_number": 36,
                    "line_text": "das pulgas não se emancipa.",
                    "line_length": null
                }
            ],
            "stanza_text": "Comprador mais exigente\nao droguista participa\nque, apesar dos pós, a gente\ndas pulgas não se emancipa."
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Então este mais que prestes",
                    "line_length": null
                },
                {
                    "line_number": 38,
                    "line_text": "as circunstâncias indaga:",
                    "line_length": null
                },
                {
                    "line_number": 39,
                    "line_text": "— de que modo vos houvestes",
                    "line_length": null
                },
                {
                    "line_number": 40,
                    "line_text": "para dar cabo da praga?",
                    "line_length": null
                }
            ],
            "stanza_text": "Então este mais que prestes\nas circunstâncias indaga:\n— de que modo vos houvestes\npara dar cabo da praga?"
        },
        {
            "stanza_type": null,
            "stanza_number": 11,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "— Derramei o pó comprado",
                    "line_length": null
                },
                {
                    "line_number": 42,
                    "line_text": "sobre o corpo, cama e lixo;",
                    "line_length": null
                },
                {
                    "line_number": 43,
                    "line_text": "nenhum lugar foi poupado",
                    "line_length": null
                },
                {
                    "line_number": 44,
                    "line_text": "onde resida tal bicho.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Derramei o pó comprado\nsobre o corpo, cama e lixo;\nnenhum lugar foi poupado\nonde resida tal bicho."
        },
        {
            "stanza_type": null,
            "stanza_number": 12,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "— Pois não é como se julga,",
                    "line_length": null
                },
                {
                    "line_number": 46,
                    "line_text": "esse o melhor dos caminhos!",
                    "line_length": null
                },
                {
                    "line_number": 47,
                    "line_text": "Apanhai com jeito a pulga,",
                    "line_length": null
                },
                {
                    "line_number": 48,
                    "line_text": "pulverizai-lhe os olhinhos.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Pois não é como se julga,\nesse o melhor dos caminhos!\nApanhai com jeito a pulga,\npulverizai-lhe os olhinhos."
        },
        {
            "stanza_type": null,
            "stanza_number": 13,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "— Se posso mais facilmente",
                    "line_length": null
                },
                {
                    "line_number": 50,
                    "line_text": "com este dedo esmagá-la...",
                    "line_length": null
                },
                {
                    "line_number": 51,
                    "line_text": "— Não serei eu quem sustente",
                    "line_length": null
                },
                {
                    "line_number": 52,
                    "line_text": "que a pulga assim não estala!",
                    "line_length": null
                }
            ],
            "stanza_text": "— Se posso mais facilmente\ncom este dedo esmagá-la...\n— Não serei eu quem sustente\nque a pulga assim não estala!"
        }
    ]
}