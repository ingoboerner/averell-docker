{
    "poem_title": "TIPO ACADÊMICO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Nhonhô Chiquinho",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "está no curso,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "e entre os colegas",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "já faz discurso.",
                    "line_length": null
                }
            ],
            "stanza_text": "Nhonhô Chiquinho\nestá no curso,\ne entre os colegas\njá faz discurso."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "É nos debates",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "da academia",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "ilustre lustre,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "que os alumia.",
                    "line_length": null
                }
            ],
            "stanza_text": "É nos debates\nda academia\nilustre lustre,\nque os alumia."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "É literato",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "tão erudito,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "que a História abrange",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "em um só dito.",
                    "line_length": null
                }
            ],
            "stanza_text": "É literato\ntão erudito,\nque a História abrange\nem um só dito."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Sobe ao Parnaso",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "da Pauliceia,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "corrige a Eneida,",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "mais a Odisseia.",
                    "line_length": null
                }
            ],
            "stanza_text": "Sobe ao Parnaso\nda Pauliceia,\ncorrige a Eneida,\nmais a Odisseia."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Vate inspirado,",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "republicano,",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "só quer assunto",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "americano.",
                    "line_length": null
                }
            ],
            "stanza_text": "Vate inspirado,\nrepublicano,\nsó quer assunto\namericano."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Diz-nos que temos",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "para poemas",
                    "line_length": null
                },
                {
                    "line_number": 23,
                    "line_text": "nossas Lindoias,",
                    "line_length": null
                },
                {
                    "line_number": 24,
                    "line_text": "nossas Moemas.",
                    "line_length": null
                }
            ],
            "stanza_text": "Diz-nos que temos\npara poemas\nnossas Lindoias,\nnossas Moemas."
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Por sermos bugres,",
                    "line_length": null
                },
                {
                    "line_number": 26,
                    "line_text": "julga desdouro",
                    "line_length": null
                },
                {
                    "line_number": 27,
                    "line_text": "falarmos língua",
                    "line_length": null
                },
                {
                    "line_number": 28,
                    "line_text": "do Tejo e Douro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Por sermos bugres,\njulga desdouro\nfalarmos língua\ndo Tejo e Douro."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "E esta sentença",
                    "line_length": null
                },
                {
                    "line_number": 30,
                    "line_text": "assim tão lisa",
                    "line_length": null
                },
                {
                    "line_number": 31,
                    "line_text": "as belas letras",
                    "line_length": null
                },
                {
                    "line_number": 32,
                    "line_text": "nacionaliza.",
                    "line_length": null
                }
            ],
            "stanza_text": "E esta sentença\nassim tão lisa\nas belas letras\nnacionaliza."
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Traz novidades,",
                    "line_length": null
                },
                {
                    "line_number": 34,
                    "line_text": "que só não cola",
                    "line_length": null
                },
                {
                    "line_number": 35,
                    "line_text": "ramerraneiro",
                    "line_length": null
                },
                {
                    "line_number": 36,
                    "line_text": "da antiga escola.",
                    "line_length": null
                }
            ],
            "stanza_text": "Traz novidades,\nque só não cola\nramerraneiro\nda antiga escola."
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Mete em debuxos",
                    "line_length": null
                },
                {
                    "line_number": 38,
                    "line_text": "os algarismos,",
                    "line_length": null
                },
                {
                    "line_number": 39,
                    "line_text": "e num chichelo",
                    "line_length": null
                },
                {
                    "line_number": 40,
                    "line_text": "os aforismos.",
                    "line_length": null
                }
            ],
            "stanza_text": "Mete em debuxos\nos algarismos,\ne num chichelo\nos aforismos."
        },
        {
            "stanza_type": null,
            "stanza_number": 11,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Um engenheiro,",
                    "line_length": null
                },
                {
                    "line_number": 42,
                    "line_text": "um alopata,",
                    "line_length": null
                },
                {
                    "line_number": 43,
                    "line_text": "na crassidade",
                    "line_length": null
                },
                {
                    "line_number": 44,
                    "line_text": "ele os empata.",
                    "line_length": null
                }
            ],
            "stanza_text": "Um engenheiro,\num alopata,\nna crassidade\nele os empata."
        },
        {
            "stanza_type": null,
            "stanza_number": 12,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Diz que às demandas",
                    "line_length": null
                },
                {
                    "line_number": 46,
                    "line_text": "dá descaminho",
                    "line_length": null
                },
                {
                    "line_number": 47,
                    "line_text": "um advogado",
                    "line_length": null
                },
                {
                    "line_number": 48,
                    "line_text": "sem pergaminho.",
                    "line_length": null
                }
            ],
            "stanza_text": "Diz que às demandas\ndá descaminho\num advogado\nsem pergaminho."
        },
        {
            "stanza_type": null,
            "stanza_number": 13,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Supõe ser crime,",
                    "line_length": null
                },
                {
                    "line_number": 50,
                    "line_text": "ou desaforo,",
                    "line_length": null
                },
                {
                    "line_number": 51,
                    "line_text": "que os tais Rebouças",
                    "line_length": null
                },
                {
                    "line_number": 52,
                    "line_text": "voguem no foro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Supõe ser crime,\nou desaforo,\nque os tais Rebouças\nvoguem no foro."
        },
        {
            "stanza_type": null,
            "stanza_number": 14,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Em vão procura",
                    "line_length": null
                },
                {
                    "line_number": 54,
                    "line_text": "no sacerdote",
                    "line_length": null
                },
                {
                    "line_number": 55,
                    "line_text": "causa que preste,",
                    "line_length": null
                },
                {
                    "line_number": 56,
                    "line_text": "e que se adote.",
                    "line_length": null
                }
            ],
            "stanza_text": "Em vão procura\nno sacerdote\ncausa que preste,\ne que se adote."
        },
        {
            "stanza_type": null,
            "stanza_number": 15,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Em tudo enxerga",
                    "line_length": null
                },
                {
                    "line_number": 58,
                    "line_text": "jesuitismo,",
                    "line_length": null
                },
                {
                    "line_number": 59,
                    "line_text": "e só é crente",
                    "line_length": null
                },
                {
                    "line_number": 60,
                    "line_text": "no espiritismo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Em tudo enxerga\njesuitismo,\ne só é crente\nno espiritismo."
        },
        {
            "stanza_type": null,
            "stanza_number": 16,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Sem ler nos livros",
                    "line_length": null
                },
                {
                    "line_number": 62,
                    "line_text": "trata de tudo,",
                    "line_length": null
                },
                {
                    "line_number": 63,
                    "line_text": "pois de nascença",
                    "line_length": null
                },
                {
                    "line_number": 64,
                    "line_text": "já trouxe o estudo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Sem ler nos livros\ntrata de tudo,\npois de nascença\njá trouxe o estudo."
        },
        {
            "stanza_type": null,
            "stanza_number": 17,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Nem da verdade",
                    "line_length": null
                },
                {
                    "line_number": 66,
                    "line_text": "aqui me aparto,",
                    "line_length": null
                },
                {
                    "line_number": 67,
                    "line_text": "se sábio o creio",
                    "line_length": null
                },
                {
                    "line_number": 68,
                    "line_text": "antes do parto.",
                    "line_length": null
                }
            ],
            "stanza_text": "Nem da verdade\naqui me aparto,\nse sábio o creio\nantes do parto."
        },
        {
            "stanza_type": null,
            "stanza_number": 18,
            "lines": [
                {
                    "line_number": 69,
                    "line_text": "De oitenta idiomas",
                    "line_length": null
                },
                {
                    "line_number": 70,
                    "line_text": "bem complicados",
                    "line_length": null
                },
                {
                    "line_number": 71,
                    "line_text": "sabe os diversos",
                    "line_length": null
                },
                {
                    "line_number": 72,
                    "line_text": "significados.",
                    "line_length": null
                }
            ],
            "stanza_text": "De oitenta idiomas\nbem complicados\nsabe os diversos\nsignificados."
        },
        {
            "stanza_type": null,
            "stanza_number": 19,
            "lines": [
                {
                    "line_number": 73,
                    "line_text": "Mas é tão firme",
                    "line_length": null
                },
                {
                    "line_number": 74,
                    "line_text": "no galicismo,",
                    "line_length": null
                },
                {
                    "line_number": 75,
                    "line_text": "que, se este falha,",
                    "line_length": null
                },
                {
                    "line_number": 76,
                    "line_text": "ainda eu cismo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Mas é tão firme\nno galicismo,\nque, se este falha,\nainda eu cismo."
        },
        {
            "stanza_type": null,
            "stanza_number": 20,
            "lines": [
                {
                    "line_number": 77,
                    "line_text": "Que jovem útil,",
                    "line_length": null
                },
                {
                    "line_number": 78,
                    "line_text": "e de esperanças,",
                    "line_length": null
                },
                {
                    "line_number": 79,
                    "line_text": "se dos parentes",
                    "line_length": null
                },
                {
                    "line_number": 80,
                    "line_text": "lhe vêm heranças!",
                    "line_length": null
                }
            ],
            "stanza_text": "Que jovem útil,\ne de esperanças,\nse dos parentes\nlhe vêm heranças!"
        },
        {
            "stanza_type": null,
            "stanza_number": 21,
            "lines": [
                {
                    "line_number": 81,
                    "line_text": "Embora fosse",
                    "line_length": null
                },
                {
                    "line_number": 82,
                    "line_text": "asno ou burrico,",
                    "line_length": null
                },
                {
                    "line_number": 83,
                    "line_text": "cessava a asneira",
                    "line_length": null
                },
                {
                    "line_number": 84,
                    "line_text": "em sendo rico.",
                    "line_length": null
                }
            ],
            "stanza_text": "Embora fosse\nasno ou burrico,\ncessava a asneira\nem sendo rico."
        },
        {
            "stanza_type": null,
            "stanza_number": 22,
            "lines": [
                {
                    "line_number": 85,
                    "line_text": "Louvo a fortuna,",
                    "line_length": null
                },
                {
                    "line_number": 86,
                    "line_text": "que lhe tem dado",
                    "line_length": null
                },
                {
                    "line_number": 87,
                    "line_text": "um pai visconde",
                    "line_length": null
                },
                {
                    "line_number": 88,
                    "line_text": "sem viscondado.",
                    "line_length": null
                }
            ],
            "stanza_text": "Louvo a fortuna,\nque lhe tem dado\num pai visconde\nsem viscondado."
        },
        {
            "stanza_type": null,
            "stanza_number": 23,
            "lines": [
                {
                    "line_number": 89,
                    "line_text": "Com tanto esmero",
                    "line_length": null
                },
                {
                    "line_number": 90,
                    "line_text": "o educara,",
                    "line_length": null
                },
                {
                    "line_number": 91,
                    "line_text": "que o Chico briga,",
                    "line_length": null
                },
                {
                    "line_number": 92,
                    "line_text": "e expõe a cara.",
                    "line_length": null
                }
            ],
            "stanza_text": "Com tanto esmero\no educara,\nque o Chico briga,\ne expõe a cara."
        },
        {
            "stanza_type": null,
            "stanza_number": 24,
            "lines": [
                {
                    "line_number": 93,
                    "line_text": "Sofre no lombo",
                    "line_length": null
                },
                {
                    "line_number": 94,
                    "line_text": "dor de peroba,",
                    "line_length": null
                },
                {
                    "line_number": 95,
                    "line_text": "e com azougue",
                    "line_length": null
                },
                {
                    "line_number": 96,
                    "line_text": "toma caroba.",
                    "line_length": null
                }
            ],
            "stanza_text": "Sofre no lombo\ndor de peroba,\ne com azougue\ntoma caroba."
        },
        {
            "stanza_type": null,
            "stanza_number": 25,
            "lines": [
                {
                    "line_number": 97,
                    "line_text": "Por passatempo",
                    "line_length": null
                },
                {
                    "line_number": 98,
                    "line_text": "pespega um couce,",
                    "line_length": null
                },
                {
                    "line_number": 99,
                    "line_text": "e quem se queixa",
                    "line_length": null
                },
                {
                    "line_number": 100,
                    "line_text": "equivocou-se.",
                    "line_length": null
                }
            ],
            "stanza_text": "Por passatempo\npespega um couce,\ne quem se queixa\nequivocou-se."
        },
        {
            "stanza_type": null,
            "stanza_number": 26,
            "lines": [
                {
                    "line_number": 101,
                    "line_text": "Razão não teve",
                    "line_length": null
                },
                {
                    "line_number": 102,
                    "line_text": "seu primo Juca,",
                    "line_length": null
                },
                {
                    "line_number": 103,
                    "line_text": "que vai doente",
                    "line_length": null
                },
                {
                    "line_number": 104,
                    "line_text": "para a Tijuca.",
                    "line_length": null
                }
            ],
            "stanza_text": "Razão não teve\nseu primo Juca,\nque vai doente\npara a Tijuca."
        },
        {
            "stanza_type": null,
            "stanza_number": 27,
            "lines": [
                {
                    "line_number": 105,
                    "line_text": "Seus atributos",
                    "line_length": null
                },
                {
                    "line_number": 106,
                    "line_text": "não amesquinho,",
                    "line_length": null
                },
                {
                    "line_number": 107,
                    "line_text": "que os tem de arromba",
                    "line_length": null
                },
                {
                    "line_number": 108,
                    "line_text": "Nhonhô Chiquinho.",
                    "line_length": null
                }
            ],
            "stanza_text": "Seus atributos\nnão amesquinho,\nque os tem de arromba\nNhonhô Chiquinho."
        },
        {
            "stanza_type": null,
            "stanza_number": 28,
            "lines": [
                {
                    "line_number": 109,
                    "line_text": "Se querem provas,",
                    "line_length": null
                },
                {
                    "line_number": 110,
                    "line_text": "guardo-as comigo;",
                    "line_length": null
                },
                {
                    "line_number": 111,
                    "line_text": "é o rapazete",
                    "line_length": null
                },
                {
                    "line_number": 112,
                    "line_text": "da Pátria amigo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Se querem provas,\nguardo-as comigo;\né o rapazete\nda Pátria amigo."
        },
        {
            "stanza_type": null,
            "stanza_number": 29,
            "lines": [
                {
                    "line_number": 113,
                    "line_text": "Daqui a pouco",
                    "line_length": null
                },
                {
                    "line_number": 114,
                    "line_text": "é candidato,",
                    "line_length": null
                },
                {
                    "line_number": 115,
                    "line_text": "e isto, que afirmo,",
                    "line_length": null
                },
                {
                    "line_number": 116,
                    "line_text": "assino e dato.",
                    "line_length": null
                }
            ],
            "stanza_text": "Daqui a pouco\né candidato,\ne isto, que afirmo,\nassino e dato."
        },
        {
            "stanza_type": null,
            "stanza_number": 30,
            "lines": [
                {
                    "line_number": 117,
                    "line_text": "o Brasil medra,",
                    "line_length": null
                },
                {
                    "line_number": 118,
                    "line_text": "se um só momento",
                    "line_length": null
                },
                {
                    "line_number": 119,
                    "line_text": "entra o fedelho",
                    "line_length": null
                },
                {
                    "line_number": 120,
                    "line_text": "no parlamento",
                    "line_length": null
                }
            ],
            "stanza_text": "o Brasil medra,\nse um só momento\nentra o fedelho\nno parlamento"
        },
        {
            "stanza_type": null,
            "stanza_number": 31,
            "lines": [
                {
                    "line_number": 121,
                    "line_text": "Que prazer, vê-lo",
                    "line_length": null
                },
                {
                    "line_number": 122,
                    "line_text": "deputadinho,",
                    "line_length": null
                },
                {
                    "line_number": 123,
                    "line_text": "ou discorrendo,",
                    "line_length": null
                },
                {
                    "line_number": 124,
                    "line_text": "ou caladinho!",
                    "line_length": null
                }
            ],
            "stanza_text": "Que prazer, vê-lo\ndeputadinho,\nou discorrendo,\nou caladinho!"
        },
        {
            "stanza_type": null,
            "stanza_number": 32,
            "lines": [
                {
                    "line_number": 125,
                    "line_text": "Um nosso digno",
                    "line_length": null
                },
                {
                    "line_number": 126,
                    "line_text": "representante",
                    "line_length": null
                },
                {
                    "line_number": 127,
                    "line_text": "eu prognostico",
                    "line_length": null
                },
                {
                    "line_number": 128,
                    "line_text": "desde este instante.",
                    "line_length": null
                }
            ],
            "stanza_text": "Um nosso digno\nrepresentante\neu prognostico\ndesde este instante."
        },
        {
            "stanza_type": null,
            "stanza_number": 33,
            "lines": [
                {
                    "line_number": 129,
                    "line_text": "Uma lei velha",
                    "line_length": null
                },
                {
                    "line_number": 130,
                    "line_text": "tem consignado",
                    "line_length": null
                },
                {
                    "line_number": 131,
                    "line_text": "que velhos entrem",
                    "line_length": null
                },
                {
                    "line_number": 132,
                    "line_text": "para o senado.",
                    "line_length": null
                }
            ],
