    "poem_title": "LXVI (sem título)",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "A respeito dos prazeres",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Que sentimos nesta vida,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Dize-me tu se o quiseres",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Com razão esclarecida",
                    "line_length": null
                },
                {
                    "line_number": 5,
                    "line_text": "Qual o maior o consideres?",
                    "line_length": null
                }
            ],
            "stanza_text": "A respeito dos prazeres\nQue sentimos nesta vida,\nDize-me tu se o quiseres\nCom razão esclarecida\nQual o maior o consideres?"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 6,
                    "line_text": "— Se é necessário dizê-lo",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Eu direi sem grande custo,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Que é prazer sem paralelo",
                    "line_length": null
                },
                {
                    "line_number": 9,
                    "line_text": "Tirar o sapato justo,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Meter o pé no chinelo.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Se é necessário dizê-lo\nEu direi sem grande custo,\nQue é prazer sem paralelo\nTirar o sapato justo,\nMeter o pé no chinelo."
        }
    ]
}