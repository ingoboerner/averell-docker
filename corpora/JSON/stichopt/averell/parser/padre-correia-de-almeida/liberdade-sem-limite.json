{
    "poem_title": "LIBERDADE SEM LIMITE",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "— Ousou proibir o Bispo",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "leituras de impiedade!",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Ai! Eu livre Imprensa dispo",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "a legal humanidade.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Ousou proibir o Bispo\nleituras de impiedade!\nAi! Eu livre Imprensa dispo\na legal humanidade."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "— Proíbem (oh! que atentado!)",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "vender-se o que é venenoso!",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Eu livre Comércio brado:",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "despojaram-me de um gozo.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Proíbem (oh! que atentado!)\nvender-se o que é venenoso!\nEu livre Comércio brado:\ndespojaram-me de um gozo."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "— Pólvora não se fabrique",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "no coração da cidade!",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Pois então querem que eu fique",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Indústria sem liberdade?!",
                    "line_length": null
                }
            ],
            "stanza_text": "— Pólvora não se fabrique\nno coração da cidade!\nPois então querem que eu fique\nIndústria sem liberdade?!"
        }
    ]
}