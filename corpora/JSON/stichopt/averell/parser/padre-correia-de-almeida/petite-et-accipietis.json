{
    "poem_title": "PETITE ET ACCIPIETIS",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "— Contra fatos não há réplica!",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Demóstenes Pederneira",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "tem nesta rica província",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "influência verdadeira!",
                    "line_length": null
                }
            ],
            "stanza_text": "— Contra fatos não há réplica!\nDemóstenes Pederneira\ntem nesta rica província\ninfluência verdadeira!"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Pode entrar na lista tríplice",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "pelos imensos recursos",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "que lhe sobram da eloquência",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "de suculentos discursos!",
                    "line_length": null
                }
            ],
            "stanza_text": "Pode entrar na lista tríplice\npelos imensos recursos\nque lhe sobram da eloquência\nde suculentos discursos!"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Prodígios consegue a súplica",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "humilíssima incessante!",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Dificilmente resiste-se",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "a importuno suplicante!",
                    "line_length": null
                }
            ],
            "stanza_text": "Prodígios consegue a súplica\nhumilíssima incessante!\nDificilmente resiste-se\na importuno suplicante!"
        }
    ]
}