{
    "poem_title": "CONCORRÊNCIA OFICIOSA",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Aquele que bem pondera",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "escreva em seu vade-mécum:",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Se Pirro ainda não dera,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "mas ia dar um espirro,",
                    "line_length": null
                },
                {
                    "line_number": 5,
                    "line_text": "não faltava quem a Pirro",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "dissesse Dominus tecum.",
                    "line_length": null
                }
            ],
            "stanza_text": "Aquele que bem pondera\nescreva em seu vade-mécum:\nSe Pirro ainda não dera,\nmas ia dar um espirro,\nnão faltava quem a Pirro\ndissesse Dominus tecum."
        }
    ]
}