{
    "poem_title": "PARÁBOLA, DESIGUALDADE DOS DEDOS",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Um dedo chama-se mínimo,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "porque não há igualdade",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "na quintúplice irmandade",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "dos dedos de nossa mão.",
                    "line_length": null
                }
            ],
            "stanza_text": "Um dedo chama-se mínimo,\nporque não há igualdade\nna quintúplice irmandade\ndos dedos de nossa mão."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Caráter, talento, espírito",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "diferem de irmão a irmão.",
                    "line_length": null
                }
            ],
            "stanza_text": "Caráter, talento, espírito\ndiferem de irmão a irmão."
        }
    ]
