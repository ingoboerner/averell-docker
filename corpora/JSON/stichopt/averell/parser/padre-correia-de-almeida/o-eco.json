{
    "poem_title": "O ECO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "No deserto e u clamava essoutro dia,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "e o eco repetindo respondia:",
                    "line_length": null
                }
            ],
            "stanza_text": "No deserto e u clamava essoutro dia,\ne o eco repetindo respondia:"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "— Como peste a licença da palavra...",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "-lavra.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Como peste a licença da palavra...\n-lavra."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "— Impunemente a sórdida trapaça...",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "— passa.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Impunemente a sórdida trapaça...\n— passa."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "— É causa eficiente do desdouro...",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "— ouro.",
                    "line_length": null
                }
            ],
            "stanza_text": "— É causa eficiente do desdouro...\n— ouro."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "— Produz na gente séria o Ímpio chasco...",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "— asco.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Produz na gente séria o Ímpio chasco...\n— asco."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "— Propalam por aí contra os roupetas...",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "— petas.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Propalam por aí contra os roupetas...\n— petas."
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "— E aquele que esbraveja contra o Papa...",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "— papa.",
                    "line_length": null
                }
            ],
            "stanza_text": "— E aquele que esbraveja contra o Papa...\n— papa."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "— Pra Igreja se este os dentes arreganha...",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "— ganha.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Pra Igreja se este os dentes arreganha...\n— ganha."
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "— Ao som da voz profética retumba...",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "-tumba.",
                    "line_length": null
                }
            ],
            "stanza_text": "— Ao som da voz profética retumba...\n-tumba."
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "— No inferno quem na terra os padres surra",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "-urra.",
                    "line_length": null
                }
            ],
            "stanza_text": "— No inferno quem na terra os padres surra\n-urra."
        }
    ]
}