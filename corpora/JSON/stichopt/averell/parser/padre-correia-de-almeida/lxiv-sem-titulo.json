{
    "poem_title": "LXIV (sem título)",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Entre as pessoas distintas",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Há esta etiqueta prima",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "— Dar o tratamento nobre",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Quase sempre um furo acima.",
                    "line_length": null
                }
            ],
            "stanza_text": "Entre as pessoas distintas\nHá esta etiqueta prima\n— Dar o tratamento nobre\nQuase sempre um furo acima."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Chama-se pois excelência",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Por urbana cortes ia,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Àquele que tem somente",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "De justiça senhoria.",
                    "line_length": null
                }
            ],
            "stanza_text": "Chama-se pois excelência\nPor urbana cortes ia,\nÀquele que tem somente\nDe justiça senhoria."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "E chamam comendador,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Seja no baile ou na missa,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Aquele que é tão somente",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Oficial de justiça.",
                    "line_length": null
                }
            ],
            "stanza_text": "E chamam comendador,\nSeja no baile ou na missa,\nAquele que é tão somente\nOficial de justiça."
        }
    ]
}