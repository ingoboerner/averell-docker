{
    "poem_title": "PARÁBOLA, O BESOURO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tão reluzente e brilhante",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Parece vestir-se de ouro",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "O profundo sussurrante",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "E monótono besouro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Tão reluzente e brilhante\nParece vestir-se de ouro\nO profundo sussurrante\nE monótono besouro."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Suponho que se enamora",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "De si próprio o tal inseto!",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Porém, coitado! onde mora?",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Qual seu palácio, seu teto?",
                    "line_length": null
                }
            ],
            "stanza_text": "Suponho que se enamora\nDe si próprio o tal inseto!\nPorém, coitado! onde mora?\nQual seu palácio, seu teto?"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "E que excelentes manjares,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Ou adubado petisco",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Fruirá nesses lugares",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Mais imundos do que o cisco?",
                    "line_length": null
                }
            ],
            "stanza_text": "E que excelentes manjares,\nOu adubado petisco\nFruirá nesses lugares\nMais imundos do que o cisco?"
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Um elegante e casquilho,",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Cuja roupa é seu tesouro,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "Imita não só no brilho,",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "Talvez no mais o besouro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Um elegante e casquilho,\nCuja roupa é seu tesouro,\nImita não só no brilho,\nTalvez no mais o besouro."
        }
    ]
}