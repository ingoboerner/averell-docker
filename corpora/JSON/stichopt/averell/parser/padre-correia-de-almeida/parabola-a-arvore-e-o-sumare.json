{
    "poem_title": "PARÁBOLA, A ÁRVORE E O SUMARÉ",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Agreste, viçoso tronco",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Tem as raízes no chão,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Estende frondosos galhos,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Que doces frutos nos dão.",
                    "line_length": null
                }
            ],
            "stanza_text": "Agreste, viçoso tronco\nTem as raízes no chão,\nEstende frondosos galhos,\nQue doces frutos nos dão."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Das eminências desta árvore,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Que tão profícua nos é,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Inquilino se apodera",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Parasita sumaré.",
                    "line_length": null
                }
            ],
            "stanza_text": "Das eminências desta árvore,\nQue tão profícua nos é,\nInquilino se apodera\nParasita sumaré."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "E gaudério, usurpador",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Não dá fruto que se coma,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Não exala seus perfumes,",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Sua flor não tem aroma.",
                    "line_length": null
                }
            ],
            "stanza_text": "E gaudério, usurpador\nNão dá fruto que se coma,\nNão exala seus perfumes,\nSua flor não tem aroma."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Nas duas classes",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Da sociedade",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "Vereis bem clara",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "A paridade.",
                    "line_length": null
                }
            ],
            "stanza_text": "Nas duas classes\nDa sociedade\nVereis bem clara\nA paridade."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "É tronco de árvore",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "O povo pobre,",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "E sumaré",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "A classe nobre.",
                    "line_length": null
                }
            ],
            "stanza_text": "É tronco de árvore\nO povo pobre,\nE sumaré\nA classe nobre."
        }
    ]
}