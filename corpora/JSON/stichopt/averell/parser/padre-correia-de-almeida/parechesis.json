{
    "poem_title": "PARECHESIS",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "O jornalista que escreve,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "e censura o potentado,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "em dano próprio se atreve,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "e não será deputado;",
                    "line_length": null
                },
                {
                    "line_number": 5,
                    "line_text": "pois, se não está demente,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "veementemente mente.",
                    "line_length": null
                }
            ],
            "stanza_text": "O jornalista que escreve,\ne censura o potentado,\nem dano próprio se atreve,\ne não será deputado;\npois, se não está demente,\nveementemente mente."
        }
    ]
}