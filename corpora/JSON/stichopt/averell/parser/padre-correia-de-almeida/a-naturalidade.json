{
    "poem_title": "A NATURALIDADE",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Sei que asseveram",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "que os tais pintores",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "sempre tiveram",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "amplos favores.",
                    "line_length": null
                }
            ],
            "stanza_text": "Sei que asseveram\nque os tais pintores\nsempre tiveram\namplos favores."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Fantasiando",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "cousas e lousas,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "iam pintando",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "estranhas cousas.",
                    "line_length": null
                }
            ],
            "stanza_text": "Fantasiando\ncousas e lousas,\niam pintando\nestranhas cousas."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "É mais exata",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "sem fantasia",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "hoje a barata",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "fotografia.",
                    "line_length": null
                }
            ],
            "stanza_text": "É mais exata\nsem fantasia\nhoje a barata\nfotografia."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "A natureza requer",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "que seja o vate fotógrafo",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "do assunto que ela lhe der,",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "e bem reproduza o autógrafo",
                    "line_length": null
                },
                {
                    "line_number": 17,
                    "line_text": "na cópia que lhe fizer.",
                    "line_length": null
                }
            ],
            "stanza_text": "A natureza requer\nque seja o vate fotógrafo\ndo assunto que ela lhe der,\ne bem reproduza o autógrafo\nna cópia que lhe fizer."
        }
    ]
}