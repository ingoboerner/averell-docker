{
    "poem_title": "QUI POTEST CAPERE CAPIAT",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Um amigo serviçal",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "teve assinantes diversos",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "de meus folhetos de versos,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "destituídos de sal.",
                    "line_length": null
                }
            ],
            "stanza_text": "Um amigo serviçal\nteve assinantes diversos\nde meus folhetos de versos,\ndestituídos de sal."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Parcelas arrecadou,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "mas vai não vai, hesitante,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "até o presente instante",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "ainda está — dou não dou!",
                    "line_length": null
                }
            ],
            "stanza_text": "Parcelas arrecadou,\nmas vai não vai, hesitante,\naté o presente instante\nainda está — dou não dou!"
        }
    ]
}