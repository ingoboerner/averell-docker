{
    "poem_title": "UM DIA DE ANOS",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Sei que o mês de Fevereiro",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "É de todos o mais curto;",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Mas ao número me furto,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Como objeto derradeiro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Sei que o mês de Fevereiro\nÉ de todos o mais curto;\nMas ao número me furto,\nComo objeto derradeiro."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Se acaso algum temerário,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Em estilo pouco ameno,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Chamar este mês pequeno",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Com razões de calendário,",
                    "line_length": null
                }
            ],
            "stanza_text": "Se acaso algum temerário,\nEm estilo pouco ameno,\nChamar este mês pequeno\nCom razões de calendário,"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Uma demanda lhe movo,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Ponho embargos de terceiro,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Pois nasceu em Fevereiro",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "O barão do Rio Novo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Uma demanda lhe movo,\nPonho embargos de terceiro,\nPois nasceu em Fevereiro\nO barão do Rio Novo."
        }
    ]
}