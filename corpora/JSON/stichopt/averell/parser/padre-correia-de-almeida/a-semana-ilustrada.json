{
    "poem_title": "A SEMANA ILUSTRADA",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "A boa caricatura",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Dos vícios é corretivo,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Sob feia catadura",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Os desenha bem ao vivo.",
                    "line_length": null
                }
            ],
            "stanza_text": "A boa caricatura\nDos vícios é corretivo,\nSob feia catadura\nOs desenha bem ao vivo."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "É, pois, útil a Semana,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Além de ser ilustrada;",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Lição proveitosa emana",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "De tal sátira pintada.",
                    "line_length": null
                }
            ],
            "stanza_text": "É, pois, útil a Semana,\nAlém de ser ilustrada;\nLição proveitosa emana\nDe tal sátira pintada."
        }
    ]
}