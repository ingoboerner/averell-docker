{
    "poem_title": "PARÁBOLA, O JAMBO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Brasílio jambo",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "É uma fruta",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Que se reputa",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Mui primorosa.",
                    "line_length": null
                }
            ],
            "stanza_text": "Brasílio jambo\nÉ uma fruta\nQue se reputa\nMui primorosa."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Tem, como a rosa,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Suave aroma,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "E que se coma",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Diz-nos a boca.",
                    "line_length": null
                }
            ],
            "stanza_text": "Tem, como a rosa,\nSuave aroma,\nE que se coma\nDiz-nos a boca."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Porém é oca,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "E só a casca",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Se engole e masca",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Da fruta jambo.",
                    "line_length": null
                }
            ],
            "stanza_text": "Porém é oca,\nE só a casca\nSe engole e masca\nDa fruta jambo."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "E nem eu lambo",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "O seu caroço,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "Porque é insosso,",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "Se não amargo.",
                    "line_length": null
                }
            ],
            "stanza_text": "E nem eu lambo\nO seu caroço,\nPorque é insosso,\nSe não amargo."
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Agora largo",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "As velas soltas,",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "E vou dar voltas",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "Por outros mares.",
                    "line_length": null
                }
            ],
            "stanza_text": "Agora largo\nAs velas soltas,\nE vou dar voltas\nPor outros mares."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Quando topares",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "Um belo aspeito",
                    "line_length": null
                },
                {
                    "line_number": 23,
                    "line_text": "Vê se o sujeito",
                    "line_length": null
                },
                {
                    "line_number": 24,
                    "line_text": "Jambo é por fora:",
                    "line_length": null
                }
            ],
            "stanza_text": "Quando topares\nUm belo aspeito\nVê se o sujeito\nJambo é por fora:"
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Se acaso mora",
                    "line_length": null
                },
                {
                    "line_number": 26,
                    "line_text": "Cascas a dentro,",
                    "line_length": null
                },
                {
                    "line_number": 27,
                    "line_text": "E bem no centro",
                    "line_length": null
                },
                {
                    "line_number": 28,
                    "line_text": "Caroço ruim.",
                    "line_length": null
                }
            ],
            "stanza_text": "Se acaso mora\nCascas a dentro,\nE bem no centro\nCaroço ruim."
        }
    ]
}