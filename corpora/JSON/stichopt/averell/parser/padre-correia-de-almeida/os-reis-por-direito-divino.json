{
    "poem_title": "OS REIS POR DIREITO DIVINO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Esses Padres que sustentam",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "serem da escolha de Deus",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "os tronos em que se assentam",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Reis que pode ser ateus,",
                    "line_length": null
                }
            ],
            "stanza_text": "Esses Padres que sustentam\nserem da escolha de Deus\nos tronos em que se assentam\nReis que pode ser ateus,"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "aplaudam quando se veja",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "o Pontífice fiel",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "perseguido com a igreja",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "por Vítor Emanuel.",
                    "line_length": null
                }
            ],
            "stanza_text": "aplaudam quando se veja\no Pontífice fiel\nperseguido com a igreja\npor Vítor Emanuel."
        }
    ]
}