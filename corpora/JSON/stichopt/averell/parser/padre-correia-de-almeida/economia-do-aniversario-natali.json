{
    "poem_title": "ECONOMIA DO ANIVERSÁRIO NATALÍCIO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Padre Correia de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Não vês ali",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "O bom vizinho",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Antagonista",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Da carne e vinho?",
                    "line_length": null
                }
            ],
            "stanza_text": "Não vês ali\nO bom vizinho\nAntagonista\nDa carne e vinho?"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "E que sustenta",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Que uma boroa",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "É refeição",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Mais do que boa?",
                    "line_length": null
                }
            ],
            "stanza_text": "E que sustenta\nQue uma boroa\nÉ refeição\nMais do que boa?"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "E que se veste",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "De belbutina,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "E com saliva",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Lustra a botina?",
                    "line_length": null
                }
            ],
            "stanza_text": "E que se veste\nDe belbutina,\nE com saliva\nLustra a botina?"
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Tão econômico,",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Como ali vês,",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "A natureza",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "Assim o fez",
                    "line_length": null
                }
            ],
            "stanza_text": "Tão econômico,\nComo ali vês,\nA natureza\nAssim o fez"
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "E como prova",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "Disto que digo,",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "Um bom exemplo",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "Trago comigo.",
                    "line_length": null
                }
            ],
            "stanza_text": "E como prova\nDisto que digo,\nUm bom exemplo\nTrago comigo."
        },
        {
            "stanza_type": null,
            "stanza_number": 6,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Não é costume",
                    "line_length": null
                },
                {
                    "line_number": 22,
                    "line_text": "Fazer despesa",
                    "line_length": null
                },
                {
                    "line_number": 23,
                    "line_text": "No dia de anos",
                    "line_length": null
                },
                {
                    "line_number": 24,
                    "line_text": "Em lauta mesa?",
                    "line_length": null
                }
            ],
            "stanza_text": "Não é costume\nFazer despesa\nNo dia de anos\nEm lauta mesa?"
        },
        {
            "stanza_type": null,
            "stanza_number": 7,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Pois vê agora",
                    "line_length": null
                },
                {
                    "line_number": 26,
                    "line_text": "A economia",
                    "line_length": null
                },
                {
                    "line_number": 27,
                    "line_text": "Da natureza",
                    "line_length": null
                },
                {
                    "line_number": 28,
                    "line_text": "No natal dia.",
                    "line_length": null
                }
            ],
            "stanza_text": "Pois vê agora\nA economia\nDa natureza\nNo natal dia."
        },
        {
            "stanza_type": null,
            "stanza_number": 8,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "O parco e sóbrio",
                    "line_length": null
                },
                {
                    "line_number": 30,
                    "line_text": "E bom vizinho,",
                    "line_length": null
                },
                {
                    "line_number": 31,
                    "line_text": "Antagonista",
                    "line_length": null
                },
                {
                    "line_number": 32,
                    "line_text": "Da carne e vinho,",
                    "line_length": null
                }
            ],
            "stanza_text": "O parco e sóbrio\nE bom vizinho,\nAntagonista\nDa carne e vinho,"
        },
        {
            "stanza_type": null,
            "stanza_number": 9,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Veio em bissexto",
                    "line_length": null
                },
                {
                    "line_number": 34,
                    "line_text": "À luz primeiro",
                    "line_length": null
                },
                {
                    "line_number": 35,
                    "line_text": "A vinte nove",
                    "line_length": null
                },
                {
                    "line_number": 36,
                    "line_text": "De Fevereiro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Veio em bissexto\nÀ luz primeiro\nA vinte nove\nDe Fevereiro."
        },
        {
            "stanza_type": null,
            "stanza_number": 10,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "E assim não faz",
                    "line_length": null
                },
                {
                    "line_number": 38,
                    "line_text": "Anualmente",
                    "line_length": null
                },
                {
                    "line_number": 39,
                    "line_text": "Os anos que",
                    "line_length": null
                },
                {
                    "line_number": 40,
                    "line_text": "Dão gasto à gente.",
                    "line_length": null
                }
            ],
            "stanza_text": "E assim não faz\nAnualmente\nOs anos que\nDão gasto à gente."
        }
    ]
}