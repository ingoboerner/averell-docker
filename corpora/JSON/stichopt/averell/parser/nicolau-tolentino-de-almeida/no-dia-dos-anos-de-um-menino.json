{
    "poem_title": "NO DIA DOS ANOS DE UM MENINO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Nicolau Tolentino de Almeida",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "De plumachos emplumado",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Manso, alegre cavalinho,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Ou torneado carrinho",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "D’alvos carneiros puxado,",
                    "line_length": null
                },
                {
                    "line_number": 5,
                    "line_text": "Deviam marchar ao lado",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Deste papel que remeto;",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Mas mostrando o meu afeto",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Como pôde o meu destino,",
                    "line_length": null
                },
                {
                    "line_number": 9,
                    "line_text": "Em obsequio de uni menino,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Vou dar aos outros sueto.",
                    "line_length": null
                }
            ],
            "stanza_text": "De plumachos emplumado\nManso, alegre cavalinho,\nOu torneado carrinho\nD’alvos carneiros puxado,\nDeviam marchar ao lado\nDeste papel que remeto;\nMas mostrando o meu afeto\nComo pôde o meu destino,\nEm obsequio de uni menino,\nVou dar aos outros sueto."
        }
    ]
}