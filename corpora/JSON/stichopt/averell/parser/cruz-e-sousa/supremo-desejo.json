{
    "poem_title": "Supremo desejo",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Cruz e Sousa",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Eternas, imortais origens vivas",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Da Luz, do Aroma, segredantes vozes",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Do mar e luares de contemplativas,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Vagas visões volúpicas, velozes...",
                    "line_length": null
                }
            ],
            "stanza_text": "Eternas, imortais origens vivas\nDa Luz, do Aroma, segredantes vozes\nDo mar e luares de contemplativas,\nVagas visões volúpicas, velozes..."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Aladas alegrias sugestivas",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "De asa radiante e branca de albornozes,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Tribos gloriosas, fulgidas, altivas,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "De condores e de águias e albatrozes...",
                    "line_length": null
                }
            ],
            "stanza_text": "Aladas alegrias sugestivas\nDe asa radiante e branca de albornozes,\nTribos gloriosas, fulgidas, altivas,\nDe condores e de águias e albatrozes..."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Espiritualizai nos Astros louros,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Do sol entre os clarões imorredouros",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Toda esta dor que na minh’alma clama...",
                    "line_length": null
                }
            ],
            "stanza_text": "Espiritualizai nos Astros louros,\nDo sol entre os clarões imorredouros\nToda esta dor que na minh’alma clama..."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Quero vê-la subir, ficar cantando",
                    "line_length": null
                },
                {
                    "line_number": 13,
                    "line_text": "Na chama das Estrelas, dardejando",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Nas luminosas sensações da chama.",
                    "line_length": null
                }
            ],
            "stanza_text": "Quero vê-la subir, ficar cantando\nNa chama das Estrelas, dardejando\nNas luminosas sensações da chama."
        }
    ]
}