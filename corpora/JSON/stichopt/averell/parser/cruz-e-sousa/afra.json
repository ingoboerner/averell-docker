{
    "poem_title": "Afra",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Cruz e Sousa",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Ressurges dos mistérios da luxúria,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Afra, tentada pelos verdes pomos,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Entre os silfos magnéticos e os gnomos",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Maravilhosos da paixão purpúrea.",
                    "line_length": null
                }
            ],
            "stanza_text": "Ressurges dos mistérios da luxúria,\nAfra, tentada pelos verdes pomos,\nEntre os silfos magnéticos e os gnomos\nMaravilhosos da paixão purpúrea."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Carne explosiva em pólvoras e fúria",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "De desejos pagãos, por entre assomos",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Da virgindade   casquinantes momos",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Rindo da carne já votada a incúria.",
                    "line_length": null
                }
            ],
            "stanza_text": "Carne explosiva em pólvoras e fúria\nDe desejos pagãos, por entre assomos\nDa virgindade   casquinantes momos\nRindo da carne já votada a incúria."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Votada cedo ao lânguido abandono,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Aos mórbidos delíquios como ao sono,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Do gozo haurindo os venenosos sucos.",
                    "line_length": null
                }
            ],
            "stanza_text": "Votada cedo ao lânguido abandono,\nAos mórbidos delíquios como ao sono,\nDo gozo haurindo os venenosos sucos."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Sonho-te a deusa das lascivas pompas,",
                    "line_length": null
                },
                {
                    "line_number": 13,
                    "line_text": "A proclamar, impávida, por trompas,",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Amores mais estéreis que os eunucos!",
                    "line_length": null
                }
            ],
            "stanza_text": "Sonho-te a deusa das lascivas pompas,\nA proclamar, impávida, por trompas,\nAmores mais estéreis que os eunucos!"
        }
    ]
}