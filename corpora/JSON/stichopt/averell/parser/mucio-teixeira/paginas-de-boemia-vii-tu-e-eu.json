            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Tu és o Anjo da Crença,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "Na Catedral imensa",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Da Natureza; e eu...",
                    "line_length": null
                }
            ],
            "stanza_text": "Tu és o Anjo da Crença,\nNa Catedral imensa\nDa Natureza; e eu..."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Eu sou o Ceticismo,",
                    "line_length": null
                },
                {
                    "line_number": 13,
                    "line_text": "Sombrio como o abismo...",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "Profundo como o Céu!...",
                    "line_length": null
                }
            ],
            "stanza_text": "Eu sou o Ceticismo,\nSombrio como o abismo...\nProfundo como o Céu!..."
