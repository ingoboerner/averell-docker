{
    "poem_title": "Ao luar",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Delminda Silveira",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Era já noite; plácida surgia,",
                    "line_length": "10"
                },
                {
                    "line_number": 2,
                    "line_text": "detrás do serro, a lua majestosa;",
                    "line_length": "10"
                },
                {
                    "line_number": 3,
                    "line_text": "uma voz meiga e triste, carinhosa,",
                    "line_length": "10"
                },
                {
                    "line_number": 4,
                    "line_text": "das flores nos perfumes se expandia.",
                    "line_length": "10"
                }
            ],
            "stanza_text": "Era já noite; plácida surgia,\ndetrás do serro, a lua majestosa;\numa voz meiga e triste, carinhosa,\ndas flores nos perfumes se expandia."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Sons de viola a viração trazia",
                    "line_length": "10"
                },
                {
                    "line_number": 6,
                    "line_text": "Juntos às notas da canção maviosa;",
                    "line_length": "10"
                },
                {
                    "line_number": 7,
                    "line_text": "e a Natureza quieta, preguiçosa,",
                    "line_length": "10"
                },
                {
                    "line_number": 8,
                    "line_text": "da paz nos braços, grata adormecia.",
                    "line_length": "10"
                }
            ],
            "stanza_text": "Sons de viola a viração trazia\nJuntos às notas da canção maviosa;\ne a Natureza quieta, preguiçosa,\nda paz nos braços, grata adormecia."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "E como bênção divinal, serena,",
                    "line_length": "10"
                },
                {
                    "line_number": 10,
                    "line_text": "plena de graças, de doçuras plena",
                    "line_length": "10"
                },
                {
                    "line_number": 11,
                    "line_text": "ia o luar suavíssimo descendo,",
                    "line_length": "10"
                }
            ],
            "stanza_text": "E como bênção divinal, serena,\nplena de graças, de doçuras plena\nia o luar suavíssimo descendo,"
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "no seu manto de luz maravilhosa",
                    "line_length": "10"
                },
                {
                    "line_number": 13,
                    "line_text": "uma casinha rústica, ditosa,",
                    "line_length": "10"
                },
                {
                    "line_number": 14,
                    "line_text": "do sonho nos mistérios envolvendo.",
                    "line_length": "10"
                }
            ],
            "stanza_text": "no seu manto de luz maravilhosa\numa casinha rústica, ditosa,\ndo sonho nos mistérios envolvendo."
        }
    ]
}