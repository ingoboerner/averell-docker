{
    "poem_title": "ELA",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Olavo Bilac e Guimarães Passos",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Maria tem vinte amantes!",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Uns tortos, outros direitos;",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Todos eles são galantes,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Todos vivem satisfeitos...",
                    "line_length": null
                }
            ],
            "stanza_text": "Maria tem vinte amantes!\nUns tortos, outros direitos;\nTodos eles são galantes,\nTodos vivem satisfeitos..."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Mulher de recursos fartos!",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Como é que esta impenitente,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Tendo no corpo dous quartos,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Dá pousada a tanta gente?",
                    "line_length": null
                }
            ],
            "stanza_text": "Mulher de recursos fartos!\nComo é que esta impenitente,\nTendo no corpo dous quartos,\nDá pousada a tanta gente?"
        }
    ]
}