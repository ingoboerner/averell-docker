{
    "poem_title": "CONSOLO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Olavo Bilac e Guimarães Passos",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Dizia o Manoel Mindelo",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Homem de haveres e sério,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Fulo e arrancando o cabelo,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Com cara de cemitério:",
                    "line_length": null
                }
            ],
            "stanza_text": "Dizia o Manoel Mindelo\nHomem de haveres e sério,\nFulo e arrancando o cabelo,\nCom cara de cemitério:"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "“Vejam isto! vejam só",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Que destino desumano!",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "Sendo pobre como Jó,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "Nasce-me um filho por ano!”",
                    "line_length": null
                }
            ],
            "stanza_text": "“Vejam isto! vejam só\nQue destino desumano!\nSendo pobre como Jó,\nNasce-me um filho por ano!”"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Responde-lhe o Zé das Quinas",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "“O’ Manoel! mágoas à rua!",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Porque é que tu te amofinas,",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Quando a culpa não é tua?”",
                    "line_length": null
                }
            ],
            "stanza_text": "Responde-lhe o Zé das Quinas\n“O’ Manoel! mágoas à rua!\nPorque é que tu te amofinas,\nQuando a culpa não é tua?”"
        }
    ]
}