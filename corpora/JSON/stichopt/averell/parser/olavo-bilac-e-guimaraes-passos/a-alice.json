{
    "poem_title": "A ALICE",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Olavo Bilac e Guimarães Passos",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Silva Lisboa, marujo,",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "Preto e desertor da Armada,",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "Entrou em casa de um cujo,",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "Como se fosse criada.",
                    "line_length": null
                }
            ],
            "stanza_text": "Silva Lisboa, marujo,\nPreto e desertor da Armada,\nEntrou em casa de um cujo,\nComo se fosse criada."
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Serviu banho às sinhazinhas,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "Como uma serva qualquer;",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "E tinha seios, e anquinhas,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "E ademanes de mulher.",
                    "line_length": null
                }
            ],
            "stanza_text": "Serviu banho às sinhazinhas,\nComo uma serva qualquer;\nE tinha seios, e anquinhas,\nE ademanes de mulher."
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Dengoso, imberbe e faceiro,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "E belo, apesar de escuro,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "Namorou muito copeiro,",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "Fez escalar muito muro.",
                    "line_length": null
                }
            ],
            "stanza_text": "Dengoso, imberbe e faceiro,\nE belo, apesar de escuro,\nNamorou muito copeiro,\nFez escalar muito muro."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Pintou a manta esta Alice",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "E, enfim, é lastima só",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "Que de serva não servisse",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "Ao nosso doutor Deiró...",
                    "line_length": null
                }
            ],
            "stanza_text": "Pintou a manta esta Alice\nE, enfim, é lastima só\nQue de serva não servisse\nAo nosso doutor Deiró..."
        }
    ]
}