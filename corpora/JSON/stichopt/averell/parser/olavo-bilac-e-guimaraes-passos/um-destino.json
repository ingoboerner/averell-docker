{
    "poem_title": "UM DESTINO",
    "corpus": "stichopt",
    "manually_checked": false,
    "author": "Olavo Bilac e Guimarães Passos",
    "stanzas": [
        {
            "stanza_type": null,
            "stanza_number": 1,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "José dizia a João:",
                    "line_length": null
                },
                {
                    "line_number": 2,
                    "line_text": "“Tu não paras um instante",
                    "line_length": null
                },
                {
                    "line_number": 3,
                    "line_text": "“Numa deliberação!",
                    "line_length": null
                },
                {
                    "line_number": 4,
                    "line_text": "“Queres ser negociante,",
                    "line_length": null
                }
            ],
            "stanza_text": "José dizia a João:\n“Tu não paras um instante\n“Numa deliberação!\n“Queres ser negociante,"
        },
        {
            "stanza_type": null,
            "stanza_number": 2,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "“Industrial, jornalista,",
                    "line_length": null
                },
                {
                    "line_number": 6,
                    "line_text": "“Agricultor, advogado,",
                    "line_length": null
                },
                {
                    "line_number": 7,
                    "line_text": "“Médico, pintor, artista,",
                    "line_length": null
                },
                {
                    "line_number": 8,
                    "line_text": "“Sacerdote, magistrado!",
                    "line_length": null
                }
            ],
            "stanza_text": "“Industrial, jornalista,\n“Agricultor, advogado,\n“Médico, pintor, artista,\n“Sacerdote, magistrado!"
        },
        {
            "stanza_type": null,
            "stanza_number": 3,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "“E assim, cansado, a suar,",
                    "line_length": null
                },
                {
                    "line_number": 10,
                    "line_text": "“E tudo fazendo às pressas,",
                    "line_length": null
                },
                {
                    "line_number": 11,
                    "line_text": "“Nunca podes acabar",
                    "line_length": null
                },
                {
                    "line_number": 12,
                    "line_text": "“Tantas cousas que começas...",
                    "line_length": null
                }
            ],
            "stanza_text": "“E assim, cansado, a suar,\n“E tudo fazendo às pressas,\n“Nunca podes acabar\n“Tantas cousas que começas..."
        },
        {
            "stanza_type": null,
            "stanza_number": 4,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Diz o João: “Já sei disto!",
                    "line_length": null
                },
                {
                    "line_number": 14,
                    "line_text": "“Já sei disto! é o diabo!",
                    "line_length": null
                },
                {
                    "line_number": 15,
                    "line_text": "“Eu insisto, insisto, insisto,",
                    "line_length": null
                },
                {
                    "line_number": 16,
                    "line_text": "“Insisto em vão, não acabo!",
                    "line_length": null
                }
            ],
            "stanza_text": "Diz o João: “Já sei disto!\n“Já sei disto! é o diabo!\n“Eu insisto, insisto, insisto,\n“Insisto em vão, não acabo!"
        },
        {
            "stanza_type": null,
            "stanza_number": 5,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "“Já é destino! que quer?",
                    "line_length": null
                },
                {
                    "line_number": 18,
                    "line_text": "“Valha-me S. Zacarias!",
                    "line_length": null
                },
                {
                    "line_number": 19,
                    "line_text": "“A minha própria mulher",
                    "line_length": null
                },
                {
                    "line_number": 20,
                    "line_text": "“Diz-me isto todos os dias.",
                    "line_length": null
                }
            ],
            "stanza_text": "“Já é destino! que quer?\n“Valha-me S. Zacarias!\n“A minha própria mulher\n“Diz-me isto todos os dias."
        }
    ]
}