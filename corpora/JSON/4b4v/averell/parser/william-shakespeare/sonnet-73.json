            ],
            "stanza_text": "In me thou seest the twilight of such day\nAs after sunset fadeth in the west;\nWhich by and by black night doth take away,\nDeath's second self, that seals up all in rest."
        },
        {
            "stanza_number": 3,
            "stanza_type": "quatrain",
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "In me thou seest the glowing of such fire,",
                    "metrical_pattern": "-+-+-+--++",
                    "line_length": null,
                    "foot": null,
                    "metre": null
                },
                {
                    "line_number": 10,
                    "line_text": "That on the ashes of his youth doth lie,",
                    "metrical_pattern": "-+-+-+-+-+",
                    "line_length": null,
                    "foot": null,
                    "metre": null
                },
                {
                    "line_number": 11,
                    "line_text": "As the deathbed whereon it must expire,",
                    "metrical_pattern": "--++-+-+-+",
                    "line_length": null,
                    "foot": null,
                    "metre": null
                },
                {
                    "line_number": 12,
                    "line_text": "Consumed with that which it was nourished by.",
                    "metrical_pattern": "-+-+-+-+-+",
                    "line_length": null,
                    "foot": null,
                    "metre": null
