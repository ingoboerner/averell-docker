{
    "manually_checked": false,
    "poem_title": "Soneto turco",
    "author": "Augusto E. Madan",
    "poem_alt_title": "Para bárbaros blancos, Ghemorúk.",
    "corpus": "disco2_1",
    "stanzas": [
        {
            "stanza_number": "1",
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": "1",
                    "line_text": "Para bárbaros blancos, Ghemorúk.",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "2",
                    "line_text": "Para zancos ridículos, Albék.",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "3",
                    "line_text": "Para maderas vírgenes Ilék.",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": "4",
                    "line_text": "Para ágiles refígeros, Ipúk.",
                    "metrical_pattern": "-+---+---+-"
                }
            ],
            "stanza_text": "Para bárbaros blancos, Ghemorúk.\nPara zancos ridículos, Albék.\nPara maderas vírgenes Ilék.\nPara ágiles refígeros, Ipúk."
        },
        {
            "stanza_number": "2",
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": "5",
                    "line_text": "Para marfiles diáfanos, Murzúk.",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": "6",
                    "line_text": "Par higiénicos lúpidos, Pysék",
                    "metrical_pattern": "+-+--+---+-"
                },
                {
                    "line_number": "7",
                    "line_text": "Para tebáicos báquicos, Hupék.",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": "8",
                    "line_text": "Para dátiles cándidos, Mabrúk.",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "Para marfiles diáfanos, Murzúk.\nPar higiénicos lúpidos, Pysék\nPara tebáicos báquicos, Hupék.\nPara dátiles cándidos, Mabrúk."
        },
        {
            "stanza_number": "3",
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": "9",
                    "line_text": "Para ostrícolas regias, Chesapík.",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "10",
                    "line_text": "Para búfalos gordos, Omenák.",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "11",
                    "line_text": "Para bálago helado, Upernavík.",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "Para ostrícolas regias, Chesapík.\nPara búfalos gordos, Omenák.\nPara bálago helado, Upernavík."
        },
        {
            "stanza_number": "4",
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": "12",
                    "line_text": "Para bichos mortíferos, Sihák.",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "13",
                    "line_text": "Par tétricas focas, Reykjhavík.",
                    "metrical_pattern": "++--+---+-"
                },
                {
                    "line_number": "14",
                    "line_text": "Para kábila cruel, Kjalikasák.",
                    "metrical_pattern": "--+--+----+-"
                }
            ],
            "stanza_text": "Para bichos mortíferos, Sihák.\nPar tétricas focas, Reykjhavík.\nPara kábila cruel, Kjalikasák."
        }
    ]
}