{
    "manually_checked": false,
    "poem_title": "Sales sin sal",
    "author": "Augusto E. Madan",
    "poem_alt_title": "¡Sal soneto sin sal! ¡Saca sensato",
    "corpus": "disco2_1",
    "stanzas": [
        {
            "stanza_number": "1",
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": "1",
                    "line_text": "¡Sal soneto sin sal! ¡Saca sensato",
                    "metrical_pattern": "+-+--+-+-"
                },
                {
                    "line_number": "2",
                    "line_text": "salobridad siquiera! ¡Sé salino!",
                    "metrical_pattern": "---+--+--+-"
                },
                {
                    "line_number": "3",
                    "line_text": "Sorbiendo sal seguido, Saladino",
                    "metrical_pattern": "-+-+-+---+-"
                },
                {
                    "line_number": "4",
                    "line_text": "salvó su situación sin ser silbato.",
                    "metrical_pattern": "-+---+-+-+-"
                }
            ],
            "stanza_text": "¡Sal soneto sin sal! ¡Saca sensato\nsalobridad siquiera! ¡Sé salino!\nSorbiendo sal seguido, Saladino\nsalvó su situación sin ser silbato."
        },
        {
            "stanza_number": "2",
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": "5",
                    "line_text": "Salicinina, salol, salicilato,",
                    "metrical_pattern": "---+--+---+-"
                },
                {
                    "line_number": "6",
                    "line_text": "sub-sales salicílicas, succino,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": "7",
                    "line_text": "solimán, sulfureto, santonino,",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "8",
                    "line_text": "sulfaminol, sílicio, silicato;",
                    "metrical_pattern": "---++----+-"
                }
            ],
            "stanza_text": "Salicinina, salol, salicilato,\nsub-sales salicílicas, succino,\nsolimán, sulfureto, santonino,\nsulfaminol, sílicio, silicato;"
        },
        {
            "stanza_number": "3",
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": "9",
                    "line_text": "si sabias sales sois, salad sextetos.",
                    "metrical_pattern": "-+-+-+-+-+-"
                },
                {
                    "line_number": "10",
                    "line_text": "Salvando salaciones superables,",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": "11",
                    "line_text": "sugiriéndome símiles secretos,",
                    "metrical_pattern": "--+-++---+-"
                }
            ],
            "stanza_text": "si sabias sales sois, salad sextetos.\nSalvando salaciones superables,\nsugiriéndome símiles secretos,"
        },
        {
            "stanza_number": "4",
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": "12",
                    "line_text": "sacaré, -sí señor- siendo sacables,",
                    "metrical_pattern": "--++-++--+-"
                },
                {
                    "line_number": "13",
                    "line_text": "salobres, saladísimos sonetos...",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": "14",
                    "line_text": "si salidos sin sal, salificables.",
                    "metrical_pattern": "--+--+---+-"
                }
            ],
            "stanza_text": "sacaré, -sí señor- siendo sacables,\nsalobres, saladísimos sonetos...\nsi salidos sin sal, salificables."
        }
    ]
}