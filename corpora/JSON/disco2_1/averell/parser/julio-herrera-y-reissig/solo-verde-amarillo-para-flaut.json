    "poem_title": "Solo verde-amarillo para flauta",
    "author": "Julio Herrera y Reissig",
    "poem_alt_title": "Úrsula punta la boyuna junta;",
    "corpus": "disco2_1",
    "stanzas": [
        {
            "stanza_number": "1",
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": "1",
                    "line_text": "Úrsula punta la boyuna junta;",
                    "metrical_pattern": "+--+---+-+-"
                },
                {
                    "line_number": "2",
                    "line_text": "la lujuria perfuma con su fruta,",
                    "metrical_pattern": "--+--+---+-"
                },
                {
                    "line_number": "3",
                    "line_text": "la púbera frescura de la ruta",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": "4",
                    "line_text": "por donde ondula la venusa junta.",
                    "metrical_pattern": "---+---+-+-"
                }
            ],
            "stanza_text": "Úrsula punta la boyuna junta;\nla lujuria perfuma con su fruta,\nla púbera frescura de la ruta\npor donde ondula la venusa junta."
        },
        {
            "stanza_number": "2",
            "stanza_type": "cuarteto",
            "lines": [
                {
                    "line_number": "5",
                    "line_text": "Recién la hirsuta barba rubia apunta",
                    "metrical_pattern": "-+-+-+-+-+-"
                },
                {
                    "line_number": "6",
                    "line_text": "al dios Agricultura. La impoluta",
                    "metrical_pattern": "-+---+---+-"
                },
                {
                    "line_number": "7",
                    "line_text": "uña fecunda del amor, debuta",
                    "metrical_pattern": "+--+---+-+-"
                },
                {
                    "line_number": "8",
                    "line_text": "cual una duda de nupcial pregunta.",
                    "metrical_pattern": "++-+---+-+-"
                }
            ],
            "stanza_text": "Recién la hirsuta barba rubia apunta\nal dios Agricultura. La impoluta\nuña fecunda del amor, debuta\ncual una duda de nupcial pregunta."
        },
        {
            "stanza_number": "3",
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": "9",
                    "line_text": "Anuncian lluvias, las adustas lunas.",
                    "metrical_pattern": "-+-+---+-+-"
                },
                {
                    "line_number": "10",
                    "line_text": "Almizcladuras, uvas, aceitunas,",
                    "metrical_pattern": "---+-+---+-"
                },
                {
                    "line_number": "11",
                    "line_text": "gulas de mar, fortunas de las musas;",
                    "metrical_pattern": "+--+-+---+-"
                }
            ],
            "stanza_text": "Anuncian lluvias, las adustas lunas.\nAlmizcladuras, uvas, aceitunas,\ngulas de mar, fortunas de las musas;"
        },
        {
            "stanza_number": "4",
            "stanza_type": "terceto",
            "lines": [
                {
                    "line_number": "12",
                    "line_text": "hay bilis en las rudas armaduras;",
                    "metrical_pattern": "++---+---+-"
                },
                {
                    "line_number": "13",
                    "line_text": "han madurado todas las verduras,",
                    "metrical_pattern": "+--+-+---+-"
                },
                {
                    "line_number": "14",
                    "line_text": "y una burra hace hablar las cornamusas.",
                    "metrical_pattern": "+-++-+---+-"
                }
            ],
            "stanza_text": "hay bilis en las rudas armaduras;\nhan madurado todas las verduras,\ny una burra hace hablar las cornamusas."
        }
    ]
}