    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Qual potrò mai formar parola o pianto",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
