                            "word_text": "Qual"
                        },
                        {
                            "word_text": "potrò"
                        },
                        {
                            "word_text": "mai"
                        },
                        {
                            "word_text": "formar"
                        },
                        {
                            "word_text": "parola"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "pianto"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Ch'appaghi parte d'ogni colpa ria",
                    "metrical_pattern": null,
