                        },
                        {
                            "word_text": "formasti"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "belle"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "dive"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Immagin nostre dal tuo stesso esempio,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
