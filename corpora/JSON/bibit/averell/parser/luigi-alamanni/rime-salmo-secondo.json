                        },
                        {
                            "word_text": "nostre"
                        },
                        {
                            "word_text": "dal"
                        },
                        {
                            "word_text": "tuo"
                        },
                        {
                            "word_text": "stesso"
                        },
                        {
                            "word_text": "esempio,"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Perché ne godan le tartaree rive.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Perché"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "godan"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "tartaree"
                        },
                        {
                            "word_text": "rive."
                        }
                    ]
                },
                {
                    "line_number": 37,
