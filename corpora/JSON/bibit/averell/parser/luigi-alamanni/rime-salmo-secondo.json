                        },
                        {
                            "word_text": "ria"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "In cui cieco al mio ben fui lieto tanto?",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "In"
                        },
                        {
                            "word_text": "cui"
                        },
                        {
                            "word_text": "cieco"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "mio"
                        },
                        {
                            "word_text": "ben"
                        },
                        {
                            "word_text": "fui"
                        },
                        {
                            "word_text": "lieto"
                        },
                        {
                            "word_text": "tanto?"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Piango, lasso! Signor, ché pur non sia",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Piango,"
                        },
                        {
                            "word_text": "lasso!"
                        },
                        {
                            "word_text": "Signor,"
                        },
                        {
                            "word_text": "ché"
                        },
                        {
                            "word_text": "pur"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "sia"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Segnata un'orma de' miei passi erranti",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Segnata"
                        },
                        {
                            "word_text": "un'orma"
                        },
                        {
                            "word_text": "de'"
                        },
                        {
                            "word_text": "miei"
                        },
                        {
                            "word_text": "passi"
                        },
                        {
                            "word_text": "erranti"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Per la tua santa vera e dritta via.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Per"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "tua"
                        },
                        {
                            "word_text": "santa"
                        },
                        {
                            "word_text": "vera"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "dritta"
                        },
                        {
                            "word_text": "via."
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Contra i precetti tuoi quai sempre e quanti",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Contra"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "precetti"
                        },
                        {
                            "word_text": "tuoi"
                        },
                        {
                            "word_text": "quai"
                        },
                        {
                            "word_text": "sempre"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "quanti"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ebb'io, folle, desir con l'opre appresso",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ebb'io,"
                        },
                        {
                            "word_text": "folle,"
                        },
                        {
                            "word_text": "desir"
                        },
                        {
                            "word_text": "con"
                        },
                        {
                            "word_text": "l'opre"
                        },
                        {
                            "word_text": "appresso"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Ch'or tra doglia e timor mi stanno avanti?",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ch'or"
                        },
                        {
                            "word_text": "tra"
                        },
                        {
                            "word_text": "doglia"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "timor"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "stanno"
                        },
                        {
                            "word_text": "avanti?"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Non ebb'io notte e dì nell'alma impresso",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "ebb'io"
                        },
                        {
                            "word_text": "notte"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "dì"
                        },
                        {
                            "word_text": "nell'alma"
                        },
                        {
                            "word_text": "impresso"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Altro giammai che mio dannaggio e morte,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Altro"
                        },
                        {
                            "word_text": "giammai"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "mio"
                        },
                        {
                            "word_text": "dannaggio"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "morte,"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Di cui sol rimembrar m'ancide spesso.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Di"
                        },
                        {
