                        },
                        {
                            "word_text": "sol"
                        },
                        {
                            "word_text": "rimembrar"
                        },
                        {
                            "word_text": "m'ancide"
                        },
                        {
                            "word_text": "spesso."
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Già dell'inferno aprir veggio le porte,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Già"
                        },
                        {
                            "word_text": "dell'inferno"
                        },
                        {
                            "word_text": "aprir"
                        },
                        {
                            "word_text": "veggio"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "porte,"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Se non s'inchina la pietosa mano",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "s'inchina"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "pietosa"
                        },
                        {
                            "word_text": "mano"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Che al perduto sentier lassù mi porte.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "perduto"
                        },
                        {
                            "word_text": "sentier"
                        },
                        {
                            "word_text": "lassù"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "porte."
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Ho cercato ancor io, quantunque invano,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ho"
                        },
                        {
                            "word_text": "cercato"
                        },
                        {
                            "word_text": "ancor"
                        },
                        {
                            "word_text": "io,"
                        },
                        {
                            "word_text": "quantunque"
                        },
                        {
                            "word_text": "invano,"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "D'acquistar qui tra noi stato e tesoro,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "D'acquistar"
                        },
                        {
                            "word_text": "qui"
                        },
                        {
                            "word_text": "tra"
                        },
                        {
                            "word_text": "noi"
                        },
                        {
                            "word_text": "stato"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "tesoro,"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Più che non chiede il viver corto umano.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Più"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "chiede"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "viver"
                        },
                        {
                            "word_text": "corto"
                        },
                        {
                            "word_text": "umano."
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Anch'io già più pregiai l'argento e l'oro,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Anch'io"
                        },
                        {
                            "word_text": "già"
                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "pregiai"
                        },
                        {
                            "word_text": "l'argento"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "l'oro,"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Piango, e 'l confesso, che tua santa immago,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Piango,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "confesso,"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "tua"
                        },
                        {
                            "word_text": "santa"
                        },
                        {
                            "word_text": "immago,"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Seguendo i molti che miei duci fôro.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Seguendo"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "molti"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "miei"
                        },
                        {
                            "word_text": "duci"
                        },
                        {
                            "word_text": "fôro."
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Anch'io di posseder fui troppo vago,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Anch'io"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "posseder"
                        },
                        {
                            "word_text": "fui"
                        },
                        {
                            "word_text": "troppo"
                        },
                        {
                            "word_text": "vago,"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Fin che tu, o Padre, mi levasti 'l tutto,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fin"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "tu,"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "Padre,"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "levasti"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "tutto,"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Ond'or più queto in povertà m'appago.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ond'or"
                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "queto"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "povertà"
                        },
                        {
                            "word_text": "m'appago."
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Spesso (nol posso io dir con volto asciutto),",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Spesso"
                        },
                        {
                            "word_text": "(nol"
                        },
                        {
                            "word_text": "posso"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "dir"
                        },
                        {
                            "word_text": "con"
                        },
                        {
                            "word_text": "volto"
                        },
                        {
                            "word_text": "asciutto),"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Avaro e sordo nei bisogni altrui,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Avaro"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "sordo"
                        },
                        {
                            "word_text": "nei"
                        },
                        {
                            "word_text": "bisogni"
                        },
                        {
                            "word_text": "altrui,"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Negato ho già di caritade il frutto.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Negato"
                        },
                        {
                            "word_text": "ho"
                        },
                        {
                            "word_text": "già"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "caritade"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "frutto."
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Non pensi or quel ch'io son né quel ch'io fui,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "pensi"
                        },
                        {
                            "word_text": "or"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "ch'io"
                        },
                        {
                            "word_text": "son"
                        },
                        {
                            "word_text": "né"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "ch'io"
                        },
                        {
                            "word_text": "fui,"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Ma quel ch'esser vorrei, l'alta pietate",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ma"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "ch'esser"
                        },
                        {
                            "word_text": "vorrei,"
                        },
                        {
                            "word_text": "l'alta"
                        },
                        {
                            "word_text": "pietate"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Che ogni uom soccorre, e non riguarda a cui.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "ogni"
                        },
                        {
                            "word_text": "uom"
                        },
                        {
                            "word_text": "soccorre,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "riguarda"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "cui."
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Deh! non sia sorda a chi l'andata etate",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Deh!"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "sia"
                        },
                        {
                            "word_text": "sorda"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "chi"
                        },
                        {
                            "word_text": "l'andata"
                        },
                        {
                            "word_text": "etate"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Biasma, e si pente, a chi piangendo scrive",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Biasma,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "pente,"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "chi"
                        },
                        {
                            "word_text": "piangendo"
                        },
                        {
                            "word_text": "scrive"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Le presenti sue colpe e le passate.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
