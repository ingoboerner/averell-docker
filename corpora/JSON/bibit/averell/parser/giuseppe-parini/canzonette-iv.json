                            "word_text": "Da"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "fiamma"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "lei"
                        },
                        {
                            "word_text": "vicina;"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Né salavar le fresche rose",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Né"
                        },
                        {
                            "word_text": "salavar"
                        },
                        {
                            "word_text": "le"
                        },
                        {
