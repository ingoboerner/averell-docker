                        },
                        {
                            "word_text": "figlio"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Abbassossi, e il baciò in viso;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Abbassossi,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "baciò"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "viso;"
