                    "line_number": 11,
                    "line_text": "Ché feriva e volto e mano",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "feriva"
                        },
                        {
