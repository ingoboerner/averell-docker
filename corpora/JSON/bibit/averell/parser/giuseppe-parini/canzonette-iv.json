                        },
                        {
                            "word_text": "dell'ali"
                        },
                        {
                            "word_text": "rinascenti"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Una subito strapposse;",
                    "metrical_pattern": null,
