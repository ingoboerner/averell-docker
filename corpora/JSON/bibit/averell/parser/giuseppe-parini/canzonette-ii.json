                        },
                        {
                            "word_text": "folleggiano"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Tra gioventù vivace;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Tra"
                        },
                        {
                            "word_text": "gioventù"
                        },
                        {
                            "word_text": "vivace;"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "E rendonvi loquace",
                    "metrical_pattern": null,
