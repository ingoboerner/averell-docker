                            "word_text": "Sol"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "ripeton"
                        },
                        {
                            "word_text": "questa"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ingrata verità.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ingrata"
                        },
                        {
                            "word_text": "verità."
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Con quelle occhiate mutole",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Con"
                        },
                        {
                            "word_text": "quelle"
                        },
                        {
                            "word_text": "occhiate"
                        },
                        {
                            "word_text": "mutole"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Con quel contegno avaro",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Con"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "contegno"
                        },
                        {
                            "word_text": "avaro"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Mi dicono assai chiaro:",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Mi"
                        },
                        {
                            "word_text": "dicono"
                        },
                        {
                            "word_text": "assai"
                        },
                        {
                            "word_text": "chiaro:"
                        }
                    ]
                },
