                {
                    "line_number": 30,
                    "line_text": "Sacro i venturi giorni.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sacro"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "venturi"
                        },
                        {
                            "word_text": "giorni."
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Cadano i mirti; e s'orni",
                    "metrical_pattern": null,
