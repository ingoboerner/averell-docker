                    "line_number": 12,
                    "line_text": "Noi non siam più per te.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Noi"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "siam"
                        },
                        {
                            "word_text": "più"
                        },
                        {
