                        {
                            "word_text": "mano"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "piè."
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Che far? Degg'io di lagrime",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "far?"
                        },
                        {
                            "word_text": "Degg'io"
                        },
                        {
