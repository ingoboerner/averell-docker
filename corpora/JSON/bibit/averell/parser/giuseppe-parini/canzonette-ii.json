                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "fingere"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Han lingua così presta",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Han"
                        },
                        {
                            "word_text": "lingua"
                        },
                        {
                            "word_text": "così"
                        },
                        {
                            "word_text": "presta"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Sol mi ripeton questa",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
