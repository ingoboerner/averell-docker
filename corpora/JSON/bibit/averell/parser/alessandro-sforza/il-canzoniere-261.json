                        {
                            "word_text": "son"
                        },
                        {
                            "word_text": "sì"
                        },
                        {
                            "word_text": "vinto"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Che, per amore, spesso al fin mi vegio",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che,"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "amore,"
                        },
