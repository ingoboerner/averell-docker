                            "word_text": "fare"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "core"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Fuor d'ogni libertà contento e vinto.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fuor"
                        },
                        {
                            "word_text": "d'ogni"
                        },
                        {
                            "word_text": "libertà"
                        },
                        {
                            "word_text": "contento"
                        },
                        {
                            "word_text": "e"
                        },
