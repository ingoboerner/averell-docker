                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "mena"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "E nei begli occhi suoi non so che porta",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "nei"
                        },
                        {
                            "word_text": "begli"
                        },
                        {
