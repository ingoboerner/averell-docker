                        },
                        {
                            "word_text": "adduce"
                        },
                        {
                            "word_text": "equale."
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "AMOR obtenne, che a guardar la grege",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "obtenne,"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "guardar"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "grege"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "D'Ameto Apollo stesse, e a lui crudele",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "D'Ameto"
                        },
                        {
                            "word_text": "Apollo"
                        },
                        {
