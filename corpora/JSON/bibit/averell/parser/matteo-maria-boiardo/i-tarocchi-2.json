                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "de"
                        },
                        {
                            "word_text": "lui"
                        },
                        {
                            "word_text": "forse"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "arse"
                        },
                        {
                            "word_text": "amante."
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "AMOR Paride fece sì animoso,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "Paride"
                        },
                        {
                            "word_text": "fece"
                        },
                        {
                            "word_text": "sì"
                        },
                        {
                            "word_text": "animoso,"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Che ardito fu rapir Elena bella,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
