                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "lui"
                        },
                        {
                            "word_text": "crudele"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Non fu al fin poi; ma cusì i soi correge.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "fu"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "fin"
                        },
                        {
                            "word_text": "poi;"
                        },
                        {
                            "word_text": "ma"
                        },
                        {
                            "word_text": "cusì"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "soi"
                        },
                        {
                            "word_text": "correge."
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "AMOR nov'arte trova; e sotto el mele",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "nov'arte"
                        },
                        {
                            "word_text": "trova;"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "sotto"
                        },
                        {
                            "word_text": "el"
                        },
                        {
                            "word_text": "mele"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "L'esca tien sempre; e i soi servi contenta,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "L'esca"
                        },
                        {
                            "word_text": "tien"
                        },
                        {
                            "word_text": "sempre;"
                        },
