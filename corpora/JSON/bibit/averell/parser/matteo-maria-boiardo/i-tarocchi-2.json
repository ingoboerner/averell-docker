                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "disìo"
                        },
                        {
                            "word_text": "tenta;"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "E se 'l ritrova vano, in forme tante",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "ritrova"
                        },
                        {
                            "word_text": "vano,"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "forme"
                        },
                        {
                            "word_text": "tante"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Il volgie, che ogni dì più se lamenta.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Il"
                        },
                        {
                            "word_text": "volgie,"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "ogni"
                        },
                        {
                            "word_text": "dì"
                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "lamenta."
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "AMOR questo gran Ciclope gigante",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "questo"
                        },
                        {
                            "word_text": "gran"
                        },
                        {
                            "word_text": "Ciclope"
                        },
                        {
                            "word_text": "gigante"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Fece per Galatea tanto amoroso,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fece"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "Galatea"
                        },
                        {
                            "word_text": "tanto"
                        },
                        {
                            "word_text": "amoroso,"
                        }
                    ]
