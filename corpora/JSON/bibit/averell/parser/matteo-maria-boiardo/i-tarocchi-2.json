                            "word_text": "e"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "soi"
                        },
                        {
                            "word_text": "servi"
                        },
                        {
                            "word_text": "contenta,"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Quando se ne ritrova alcun fidele.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quando"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "ritrova"
                        },
                        {
                            "word_text": "alcun"
                        },
                        {
                            "word_text": "fidele."
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "AMOR de ciascun servo il disìo tenta;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "de"
                        },
                        {
