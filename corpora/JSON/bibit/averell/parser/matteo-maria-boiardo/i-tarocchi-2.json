                        },
                        {
                            "word_text": "disio"
                        },
                        {
                            "word_text": "stringe"
                        },
                        {
                            "word_text": "sì"
                        },
                        {
                            "word_text": "forte"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Di consequir quel che gl'imprime al core,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Di"
                        },
                        {
                            "word_text": "consequir"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "gl'imprime"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "core,"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Che a l'effecto non par che se aprin porte.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "l'effecto"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "par"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "aprin"
                        },
                        {
                            "word_text": "porte."
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "AMOR ce insegna non aver timore",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "ce"
                        },
                        {
                            "word_text": "insegna"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "aver"
                        },
                        {
                            "word_text": "timore"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "In qual se voglia impresa: ché un ardito",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "In"
                        },
                        {
                            "word_text": "qual"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "voglia"
                        },
                        {
                            "word_text": "impresa:"
                        },
                        {
                            "word_text": "ché"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "ardito"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Sempre ne la sua corte è vincitore.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sempre"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "sua"
                        },
                        {
                            "word_text": "corte"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "vincitore."
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "AMOR, se qualche volta ha un cor ferito,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR,"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "qualche"
                        },
                        {
                            "word_text": "volta"
                        },
                        {
                            "word_text": "ha"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "cor"
                        },
                        {
                            "word_text": "ferito,"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "E lo resani cum quel proprio strale,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "lo"
                        },
                        {
                            "word_text": "resani"
                        },
                        {
                            "word_text": "cum"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "proprio"
                        },
                        {
                            "word_text": "strale,"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Oh quanto è nel suo regno favorito!",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Oh"
                        },
                        {
                            "word_text": "quanto"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "nel"
                        },
                        {
                            "word_text": "suo"
                        },
                        {
                            "word_text": "regno"
                        },
                        {
                            "word_text": "favorito!"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "AMOR septe anni andar, come animale,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR"
                        },
                        {
                            "word_text": "septe"
                        },
                        {
                            "word_text": "anni"
                        },
                        {
                            "word_text": "andar,"
                        },
                        {
                            "word_text": "come"
                        },
                        {
                            "word_text": "animale,"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Fece quel savio re: ché la sua lege",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fece"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "savio"
                        },
                        {
                            "word_text": "re:"
                        },
                        {
                            "word_text": "ché"
                        },
                        {
                            "word_text": "la"
                        },
                        {
