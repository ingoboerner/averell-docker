                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "gelosia"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "In qualche parte ognor non te acompagni:",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "In"
                        },
                        {
                            "word_text": "qualche"
                        },
                        {
