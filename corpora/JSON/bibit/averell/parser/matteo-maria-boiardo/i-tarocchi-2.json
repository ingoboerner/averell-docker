                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "fin"
                        },
                        {
                            "word_text": "de"
                        },
                        {
                            "word_text": "toi"
                        },
                        {
                            "word_text": "guadagni"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "È un sempre sospirar infin a morte;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "È"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "sempre"
                        },
                        {
                            "word_text": "sospirar"
                        },
                        {
                            "word_text": "infin"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "morte;"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "E chi un dì ride, un anno advien se lagni.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "chi"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "dì"
                        },
                        {
