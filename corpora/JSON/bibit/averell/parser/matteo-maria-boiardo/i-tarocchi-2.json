                        },
                        {
                            "word_text": "bon"
                        },
                        {
                            "word_text": "stato,"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Sollicito, animoso e prompto sia,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sollicito,"
                        },
                        {
                            "word_text": "animoso"
                        },
                        {
                            "word_text": "e"
                        },
                        {
