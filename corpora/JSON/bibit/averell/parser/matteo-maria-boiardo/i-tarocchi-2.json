                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "anno"
                        },
                        {
                            "word_text": "advien"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "lagni."
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "AMOR, questo disio stringe sì forte",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "AMOR,"
                        },
                        {
