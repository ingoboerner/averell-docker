                    "words": [
                        {
                            "word_text": "Amor"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "grande"
                        },
                        {
                            "word_text": "Cyclope"
                        },
                        {
                            "word_text": "gigante"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Fece per Galatea tanto amoroso,",
