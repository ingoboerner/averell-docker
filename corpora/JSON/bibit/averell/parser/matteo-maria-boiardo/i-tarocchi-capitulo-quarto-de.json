                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "forme"
                        },
                        {
                            "word_text": "tante"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "El volgie, ch'ogni dì par si lamenta.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "El"
                        },
                        {
                            "word_text": "volgie,"
                        },
                        {
                            "word_text": "ch'ogni"
                        },
                        {
                            "word_text": "dì"
                        },
                        {
                            "word_text": "par"
                        },
                        {
