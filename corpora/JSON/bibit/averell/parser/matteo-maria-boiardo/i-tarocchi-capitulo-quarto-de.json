                        },
                        {
                            "word_text": "stato"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Sollicito, amoroso e prompto sia,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sollicito,"
                        },
