                        },
                        {
                            "word_text": "leggie"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "El principe al suo servo adduce equale.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
