                        },
                        {
                            "word_text": "effecto"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "par"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "s'aprin"
                        },
                        {
                            "word_text": "porte."
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Amor ce insegna non aver timore",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Amor"
                        },
                        {
                            "word_text": "ce"
                        },
                        {
                            "word_text": "insegna"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "aver"
                        },
                        {
                            "word_text": "timore"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "In qual si vogli impresa, ché un ardito",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "In"
                        },
                        {
                            "word_text": "qual"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "vogli"
                        },
                        {
                            "word_text": "impresa,"
                        },
                        {
                            "word_text": "ché"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "ardito"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Sempre nella sua corte è vincitore.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sempre"
                        },
                        {
