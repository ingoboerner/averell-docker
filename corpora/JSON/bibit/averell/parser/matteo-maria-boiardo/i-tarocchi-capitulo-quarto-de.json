                        },
                        {
                            "word_text": "tenta"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "E s'il ritrova vano, in forme tante",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "s'il"
                        },
                        {
                            "word_text": "ritrova"
                        },
