{
    "poem_title": "I tarocchi - Capitulo secondo de gellosia",
    "author": "Matteo Maria Boiardo",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Gellosia un vero amor non pò smarrire,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "vero"
                        },
                        {
                            "word_text": "amor"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "pò"
                        },
                        {
                            "word_text": "smarrire,"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Ché si uno amante va con pura fede,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "uno"
                        },
                        {
                            "word_text": "amante"
                        },
                        {
                            "word_text": "va"
                        },
                        {
                            "word_text": "con"
                        },
                        {
                            "word_text": "pura"
                        },
                        {
                            "word_text": "fede,"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Amore il premia al fin del suo servire.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Amore"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "premia"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "fin"
                        },
                        {
                            "word_text": "del"
                        },
                        {
                            "word_text": "suo"
                        },
                        {
                            "word_text": "servire."
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Gellosia è dura cosa, ove esser vede",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "dura"
                        },
                        {
                            "word_text": "cosa,"
                        },
                        {
                            "word_text": "ove"
                        },
                        {
                            "word_text": "esser"
                        },
                        {
                            "word_text": "vede"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Commodo al concorrente nello amore,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Commodo"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "concorrente"
                        },
                        {
                            "word_text": "nello"
                        },
                        {
                            "word_text": "amore,"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Ché al spesso supplicar segue mercede.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "spesso"
                        },
                        {
                            "word_text": "supplicar"
                        },
                        {
                            "word_text": "segue"
                        },
                        {
                            "word_text": "mercede."
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Gellosia mesto rende un lieto core,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "mesto"
                        },
                        {
                            "word_text": "rende"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "lieto"
                        },
                        {
                            "word_text": "core,"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Ma spesso è causa ancor, dove ella sprona,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ma"
                        },
                        {
                            "word_text": "spesso"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "causa"
                        },
                        {
                            "word_text": "ancor,"
                        },
                        {
                            "word_text": "dove"
                        },
                        {
                            "word_text": "ella"
                        },
                        {
                            "word_text": "sprona,"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Condurre un ch'ama a un virtuoso onore.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Condurre"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "ch'ama"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "virtuoso"
                        },
                        {
                            "word_text": "onore."
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Gellosia quando ven, non si propona",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "quando"
                        },
                        {
                            "word_text": "ven,"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "propona"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Contrastargli alcun mai, ché sforza ognuno:",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Contrastargli"
                        },
                        {
                            "word_text": "alcun"
                        },
                        {
                            "word_text": "mai,"
                        },
                        {
                            "word_text": "ché"
                        },
                        {
                            "word_text": "sforza"
                        },
                        {
                            "word_text": "ognuno:"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Ma il saper tollerarla è cosa bona.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ma"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "saper"
                        },
                        {
                            "word_text": "tollerarla"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "cosa"
                        },
                        {
                            "word_text": "bona."
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Gellosia ciascun cerca; e poi ciascuno",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "ciascun"
                        },
                        {
                            "word_text": "cerca;"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "poi"
                        },
                        {
                            "word_text": "ciascuno"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "La fugge; e prima ogniun vorria sapere,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "La"
                        },
                        {
                            "word_text": "fugge;"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "prima"
                        },
                        {
                            "word_text": "ogniun"
                        },
                        {
                            "word_text": "vorria"
                        },
                        {
                            "word_text": "sapere,"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Poi de saper vorrebbe esser degiuno.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Poi"
                        },
                        {
                            "word_text": "de"
                        },
                        {
                            "word_text": "saper"
                        },
                        {
                            "word_text": "vorrebbe"
                        },
                        {
                            "word_text": "esser"
                        },
                        {
                            "word_text": "degiuno."
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Gellosia sempre non debbia volere",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "sempre"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "debbia"
                        },
                        {
                            "word_text": "volere"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "El concorrente per nemico; anzi esso,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "El"
                        },
                        {
                            "word_text": "concorrente"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "nemico;"
                        },
                        {
                            "word_text": "anzi"
                        },
                        {
                            "word_text": "esso,"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Se vincer vol, dee patienzia avere.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "vincer"
                        },
                        {
                            "word_text": "vol,"
                        },
                        {
                            "word_text": "dee"
                        },
                        {
                            "word_text": "patienzia"
                        },
                        {
                            "word_text": "avere."
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Gellosia si te gionge a veder presso",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "te"
                        },
                        {
                            "word_text": "gionge"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "veder"
                        },
                        {
                            "word_text": "presso"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "A la cosa che tu ami il tuo rivale,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "cosa"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "ami"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "tuo"
                        },
                        {
                            "word_text": "rivale,"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Stimi che 'l parli sempre, a tuo interesso.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Stimi"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "parli"
                        },
                        {
                            "word_text": "sempre,"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "tuo"
                        },
                        {
                            "word_text": "interesso."
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Gellosia dove ponsi è gran male,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "dove"
                        },
                        {
                            "word_text": "ponsi"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "gran"
                        },
                        {
                            "word_text": "male,"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Ché non si trova medicina a lei;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "trova"
                        },
                        {
                            "word_text": "medicina"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "lei;"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "E si troppo oltra va, cosa è mortale.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "troppo"
                        },
                        {
                            "word_text": "oltra"
                        },
                        {
                            "word_text": "va,"
                        },
                        {
                            "word_text": "cosa"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "mortale."
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Gellosia non vien manco fra li dei",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "vien"
                        },
                        {
                            "word_text": "manco"
                        },
                        {
                            "word_text": "fra"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "dei"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Che fra li omini facia; ecco Iunone",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "fra"
                        },
                        {
                            "word_text": "li"
                        },
                        {
                            "word_text": "omini"
                        },
                        {
                            "word_text": "facia;"
                        },
                        {
                            "word_text": "ecco"
                        },
                        {
                            "word_text": "Iunone"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Del suo Iove gellosa, ah casi rei!",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Del"
                        },
                        {
                            "word_text": "suo"
                        },
                        {
                            "word_text": "Iove"
                        },
                        {
                            "word_text": "gellosa,"
                        },
                        {
                            "word_text": "ah"
                        },
                        {
                            "word_text": "casi"
                        },
                        {
                            "word_text": "rei!"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Gellosia di certezza mai non pone",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "certezza"
                        },
                        {
                            "word_text": "mai"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "pone"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Alcuno in strada e al ver non apre porte",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Alcuno"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "strada"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "ver"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "apre"
                        },
                        {
                            "word_text": "porte"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "E tien fra speme e dubbio le persone.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "tien"
                        },
                        {
                            "word_text": "fra"
                        },
                        {
                            "word_text": "speme"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "dubbio"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "persone."
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Gellosia d'Argo e de sue viste acorte",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "d'Argo"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "de"
                        },
                        {
                            "word_text": "sue"
                        },
                        {
                            "word_text": "viste"
                        },
                        {
                            "word_text": "acorte"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Non è sicura mai, fin che 'l tuo pede",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "sicura"
                        },
                        {
                            "word_text": "mai,"
                        },
                        {
                            "word_text": "fin"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "tuo"
                        },
                        {
                            "word_text": "pede"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "A le adamate mura ti transporte.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "adamate"
                        },
                        {
                            "word_text": "mura"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "transporte."
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Gellosia Turno re promisse erede",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "Turno"
                        },
                        {
                            "word_text": "re"
                        },
                        {
                            "word_text": "promisse"
                        },
                        {
                            "word_text": "erede"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Del latin regno e indusse a mortal guerra;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Del"
                        },
                        {
                            "word_text": "latin"
                        },
                        {
                            "word_text": "regno"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "indusse"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "mortal"
                        },
                        {
                            "word_text": "guerra;"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "E morto fu, ché morte indi procede.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "morto"
                        },
                        {
                            "word_text": "fu,"
                        },
                        {
                            "word_text": "ché"
                        },
                        {
                            "word_text": "morte"
                        },
                        {
                            "word_text": "indi"
                        },
                        {
                            "word_text": "procede."
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Gellosia Iuno dea, più volte in terra",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Gellosia"
                        },
                        {
                            "word_text": "Iuno"
                        },
                        {
                            "word_text": "dea,"
                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "volte"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "terra"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "Fece venir, per varii amor di Giove,",
                    "metrical_pattern": null,
