            "lines": [
                {
                    "line_number": 1,
                    "line_text": "TIMOR un'alma tien tanto dubiosa",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
