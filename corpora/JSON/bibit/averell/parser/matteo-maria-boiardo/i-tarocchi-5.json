                        },
                        {
                            "word_text": "tanto"
                        },
                        {
                            "word_text": "dubiosa"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Ch'ella ha poca ragion di viver lieta,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ch'ella"
                        },
                        {
                            "word_text": "ha"
                        },
                        {
                            "word_text": "poca"
                        },
                        {
                            "word_text": "ragion"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "viver"
                        },
                        {
                            "word_text": "lieta,"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": " Qual mai non gode e sempre è paurosa.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Qual"
                        },
                        {
                            "word_text": "mai"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "gode"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "sempre"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "paurosa."
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "TIMOR, dov'è qualche pericol, vieta",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR,"
                        },
                        {
                            "word_text": "dov'è"
                        },
                        {
                            "word_text": "qualche"
                        },
                        {
                            "word_text": "pericol,"
                        },
                        {
                            "word_text": "vieta"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Pigliar piacere, e tanto un om fa vile,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Pigliar"
                        },
                        {
                            "word_text": "piacere,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "tanto"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "om"
                        },
                        {
                            "word_text": "fa"
                        },
                        {
                            "word_text": "vile,"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Che l'animo ragion mai non acquieta.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "l'animo"
                        },
                        {
                            "word_text": "ragion"
                        },
                        {
                            "word_text": "mai"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "acquieta."
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "TIMOR tremar fa l'agnel ne l'ovile",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "tremar"
                        },
                        {
                            "word_text": "fa"
                        },
                        {
                            "word_text": "l'agnel"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "l'ovile"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Se di fuor sente il lupo, e sì sta chiuso,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Se"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "fuor"
                        },
                        {
                            "word_text": "sente"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "lupo,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "sì"
                        },
                        {
                            "word_text": "sta"
                        },
                        {
                            "word_text": "chiuso,"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Che appena intrar gli può il vento sottile.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "appena"
                        },
                        {
                            "word_text": "intrar"
                        },
                        {
                            "word_text": "gli"
                        },
                        {
                            "word_text": "può"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "vento"
                        },
                        {
                            "word_text": "sottile."
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "TIMOR quattro destrier d'un carro a l'usol,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "quattro"
                        },
                        {
                            "word_text": "destrier"
                        },
                        {
                            "word_text": "d'un"
                        },
                        {
                            "word_text": "carro"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "l'usol,"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Sotto una virga tiene a un giogo stretti;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sotto"
                        },
                        {
                            "word_text": "una"
                        },
                        {
                            "word_text": "virga"
                        },
                        {
                            "word_text": "tiene"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "giogo"
                        },
                        {
                            "word_text": "stretti;"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "E molti in servitù, che non gli excuso.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "molti"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "servitù,"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "gli"
                        },
                        {
                            "word_text": "excuso."
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "TIMOR ci tien talor, che i nostri effetti",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "ci"
                        },
                        {
                            "word_text": "tien"
                        },
                        {
                            "word_text": "talor,"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "nostri"
                        },
                        {
                            "word_text": "effetti"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Non possiam dimostrar, che assai ne offende,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "possiam"
                        },
                        {
                            "word_text": "dimostrar,"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "assai"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "offende,"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Ché compagni al timor sono i rispetti.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "compagni"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "timor"
                        },
                        {
                            "word_text": "sono"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "rispetti."
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "TIMOR fa sempre che un non si difende,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "fa"
                        },
                        {
                            "word_text": "sempre"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "difende,"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Ma supplice ai contrasti se dimostra",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ma"
                        },
                        {
                            "word_text": "supplice"
                        },
                        {
                            "word_text": "ai"
                        },
                        {
                            "word_text": "contrasti"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "dimostra"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "E senza arme adoprar vinto se rende.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "senza"
                        },
                        {
                            "word_text": "arme"
                        },
                        {
                            "word_text": "adoprar"
                        },
                        {
                            "word_text": "vinto"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "rende."
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "TIMOR, se tu ti accosti a armati in giostra,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR,"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "accosti"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "armati"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "giostra,"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "La lor virtù sarà sotto te morta;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "La"
                        },
                        {
                            "word_text": "lor"
                        },
                        {
                            "word_text": "virtù"
                        },
                        {
                            "word_text": "sarà"
                        },
                        {
                            "word_text": "sotto"
                        },
                        {
                            "word_text": "te"
                        },
                        {
                            "word_text": "morta;"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Dove tu sei, sempre la fronte il mostra.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Dove"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "sei,"
                        },
                        {
                            "word_text": "sempre"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "fronte"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "mostra."
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "TIMOR obturba i sensi, e facia smorta",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "obturba"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "sensi,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "facia"
                        },
                        {
                            "word_text": "smorta"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Rende, e tremito il cor per lui si sente,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Rende,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "tremito"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "cor"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "lui"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "sente,"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "E l'occhio il mostra con sua vista torta.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "l'occhio"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "mostra"
                        },
                        {
                            "word_text": "con"
                        },
                        {
                            "word_text": "sua"
                        },
                        {
                            "word_text": "vista"
                        },
                        {
                            "word_text": "torta."
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "TIMOR non ha sol di quel ch'è presente,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "ha"
                        },
                        {
                            "word_text": "sol"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "ch'è"
                        },
                        {
                            "word_text": "presente,"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Dubbio: ma teme, ben che sia lontano,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Dubbio:"
                        },
                        {
                            "word_text": "ma"
                        },
                        {
                            "word_text": "teme,"
                        },
                        {
                            "word_text": "ben"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "sia"
                        },
                        {
                            "word_text": "lontano,"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Il periculo, e a sé pargli imminente.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Il"
                        },
                        {
                            "word_text": "periculo,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "sé"
                        },
                        {
                            "word_text": "pargli"
                        },
                        {
                            "word_text": "imminente."
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "TIMOR de certo è a imaginarlo vano,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "de"
                        },
                        {
                            "word_text": "certo"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "imaginarlo"
                        },
                        {
                            "word_text": "vano,"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "E dove timor regna, ogniun concorre",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "dove"
                        },
                        {
                            "word_text": "timor"
                        },
                        {
                            "word_text": "regna,"
                        },
                        {
                            "word_text": "ogniun"
                        },
                        {
                            "word_text": "concorre"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Che invalido quel corpo sia e mal sano.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "invalido"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "corpo"
                        },
                        {
                            "word_text": "sia"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "mal"
                        },
                        {
                            "word_text": "sano."
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "TIMOR Fineo tra gli omini una torre",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "Fineo"
                        },
                        {
                            "word_text": "tra"
                        },
                        {
                            "word_text": "gli"
                        },
                        {
                            "word_text": "omini"
                        },
                        {
                            "word_text": "una"
                        },
                        {
                            "word_text": "torre"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Converse in saxo col meduseo volto,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Converse"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "saxo"
                        },
                        {
                            "word_text": "col"
                        },
                        {
                            "word_text": "meduseo"
                        },
                        {
                            "word_text": "volto,"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Ché a' timidi fortuna non soccorre.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "a'"
                        },
                        {
                            "word_text": "timidi"
                        },
                        {
                            "word_text": "fortuna"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "soccorre."
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "TIMOR Ptolemeo re sùbito vòlto",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "TIMOR"
                        },
                        {
                            "word_text": "Ptolemeo"
                        },
                        {
                            "word_text": "re"
                        },
                        {
                            "word_text": "sùbito"
                        },
                        {
                            "word_text": "vòlto"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Ebbe contra Pompeo, sol per paura",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ebbe"
                        },
                        {
                            "word_text": "contra"
                        },
                        {
                            "word_text": "Pompeo,"
                        },
                        {
                            "word_text": "sol"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "paura"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Che Cesar non gli avesse il regno tolto.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "Cesar"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "gli"
                        },
                        {
                            "word_text": "avesse"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "regno"
                        },
                        {
                            "word_text": "tolto."
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "TIMOR non lassò Andromeca secura",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
