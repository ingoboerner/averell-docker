                        },
                        {
                            "word_text": "mio!"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Ella prende marito, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ella"
                        },
                        {
                            "word_text": "prende"
                        },
                        {
                            "word_text": "marito,"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "Nè il marito son io. ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Nè"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "marito"
                        },
                        {
                            "word_text": "son"
                        },
                        {
                            "word_text": "io."
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": " Ahi pel mio cor tu mi portasti affetto! ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ahi"
                        },
                        {
                            "word_text": "pel"
                        },
                        {
                            "word_text": "mio"
                        },
                        {
                            "word_text": "cor"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "portasti"
                        },
                        {
                            "word_text": "affetto!"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "Ora per lingua d'altri tu mi pianti! ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ora"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "lingua"
                        },
                        {
                            "word_text": "d'altri"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "pianti!"
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "So pur quel che t'han detto ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "So"
                        },
                        {
                            "word_text": "pur"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "t'han"
                        },
                        {
                            "word_text": "detto"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Certe amiche zelanti. ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Certe"
                        },
                        {
                            "word_text": "amiche"
                        },
                        {
                            "word_text": "zelanti."
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": " Viene un'amica e dice: \"Ho conosciuto ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Viene"
                        },
                        {
                            "word_text": "un'amica"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "dice:"
                        },
                        {
                            "word_text": "\"Ho"
                        },
                        {
                            "word_text": "conosciuto"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Quel tale, sai - m'intendi già, quel tale…. ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quel"
                        },
                        {
                            "word_text": "tale,"
                        },
                        {
                            "word_text": "sai"
                        },
                        {
                            "word_text": "-"
                        },
                        {
                            "word_text": "m'intendi"
                        },
                        {
                            "word_text": "già,"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "tale…."
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Stamani l'ho veduto, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Stamani"
                        },
                        {
                            "word_text": "l'ho"
                        },
                        {
                            "word_text": "veduto,"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Proprio un bel capitale! \"",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Proprio"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "bel"
                        },
                        {
                            "word_text": "capitale!"
                        },
                        {
                            "word_text": "\""
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": " Ed un'altra dipoi: \"Sta un po' a vedere, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ed"
                        },
                        {
                            "word_text": "un'altra"
                        },
                        {
                            "word_text": "dipoi:"
                        },
                        {
                            "word_text": "\"Sta"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "po'"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "vedere,"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Che tu ti pensi ch'ei t'abbia a pigliare! ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "pensi"
                        },
                        {
                            "word_text": "ch'ei"
                        },
                        {
                            "word_text": "t'abbia"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "pigliare!"
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Non sai ch'è forastiere, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "sai"
                        },
                        {
                            "word_text": "ch'è"
                        },
                        {
                            "word_text": "forastiere,"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Oltre ad esser scolare? ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Oltre"
                        },
                        {
                            "word_text": "ad"
                        },
                        {
                            "word_text": "esser"
                        },
                        {
                            "word_text": "scolare?"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": " Costor fanno a chi meglio ne canzona; ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Costor"
                        },
                        {
                            "word_text": "fanno"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "chi"
                        },
                        {
                            "word_text": "meglio"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "canzona;"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Oh dà lor retta!… e inoltre un veneziano, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Oh"
                        },
                        {
                            "word_text": "dà"
                        },
                        {
                            "word_text": "lor"
                        },
                        {
                            "word_text": "retta!…"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "inoltre"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "veneziano,"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Non si sa che persona, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "sa"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "persona,"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Fosse almanco toscano! \"",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fosse"
                        },
                        {
                            "word_text": "almanco"
                        },
                        {
                            "word_text": "toscano!"
                        },
                        {
                            "word_text": "\""
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": " E quindi un'altra: \"Creder mi si fece ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "quindi"
                        },
                        {
                            "word_text": "un'altra:"
                        },
                        {
                            "word_text": "\"Creder"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "fece"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Ch'egli è un gran ricco e con te si trastulla… \"",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ch'egli"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "gran"
                        },
                        {
                            "word_text": "ricco"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "con"
                        },
                        {
                            "word_text": "te"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "trastulla…"
                        },
                        {
                            "word_text": "\""
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "E un'altra: \"Io credo invece ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "un'altra:"
                        },
                        {
                            "word_text": "\"Io"
                        },
                        {
                            "word_text": "credo"
                        },
                        {
                            "word_text": "invece"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Ch'egli non abbia nulla: ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ch'egli"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "abbia"
                        },
                        {
                            "word_text": "nulla:"
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": " Quando s'è ricchi a casa si rimane ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quando"
                        },
                        {
                            "word_text": "s'è"
                        },
                        {
                            "word_text": "ricchi"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "casa"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "rimane"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "E non si va pel mondo a fare e dire, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "va"
                        },
                        {
                            "word_text": "pel"
                        },
                        {
                            "word_text": "mondo"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "fare"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "dire,"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Quando s'ha in casa il pane ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quando"
                        },
                        {
                            "word_text": "s'ha"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "casa"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "pane"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Non occorre d'uscire \"- ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Non"
                        },
                        {
                            "word_text": "occorre"
                        },
                        {
                            "word_text": "d'uscire"
                        },
                        {
                            "word_text": "\"-"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": " Poi viene, un altro giorno, un'altra amica ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Poi"
                        },
                        {
                            "word_text": "viene,"
                        },
                        {
                            "word_text": "un"
                        },
                        {
