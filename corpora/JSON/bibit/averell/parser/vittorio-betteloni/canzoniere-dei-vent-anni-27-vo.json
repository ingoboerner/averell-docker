                        },
                        {
                            "word_text": "giorno,"
                        },
                        {
                            "word_text": "un'altra"
                        },
                        {
                            "word_text": "amica"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Che comincia: \"Tacer volevo pria ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "comincia:"
                        },
                        {
                            "word_text": "\"Tacer"
                        },
                        {
                            "word_text": "volevo"
                        },
                        {
                            "word_text": "pria"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Ma convien che tel dica; ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ma"
                        },
                        {
                            "word_text": "convien"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "tel"
                        },
                        {
                            "word_text": "dica;"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Ahimè, bambina mia, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ahimè,"
                        },
                        {
                            "word_text": "bambina"
                        },
                        {
                            "word_text": "mia,"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": " Sai ch'egli un'altra ne vagheggia? e come ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sai"
                        },
                        {
                            "word_text": "ch'egli"
                        },
                        {
                            "word_text": "un'altra"
                        },
                        {
                            "word_text": "ne"
                        },
                        {
                            "word_text": "vagheggia?"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "come"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Le fa il galante! il sanno gli occhi miei; ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Le"
                        },
                        {
                            "word_text": "fa"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "galante!"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "sanno"
                        },
                        {
                            "word_text": "gli"
                        },
                        {
                            "word_text": "occhi"
                        },
                        {
                            "word_text": "miei;"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Or io non faccio il nome, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Or"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "faccio"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "nome,"
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Ma giuocata tu sei: ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ma"
                        },
                        {
                            "word_text": "giuocata"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "sei:"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": " Oh! digliene un po' tu quel che ti pare… \"- ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Oh!"
                        },
                        {
                            "word_text": "digliene"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "po'"
                        },
                        {
                            "word_text": "tu"
                        },
                        {
                            "word_text": "quel"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "pare…"
                        },
                        {
                            "word_text": "\"-"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "E così via, ciascuna a inventar peggio, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "così"
                        },
                        {
                            "word_text": "via,"
                        },
                        {
                            "word_text": "ciascuna"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "inventar"
                        },
                        {
                            "word_text": "peggio,"
                        }
                    ]
                },
                {
                    "line_number": 39,
                    "line_text": "Tutte di me a sparlare, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Tutte"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "me"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "sparlare,"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Tutte a pormi in dileggio. ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Tutte"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "pormi"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "dileggio."
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": " Dicono che son questo, oppur quest'altro, ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Dicono"
                        },
                        {
                            "word_text": "che"
                        },
