                {
                    "line_number": 1,
                    "line_text": " Voi non sapete quel che m'è seguito! ",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Voi"
                        },
                        {
                            "word_text": "non"
                        },
                        {
