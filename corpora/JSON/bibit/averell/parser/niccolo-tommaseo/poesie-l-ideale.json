        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Né mai, nell'atto d'andar, muta i passi,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
