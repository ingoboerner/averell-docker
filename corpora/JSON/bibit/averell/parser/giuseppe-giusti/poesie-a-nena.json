                        },
                        {
                            "word_text": "correggersi"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "è tempo perso.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "tempo"
                        },
                        {
                            "word_text": "perso."
                        }
