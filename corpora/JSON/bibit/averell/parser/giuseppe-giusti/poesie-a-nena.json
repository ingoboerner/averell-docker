                        },
                        {
                            "word_text": "tempera"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "più sensuale,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "sensuale,"
                        }
                    ]
                },
                {
