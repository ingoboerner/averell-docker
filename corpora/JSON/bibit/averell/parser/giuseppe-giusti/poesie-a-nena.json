                    "line_length": null,
                    "words": [
                        {
                            "word_text": "fole"
                        },
                        {
                            "word_text": "nutrita,"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "vorresti in estasi",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "vorresti"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "estasi"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "passar la vita:",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "passar"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "vita:"
                        }
                    ]
