                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "particolare."
                        }
                    ]
                },
                {
                    "line_number": 7,
                    "line_text": "Che vuoi? rasségnati:",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "vuoi?"
