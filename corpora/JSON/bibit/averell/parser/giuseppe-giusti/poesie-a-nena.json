                    "line_length": null,
                    "words": [
                        {
                            "word_text": "nostro"
                        },
                        {
                            "word_text": "è"
                        },
                        {
                            "word_text": "diverso:"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "voler correggersi",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
