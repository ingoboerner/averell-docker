    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Pur nel sonno almen talora",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Pur"
                        },
                        {
                            "word_text": "nel"
                        },
                        {
                            "word_text": "sonno"
                        },
                        {
                            "word_text": "almen"
                        },
                        {
                            "word_text": "talora"
                        }
                    ]
                },
                {
                    "line_number": 2,
                    "line_text": "Vien colei che m'innamora",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Vien"
                        },
                        {
                            "word_text": "colei"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "m'innamora"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "Le mie pene a consolar.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Le"
                        },
                        {
                            "word_text": "mie"
                        },
                        {
                            "word_text": "pene"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "consolar."
                        }
                    ]
                }
            ],
            "stanza_text": "Pur nel sonno almen talora\nVien colei che m'innamora\nLe mie pene a consolar."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Rendi, Amor, se giusto sei,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Rendi,"
                        },
                        {
                            "word_text": "Amor,"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "giusto"
                        },
                        {
                            "word_text": "sei,"
                        }
                    ]
                },
                {
                    "line_number": 5,
                    "line_text": "Più veraci i sogni miei,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Più"
                        },
                        {
                            "word_text": "veraci"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "sogni"
                        },
                        {
                            "word_text": "miei,"
                        }
                    ]
                },
                {
                    "line_number": 6,
                    "line_text": "O non farmi risvegliar.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "O"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "farmi"
                        },
                        {
                            "word_text": "risvegliar."
                        }
                    ]
                }
            ],
            "stanza_text": "Rendi, Amor, se giusto sei,\nPiù veraci i sogni miei,\nO non farmi risvegliar."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Di solitaria fonte",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Di"
                        },
                        {
                            "word_text": "solitaria"
                        },
                        {
                            "word_text": "fonte"
                        }
                    ]
                },
                {
                    "line_number": 8,
                    "line_text": "Sul margo assiso al primo albore, o Fille,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sul"
                        },
                        {
                            "word_text": "margo"
                        },
                        {
                            "word_text": "assiso"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "primo"
                        },
                        {
                            "word_text": "albore,"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "Fille,"
                        }
                    ]
                },
                {
                    "line_number": 9,
                    "line_text": "Sognai d'esser con te. Sognai, ma in guisa",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sognai"
                        },
                        {
                            "word_text": "d'esser"
                        },
                        {
                            "word_text": "con"
                        },
                        {
                            "word_text": "te."
                        },
                        {
                            "word_text": "Sognai,"
                        },
                        {
                            "word_text": "ma"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "guisa"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Che sognar non credei. Garrir gli augelli,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "sognar"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "credei."
                        },
                        {
                            "word_text": "Garrir"
                        },
                        {
                            "word_text": "gli"
                        },
                        {
                            "word_text": "augelli,"
                        }
                    ]
                },
                {
                    "line_number": 11,
                    "line_text": "Frangersi l'acque e susurrar le foglie",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Frangersi"
                        },
                        {
                            "word_text": "l'acque"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "susurrar"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "foglie"
                        }
                    ]
                },
                {
                    "line_number": 12,
                    "line_text": "Pareami udir. De' tuoi begli occhi al lume,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Pareami"
                        },
                        {
                            "word_text": "udir."
                        },
                        {
                            "word_text": "De'"
                        },
                        {
                            "word_text": "tuoi"
                        },
                        {
                            "word_text": "begli"
                        },
                        {
                            "word_text": "occhi"
                        },
                        {
                            "word_text": "al"
                        },
                        {
                            "word_text": "lume,"
                        }
                    ]
                },
                {
                    "line_number": 13,
                    "line_text": "Come suol per costume,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Come"
                        },
                        {
                            "word_text": "suol"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "costume,"
                        }
                    ]
                },
                {
                    "line_number": 14,
                    "line_text": "Fra' suoi palpiti usati era il cor mio.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fra'"
                        },
                        {
                            "word_text": "suoi"
                        },
                        {
                            "word_text": "palpiti"
                        },
                        {
                            "word_text": "usati"
                        },
                        {
                            "word_text": "era"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "cor"
                        },
                        {
                            "word_text": "mio."
                        }
                    ]
                },
                {
                    "line_number": 15,
                    "line_text": "Sol nel vederti, oh Dio!",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Sol"
                        },
                        {
                            "word_text": "nel"
                        },
                        {
                            "word_text": "vederti,"
                        },
                        {
                            "word_text": "oh"
                        },
                        {
                            "word_text": "Dio!"
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Pietosa a me qual non ti vidi mai,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Pietosa"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "me"
                        },
                        {
                            "word_text": "qual"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "ti"
                        },
                        {
                            "word_text": "vidi"
                        },
                        {
                            "word_text": "mai,"
                        }
                    ]
                },
                {
                    "line_number": 17,
                    "line_text": "Di sognar qualche volta io dubitai.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Di"
                        },
                        {
                            "word_text": "sognar"
                        },
                        {
                            "word_text": "qualche"
                        },
                        {
                            "word_text": "volta"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "dubitai."
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Quai voci udii! Che dolci nomi ottenni,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quai"
                        },
                        {
                            "word_text": "voci"
                        },
                        {
                            "word_text": "udii!"
                        },
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "dolci"
                        },
                        {
                            "word_text": "nomi"
                        },
                        {
                            "word_text": "ottenni,"
                        }
                    ]
                },
                {
                    "line_number": 19,
                    "line_text": "Cara, da' labbri tuoi! Quali in quei molli",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Cara,"
                        },
                        {
                            "word_text": "da'"
                        },
                        {
                            "word_text": "labbri"
                        },
                        {
                            "word_text": "tuoi!"
                        },
                        {
                            "word_text": "Quali"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "quei"
                        },
                        {
                            "word_text": "molli"
                        }
                    ]
                },
                {
                    "line_number": 20,
                    "line_text": "Tremuli rai teneri sensi io lessi!",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Tremuli"
                        },
                        {
                            "word_text": "rai"
                        },
                        {
                            "word_text": "teneri"
                        },
                        {
                            "word_text": "sensi"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "lessi!"
                        }
                    ]
                },
                {
                    "line_number": 21,
                    "line_text": "Ah se mirar potessi",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ah"
                        },
                        {
                            "word_text": "se"
                        },
                        {
                            "word_text": "mirar"
                        },
                        {
                            "word_text": "potessi"
                        }
                    ]
                },
                {
                    "line_number": 22,
                    "line_text": "Quanto splendan più belle",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quanto"
                        },
                        {
                            "word_text": "splendan"
                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "belle"
                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Fra i lampi di pietà le tue pupille,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fra"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "lampi"
                        },
                        {
                            "word_text": "di"
                        },
                        {
                            "word_text": "pietà"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "tue"
                        },
                        {
                            "word_text": "pupille,"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Mai più crudel non mi saresti, o Fille.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Mai"
                        },
                        {
                            "word_text": "più"
                        },
                        {
                            "word_text": "crudel"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "saresti,"
                        },
                        {
                            "word_text": "o"
                        },
                        {
                            "word_text": "Fille."
                        }
                    ]
                },
                {
                    "line_number": 25,
                    "line_text": "Qual io divenni allora,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Qual"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "divenni"
                        },
                        {
                            "word_text": "allora,"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Quel che allora io pensai, ciò che allor dissi,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quel"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "allora"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "pensai,"
                        },
                        {
                            "word_text": "ciò"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "allor"
                        },
                        {
                            "word_text": "dissi,"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Ridir non so. So che sul vivo latte",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ridir"
                        },
                        {
                            "word_text": "non"
                        },
                        {
                            "word_text": "so."
                        },
                        {
                            "word_text": "So"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "sul"
                        },
                        {
                            "word_text": "vivo"
                        },
                        {
                            "word_text": "latte"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "Della tua mano io mille baci impressi;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Della"
                        },
                        {
                            "word_text": "tua"
                        },
                        {
                            "word_text": "mano"
                        },
                        {
                            "word_text": "io"
                        },
                        {
                            "word_text": "mille"
                        },
                        {
                            "word_text": "baci"
                        },
                        {
                            "word_text": "impressi;"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "Tu d'un vago rossor tingesti il volto.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Tu"
                        },
                        {
                            "word_text": "d'un"
                        },
                        {
                            "word_text": "vago"
                        },
                        {
                            "word_text": "rossor"
                        },
                        {
                            "word_text": "tingesti"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "volto."
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Quando improvviso ascolto",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Quando"
                        },
                        {
                            "word_text": "improvviso"
                        },
                        {
                            "word_text": "ascolto"
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "D'un cespuglio vicin scuoter le fronde:",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "D'un"
                        },
                        {
                            "word_text": "cespuglio"
                        },
                        {
                            "word_text": "vicin"
                        },
                        {
                            "word_text": "scuoter"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "fronde:"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "Mi volgo, e mezzo ascoso",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Mi"
                        },
                        {
                            "word_text": "volgo,"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "mezzo"
                        },
                        {
                            "word_text": "ascoso"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "Scopro il rival Fileno",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Scopro"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "rival"
                        },
                        {
                            "word_text": "Fileno"
                        }
                    ]
                },
                {
                    "line_number": 34,
                    "line_text": "Che, d'invido veleno",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che,"
                        },
                        {
                            "word_text": "d'invido"
                        },
                        {
                            "word_text": "veleno"
                        }
                    ]
                },
                {
                    "line_number": 35,
                    "line_text": "Livido in faccia, i furti miei rimira.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Livido"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "faccia,"
                        },
                        {
                            "word_text": "i"
                        },
                        {
                            "word_text": "furti"
                        },
                        {
                            "word_text": "miei"
                        },
                        {
                            "word_text": "rimira."
                        }
                    ]
                },
                {
                    "line_number": 36,
                    "line_text": "Fra la sorpresa e l'ira",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Fra"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "sorpresa"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "l'ira"
                        }
                    ]
                },
                {
                    "line_number": 37,
                    "line_text": "Avvampai, mi riscossi in un momento,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Avvampai,"
                        },
                        {
                            "word_text": "mi"
                        },
                        {
                            "word_text": "riscossi"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "un"
                        },
                        {
                            "word_text": "momento,"
                        }
                    ]
                },
                {
                    "line_number": 38,
                    "line_text": "E fu breve anche in sogno il mio contento.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "fu"
                        },
                        {
                            "word_text": "breve"
                        },
                        {
                            "word_text": "anche"
                        },
                        {
                            "word_text": "in"
                        },
                        {
                            "word_text": "sogno"
                        },
                        {
                            "word_text": "il"
                        },
                        {
                            "word_text": "mio"
                        },
                        {
                            "word_text": "contento."
                        }
                    ]
                }
            ],
            "stanza_text": "Di solitaria fonte\nSul margo assiso al primo albore, o Fille,\nSognai d'esser con te. Sognai, ma in guisa\nChe sognar non credei. Garrir gli augelli,\nFrangersi l'acque e susurrar le foglie\nPareami udir. De' tuoi begli occhi al lume,\nCome suol per costume,\nFra' suoi palpiti usati era il cor mio.\nSol nel vederti, oh Dio!\nPietosa a me qual non ti vidi mai,\nDi sognar qualche volta io dubitai.\nQuai voci udii! Che dolci nomi ottenni,\nCara, da' labbri tuoi! Quali in quei molli\nTremuli rai teneri sensi io lessi!\nAh se mirar potessi\nQuanto splendan più belle\nFra i lampi di pietà le tue pupille,\nMai più crudel non mi saresti, o Fille.\nQual io divenni allora,\nQuel che allora io pensai, ciò che allor dissi,\nRidir non so. So che sul vivo latte\nDella tua mano io mille baci impressi;\nTu d'un vago rossor tingesti il volto.\nQuando improvviso ascolto\nD'un cespuglio vicin scuoter le fronde:\nMi volgo, e mezzo ascoso\nScopro il rival Fileno\nChe, d'invido veleno\nLivido in faccia, i furti miei rimira.\nFra la sorpresa e l'ira\nAvvampai, mi riscossi in un momento,\nE fu breve anche in sogno il mio contento."
