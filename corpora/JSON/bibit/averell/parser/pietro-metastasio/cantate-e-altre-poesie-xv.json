        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Voi, che dal primo istante,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
