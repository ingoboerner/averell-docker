        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Ah semplice ch'io sono! Io la cagione",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ah"
                        },
                        {
                            "word_text": "semplice"
                        },
                        {
                            "word_text": "ch'io"
                        },
                        {
