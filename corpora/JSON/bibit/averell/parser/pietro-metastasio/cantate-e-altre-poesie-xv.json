                        },
                        {
                            "word_text": "Altri"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "pena,"
                        }
                    ]
                },
                {
                    "line_number": 40,
                    "line_text": "Altri dice che muor. Lodano a gara",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Altri"
                        },
                        {
                            "word_text": "dice"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "muor."
                        },
                        {
                            "word_text": "Lodano"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "gara"
                        }
                    ]
                },
                {
                    "line_number": 41,
                    "line_text": "Questo i labbri vermigli,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Questo"
                        },
                        {
                            "word_text": "i"
