                        {
                            "word_text": "Pensava"
                        },
                        {
                            "word_text": "allora"
                        }
                    ]
                },
                {
                    "line_number": 33,
                    "line_text": "A custodir le sue conquiste: e forse",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
                            "word_text": "custodir"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "sue"
                        },
                        {
                            "word_text": "conquiste:"
                        },
                        {
