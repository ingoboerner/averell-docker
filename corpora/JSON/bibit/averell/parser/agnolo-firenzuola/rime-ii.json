                            "word_text": "Vi"
                        },
                        {
                            "word_text": "fa"
                        },
                        {
                            "word_text": "parer"
                        },
                        {
                            "word_text": "coteste"
                        },
                        {
                            "word_text": "vostre"
                        },
                        {
                            "word_text": "guance"
                        }
                    ]
                },
                {
                    "line_number": 27,
                    "line_text": "Lustranti e tonde com'un paternostro;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Lustranti"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "tonde"
                        },
                        {
                            "word_text": "com'un"
                        },
                        {
                            "word_text": "paternostro;"
                        }
                    ]
                },
                {
                    "line_number": 28,
                    "line_text": "E' vi farieno intorno manco ciance",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
