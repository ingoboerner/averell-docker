            "lines": [
                {
                    "line_number": 1,
                    "line_text": "A che andar sì superba, o Verdespina,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "A"
                        },
                        {
