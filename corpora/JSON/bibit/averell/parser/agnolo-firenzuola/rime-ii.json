                        },
                        {
                            "word_text": "avreste"
                        }
                    ]
                },
                {
                    "line_number": 30,
                    "line_text": "Men pinocchiati e manco melarance.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Men"
                        },
                        {
                            "word_text": "pinocchiati"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "manco"
                        },
                        {
                            "word_text": "melarance."
                        }
                    ]
                },
                {
                    "line_number": 31,
                    "line_text": "Ché 'l favor che voi avete in su le feste,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ché"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "favor"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "voi"
                        },
                        {
                            "word_text": "avete"
                        },
