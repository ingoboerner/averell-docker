                        },
                        {
                            "word_text": "ciance"
                        }
                    ]
                },
                {
                    "line_number": 29,
                    "line_text": "E vagheggini, e con la neve avreste",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "vagheggini,"
                        },
                        {
                            "word_text": "e"
