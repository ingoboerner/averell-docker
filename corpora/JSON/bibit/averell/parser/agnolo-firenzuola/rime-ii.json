                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "suo"
                        },
                        {
                            "word_text": "sacro"
                        },
                        {
                            "word_text": "inchiostro"
                        }
                    ]
                },
                {
                    "line_number": 26,
                    "line_text": "Vi fa parer coteste vostre guance",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
