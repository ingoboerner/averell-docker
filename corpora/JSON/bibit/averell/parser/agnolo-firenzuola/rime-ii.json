                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "pianelle."
                        }
                    ]
                },
                {
                    "line_number": 16,
                    "line_text": "Ben cova l'uova in altra paglia Amore,",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ben"
                        },
                        {
                            "word_text": "cova"
                        },
                        {
                            "word_text": "l'uova"
