                        },
                        {
                            "word_text": "musa"
                        },
                        {
                            "word_text": "e"
                        },
                        {
                            "word_text": "sì"
                        },
                        {
                            "word_text": "divina?"
                        }
                    ]
                },
                {
                    "line_number": 4,
                    "line_text": "E a che star, Selvaggia, sì gonfiata",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "E"
                        },
                        {
                            "word_text": "a"
                        },
                        {
                            "word_text": "che"
                        },
                        {
                            "word_text": "star,"
                        },
                        {
                            "word_text": "Selvaggia,"
