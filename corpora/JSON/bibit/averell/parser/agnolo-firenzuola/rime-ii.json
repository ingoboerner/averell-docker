                        },
                        {
                            "word_text": "poppe:"
                        }
                    ]
                },
                {
                    "line_number": 18,
                    "line_text": "Ben per altre si spasma, e sì si muore;",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Ben"
                        },
                        {
                            "word_text": "per"
                        },
                        {
                            "word_text": "altre"
                        },
                        {
                            "word_text": "si"
                        },
                        {
                            "word_text": "spasma,"
