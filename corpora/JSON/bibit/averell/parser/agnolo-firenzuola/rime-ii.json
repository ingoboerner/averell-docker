                        },
                        {
                            "word_text": "divario;"
                        }
                    ]
                },
                {
                    "line_number": 10,
                    "line_text": "Che sarà poi, se ben son ostro e latte",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "sarà"
                        },
