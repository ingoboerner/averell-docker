                        },
                        {
                            "word_text": "cantata"
                        }
                    ]
                },
                {
                    "line_number": 3,
                    "line_text": "È da sì dolce musa e sì divina?",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "È"
                        },
                        {
                            "word_text": "da"
                        },
                        {
                            "word_text": "sì"
                        },
