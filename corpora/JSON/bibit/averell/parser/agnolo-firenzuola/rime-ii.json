                            "word_text": "in"
                        },
                        {
                            "word_text": "su"
                        },
                        {
                            "word_text": "le"
                        },
                        {
                            "word_text": "feste,"
                        }
                    ]
                },
                {
                    "line_number": 32,
                    "line_text": "È la cagion perché voi tre vi siate",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "È"
                        },
                        {
                            "word_text": "la"
                        },
                        {
                            "word_text": "cagion"
                        },
                        {
                            "word_text": "perché"
                        },
                        {
                            "word_text": "voi"
                        },
                        {
                            "word_text": "tre"
                        },
                        {
