                        },
                        {
                            "word_text": "fa"
                        },
                        {
                            "word_text": "'l"
                        },
                        {
                            "word_text": "vostro,"
                        }
                    ]
                },
                {
                    "line_number": 24,
                    "line_text": "Che sa far d'un prun boccio un ramerino.",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Che"
                        },
                        {
                            "word_text": "sa"
                        },
                        {
                            "word_text": "far"
                        },
                        {
