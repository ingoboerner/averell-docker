                        }
                    ]
                },
                {
                    "line_number": 23,
                    "line_text": "Perché sì tosto, oimè! lasciata m'hai?",
                    "metrical_pattern": null,
                    "line_length": null,
                    "words": [
                        {
                            "word_text": "Perché"
                        },
                        {
                            "word_text": "sì"
                        },
                        {
