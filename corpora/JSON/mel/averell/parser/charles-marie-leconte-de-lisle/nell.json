            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Ta rose de pourpre, à ton clair soleil,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Penche aussi vers moi ta coupe dorée :",
