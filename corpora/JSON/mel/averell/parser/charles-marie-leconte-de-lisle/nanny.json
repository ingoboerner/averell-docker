    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Bois chers aux ramiers, pleurez, doux feuillages,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Et toi, source vive, et vous, frais sentiers ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Bois chers aux ramiers, pleurez, doux feuillages,\nEt toi, source vive, et vous, frais sentiers ;"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Du courlis siffleur l'aube saluée",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Suspend au brin d'herbe une perle en feu ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Du courlis siffleur l'aube saluée\nSuspend au brin d'herbe une perle en feu ;"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Pleurez, ô courlis ; pleure, blanche aurore ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Gémissez, lac bleu, poules, coqs pourprés ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pleurez, ô courlis ; pleure, blanche aurore ;\nGémissez, lac bleu, poules, coqs pourprés ;"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Printemps, roi fleuri de la verte année,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Ô jeune dieu, pleure ! Été mûrissant,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Printemps, roi fleuri de la verte année,\nÔ jeune dieu, pleure ! Été mûrissant,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "L'angoisse d'aimer brise un cœur fidèle.",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Terre et ciel, pleurez : oh ! Que je l'aimais !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'angoisse d'aimer brise un cœur fidèle.\nTerre et ciel, pleurez : oh ! Que je l'aimais !"
        }
    ],
    "corpus": "mel"
