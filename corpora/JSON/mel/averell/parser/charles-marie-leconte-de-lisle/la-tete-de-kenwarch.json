                {
                    "line_number": 1,
                    "line_text": "LOIN du Cap de Penn'hor, où hurlait la mêlée",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Sombre comme le rire amer des grandes Eaux,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Bonds sur bonds, queue au vent, crinière échevelée,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Va ! cours, mon bon cheval, en ronflant des naseaux.",
