        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "Le divin Bouvier des monts de Phrygie",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Goûte, les yeux clos, l'éternel sommeil ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 36,
                    "line_text": "Et de son beau corps, dans l'herbe rougie,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Le divin Bouvier des monts de Phrygie\nGoûte, les yeux clos, l'éternel sommeil ;\nEt de son beau corps, dans l'herbe rougie,"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "En boucles de lin, sur la pâle joue",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Qu'enviaient les fruits honneur des vergers,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 39,
                    "line_text": "Tombent, du réseau pourpré qui les noue,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "En boucles de lin, sur la pâle joue\nQu'enviaient les fruits honneur des vergers,\nTombent, du réseau pourpré qui les noue,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "Voici Kythèrè, l'amante immortelle,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "Qui gémit et pleure auprès du Bouvier.",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 42,
                    "line_text": "Les Éros chasseurs tiennent devant elle",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Voici Kythèrè, l'amante immortelle,\nQui gémit et pleure auprès du Bouvier.\nLes Éros chasseurs tiennent devant elle"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Lui, pour expier d'amères offenses,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "D'un autel qui fume attisant le feu,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 45,
                    "line_text": "Consume et punit ses blanches défenses",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Lui, pour expier d'amères offenses,\nD'un autel qui fume attisant le feu,\nConsume et punit ses blanches défenses"
        }
