{
    "poem_title": "Le Baiser I",
    "author": "Germain Nouveau",
    "structure": "suite périodique",
    "year": "1922",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "N’êtes-vous pas toute petite",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Dans votre vaste appartement,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Où comme un oiseau qui palpite",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Voltige votre pied normand ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "N’êtes-vous pas toute petite\nDans votre vaste appartement,\nOù comme un oiseau qui palpite\nVoltige votre pied normand ?"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "N’est-elle pas toute mignonne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Blanche dans l’ombre où tu souris,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Votre taille qui s’abandonne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Parisienne de Paris ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "N’est-elle pas toute mignonne,\nBlanche dans l’ombre où tu souris,\nVotre taille qui s’abandonne,\nParisienne de Paris ?"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "N’est-il pas à Vous, pleine d’âme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Franc comme on doit l’être, à l’excès,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Votre cœur d’adorable femme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Nu, comme votre corps français ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "N’est-il pas à Vous, pleine d’âme,\nFranc comme on doit l’être, à l’excès,\nVotre cœur d’adorable femme,\nNu, comme votre corps français ?"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Ne sont-ils pas, à Vous si fière,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Les neiges sous la nuit qui dort",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "Dans leur silence et leur lumière,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Vos magnifiques seins du Nord ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ne sont-ils pas, à Vous si fière,\nLes neiges sous la nuit qui dort\nDans leur silence et leur lumière,\nVos magnifiques seins du Nord ?"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "N’est-il pas doux, à Vous sans haine,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Frémissante aux bruits de l’airain,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "Votre ventre d’Européenne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Oui votre ventre européen ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "N’est-il pas doux, à Vous sans haine,\nFrémissante aux bruits de l’airain,\nVotre ventre d’Européenne,\nOui votre ventre européen ;"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "N’est-elle pas semblable au Monde,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Pareille au globe entouré d’air,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 23,
                    "line_text": "Ta croupe terrestre aussi ronde",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Que la montagne et que la mer ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "N’est-elle pas semblable au Monde,\nPareille au globe entouré d’air,\nTa croupe terrestre aussi ronde\nQue la montagne et que la mer ?"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "N’est-il pas infini le râle",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "De bonheur pur comme le sel,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 27,
                    "line_text": "Dans ta matrice interastrale",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Sous ton baiser universel ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "N’est-il pas infini le râle\nDe bonheur pur comme le sel,\nDans ta matrice interastrale\nSous ton baiser universel ?"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Et par la foi qui me fait vivre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Dans ton parfum et dans ton jour,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 31,
                    "line_text": "N’entre-t-elle pas, mon âme ivre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "En plein, au plein de ton amour ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et par la foi qui me fait vivre\nDans ton parfum et dans ton jour,\nN’entre-t-elle pas, mon âme ivre,\nEn plein, au plein de ton amour ?"
        }
