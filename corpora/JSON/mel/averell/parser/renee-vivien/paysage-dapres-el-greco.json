{
    "poem_title": "PAYSAGE D’APRÈS EL GRECO",
    "author": "Renée Vivien",
    "structure": "suite périodique",
    "year": "1904",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Parmi le boréal silence, le zénith",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Irradie âprement aux jardins d’aconit.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Parmi le boréal silence, le zénith\nIrradie âprement aux jardins d’aconit."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Énigmes et remords, les yeux des Nyctalopes",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Reflètent la perplexité des horoscopes,",
                    "metrical_pattern": null,
                    "line_length": "8+4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Énigmes et remords, les yeux des Nyctalopes\nReflètent la perplexité des horoscopes,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Et les Musiciens, frères des Séraphim,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Écoutent murmurer la harpe d’Éloïm.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et les Musiciens, frères des Séraphim,\nÉcoutent murmurer la harpe d’Éloïm."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "De glauques nénuphars charment le regard fixe",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "D’une perverse Ondine éprise d’une Nixe.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "De glauques nénuphars charment le regard fixe\nD’une perverse Ondine éprise d’une Nixe."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Et l’écho jette au vent le rire des sabbats,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "L’effroi des lits pareils à des champs de combats.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et l’écho jette au vent le rire des sabbats,\nL’effroi des lits pareils à des champs de combats."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Les tentes d’écarlate où dorment les bourrasques",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Crèvent sur le repos seigneurial des vasques.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Les tentes d’écarlate où dorment les bourrasques\nCrèvent sur le repos seigneurial des vasques."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Trouant l’opacité démente, le zénith",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Irradie âprement aux jardins d’aconit.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Trouant l’opacité démente, le zénith\nIrradie âprement aux jardins d’aconit."
        }
    ],
