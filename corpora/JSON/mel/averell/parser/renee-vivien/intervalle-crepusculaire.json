{
    "poem_title": "INTERVALLE CRÉPUSCULAIRE",
    "author": "Renée Vivien",
    "structure": "suite périodique",
    "year": "1904",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tes yeux sous tes cheveux sont comme des poignées",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "De rayons à travers des toiles d’araignées.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tes yeux sous tes cheveux sont comme des poignées\nDe rayons à travers des toiles d’araignées."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Ton sourire d’été, que l’aube colora,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Est pareil au sourire orgueilleux de Sara.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ton sourire d’été, que l’aube colora,\nEst pareil au sourire orgueilleux de Sara."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Mon regard s’hypnotise à cette fauve boucle",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Où le divin saphyr épouse l’escarboucle,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mon regard s’hypnotise à cette fauve boucle\nOù le divin saphyr épouse l’escarboucle,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Tes parfums indiens, tes onguents et tes fards",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Étonnent la candeur simple des nénuphars.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tes parfums indiens, tes onguents et tes fards\nÉtonnent la candeur simple des nénuphars."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "La haine de l’amour et l’amour de la haine",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Se partagent mon cœur et mon âme incertaine.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La haine de l’amour et l’amour de la haine\nSe partagent mon cœur et mon âme incertaine."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "La bienfaisante Mort montre d’un pâle index",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "La colline lunaire où blondit le silex.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La bienfaisante Mort montre d’un pâle index\nLa colline lunaire où blondit le silex."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Au lointain s’exaspère et s’exalte un arpège.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Je veux purifier mon âme dans la neige…",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Au lointain s’exaspère et s’exalte un arpège.\nJe veux purifier mon âme dans la neige…"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Vois, plus belle que le puéril Adonis,",
                    "metrical_pattern": null,
                    "line_length": "6−6",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Mourir Adonéa dans un linceul de lys.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Vois, plus belle que le puéril Adonis,\nMourir Adonéa dans un linceul de lys."
        }
    ],
