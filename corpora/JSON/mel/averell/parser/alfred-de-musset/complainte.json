        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Il avait trouvé en route",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
