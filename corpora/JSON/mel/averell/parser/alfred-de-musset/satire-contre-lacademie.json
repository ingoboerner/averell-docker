{
    "poem_title": "Satire contre l’Académie",
    "author": "Alfred de Musset",
    "structure": "",
    "year": "1857",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Hier s’ouvrit avec bienséance",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 2,
                    "line_text": "Qui fit l’auteur du Chandelier",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Hier s’ouvrit avec bienséance\nQui fit l’auteur du Chandelier"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Debout ruisselait comme un fleuve",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 4,
                    "line_text": "Dans un angle le beau Mignet",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Debout ruisselait comme un fleuve\nDans un angle le beau Mignet"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Dupin aîné, tribun honnête,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 6,
                    "line_text": "Rêvait de ses chers montagnards",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Dupin aîné, tribun honnête,\nRêvait de ses chers montagnards"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "On entendait, voix de crécelle,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 8,
                    "line_text": "Comme un vieux coq dans un jardin",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "On entendait, voix de crécelle,\nComme un vieux coq dans un jardin"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Grand Romain en habit de ville,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 10,
                    "line_text": "Semblait être à la fois César",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Grand Romain en habit de ville,\nSemblait être à la fois César"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Briffaut avait des soins de père",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 12,
                    "line_text": "Et roucoulait comme un ramier :",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Briffaut avait des soins de père\nEt roucoulait comme un ramier :"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Baour, sourd de ses vers qu’il beugle",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 14,
                    "line_text": "Allait chantant d’un ton sciant",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Baour, sourd de ses vers qu’il beugle\nAllait chantant d’un ton sciant"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Viennet disait d’un air affable",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 16,
                    "line_text": "On le trouvait bête, et Tissot",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Viennet disait d’un air affable\nOn le trouvait bête, et Tissot"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Cousin cherchait d’un air tragique",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 18,
                    "line_text": "Et tonnait, dévot éloquent,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Cousin cherchait d’un air tragique\nEt tonnait, dévot éloquent,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Un autre narrait la surprise",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 20,
                    "line_text": "il fallait bien qu’il s’amusât",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Un autre narrait la surprise\nil fallait bien qu’il s’amusât"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Mais soudain en trembla d’emblée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 22,
                    "line_text": "De par Bacchus ! c’était Musset",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Mais soudain en trembla d’emblée\nDe par Bacchus ! c’était Musset"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "« Crois-tu qu’on lise pour des prunes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 24,
                    "line_text": "Ton long poème peu commun,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "« Crois-tu qu’on lise pour des prunes\nTon long poème peu commun,"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Sois tranquille, la chaste muse",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 26,
                    "line_text": "Ne deviendrait jamais catin",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Sois tranquille, la chaste muse\nNe deviendrait jamais catin"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Nous montrant à la fois Narcisse",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 28,
                    "line_text": "Parleras-tu chaque jeudi,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Nous montrant à la fois Narcisse\nParleras-tu chaque jeudi,"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Quand tu reçus ta grosse épouse",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 30,
                    "line_text": "Tu ne gagnas pas le gros lot,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Quand tu reçus ta grosse épouse\nTu ne gagnas pas le gros lot,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Ajoutant à la platitude",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 32,
                    "line_text": "Tomberas-tu de mal en pis,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Ajoutant à la platitude\nTomberas-tu de mal en pis,"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Ne feras-tu donc rien qui vaille",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 34,
                    "line_text": "Depuis que j’ai lu Maintenon,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Ne feras-tu donc rien qui vaille\nDepuis que j’ai lu Maintenon,"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Sur ton dos, Riquet à la Houppe,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 36,
                    "line_text": "Tu ne suis pais ton droit chemin,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Sur ton dos, Riquet à la Houppe,\nTu ne suis pais ton droit chemin,"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Dans tes culottes sans bretelles,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 38,
                    "line_text": "Dis-moi, prolixe historien,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Dans tes culottes sans bretelles,\nDis-moi, prolixe historien,"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "Tu te crois donc, gendre de Dosne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 40,
                    "line_text": "D’un homme tu n’es pas le tiers,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Tu te crois donc, gendre de Dosne,\nD’un homme tu n’es pas le tiers,"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "De peur de devenir enceinte,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 42,
                    "line_text": "Se gare au lit — de son époux —",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "De peur de devenir enceinte,\nSe gare au lit — de son époux —"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Dans cette légende érotique",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 44,
                    "line_text": "Tu te montres, Montalembert,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Dans cette légende érotique\nTu te montres, Montalembert,"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Pédant entre tous les quarante,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 46,
                    "line_text": "J’ai ton froid récit bourguignon",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Pédant entre tous les quarante,\nJ’ai ton froid récit bourguignon"
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "Au loin va te faire lanlaire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 48,
                    "line_text": "Et redeviens ambassadeur",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Au loin va te faire lanlaire\nEt redeviens ambassadeur"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Pasquier, chez madame de Boigne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 50,
                    "line_text": "Console-toi, près d’un bon feu,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Pasquier, chez madame de Boigne,\nConsole-toi, près d’un bon feu,"
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Aux vieux chats de l’ancienne Chambre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 52,
                    "line_text": "Vieux rat, tu fus donc immolé,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Aux vieux chats de l’ancienne Chambre\nVieux rat, tu fus donc immolé,"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Guizot, d’une autre dynastie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 54,
                    "line_text": "Flattant Berryer, tu prends pour saint",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Guizot, d’une autre dynastie\nFlattant Berryer, tu prends pour saint"
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Flourens, dans ton Jardin des Plantes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 56,
                    "line_text": "Pour garder ta longévité",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Flourens, dans ton Jardin des Plantes\nPour garder ta longévité"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Scribe, vrai scribe, par douzaines",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 58,
                    "line_text": "Bâcle des Bertrand et Raton,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Scribe, vrai scribe, par douzaines\nBâcle des Bertrand et Raton,"
        },
        {
            "stanza_number": 30,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 59,
                    "line_text": "Lorsque ta verve est comprimée,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 60,
                    "line_text": "Bayle te sert à nier Dieu,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Lorsque ta verve est comprimée,\nBayle te sert à nier Dieu,"
        },
        {
            "stanza_number": 31,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Nous trouvant un peuple servile,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 62,
                    "line_text": "Aux radotages de Franklin",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Nous trouvant un peuple servile,\nAux radotages de Franklin"
        },
        {
            "stanza_number": 32,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 63,
                    "line_text": "Sage et mou, dans sa pâle prose,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 64,
                    "line_text": "J’ai deviné ce que Vitet",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Sage et mou, dans sa pâle prose,\nJ’ai deviné ce que Vitet"
        },
        {
            "stanza_number": 33,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Vigny, berger de sa montagne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 66,
                    "line_text": "Soufflant dans ses plus doux pipeaux,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Vigny, berger de sa montagne,\nSoufflant dans ses plus doux pipeaux,"
        },
        {
            "stanza_number": 34,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 67,
                    "line_text": "Hugo, dans sa verve énergique,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 68,
                    "line_text": "Nous a lancé comme un soufflet",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Hugo, dans sa verve énergique,\nNous a lancé comme un soufflet"
        },
        {
            "stanza_number": 35,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 69,
                    "line_text": "Chaque jour leur chantant matines,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 70,
                    "line_text": "Rappelle à ses chers souscripteurs",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Chaque jour leur chantant matines,\nRappelle à ses chers souscripteurs"
        }
