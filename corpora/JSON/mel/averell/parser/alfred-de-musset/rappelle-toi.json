                {
                    "line_number": 1,
                    "line_text": "A l'appel du plaisir lorsque ton sein palpite,",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Aux doux songes du soir lorsque l'ombre l'invite,",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "b"
