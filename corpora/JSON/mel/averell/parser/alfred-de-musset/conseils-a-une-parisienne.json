{
    "poem_title": "CONSEILS A UNE PARISIENNE",
    "author": "Alfred de Musset",
    "structure": "suite périodique",
    "year": "1836-1852",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Oui, si j'étais femme, aimable et jolie,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Sans peur ni pitié, sans choix ni mystère,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Oui, si j'étais femme, aimable et jolie,\nSans peur ni pitié, sans choix ni mystère,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Je voudrais n'avoir de soucis au monde",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "El de pied en cap être la poupée",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je voudrais n'avoir de soucis au monde\nEl de pied en cap être la poupée"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Je voudrais garder pour toute science",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Joindre, comme vous, à l'étourderie,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je voudrais garder pour toute science\nJoindre, comme vous, à l'étourderie,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Je voudrais pour moi qu'il fût toujours fête,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Être en même temps de glace et de flamme,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je voudrais pour moi qu'il fût toujours fête,\nÊtre en même temps de glace et de flamme,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Je délesterais, avant toute chose,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Je rayonnerais, sous ma tresse brune,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je délesterais, avant toute chose,\nJe rayonnerais, sous ma tresse brune,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Car c'est si charmant, et c'est si commode,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Ah ! que la pâleur est d'un bel usage !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Car c'est si charmant, et c'est si commode,\nAh ! que la pâleur est d'un bel usage !"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Je voudrais encore avoir vos caprices,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Je voudrais enfin, tant mon cœur vous aime,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je voudrais encore avoir vos caprices,\nJe voudrais enfin, tant mon cœur vous aime,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Il est un seul point, je vous le confesse,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Vous n'osez pas être assez inhumaine.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Il est un seul point, je vous le confesse,\nVous n'osez pas être assez inhumaine."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Je ne voudrais pas, à la contredanse,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Puis, au cotillon, laisser ma main blanche",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je ne voudrais pas, à la contredanse,\nPuis, au cotillon, laisser ma main blanche"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Si mon fin corset, si souple et si juste,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "J'aurais, je l'avoue, une peur mortelle",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Si mon fin corset, si souple et si juste,\nJ'aurais, je l'avoue, une peur mortelle"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Chacun, en valsant, vient sur votre épaule",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Ma beauté, du moins, sinon ma pensée,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Chacun, en valsant, vient sur votre épaule\nMa beauté, du moins, sinon ma pensée,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Je ne voudrais pas, si j'étais Julie,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Jusqu'au bout des doigts je serais duchesse.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je ne voudrais pas, si j'étais Julie,\nJusqu'au bout des doigts je serais duchesse."
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Voyez-vous, ma chère, au siècle où nous sommes,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Sur deux amoureux pleins d'un zèle extrême,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Voyez-vous, ma chère, au siècle où nous sommes,\nSur deux amoureux pleins d'un zèle extrême,"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Quand on est coquette, il faut être sage.",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Ne dort pas en l'air comme une hirondelle,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Quand on est coquette, il faut être sage.\nNe dort pas en l'air comme une hirondelle,"
        }
    ],
    "corpus": "mel"
}