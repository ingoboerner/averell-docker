{
    "poem_title": "Le Voyage à Pontchartrain",
    "author": "Alfred de Musset",
    "structure": "suite périodique",
    "year": "1824-1857",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Paul, un soir, par la gauche rive",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Croyant voir Madame Aubernon,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Paul, un soir, par la gauche rive\nCroyant voir Madame Aubernon,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Où faut-il en quittant Versaille",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Retrouver Hetzel à Meudon ?",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Où faut-il en quittant Versaille\nRetrouver Hetzel à Meudon ?"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Hetzel, dînant sur la pelouse,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Régalait un de ses amis",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Hetzel, dînant sur la pelouse,\nRégalait un de ses amis"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "La compagnie offre une prise,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "On sert au convive nouveau",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La compagnie offre une prise,\nOn sert au convive nouveau"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Mais, dit Hetzel, cassant sa croûte,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Pour voir Montfort et Pontchartrain",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais, dit Hetzel, cassant sa croûte,\nPour voir Montfort et Pontchartrain"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Je crois, dit Paul, que l’on m’invite",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Ce n’est pas d’aller à Montfort",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je crois, dit Paul, que l’on m’invite\nCe n’est pas d’aller à Montfort"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Sur un cheval ou sur un âne",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Mais, dit Hetzel, nous n’irons pas",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Sur un cheval ou sur un âne\nMais, dit Hetzel, nous n’irons pas"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Je vais tirer de ma sacoche",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je vais tirer de ma sacoche"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Paul accède, et, bravant la Parque,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Il quitte pour faire sept lieues",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Paul accède, et, bravant la Parque,\nIl quitte pour faire sept lieues"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 18,
                    "line_text": "— Obeuf, je trouve que ta hotte",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 19,
                    "line_text": "Nous sommes comme des harengs",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "— Obeuf, je trouve que ta hotte\nNous sommes comme des harengs"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 20,
                    "line_text": "Mais, laisserons-nous dans l’attente",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 21,
                    "line_text": "Dit Obeuf ; j’ai d’un souper froid",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais, laisserons-nous dans l’attente\nDit Obeuf ; j’ai d’un souper froid"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Hetzel, tranquille et sans rancune",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Dit : J’ai, ma foi, dans ce réchaud",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Hetzel, tranquille et sans rancune\nDit : J’ai, ma foi, dans ce réchaud"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 24,
                    "line_text": "Le coche près d’une charrette",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 25,
                    "line_text": "O spectacle ! on découvre au loin",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Le coche près d’une charrette\nO spectacle ! on découvre au loin"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 26,
                    "line_text": "Mais, déjà, sur la nappe blanche,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Fumait, écrasant de son poids",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais, déjà, sur la nappe blanche,\nFumait, écrasant de son poids"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Et, couvrant d’un vin délectable",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 29,
                    "line_text": "Une jeune enfant, douce à voir,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et, couvrant d’un vin délectable\nUne jeune enfant, douce à voir,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 30,
                    "line_text": "Le front baissé sous sa cornette",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 31,
                    "line_text": "Faisait froufrou de son jupon",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Le front baissé sous sa cornette\nFaisait froufrou de son jupon"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 32,
                    "line_text": "— Messieurs, dit avec politesse",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 33,
                    "line_text": "Vous aviez deux coussins étroits",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "— Messieurs, dit avec politesse\nVous aviez deux coussins étroits"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "— Non pas, dit Hetzel : sur mon âme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "J’ai trouvé ce cabriolet",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "— Non pas, dit Hetzel : sur mon âme,\nJ’ai trouvé ce cabriolet"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 36,
                    "line_text": "Mais Obeuf comme une torpille",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 37,
                    "line_text": "— Tu t’en vas déjà te coucher,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais Obeuf comme une torpille\n— Tu t’en vas déjà te coucher,"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 38,
                    "line_text": "Paul pourfend comme une flamberge",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Hetzel va dans le poulailler",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Paul pourfend comme une flamberge\nHetzel va dans le poulailler"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "Aussitôt viennent les punaises,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "De pouvoir d’un jeune étranger",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Aussitôt viennent les punaises,\nDe pouvoir d’un jeune étranger"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 42,
                    "line_text": "Mais Hetzel, trouvant l’Estafette",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 43,
                    "line_text": "Lit jusqu’au jour ce matinal",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais Hetzel, trouvant l’Estafette\nLit jusqu’au jour ce matinal"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 44,
                    "line_text": "Dans son lit, Paul, dont le nez gonfle",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 45,
                    "line_text": "Donne au Diable tous ces taudis",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Dans son lit, Paul, dont le nez gonfle\nDonne au Diable tous ces taudis"
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 46,
                    "line_text": "Un roulier, tenant sa chandelle",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Le réveille avec ses sabots",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Un roulier, tenant sa chandelle\nLe réveille avec ses sabots"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 48,
                    "line_text": "Mais déjà dans la cheminée,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 49,
                    "line_text": "Voit ses enfants effarouchés",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais déjà dans la cheminée,\nVoit ses enfants effarouchés"
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 50,
                    "line_text": "Et sur la gouttière que dore",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Fait sa toilette un freluquet",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et sur la gouttière que dore\nFait sa toilette un freluquet"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 52,
                    "line_text": "Paul, se penchant à sa croisée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 53,
                    "line_text": "Découvre Hetzel, sous un hangard,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Paul, se penchant à sa croisée\nDécouvre Hetzel, sous un hangard,"
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 54,
                    "line_text": "— Oh ! dit-il, l’air vous enlumine",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 55,
                    "line_text": "Vous n’avez pas très bien dormi,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "— Oh ! dit-il, l’air vous enlumine\nVous n’avez pas très bien dormi,"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 56,
                    "line_text": "— J’ai, dit Hetzel, fait un bon somme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 57,
                    "line_text": "Mais je me suis levé matin,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "— J’ai, dit Hetzel, fait un bon somme,\nMais je me suis levé matin,"
        },
        {
            "stanza_number": 30,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 58,
                    "line_text": "Obeuf, devant son haridelle",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 59,
                    "line_text": "Sous l’enseigne d’un cabaret",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Obeuf, devant son haridelle\nSous l’enseigne d’un cabaret"
        },
        {
            "stanza_number": 31,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 60,
                    "line_text": "Adieu, vallons, coteaux, campagnes,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 61,
                    "line_text": "Paul rentre sur ses échalas",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Adieu, vallons, coteaux, campagnes,\nPaul rentre sur ses échalas"
        },
        {
            "stanza_number": 32,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 62,
                    "line_text": "Et, de retour, dans sa chambrette",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 63,
                    "line_text": "Il trouve, sur son canapé",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et, de retour, dans sa chambrette\nIl trouve, sur son canapé"
        },
        {
            "stanza_number": 33,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 64,
                    "line_text": "Bonnaire, qui, sombre, à peine ivre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 65,
                    "line_text": "A d’inconséquents et fréquents",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Bonnaire, qui, sombre, à peine ivre,\nA d’inconséquents et fréquents"
        }
