                {
                    "line_number": 1,
                    "line_text": "Gouttes d'extase, éplorement délicieux,",
                    "metrical_pattern": null,
                    "line_length": "4+4+4",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Tombez des nuits ! Tombez des fleurs ! Tombez des yeux !",
                    "metrical_pattern": null,
