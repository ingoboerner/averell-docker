    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "La lyre du pendule à peine se balance.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La lyre du pendule à peine se balance."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 2,
                    "line_text": "Au vague, sur le lac enchanté du Silence.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Au vague, sur le lac enchanté du Silence."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Au long des fils vibrants se prolonge et se pâme.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Au long des fils vibrants se prolonge et se pâme."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "En ondes de langueur s'élargit dans notre âme.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "En ondes de langueur s'élargit dans notre âme."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Une rose blessée et penchante agonise ;",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Une rose blessée et penchante agonise ;"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 6,
                    "line_text": "Comme une extase en nous monte et se divinise.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Comme une extase en nous monte et se divinise."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Comme un dais triomphal, pour des pompes célèbres",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Comme un dais triomphal, pour des pompes célèbres"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 8,
                    "line_text": "Médite obscurément nos baisers des ténèbres.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Médite obscurément nos baisers des ténèbres."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "De ton parfum subtil, obsesseur et complexe.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "De ton parfum subtil, obsesseur et complexe."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Fleur tiède épanouie au soleil de ton sexe.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Fleur tiède épanouie au soleil de ton sexe."
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "M'ouvrent les profondeurs des verts mélancoliques,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "M'ouvrent les profondeurs des verts mélancoliques,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Font trembler tout au fond des flammes symboliques.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Font trembler tout au fond des flammes symboliques."
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "De l'heure disparue, et du mal et des peines ;",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "De l'heure disparue, et du mal et des peines ;"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 14,
                    "line_text": "Notre amour se pénètre au plus fin de nos veines.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Notre amour se pénètre au plus fin de nos veines."
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Au-dessus de la terre ironique et brutale,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Au-dessus de la terre ironique et brutale,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Révélés à la Vie Unique et Musicale…",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Révélés à la Vie Unique et Musicale…"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Que ce soit un secret vaporeux qu'on devine,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Que ce soit un secret vaporeux qu'on devine,"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 18,
                    "line_text": "Comme une haleine d'ange en un duvet d'hermine.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Comme une haleine d'ange en un duvet d'hermine."
        }
    ],
