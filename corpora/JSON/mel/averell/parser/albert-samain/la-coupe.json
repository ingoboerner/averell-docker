                {
                    "line_number": 1,
                    "line_text": "Au temps des Immortels, fils de la vie en fête,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Où la Lyre élevait les assises des tours.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Un artisan sacré modela mes contours",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Sur le sein d'une vierge, entre ses sœurs parfaite.",
                    "metrical_pattern": null,
