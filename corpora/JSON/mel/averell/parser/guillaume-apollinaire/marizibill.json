                },
                {
                    "line_number": 7,
                    "line_text": "Pour un maquereau roux et rose",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 8,
                    "line_text": "C'était un juif il sentait l'ail",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 9,
                    "line_text": "Et l'avait venant de Formose",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": ""
                },
                {
                    "line_number": 10,
                    "line_text": "Tirée d'un bordel de Changaï",
                    "metrical_pattern": null,
