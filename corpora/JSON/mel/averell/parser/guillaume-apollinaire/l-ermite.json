            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Un ermite déchaux près d'un crâne blanchi",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": ""
                },
                {
                    "line_number": 2,
                    "line_text": "Cria Je vous maudis martyres et détresses",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": ""
                },
                {
                    "line_number": 3,
                    "line_text": "Trop de tentations malgré moi me caressent",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": ""
                },
                {
                    "line_number": 4,
                    "line_text": "Tentations de lune et de logomachies",
