        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Sur un ton monotone",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
