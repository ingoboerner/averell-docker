                {
                    "line_number": 1,
                    "line_text": "Oui, l'œuvre sort plus belle",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "D'une forme au travail",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Vers, marbre, onyx, émail.",
