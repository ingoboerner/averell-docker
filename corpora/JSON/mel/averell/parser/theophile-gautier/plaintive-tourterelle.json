    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Plaintive tourterelle,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Qui roucoules toujours,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Veux-tu prêter ton aile",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Pour servir mes amours !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Plaintive tourterelle,\nQui roucoules toujours,\nVeux-tu prêter ton aile\nPour servir mes amours !"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Comme toi, pauvre amante,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Bien loin de mon ramier,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Je pleure et me lamente",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Sans pouvoir l'oublier.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Comme toi, pauvre amante,\nBien loin de mon ramier,\nJe pleure et me lamente\nSans pouvoir l'oublier."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Vole et que ton pied rose",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Sur l'arbre ou sur la tour",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Jamais ne se repose,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Car je languis d'amour.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Vole et que ton pied rose\nSur l'arbre ou sur la tour\nJamais ne se repose,\nCar je languis d'amour."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Évite, ô ma colombe,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "La halte des palmiers",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "Et tous les toits où tombe",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "La neige des ramiers.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Évite, ô ma colombe,\nLa halte des palmiers\nEt tous les toits où tombe\nLa neige des ramiers."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Va droit sur sa fenêtre,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Près du palais du roi,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "Donne-lui cette lettre",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Et deux baisers pour moi.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Va droit sur sa fenêtre,\nPrès du palais du roi,\nDonne-lui cette lettre\nEt deux baisers pour moi."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Puis sur mon sein en flamme",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Qui ne peut s'apaiser,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 23,
                    "line_text": "Reviens, avec son âme,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Reviens te reposer.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puis sur mon sein en flamme\nQui ne peut s'apaiser,\nReviens, avec son âme,\nReviens te reposer."
        }
    ],
