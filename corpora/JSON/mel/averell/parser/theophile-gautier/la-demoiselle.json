{
    "poem_title": "LA DEMOISELLE",
    "author": "Théophile Gautier",
    "structure": "suite périodique",
    "year": "1833",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Sur la bruyère arrosée",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Sur le buisson d'églantier ;",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Sur les ombreuses futaies ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Croissant au bord du sentier ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Sur la bruyère arrosée\nSur le buisson d'églantier ;\nSur les ombreuses futaies ;\nCroissant au bord du sentier ;"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Sur la modeste et petite",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Qui penche son front rêvant ;",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 7,
                    "line_text": "Sur le seigle, verte houle",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Le caprice ailé du vent ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Sur la modeste et petite\nQui penche son front rêvant ;\nSur le seigle, verte houle\nLe caprice ailé du vent ;"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Sur les prés, sur la colline",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Vers le champ bariolé",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "De pittoresques guirlandes ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Sur le grand orme isolé ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Sur les prés, sur la colline\nVers le champ bariolé\nDe pittoresques guirlandes ;\nSur le grand orme isolé ;"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "La demoiselle se berce ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Dans la bruine, au bord du ciel,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Un rayon d'or qui scintille,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "Comme un regard d'Ariel.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "La demoiselle se berce ;\nDans la bruine, au bord du ciel,\nUn rayon d'or qui scintille,\nComme un regard d'Ariel."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Traversant près des charmilles,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Des bourdonnants moucherons,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 19,
                    "line_text": "Elle se mêle à leur ronde",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 20,
                    "line_text": "Et comme eux décrit des ronds.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Traversant près des charmilles,\nDes bourdonnants moucherons,\nElle se mêle à leur ronde\nEt comme eux décrit des ronds."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Bientôt elle vole et joue",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Du jet d'eau qui, s'élançant",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Dans les airs, retombe, roule",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "En un ruisseau bruissant.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Bientôt elle vole et joue\nDu jet d'eau qui, s'élançant\nDans les airs, retombe, roule\nEn un ruisseau bruissant."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Plus rapide que la brise,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Dans son vol capricieux,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "L'eau transparente où se mire",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 28,
                    "line_text": "Le saule au front soucieux ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Plus rapide que la brise,\nDans son vol capricieux,\nL'eau transparente où se mire\nLe saule au front soucieux ;"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Où, s'entr'ouvrant blancs et jaunes,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Les deux nénuphars en fleurs,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 31,
                    "line_text": "Au gré du flot qui gazouille",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 32,
                    "line_text": "Étalent leurs deux couleurs ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Où, s'entr'ouvrant blancs et jaunes,\nLes deux nénuphars en fleurs,\nAu gré du flot qui gazouille\nÉtalent leurs deux couleurs ;"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Où se baigne le nuage,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Le ciel d'été souriant ;",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Où le soleil plonge, tremble,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 36,
                    "line_text": "Au beau soleil d'Orient.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Où se baigne le nuage,\nLe ciel d'été souriant ;\nOù le soleil plonge, tremble,\nAu beau soleil d'Orient."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Et quand la grise hirondelle",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Passe, et ride à plis d'azur,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Dans sa chasse circulaire,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 40,
                    "line_text": "Elle s'enfuit d'un vol sûr.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et quand la grise hirondelle\nPasse, et ride à plis d'azur,\nDans sa chasse circulaire,\nElle s'enfuit d'un vol sûr."
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Bois qui chantent, fraîches plaines",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Lacs de moire, coteaux bleus,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 43,
                    "line_text": "Ciel où le nuage passe,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 44,
                    "line_text": "Monts aux rochers anguleux ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Bois qui chantent, fraîches plaines\nLacs de moire, coteaux bleus,\nCiel où le nuage passe,\nMonts aux rochers anguleux ;"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Voilà l'immense domaine",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Ses caprices, fleur des airs,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "La demoiselle nacrée,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 48,
                    "line_text": "De reflets roses et verts.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Voilà l'immense domaine\nSes caprices, fleur des airs,\nLa demoiselle nacrée,\nDe reflets roses et verts."
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Dans son étroite famille,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "N'a pas vingt fois souhaité,",
                    "metrical_pattern": null,
                    "line_length": "3",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Rêveuse, d'être comme elle",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                },
                {
                    "line_number": 52,
                    "line_text": "Demoiselle en liberté ?",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Dans son étroite famille,\nN'a pas vingt fois souhaité,\nRêveuse, d'être comme elle\nDemoiselle en liberté ?"
        }
    ],
    "corpus": "mel"
}