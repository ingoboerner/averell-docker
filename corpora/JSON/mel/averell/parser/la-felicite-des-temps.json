    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Est-il encor des satiriques",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Qui, du présent toujours blessés,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Dans leurs malins panégyriques",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Exaltent les siècles passés ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 5,
                    "line_text": "Qui, plus injustes que sévères,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 6,
                    "line_text": "D'un crayon faux peignent leurs pères",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 7,
                    "line_text": "Dégénérant de leurs aïeux,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                },
                {
                    "line_number": 8,
                    "line_text": "Et leurs contemporains coupables,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 9,
                    "line_text": "Suivis d'enfants plus condamnables,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 10,
                    "line_text": "Menacés de pires neveux ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Est-il encor des satiriques\nQui, du présent toujours blessés,\nDans leurs malins panégyriques\nExaltent les siècles passés ;\nQui, plus injustes que sévères,\nD'un crayon faux peignent leurs pères\nDégénérant de leurs aïeux,\nEt leurs contemporains coupables,\nSuivis d'enfants plus condamnables,\nMenacés de pires neveux ?"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Silence, imposture outrageante ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Déchirez-vous, voiles affreux ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 13,
                    "line_text": "Patrie auguste et florissante,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Connais-tu des temps plus heureux ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "De la cime des Pyrénées",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 16,
                    "line_text": "Jusqu'à ces rives étonnées",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 17,
                    "line_text": "Où la Mort vole avec l'Effroi,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                },
                {
                    "line_number": 18,
                    "line_text": "Montre ta gloire et ta puissance ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 19,
                    "line_text": "Mais pour mieux connaître la France,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 20,
                    "line_text": "Qu'on la contemple dans son roi.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Silence, imposture outrageante ;\nDéchirez-vous, voiles affreux ;\nPatrie auguste et florissante,\nConnais-tu des temps plus heureux ?\nDe la cime des Pyrénées\nJusqu'à ces rives étonnées\nOù la Mort vole avec l'Effroi,\nMontre ta gloire et ta puissance ;\nMais pour mieux connaître la France,\nQu'on la contemple dans son roi."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Quelquefois la grandeur trop fière,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Sur son front portant les dédains,",
