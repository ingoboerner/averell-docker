            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Et du matin chantant",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Et de la fraîche aurore,",
