{
    "poem_title": "LXX",
    "author": "Victor Hugo",
    "structure": "suite périodique",
    "year": "1902",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "C'était la première soirée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "0"
                },
                {
                    "line_number": 2,
                    "line_text": "Je m'en souviens, mon adorée.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "1"
                }
            ],
            "stanza_text": "C'était la première soirée\nJe m'en souviens, mon adorée."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Nous errions dans la ville immense,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "2"
                },
                {
                    "line_number": 4,
                    "line_text": "À l'heure où le repos commence",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "3"
                }
            ],
            "stanza_text": "Nous errions dans la ville immense,\nÀ l'heure où le repos commence"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Heure calme, charmante, austère,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "5"
                },
                {
                    "line_number": 6,
                    "line_text": "Dans cet ineffable mystère",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "6"
                }
            ],
            "stanza_text": "Heure calme, charmante, austère,\nDans cet ineffable mystère"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Tout ! l'amour dans tes yeux sans voile,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "7"
                },
                {
                    "line_number": 8,
                    "line_text": "Aux vitres mainte pauvre étoile,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "8"
                }
            ],
            "stanza_text": "Tout ! l'amour dans tes yeux sans voile,\nAux vitres mainte pauvre étoile,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Notre-Dame, parmi les dômes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "9"
                },
                {
                    "line_number": 10,
                    "line_text": "Dressait comme deux grands fantômes",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "10"
                }
            ],
            "stanza_text": "Notre-Dame, parmi les dômes\nDressait comme deux grands fantômes"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "La Seine, découpant les ombres",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "12"
                },
                {
                    "line_number": 12,
                    "line_text": "Faisait luire sous les ponts sombres",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "13"
                }
            ],
            "stanza_text": "La Seine, découpant les ombres\nFaisait luire sous les ponts sombres"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "L'œil voyait sur la plage amie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "14"
                },
                {
                    "line_number": 14,
                    "line_text": "Comme une couleuvre endormie",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "15"
                }
            ],
            "stanza_text": "L'œil voyait sur la plage amie\nComme une couleuvre endormie"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Et les passants, le long des grèves",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "16"
                },
                {
                    "line_number": 16,
                    "line_text": "Étaient vagues comme les rêves",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "3"
                }
            ],
            "stanza_text": "Et les passants, le long des grèves\nÉtaient vagues comme les rêves"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Je te disais : — « Clartés bénies,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "17"
                },
                {
                    "line_number": 18,
                    "line_text": "Dieu met toutes les harmonies",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "18"
                }
            ],
            "stanza_text": "Je te disais : — « Clartés bénies,\nDieu met toutes les harmonies"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Aube qui luit, soir qui flamboie,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "20"
                },
                {
                    "line_number": 20,
                    "line_text": "Et j'ai l'âme pleine de joie,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "21"
                }
            ],
            "stanza_text": "Aube qui luit, soir qui flamboie,\nEt j'ai l'âme pleine de joie,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Que m'importe que la nuit tombe,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "22"
                },
                {
                    "line_number": 22,
                    "line_text": "Semblable au plafond d'une tombe",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "23"
                }
            ],
            "stanza_text": "Que m'importe que la nuit tombe,\nSemblable au plafond d'une tombe"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Qué m'importe que Paris dorme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "24"
                },
                {
                    "line_number": 24,
                    "line_text": "Dans la brume épaisse et sans forme",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "25"
                }
            ],
            "stanza_text": "Qué m'importe que Paris dorme,\nDans la brume épaisse et sans forme"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Que m'importe, aux heures nocturnes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "26"
                },
                {
                    "line_number": 26,
                    "line_text": "Les ombres qui versent leurs urnes",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "27"
                }
            ],
            "stanza_text": "Que m'importe, aux heures nocturnes\nLes ombres qui versent leurs urnes"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Et, noyant de' leurs plis funèbres",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "29"
                },
                {
                    "line_number": 28,
                    "line_text": "Font les' vivants dans les ténèbres",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "30"
                }
            ],
            "stanza_text": "Et, noyant de' leurs plis funèbres\nFont les' vivants dans les ténèbres"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Moi, lorsque tout subit l'empire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "32"
                },
                {
                    "line_number": 30,
                    "line_text": "J'ai ton regard, j'ai ton sourire,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "33"
                }
            ],
            "stanza_text": "Moi, lorsque tout subit l'empire\nJ'ai ton regard, j'ai ton sourire,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Je te parlais, ma bien-aimée ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "0"
                },
                {
                    "line_number": 32,
                    "line_text": "Ta, main pressait ma main charmée.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "34"
                }
            ],
            "stanza_text": "Je te parlais, ma bien-aimée ;\nTa, main pressait ma main charmée."
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Nous nous regardions pleins de flamme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "36"
                },
                {
                    "line_number": 34,
                    "line_text": "Et l'âme répondait à l'âme,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "37"
                }
            ],
            "stanza_text": "Nous nous regardions pleins de flamme,\nEt l'âme répondait à l'âme,"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Sous tes cils une larmeobscure",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "39"
                },
                {
                    "line_number": 36,
                    "line_text": "Puis ta voix parlait, tendre et pure,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "40"
                }
            ],
            "stanza_text": "Sous tes cils une larmeobscure\nPuis ta voix parlait, tendre et pure,"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Comme on entend dans la coupole",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "42"
                },
                {
                    "line_number": 38,
                    "line_text": "Comme après un ,oiseau s'envole",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "43"
                }
            ],
            "stanza_text": "Comme on entend dans la coupole\nComme après un ,oiseau s'envole"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "Tu disais : « Je suis calme et fière,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "5"
                },
                {
                    "line_number": 40,
                    "line_text": "Et je rêvais à ta lumière",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "25"
                }
            ],
            "stanza_text": "Tu disais : « Je suis calme et fière,\nEt je rêvais à ta lumière"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Oh ! ce fut une heure sacrée,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "0"
                },
                {
                    "line_number": 42,
                    "line_text": "Que cette première soirée",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "1"
                }
            ],
            "stanza_text": "Oh ! ce fut une heure sacrée,\nQue cette première soirée"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Tout en disant toutes les choses,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Qu'on dit dans lasaison des, roses",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tout en disant toutes les choses,\nQu'on dit dans lasaison des, roses"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Nous allions, contemplant dans l'onde",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Cette lune qui jette au monde",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Nous allions, contemplant dans l'onde\nCette lune qui jette au monde"
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "Et qui, d'en haut, sereine comme",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Regarde le bonheur de l'homme",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et qui, d'en haut, sereine comme\nRegarde le bonheur de l'homme"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Tu disais : « Ô soleils sans nombre !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Dans vos clartés et dans votre ombre,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tu disais : « Ô soleils sans nombre !\nDans vos clartés et dans votre ombre,"
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Rien ne se perd ! Cendre, étincelle,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 52,
                    "line_text": "Le moindre battement d'une aile",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Rien ne se perd ! Cendre, étincelle,\nLe moindre battement d'une aile"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Le chant du nid qui sous la feuille",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Du cœur pensif qui se recueille",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le chant du nid qui sous la feuille\nDu cœur pensif qui se recueille"
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Les rêves de l'âme enivrée,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "La nature immense et sacrée",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Les rêves de l'âme enivrée,\nLa nature immense et sacrée"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Car tout suit sa loi grave et douce !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "L'herbe verdit, la branche pousse",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Car tout suit sa loi grave et douce !\nL'herbe verdit, la branche pousse"
        },
        {
            "stanza_number": 30,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 59,
                    "line_text": "La nuit endort les champs, la foule,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 60,
                    "line_text": "Le vent fuit, l'astre luit, l'eau coule,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La nuit endort les champs, la foule,\nLe vent fuit, l'astre luit, l'eau coule,"
        },
        {
            "stanza_number": 31,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Nous aimons parce que nous sommes !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Aimer, c'est vivre loin des hommes",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Nous aimons parce que nous sommes !\nAimer, c'est vivre loin des hommes"
        },
        {
            "stanza_number": 32,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 63,
                    "line_text": "C'est s'ouvrir à la clarté pure,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 64,
                    "line_text": "C'est sentir toute la nature",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "C'est s'ouvrir à la clarté pure,\nC'est sentir toute la nature"
        },
        {
            "stanza_number": 33,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "C'est accomplir le code auguste",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "Que suivait devant le ciel juste",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "C'est accomplir le code auguste\nQue suivait devant le ciel juste"
        },
        {
            "stanza_number": 34,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 67,
                    "line_text": "Soyons heureux, ô toi que j'aime !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 68,
                    "line_text": "Car seuls à cette heure suprême,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Soyons heureux, ô toi que j'aime !\nCar seuls à cette heure suprême,"
        },
        {
            "stanza_number": 35,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 69,
                    "line_text": "Dédaignant d'un monde où tout tremble",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 70,
                    "line_text": "Sûrs d'être en paix avec l'ensemble",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Dédaignant d'un monde où tout tremble\nSûrs d'être en paix avec l'ensemble"
        },
        {
            "stanza_number": 36,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 71,
                    "line_text": "Comme en un temple où l'ombre rampe",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 72,
                    "line_text": "On suit la lueur d'une lampe",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Comme en un temple où l'ombre rampe\nOn suit la lueur d'une lampe"
        },
        {
            "stanza_number": 37,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 73,
                    "line_text": "Nous sentons sur notre âme fière,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 74,
                    "line_text": "L'œil sans sommeil, l'œil sans paupière",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
