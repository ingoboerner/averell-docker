                {
                    "line_number": 1,
                    "line_text": "Oui, le tonnerre éclaire et gronde sous mon front,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": ""
                },
                {
                    "line_number": 2,
                    "line_text": "J'ai sous mon crâne obscur le gouffre et la tempête,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Et l'indignation du flot que rien n'arrête ;",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "J'ai dans mon cœur le roc et toute sa fierté ;",
                    "metrical_pattern": null,
                    "line_length": "6+6",
