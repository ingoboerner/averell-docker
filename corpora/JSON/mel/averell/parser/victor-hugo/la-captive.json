                {
                    "line_number": 1,
                    "line_text": "Si je n'étais captive,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": ""
                },
                {
                    "line_number": 2,
                    "line_text": "J'aimerais ce pays,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": ""
                },
                {
                    "line_number": 3,
                    "line_text": "Et cette mer plaintive,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": ""
                },
                {
                    "line_number": 4,
                    "line_text": "Et ces champs de maïs,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": ""
