                {
                    "line_number": 1,
                    "line_text": "Puisqu'ici-bas toute âme",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Sa musique, sa flamme,",
