            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Entre deux rocs d'un noir d'ébène",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Voyez-vous ce sombre hallier",
