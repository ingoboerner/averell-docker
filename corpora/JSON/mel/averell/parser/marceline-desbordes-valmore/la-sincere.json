            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Moi, j’en fais le prix ;",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Veux-tu le connaître ?",
