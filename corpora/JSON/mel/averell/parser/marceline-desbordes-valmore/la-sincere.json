        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Le mien glissera,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "9"
                },
                {
                    "line_number": 22,
                    "line_text": "Fermé dans la vie ;",
                    "metrical_pattern": null,
