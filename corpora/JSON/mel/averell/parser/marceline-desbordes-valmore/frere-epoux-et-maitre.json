                {
                    "line_number": 1,
                    "line_text": "Frère, époux, et maître,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Vous voulez connaître",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Ce que je fais là :",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Eh bien ! le voilà !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Frère, époux, et maître,\nVous voulez connaître\nCe que je fais là :\nEh bien ! le voilà !"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Je vous aime encore,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Comme à celte aurore",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 7,
                    "line_text": "Qui commence un jour",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Qu'on appelle amour.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Je vous aime encore,\nComme à celte aurore\nQui commence un jour\nQu'on appelle amour."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Jamais endormie,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Mon âme est l'amie",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "De vos jours souffrans,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "De vos vœux errans !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Jamais endormie,\nMon âme est l'amie\nDe vos jours souffrans,\nDe vos vœux errans !"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Je crois que votre âme",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Est une autre flamme,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Que la même loi",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "Ramène vers moi !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Je crois que votre âme\nEst une autre flamme,\nQue la même loi\nRamène vers moi !"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Près ou loin, ma vie,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Je n'ai pas envie",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 19,
                    "line_text": "D'échapper au sort",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 20,
                    "line_text": "Si doux et si fort !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Près ou loin, ma vie,\nJe n'ai pas envie\nD'échapper au sort\nSi doux et si fort !"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Femme ou demoiselle,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Je demeure celle",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Qui cueillit pour vous",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "Le bouquet d'époux.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Femme ou demoiselle,\nJe demeure celle\nQui cueillit pour vous\nLe bouquet d'époux."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Ainsi, je vous aime,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Fidèle à moi-même ;",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Et je vous attends,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 28,
                    "line_text": "Même après le temps !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ainsi, je vous aime,\nFidèle à moi-même ;\nEt je vous attends,\nMême après le temps !"
        }
