            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Chez elle, quand le matin",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "5"
                },
                {
                    "line_number": 18,
                    "line_text": "Ma femme est à sa toilette,",
                    "metrical_pattern": null,
