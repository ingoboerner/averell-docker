    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tu penches, grand Platane, et te proposes nu,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Mais ta candeur est prise, et ton pied retenu",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tu penches, grand Platane, et te proposes nu,\nMais ta candeur est prise, et ton pied retenu"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Ombre retentissante en qui le même azur",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "La noire mère astreint ce pied natal et pur",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ombre retentissante en qui le même azur\nLa noire mère astreint ce pied natal et pur"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "De ton front voyageur les vents ne veulent pas ;",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Ô Platane, jamais ne laissera d’un pas",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "De ton front voyageur les vents ne veulent pas ;\nÔ Platane, jamais ne laissera d’un pas"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Ce front n’aura d´accès qu´aux degrés lumineux",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Tu peux grandir, candeur, mais non rompre les nœuds",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ce front n’aura d´accès qu´aux degrés lumineux\nTu peux grandir, candeur, mais non rompre les nœuds"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Pressens autour de toi d´autres vivants liés",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Tes pareils sont nombreux, des pins aux peupliers,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pressens autour de toi d´autres vivants liés\nTes pareils sont nombreux, des pins aux peupliers,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Qui, par les morts saisis, les pieds échevelés",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Sentent les fuir les fleurs, et leurs spermes ailés,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qui, par les morts saisis, les pieds échevelés\nSentent les fuir les fleurs, et leurs spermes ailés,"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Le tremble pur, le charme, et ce hêtre formé,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Ne cessent point de battre un ciel toujours fermé,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le tremble pur, le charme, et ce hêtre formé,\nNe cessent point de battre un ciel toujours fermé,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Ils vivent séparés, ils pleurent confondus",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Et leurs membres d´argent sont vainement fendus",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ils vivent séparés, ils pleurent confondus\nEt leurs membres d´argent sont vainement fendus"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Quand l’âme lentement qu’ils expirent le soir",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "La vierge doit dans l’ombre, en silence, s’asseoir,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Quand l’âme lentement qu’ils expirent le soir\nLa vierge doit dans l’ombre, en silence, s’asseoir,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Elle se sent surprendre, et pâle, appartenir",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Qu’une présente chair tourne vers l’avenir",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Elle se sent surprendre, et pâle, appartenir\nQu’une présente chair tourne vers l’avenir"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Mais toi, de bras plus purs que les bras animaux,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Toi qui formes au jour le fantôme des maux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais toi, de bras plus purs que les bras animaux,\nToi qui formes au jour le fantôme des maux"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Haute profusion de feuilles, trouble fier",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Sonne, au comble de l’or, l’azur du jeune hiver",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Haute profusion de feuilles, trouble fier\nSonne, au comble de l’or, l’azur du jeune hiver"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Ose gémir !… Il faut, ô souple chair du bois,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Te plaindre sans te rompre, et rendre aux vents la voix",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ose gémir !… Il faut, ô souple chair du bois,\nTe plaindre sans te rompre, et rendre aux vents la voix"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Flagelle-toi !… Parais l’impatient martyr",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Et dispute à la flamme impuissante à partir",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Flagelle-toi !… Parais l’impatient martyr\nEt dispute à la flamme impuissante à partir"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Afin que l’hymne monte aux oiseaux qui naîtront,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Fasse frémir d’espoir les feuillages d’un tronc",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Afin que l’hymne monte aux oiseaux qui naîtront,\nFasse frémir d’espoir les feuillages d’un tronc"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Je t’ai choisi, puissant personnage d’un parc,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Puisque le ciel t’exerce, et te presse, ô grand arc,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Je t’ai choisi, puissant personnage d’un parc,\nPuisque le ciel t’exerce, et te presse, ô grand arc,"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Ô qu’amoureusement des Dryades rival,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Flatter ton corps poli comme il fait du Cheval",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ô qu’amoureusement des Dryades rival,\nFlatter ton corps poli comme il fait du Cheval"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "— Non, dit l’arbre. Il dit : Non ! par l’étincellement",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Que la tempête traite universellement",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "— Non, dit l’arbre. Il dit : Non ! par l’étincellement\nQue la tempête traite universellement"
        }
