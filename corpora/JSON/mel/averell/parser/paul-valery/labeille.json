                {
                    "line_number": 1,
                    "line_text": "Quelle, et si fine, et si mortelle,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Que soit ta pointe, blonde abeille,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Je n’ai, sur ma tendre corbeille,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Jeté qu’un songe de dentelle.",
                    "metrical_pattern": null,
