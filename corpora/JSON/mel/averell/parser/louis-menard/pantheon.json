    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Renferme tous les dieux que le monde a connus.",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Évoqués à la fois de tous les sanctuaires,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Renferme tous les dieux que le monde a connus.\nÉvoqués à la fois de tous les sanctuaires,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Avant le premier jour de la création,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Ceux qu'adore, en ses jours de vieillesse tardive,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Avant le premier jour de la création,\nCeux qu'adore, en ses jours de vieillesse tardive,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Contemplent, à travers l'éternité sans fin,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Le monde, qui toujours finit et recommence",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Contemplent, à travers l'éternité sans fin,\nLe monde, qui toujours finit et recommence"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Qui, dans les profondeurs du multiple univers,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Font ruisseler les flots bouillonnants de la vie,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qui, dans les profondeurs du multiple univers,\nFont ruisseler les flots bouillonnants de la vie,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Qui marchent dans leur force et leur mâle beauté,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Guidant les peuples fiers et les races puissantes",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qui marchent dans leur force et leur mâle beauté,\nGuidant les peuples fiers et les races puissantes"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "La voix des hymnes monte ainsi qu'aux jours de foi ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "À l'entour de l'autel, un peuple immense adore",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La voix des hymnes monte ainsi qu'aux jours de foi ;\nÀ l'entour de l'autel, un peuple immense adore"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Il faut le bec sanglant du vautour éternel",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Ou l'infâme gibet de l'éternel supplice,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Il faut le bec sanglant du vautour éternel\nOu l'infâme gibet de l'éternel supplice,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Veulent avoir leur part des divines douleurs ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Le bûcher sur l'Œta, la croix sur le Calvaire,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Veulent avoir leur part des divines douleurs ;\nLe bûcher sur l'Œta, la croix sur le Calvaire,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Discrète et recueillie, où, des cieux entr'ouverts,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "La colombe divine ombrage de son aile",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Discrète et recueillie, où, des cieux entr'ouverts,\nLa colombe divine ombrage de son aile"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Puisque ton chaste sein conçut le dernier dieu,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Règne auprès de ton fils, rayonnante, étoilée,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puisque ton chaste sein conçut le dernier dieu,\nRègne auprès de ton fils, rayonnante, étoilée,"
        }
    ],
    "corpus": "mel"
