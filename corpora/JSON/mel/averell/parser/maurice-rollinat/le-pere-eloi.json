            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Or, en fait d'oraison longue, le vieux narquois",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Partit tout simplement, sur un signe de croix,",
