{
    "poem_title": "L'Interprète",
    "author": "Maurice Rollinat",
    "structure": "sonnet",
    "year": "1899",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "L'inclinaison de ce vieux saule",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Sur le vieil étang soucieux",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Que pas une brise ne frôle,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "A quelque chose de pieux.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'inclinaison de ce vieux saule\nSur le vieil étang soucieux\nQue pas une brise ne frôle,\nA quelque chose de pieux."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Et l'on dirait que chaque feuille,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 6,
                    "line_text": "Ayant cessé son trémolo,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                },
                {
                    "line_number": 7,
                    "line_text": "Pompe le mystère de l'eau",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                },
                {
                    "line_number": 8,
                    "line_text": "Et dévotement se recueille.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et l'on dirait que chaque feuille,\nAyant cessé son trémolo,\nPompe le mystère de l'eau\nEt dévotement se recueille."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Or, soudain, y perchant son vol,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 10,
                    "line_text": "Voici qu'un petit rossignol,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 11,
                    "line_text": "Tendre interprète d'aventure,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "f"
                }
            ],
            "stanza_text": "Or, soudain, y perchant son vol,\nVoici qu'un petit rossignol,\nTendre interprète d'aventure,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Pour l'arbre adresse à l'Inconnu,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "g"
                },
                {
                    "line_number": 13,
                    "line_text": "Dans un lamento soutenu,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "g"
                },
                {
                    "line_number": 14,
                    "line_text": "La prière de la Nature !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "f"
                }
            ],
            "stanza_text": "Pour l'arbre adresse à l'Inconnu,\nDans un lamento soutenu,\nLa prière de la Nature !"
        }
    ],
    "corpus": "mel"
}