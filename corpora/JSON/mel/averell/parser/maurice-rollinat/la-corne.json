                },
                {
                    "line_number": 7,
                    "line_text": "Comme eux secrets, obscurs, invisibles, ils ont",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Autour, dessus, dessous, le mystère pour hôte.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
