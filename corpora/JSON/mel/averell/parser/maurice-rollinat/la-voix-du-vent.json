{
    "poem_title": "La Voix du vent",
    "author": "Maurice Rollinat",
    "structure": "sonnet",
    "year": "1899",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Les nuits d'hiver quand le vent pleure,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Se plaint, hurle, siffle et vagit,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "On ne sait quel drame surgit",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Dans l'homme ainsi qu'en la demeure.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Les nuits d'hiver quand le vent pleure,\nSe plaint, hurle, siffle et vagit,\nOn ne sait quel drame surgit\nDans l'homme ainsi qu'en la demeure."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Sa grande musique mineure",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Qui, tour à tour, grince et mugit,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Sur toute la pensée agit",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Comme une voix intérieure.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Sa grande musique mineure\nQui, tour à tour, grince et mugit,\nSur toute la pensée agit\nComme une voix intérieure."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Ces cris, cette clameur immense,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 10,
                    "line_text": "Chantent la rage, la démence,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 11,
                    "line_text": "La peur, le crime, le remord…",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Ces cris, cette clameur immense,\nChantent la rage, la démence,\nLa peur, le crime, le remord…"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Et, voluptueux et funèbres,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 13,
                    "line_text": "Accompagnent dans les ténèbres",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 14,
                    "line_text": "Les râles d'amour et de mort.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Et, voluptueux et funèbres,\nAccompagnent dans les ténèbres\nLes râles d'amour et de mort."
        }
    ],
    "corpus": "mel"
}