{
    "poem_title": "LE CRAPAUD",
    "author": "Maurice Rollinat",
    "structure": "suite périodique",
    "year": "1877",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "O vivante et visqueuse extase",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Accroupie au bord des marais,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Pèlerin morne de la vase,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Des vignes et des bruns guérets,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O vivante et visqueuse extase\nAccroupie au bord des marais,\nPèlerin morne de la vase,\nDes vignes et des bruns guérets,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Paria, dont la vue inspire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "De l'horreur aux pestiférés,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Crapaud, inconscient vampire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Des vaches sommeillant aux prés ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Paria, dont la vue inspire\nDe l'horreur aux pestiférés,\nCrapaud, inconscient vampire\nDes vaches sommeillant aux prés ;"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Infime roi des culs-de-jatte",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Écrasé par ta pesanteur,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Sombre forçat tirant la patte",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Avec une affreuse lenteur,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Infime roi des culs-de-jatte\nÉcrasé par ta pesanteur,\nSombre forçat tirant la patte\nAvec une affreuse lenteur,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "A toi que Dieu semble maudire,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "A toi, doux martyr des enfants,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "Le cœur ému, je viens te dire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Que je te plains et te défends.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "A toi que Dieu semble maudire,\nA toi, doux martyr des enfants,\nLe cœur ému, je viens te dire\nQue je te plains et te défends."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Ton pauvre corps, lorsque tu bouges,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Est inquiet et tourmenté,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "Et ce qui sort de tes yeux rouges,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "C'est une immense humilité.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ton pauvre corps, lorsque tu bouges,\nEst inquiet et tourmenté,\nEt ce qui sort de tes yeux rouges,\nC'est une immense humilité."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Je t'aime, monstre épouvantable.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Que j'ai vu grimpant l'autre soir,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 23,
                    "line_text": "Avec un effort lamentable,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Dans l'épaisseur du buisson noir.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Je t'aime, monstre épouvantable.\nQue j'ai vu grimpant l'autre soir,\nAvec un effort lamentable,\nDans l'épaisseur du buisson noir."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Loin de l'homme et de la vipère,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Loin de tout ce qui frappe et mord,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 27,
                    "line_text": "Je te souhaite un bon repaire,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Obscur et froid comme la mort.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Loin de l'homme et de la vipère,\nLoin de tout ce qui frappe et mord,\nJe te souhaite un bon repaire,\nObscur et froid comme la mort."
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Fuis vers une mare chargée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "De brume opaque et de sommeil,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 31,
                    "line_text": "Et que n'auront jamais figée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Les yeux calcinants du soleil.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Fuis vers une mare chargée\nDe brume opaque et de sommeil,\nEt que n'auront jamais figée\nLes yeux calcinants du soleil."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Qu'un ciel à teintes orageuses,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Toujours plein de morosité,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 35,
                    "line_text": "Sur tes landes marécageuses",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Éternise l'humidité ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qu'un ciel à teintes orageuses,\nToujours plein de morosité,\nSur tes landes marécageuses\nÉternise l'humidité ;"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Pour que toi, le rôdeur des flaques,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Tu puisses faire tes plongeons",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 39,
                    "line_text": "Dans de délicieux cloaques",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 40,
                    "line_text": "Frais, sous le fouillis vert des joncs.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pour que toi, le rôdeur des flaques,\nTu puisses faire tes plongeons\nDans de délicieux cloaques\nFrais, sous le fouillis vert des joncs."
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Dans la grande paix sépulcrale",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "De la nuit qui tombe des cieux,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 43,
                    "line_text": "Lorsque le vent n'est plus qu'un râle",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Dans les arbres silencieux,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Dans la grande paix sépulcrale\nDe la nuit qui tombe des cieux,\nLorsque le vent n'est plus qu'un râle\nDans les arbres silencieux,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Unis-toi sous la froide lune,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Qui t'enverra son regard blanc,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 47,
                    "line_text": "A la femelle molle et brune",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Bavant de plaisir à ton flanc !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Unis-toi sous la froide lune,\nQui t'enverra son regard blanc,\nA la femelle molle et brune\nBavant de plaisir à ton flanc !"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Dans les nénuphars, jamais traîtres,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Humez l'amour, l'amour béni,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 51,
                    "line_text": "Qui donne aux plus horribles êtres",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 52,
                    "line_text": "Les ivresses de l'infini.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Dans les nénuphars, jamais traîtres,\nHumez l'amour, l'amour béni,\nQui donne aux plus horribles êtres\nLes ivresses de l'infini."
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Et puis, chemine, lent touriste,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "De la mare au creux du sapin,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 55,
                    "line_text": "En chuchotant ton cri plus triste",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "Que tous les mineurs de Chopin.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et puis, chemine, lent touriste,\nDe la mare au creux du sapin,\nEn chuchotant ton cri plus triste\nQue tous les mineurs de Chopin."
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Rampe à l'aise, deviens superbe",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "De laideur grasse et de repos,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 59,
                    "line_text": "Dans la sécurité d'une herbe",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 60,
                    "line_text": "Où ne vivront que des crapauds !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Rampe à l'aise, deviens superbe\nDe laideur grasse et de repos,\nDans la sécurité d'une herbe\nOù ne vivront que des crapauds !"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "De l'hiver à la canicule",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Puisses-tu savourer longtemps",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 63,
                    "line_text": "L'ombre vague du crépuscule",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 64,
                    "line_text": "Près des solitaires étangs !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "De l'hiver à la canicule\nPuisses-tu savourer longtemps\nL'ombre vague du crépuscule\nPrès des solitaires étangs !"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Puisse ta vie être un long rêve",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "D'amour et de sérénité !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 67,
                    "line_text": "Sois la hideur ravie, et crève",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 68,
                    "line_text": "De vieillesse ou de volupté !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puisse ta vie être un long rêve\nD'amour et de sérénité !\nSois la hideur ravie, et crève\nDe vieillesse ou de volupté !"
        }
    ],
    "corpus": "mel"
}