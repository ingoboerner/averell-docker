{
    "poem_title": "Tempête obscure",
    "author": "Maurice Rollinat",
    "structure": "forme RegExp (appariements)",
    "year": "1899",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "L'orage, après de longs repos,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Ce soir-là, par ses deux suppôts,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "L'orage, après de longs repos,\nCe soir-là, par ses deux suppôts,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "La nuée et le vent qui claque,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Se présageait pour l'onde opaque.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La nuée et le vent qui claque,\nSe présageait pour l'onde opaque."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Grondante sous le ciel muet,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Par quintes, la mer se ruait ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Grondante sous le ciel muet,\nPar quintes, la mer se ruait ;"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Puis, elle se tut, la perfide,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Reprit son niveau brun livide.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puis, elle se tut, la perfide,\nReprit son niveau brun livide."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Malheur aux coquilles de noix",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Alors sur l'élément sournois",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Malheur aux coquilles de noix\nAlors sur l'élément sournois"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "D'un plat, d'un silence de planche,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Risquant leur petite aile blanche !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "D'un plat, d'un silence de planche,\nRisquant leur petite aile blanche !"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Car, on le sent à l'angoissé,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Au guettant de l'air oppressé,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Car, on le sent à l'angoissé,\nAu guettant de l'air oppressé,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "La paix du gouffre qui se fige",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "Couve la trame du vertige ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La paix du gouffre qui se fige\nCouve la trame du vertige ;"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Si calme en dessus, ses dessous",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Cherchent, ramassent leurs courroux,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Si calme en dessus, ses dessous\nCherchent, ramassent leurs courroux,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "En effet, soudain l'eau tranquille",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 20,
                    "line_text": "Bomba sa face d'encre et d'huile,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "En effet, soudain l'eau tranquille\nBomba sa face d'encre et d'huile,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Perdit son taciturne intact,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Prit un clapotement compact.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Perdit son taciturne intact,\nPrit un clapotement compact."
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Et voilà qu'à rumeurs funèbres",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "La tempête emplit les ténèbres.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et voilà qu'à rumeurs funèbres\nLa tempête emplit les ténèbres."
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Mais, pas un éclair zigzaguant :",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Rien que l'obscur de l'ouragan !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais, pas un éclair zigzaguant :\nRien que l'obscur de l'ouragan !"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Ballottée en ce ciel de bistre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 28,
                    "line_text": "La lune folle, errant sinistre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ballottée en ce ciel de bistre\nLa lune folle, errant sinistre,"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Comme une morte promenant",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Sa lanterne de revenant,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Comme une morte promenant\nSa lanterne de revenant,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "À hideuses lueurs moroses",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 32,
                    "line_text": "Éclairait ce drame des choses.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "À hideuses lueurs moroses\nÉclairait ce drame des choses."
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Souffle monstre, outrant sa fureur,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Le vent démesurait l'horreur",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Souffle monstre, outrant sa fureur,\nLe vent démesurait l'horreur"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Des montagnes d'eau dont les cimes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 36,
                    "line_text": "Pivotaient, croulant en abîmes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Des montagnes d'eau dont les cimes\nPivotaient, croulant en abîmes"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Qui, l'un par l'autre chevauchés,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Distordus, engloutis, crachés,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Qui, l'un par l'autre chevauchés,\nDistordus, engloutis, crachés,"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "Redressaient leurs masses béantes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 40,
                    "line_text": "En Himalayas tournoyantes,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Redressaient leurs masses béantes\nEn Himalayas tournoyantes,"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Spectrales des froids rayons verts",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Se multipliant au travers.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Spectrales des froids rayons verts\nSe multipliant au travers."
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Et, toujours, la houle élastique",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 44,
                    "line_text": "Réopérait plus frénétique",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et, toujours, la houle élastique\nRéopérait plus frénétique"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "La métamorphose des flots",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Dans des tonnerres de sanglots.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La métamorphose des flots\nDans des tonnerres de sanglots."
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "Vint alors tant d'obscurité",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 48,
                    "line_text": "Que ce fracas précipité",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Vint alors tant d'obscurité\nQue ce fracas précipité"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "N'était plus que la plainte immense,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "La clameur du vide en démence.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "N'était plus que la plainte immense,\nLa clameur du vide en démence."
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Puis, l'astre blêmissant, terni,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 52,
                    "line_text": "Sombra dans le noir infini",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puis, l'astre blêmissant, terni,\nSombra dans le noir infini"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Où son vert-de-gris jaune-soufre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Se convulsait avec le gouffre.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Où son vert-de-gris jaune-soufre\nSe convulsait avec le gouffre."
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Les vagues par leurs bonds si hauts",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 56,
                    "line_text": "Brassaient le ciel dans le chaos ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Les vagues par leurs bonds si hauts\nBrassaient le ciel dans le chaos ;"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Tout tourbillonnait : l'eau, la brume,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "La voûte, les airs et l'écume,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tout tourbillonnait : l'eau, la brume,\nLa voûte, les airs et l'écume,"
        },
        {
            "stanza_number": 30,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 59,
                    "line_text": "Tout : fond, sommet, milieu, côtés",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 60,
                    "line_text": "Dans le pêle-mêle emportés !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tout : fond, sommet, milieu, côtés\nDans le pêle-mêle emportés !"
        },
        {
            "stanza_number": 31,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Tellement que la mer, les nues,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Étaient par degrés devenues",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tellement que la mer, les nues,\nÉtaient par degrés devenues"
        },
        {
            "stanza_number": 32,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 63,
                    "line_text": "Un même et confus océan",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 64,
                    "line_text": "Roulant tout seul dans le Néant.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Un même et confus océan\nRoulant tout seul dans le Néant."
        },
        {
            "stanza_number": 33,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Et, pour l'œil comme pour l'oreille,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "Existait l'affreuse merveille,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et, pour l'œil comme pour l'oreille,\nExistait l'affreuse merveille,"
        },
        {
            "stanza_number": 34,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 67,
                    "line_text": "L'âme vivait l'illusion",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 68,
                    "line_text": "De cette énorme vision,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'âme vivait l'illusion\nDe cette énorme vision,"
        },
        {
            "stanza_number": 35,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 69,
                    "line_text": "Tout l'être croyait au mensonge",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 70,
                    "line_text": "Du terrible tableau mouvant",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Tout l'être croyait au mensonge\nDu terrible tableau mouvant"
        },
        {
            "stanza_number": 36,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 71,
                    "line_text": "Qu'avec l'eau, la lune, et le vent,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 72,
                    "line_text": "La Nuit composait pour le Songe.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qu'avec l'eau, la lune, et le vent,\nLa Nuit composait pour le Songe."
        }
    ],
    "corpus": "mel"
}