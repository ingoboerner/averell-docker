            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Il mangeait le cuir, le liège, et cætera,",
                    "metrical_pattern": null,
                    "line_length": "5+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Renversait les pots et traînait l'écumoire ;",
