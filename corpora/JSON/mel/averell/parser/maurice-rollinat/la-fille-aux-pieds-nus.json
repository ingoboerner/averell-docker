{
    "poem_title": "LA FILLE AUX PIEDS NUS",
    "author": "Maurice Rollinat",
    "structure": "rondel classique",
    "year": "1877",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Dans le champ planté de colzas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "De luzerne et de betteraves,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Devant les grands bœufs doux et graves",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Je passais comme tu passas.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Dans le champ planté de colzas,\nDe luzerne et de betteraves,\nDevant les grands bœufs doux et graves\nJe passais comme tu passas."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Longtemps avec moi tu causas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Par un matin des plus suaves,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Dans le champ planté de colzas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "De luzerne et de betteraves.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Longtemps avec moi tu causas,\nPar un matin des plus suaves,\nDans le champ planté de colzas,\nDe luzerne et de betteraves."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Et si bien tu t'apprivoisas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Toi la fille aux pieds nus, qui braves",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "L'herbe humide et le bord des gaves,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Qu'en souriant tu me baisas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 13,
                    "line_text": "Dans le champ planté de colzas !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et si bien tu t'apprivoisas,\nToi la fille aux pieds nus, qui braves\nL'herbe humide et le bord des gaves,\nQu'en souriant tu me baisas,\nDans le champ planté de colzas !"
        }
    ],
    "corpus": "mel"
}