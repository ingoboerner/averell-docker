                {
                    "line_number": 1,
                    "line_text": "Yeux, lacs avec ma simple ivresse de renaître",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Autre que l'histrion qui du geste évoquais",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Comme plume la suie ignoble des quinquets,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "J'ai troué dans le mur de toile une fenêtre.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
