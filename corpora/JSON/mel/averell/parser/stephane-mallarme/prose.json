{
    "poem_title": "PROSE",
    "author": "Stéphane Mallarmé",
    "structure": "suite périodique",
    "year": "1887",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Hyperbole ! de ma mémoire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Triomphalement ne sais-tu",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Te lever, aujourd'hui grimoire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Dans un livre de fer vêtu :",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Hyperbole ! de ma mémoire\nTriomphalement ne sais-tu\nTe lever, aujourd'hui grimoire\nDans un livre de fer vêtu :"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Car j'installe, par la science,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "L'hymne des cœurs spirituels",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "En l'œuvre de ma patience,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Atlas, herbiers et rituels.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Car j'installe, par la science,\nL'hymne des cœurs spirituels\nEn l'œuvre de ma patience,\nAtlas, herbiers et rituels."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Nous promenions notre visage",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "(Nous fûmes deux, je le maintiens)",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Sur maints charmes de paysage,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Ô sœur, y comparant les tiens.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Nous promenions notre visage\n(Nous fûmes deux, je le maintiens)\nSur maints charmes de paysage,\nÔ sœur, y comparant les tiens."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "L'ère d'autorité se trouble",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Lorsque, sans nul motif, on dit",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "De ce midi que notre double",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Inconscience approfondit",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'ère d'autorité se trouble\nLorsque, sans nul motif, on dit\nDe ce midi que notre double\nInconscience approfondit"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Que, sol des cent iris, son site",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Il savent s'il a bien été,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "Ne porte pas de nom que cite",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "L'or de la trompette d'Été.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Que, sol des cent iris, son site\nIl savent s'il a bien été,\nNe porte pas de nom que cite\nL'or de la trompette d'Été."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Oui, dans une île que l'air charge",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "De vue et non de visions",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 23,
                    "line_text": "Toute fleur s'étalait plus large",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Sans que nous en devisions.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Oui, dans une île que l'air charge\nDe vue et non de visions\nToute fleur s'étalait plus large\nSans que nous en devisions."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Telles, immenses, que chacune",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Ordinairement se para",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 27,
                    "line_text": "D'un lucide contour, lacune,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Qui des jardins la sépara.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Telles, immenses, que chacune\nOrdinairement se para\nD'un lucide contour, lacune,\nQui des jardins la sépara."
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Gloire du long désir, Idées",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Tout en moi s'exaltait de voir",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 31,
                    "line_text": "La famille des iridées",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Surgir à ce nouveau devoir.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Gloire du long désir, Idées\nTout en moi s'exaltait de voir\nLa famille des iridées\nSurgir à ce nouveau devoir."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Mais cette sœur sensée et tendre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Ne porta son regard plus loin",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 35,
                    "line_text": "Que sourire, et comme à l'entendre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "J'occupe mon antique soin.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais cette sœur sensée et tendre\nNe porta son regard plus loin\nQue sourire, et comme à l'entendre\nJ'occupe mon antique soin."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Oh ! sache l'Esprit de litige,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "À cette heure où nous nous taisons,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 39,
                    "line_text": "Que de lis multiples la tige",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 40,
                    "line_text": "Grandissait trop pour nos raisons",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Oh ! sache l'Esprit de litige,\nÀ cette heure où nous nous taisons,\nQue de lis multiples la tige\nGrandissait trop pour nos raisons"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Et non comme pleure la rive",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Quand son jeu monotone ment",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 43,
                    "line_text": "À vouloir que l'ampleur arrive",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Parmi mon jeune étonnement",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et non comme pleure la rive\nQuand son jeu monotone ment\nÀ vouloir que l'ampleur arrive\nParmi mon jeune étonnement"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "D'ouïr tout le ciel et la carte",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Sans fin attestés sur mes pas",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 47,
                    "line_text": "Par le flot même qui s'écarte,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Que ce pays n'exista pas.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "D'ouïr tout le ciel et la carte\nSans fin attestés sur mes pas\nPar le flot même qui s'écarte,\nQue ce pays n'exista pas."
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "L'enfant abdique son extase",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Et docte déjà par chemins",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 51,
                    "line_text": "Elle dit le mot : Anastase !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 52,
                    "line_text": "Né pour d'éternels parchemins,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'enfant abdique son extase\nEt docte déjà par chemins\nElle dit le mot : Anastase !\nNé pour d'éternels parchemins,"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Avant qu'un sépulcre ne rie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Sous aucun climat, son aïeul,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 55,
                    "line_text": "De porter ce nom : Pulchérie !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "Caché par le trop grand glaïeul.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Avant qu'un sépulcre ne rie\nSous aucun climat, son aïeul,\nDe porter ce nom : Pulchérie !\nCaché par le trop grand glaïeul."
        }
    ],
    "corpus": "mel"
}