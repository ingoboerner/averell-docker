{
    "poem_title": "REMÉMORATION D'AMIS BELGES",
    "author": "Stéphane Mallarmé",
    "structure": "sonnet",
    "year": "1887",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "A des heures et sans que tel souffle l'émeuve",
                    "metrical_pattern": null,
                    "line_length": "6−6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Toute la vétusté presque couleur encens",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Comme furtive d'elle et visible je sens",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Que se dévêt pli selon pli la pierre veuve",
                    "metrical_pattern": null,
                    "line_length": "4+4+4",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "A des heures et sans que tel souffle l'émeuve\nToute la vétusté presque couleur encens\nComme furtive d'elle et visible je sens\nQue se dévêt pli selon pli la pierre veuve"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Flotte ou semble par soi n'apporter une preuve",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Sinon d'épandre pour baume antique le temps",
                    "metrical_pattern": null,
                    "line_length": "6−6",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Nous immémoriaux quelques-uns si contents",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Sur la soudaineté de notre amitié neuve",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Flotte ou semble par soi n'apporter une preuve\nSinon d'épandre pour baume antique le temps\nNous immémoriaux quelques-uns si contents\nSur la soudaineté de notre amitié neuve"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "O très chers rencontrés en le jamais banal",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "c"
                },
                {
                    "line_number": 10,
                    "line_text": "Bruges multipliant l'aube au défunt canal",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "c"
                },
                {
                    "line_number": 11,
                    "line_text": "Avec la promenade éparse de maint cygne",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "O très chers rencontrés en le jamais banal\nBruges multipliant l'aube au défunt canal\nAvec la promenade éparse de maint cygne"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Quand solennellement cette cité m'apprit",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "e"
                },
                {
                    "line_number": 13,
                    "line_text": "Lesquels entre ses fils un autre vol désigne",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "d"
                },
                {
                    "line_number": 14,
                    "line_text": "À prompte irradier ainsi qu'aile l'esprit.",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "e"
                }
            ],
            "stanza_text": "Quand solennellement cette cité m'apprit\nLesquels entre ses fils un autre vol désigne\nÀ prompte irradier ainsi qu'aile l'esprit."
        }
    ],
    "corpus": "mel"
}