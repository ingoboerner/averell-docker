                {
                    "line_number": 1,
                    "line_text": "Quand sa chasse est finie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Le poëte oiseleur",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "L'outil du ciseleur.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Quand sa chasse est finie,\nLe poëte oiseleur\nL'outil du ciseleur."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Car il faut qu'il meurtrisse,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Pour y graver son pur",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 6,
                    "line_text": "Un métal au cœur dur.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Car il faut qu'il meurtrisse,\nPour y graver son pur\nUn métal au cœur dur."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Pas de travail commode !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Tu prétends, comme moi,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 9,
                    "line_text": "Garde sa vieille loi,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Pas de travail commode !\nTu prétends, comme moi,\nGarde sa vieille loi,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Et que, brillant et ferme,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Le beau rhythme d'airain",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "L'idée au front serein.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et que, brillant et ferme,\nLe beau rhythme d'airain\nL'idée au front serein."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Car toi qui, fou d'extase,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Mènes par les grands cieux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "Le cheval aux beaux yeux ;",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Car toi qui, fou d'extase,\nMènes par les grands cieux\nLe cheval aux beaux yeux ;"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Toi qui sur une grève",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Sais prendre en ton réseau",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 18,
                    "line_text": "Comme un farouche oiseau ;",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Toi qui sur une grève\nSais prendre en ton réseau\nComme un farouche oiseau ;"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Maître, qui nous enseignes",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "L'amour du vert laurier,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 21,
                    "line_text": "Être un bon ouvrier.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Maître, qui nous enseignes\nL'amour du vert laurier,\nÊtre un bon ouvrier."
        }
