{
    "poem_title": "A la Font-Georges",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1843-1846",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "O champs pleins de silence,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Où mon heureuse enfance",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Avait des jours encor",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O champs pleins de silence,\nOù mon heureuse enfance\nAvait des jours encor"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "O ma vieille Font-Georges,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Vers qui les rouges-gorges",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Et le doux rossignol",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O ma vieille Font-Georges,\nVers qui les rouges-gorges\nEt le doux rossignol"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Maison blanche où la vigne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Tordait en longue ligne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 9,
                    "line_text": "Son feuillage qui boit",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Maison blanche où la vigne\nTordait en longue ligne\nSon feuillage qui boit"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "O claire source froide,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Qu'ombrageait, vieux et roide,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Un noyer vigoureux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O claire source froide,\nQu'ombrageait, vieux et roide,\nUn noyer vigoureux"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Sources ! fraîches fontaines !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Qui, douces à mes peines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Frémissiez autrefois",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "Rien qu'à ma voix !",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sources ! fraîches fontaines !\nQui, douces à mes peines,\nFrémissiez autrefois\nRien qu'à ma voix !"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Bassin où les laveuses",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Chantaient insoucieuses",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 19,
                    "line_text": "En battant sur leur banc",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Bassin où les laveuses\nChantaient insoucieuses\nEn battant sur leur banc"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 20,
                    "line_text": "O sorbier centenaire,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 21,
                    "line_text": "Dont trois coups de tonnerre",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Avaient laissé tout nu",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O sorbier centenaire,\nDont trois coups de tonnerre\nAvaient laissé tout nu"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Tonnelles et coudrettes,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Verdoyantes retraites",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 25,
                    "line_text": "De peupliers mouvants",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tonnelles et coudrettes,\nVerdoyantes retraites\nDe peupliers mouvants"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 26,
                    "line_text": "O vignes purpurines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Dont, le long des collines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Les ceps accumulés",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O vignes purpurines,\nDont, le long des collines,\nLes ceps accumulés"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Où, l'automne venue,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "La Vendange mi-nue",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 31,
                    "line_text": "A l'entour du pressoir",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 32,
                    "line_text": "Dansait le soir !",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Où, l'automne venue,\nLa Vendange mi-nue\nA l'entour du pressoir\nDansait le soir !"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "O buissons d'églantines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Jetant dans les ravines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Comme un chêne le gland,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O buissons d'églantines,\nJetant dans les ravines,\nComme un chêne le gland,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 36,
                    "line_text": "Murmurante oseraie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 37,
                    "line_text": "Où le ramier s'effraie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Saule au feuillage bleu,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Murmurante oseraie,\nOù le ramier s'effraie,\nSaule au feuillage bleu,"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "Rameaux lourds de cerises !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 40,
                    "line_text": "Moissonneuses surprises",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "A mi-jambe dans l'eau",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Rameaux lourds de cerises !\nMoissonneuses surprises\nA mi-jambe dans l'eau"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 42,
                    "line_text": "Antres, chemins, fontaines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 43,
                    "line_text": "Acres parfums et plaines,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Ombrages et rochers",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Antres, chemins, fontaines,\nAcres parfums et plaines,\nOmbrages et rochers"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Ruisseaux ! forêts ! silence !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "O mes amours d'enfance !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Mon âme, sans témoins,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 48,
                    "line_text": "Vous aime moins",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ruisseaux ! forêts ! silence !\nO mes amours d'enfance !\nMon âme, sans témoins,\nVous aime moins"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Que ce jardin morose",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Sans verdure et sans rose",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Et ces sombres massifs",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Que ce jardin morose\nSans verdure et sans rose\nEt ces sombres massifs"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 52,
                    "line_text": "Et ce chemin de sable,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 53,
                    "line_text": "Où j'eus l'heur ineffable,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Pour la première fois,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et ce chemin de sable,\nOù j'eus l'heur ineffable,\nPour la première fois,"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Où rêveuse, l'amie",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "Doucement obéie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 57,
                    "line_text": "S'appuyant à mon bras,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Où rêveuse, l'amie\nDoucement obéie,\nS'appuyant à mon bras,"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 58,
                    "line_text": "Pensive et recueillie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 59,
                    "line_text": "Et d'une fleur cueillie",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 60,
                    "line_text": "Brisant le cœur discret",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pensive et recueillie,\nEt d'une fleur cueillie\nBrisant le cœur discret"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "A l'heure où les étoiles",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Frissonnant sous leurs voiles",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 63,
                    "line_text": "Brodent le ciel changeant",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "A l'heure où les étoiles\nFrissonnant sous leurs voiles\nBrodent le ciel changeant"
        }
