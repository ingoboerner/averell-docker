{
    "poem_title": "Lutte",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1888",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "La Muse est divine, et sans cesse",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Elle restera ma déesse",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La Muse est divine, et sans cesse\nElle restera ma déesse"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Jamais celle que j'ai suivie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Mais, certes, lutter pour la vie",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Jamais celle que j'ai suivie\nMais, certes, lutter pour la vie"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Ah ! sous le rouge crépuscule",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Le meilleur lutteur fut Hercule,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ah ! sous le rouge crépuscule\nLe meilleur lutteur fut Hercule,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Car agile en ses peccadilles",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Il faisait à cinquante filles",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Car agile en ses peccadilles\nIl faisait à cinquante filles"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Prolixité digne d'envie !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "De pouvoir prodiguer la vie",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Prolixité digne d'envie !\nDe pouvoir prodiguer la vie"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Mais songer à soi-même, vivre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Si bien que parfois on est ivre",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais songer à soi-même, vivre,\nSi bien que parfois on est ivre"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Au Paris de la rive gauche,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Persiste encore la débauche",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Au Paris de la rive gauche,\nPersiste encore la débauche"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Plus d'un jeune mélancolique,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Y meurtrit sa dent famélique",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Plus d'un jeune mélancolique,\nY meurtrit sa dent famélique"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Mais, ô lutte sourde et hagarde !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Le bifteck, pareil à la garde,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais, ô lutte sourde et hagarde !\nLe bifteck, pareil à la garde,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Tel esquisse, en mots d'une lieue,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Héros d'une histoire sans queue",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tel esquisse, en mots d'une lieue,\nHéros d'une histoire sans queue"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Le clown, dans une triste fête",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Orne d'un toupet bleu sa tête.",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le clown, dans une triste fête\nOrne d'un toupet bleu sa tête."
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Près des vieux aux lourdes paupières,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Vois, les pierreuses, dans les pierres,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Près des vieux aux lourdes paupières,\nVois, les pierreuses, dans les pierres,"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Et c'est la même coqueluche",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Que sur les coussins de peluche",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et c'est la même coqueluche\nQue sur les coussins de peluche"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Torgnole, fuyant comme un lièvre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Vend le froid baiser de la fièvre",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Torgnole, fuyant comme un lièvre\nVend le froid baiser de la fièvre"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Et de même, Anna que mignote",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Avec ses quenottes grignote",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et de même, Anna que mignote\nAvec ses quenottes grignote"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Pour toute l'humanité blême",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "C'est toujours l'unique problème :",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pour toute l'humanité blême\nC'est toujours l'unique problème :"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "L'Homme obéit, l'Or est le maître",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Et c'est bien l'Être ou ne pas être",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'Homme obéit, l'Or est le maître\nEt c'est bien l'Être ou ne pas être"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Et toi qui n'as pas de colère,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Puisque tu n'obtiens pour salaire",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et toi qui n'as pas de colère,\nPuisque tu n'obtiens pour salaire"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Puisque la salle où se goberge",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Te dédaigne, comme l'auberge,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puisque la salle où se goberge\nTe dédaigne, comme l'auberge,"
        }
    ],
    "corpus": "mel"
}