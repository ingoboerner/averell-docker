{
    "poem_title": "Retour",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1892",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Chasseurs, gent carnassière,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Secouez la poussière",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "De vos chapeaux flétris.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Chasseurs, gent carnassière,\nSecouez la poussière\nDe vos chapeaux flétris."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Et vous, rois du beau monde,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Qui, dans la clarté blonde,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Traîniez par les chemins",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et vous, rois du beau monde,\nQui, dans la clarté blonde,\nTraîniez par les chemins"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Rentrez, battant d'une aile,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Dans la ville éternelle",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 9,
                    "line_text": "Aux vastes horizons",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Rentrez, battant d'une aile,\nDans la ville éternelle\nAux vastes horizons"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Vous couvriez de malles",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Nos stations thermales.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Toutes ces villes d'eaux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Vous couvriez de malles\nNos stations thermales.\nToutes ces villes d'eaux"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "On y flirte à son aise",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Dans l'or d'une fournaise",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Où les seuls gens marris",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "On y flirte à son aise\nDans l'or d'une fournaise\nOù les seuls gens marris"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Ah ! lorsque juin farouche",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Pose sur notre bouche",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "D'ardents charbons de feu",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ah ! lorsque juin farouche\nPose sur notre bouche\nD'ardents charbons de feu"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "S'en aller, c'est un rêve !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Errer sur quelque grève,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 21,
                    "line_text": "C'est un luxe enchanté",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "S'en aller, c'est un rêve !\nErrer sur quelque grève,\nC'est un luxe enchanté"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "On s'enivre à l'aurore",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Du hurlement sonore",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Qui monte de la mer",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "On s'enivre à l'aurore\nDu hurlement sonore\nQui monte de la mer"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Ou la forêt hagarde",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Longuement vous regarde",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Avec ses profonds yeux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ou la forêt hagarde\nLonguement vous regarde\nAvec ses profonds yeux"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Sur les montagnes blanches",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 29,
                    "line_text": "Où sont les avalanches,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "La neige des sommets",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sur les montagnes blanches\nOù sont les avalanches,\nLa neige des sommets"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Paris, qui chante et souffre,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Est la mer et le gouffre",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 33,
                    "line_text": "Avec ses larges flots",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Paris, qui chante et souffre,\nEst la mer et le gouffre\nAvec ses larges flots"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "La mégère hargneuse",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Près d'une Maufrigneuse,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "La fleur rose et l'égout,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La mégère hargneuse\nPrès d'une Maufrigneuse,\nLa fleur rose et l'égout,"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Les toisons de ces femmes",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Y déroulent des gammes,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Et les cœurs des huissiers",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Les toisons de ces femmes\nY déroulent des gammes,\nEt les cœurs des huissiers"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "A Paris, Cléopâtre",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "Vit, et montrant l'albâtre",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "De son sein effréné,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "A Paris, Cléopâtre\nVit, et montrant l'albâtre\nDe son sein effréné,"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "C'est la métempsycose !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Et comme gaîment cause",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 45,
                    "line_text": "Cet effronté bavard,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "C'est la métempsycose !\nEt comme gaîment cause\nCet effronté bavard,"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 46,
                    "line_text": "Il sied d'y faire halte.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Oh ! comme sur l'asphalte",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "On y voit des palais",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Il sied d'y faire halte.\nOh ! comme sur l'asphalte\nOn y voit des palais"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Mais parlons des théâtres",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Sérieux et folâtres.",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Verrons-nous un Reszké ?",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais parlons des théâtres\nSérieux et folâtres.\nVerrons-nous un Reszké ?"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 52,
                    "line_text": "Oui, dans notre caverne,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 53,
                    "line_text": "La Mode offre et gouverne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Bien plus de Koh-innors",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Oui, dans notre caverne,\nLa Mode offre et gouverne\nBien plus de Koh-innors"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Des lèvres et des âmes",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "Qu'en vain nous épuisâmes",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 57,
                    "line_text": "Coule un flot d'infini",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Des lèvres et des âmes\nQu'en vain nous épuisâmes\nCoule un flot d'infini"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 58,
                    "line_text": "L'esprit joue et s'envole",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 59,
                    "line_text": "Comme un oiseau frivole",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 60,
                    "line_text": "Montant et descendant ;",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "L'esprit joue et s'envole\nComme un oiseau frivole\nMontant et descendant ;"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Comme, la trouvant belle,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Un enfant saisit l'aile",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 63,
                    "line_text": "Peinte de vermillon",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Comme, la trouvant belle,\nUn enfant saisit l'aile\nPeinte de vermillon"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 64,
                    "line_text": "Le flâneur, qui s'amuse",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 65,
                    "line_text": "Du babil de la muse,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "Prend et saisit au vol",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le flâneur, qui s'amuse\nDu babil de la muse,\nPrend et saisit au vol"
        }
    ],
    "corpus": "mel"
}