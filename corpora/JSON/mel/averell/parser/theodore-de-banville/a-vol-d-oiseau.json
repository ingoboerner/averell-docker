            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Oh ! de l'azur, où mille astres exorbitants",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Comment vois-tu ce globe écrasé d'habitants ?",
