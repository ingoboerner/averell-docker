{
    "poem_title": "A Arsène Houssaye",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1856",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Grâce aux Dalilas,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Nos rimeurs sont las",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Et, comme un hochet,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Ont jeté l'archet",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Grâce aux Dalilas,\nNos rimeurs sont las\nEt, comme un hochet,\nOnt jeté l'archet"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Au rhythme ailé d'or",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Il fallait encor",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 7,
                    "line_text": "Fou de volupté,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Alors j'ai dompté",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Au rhythme ailé d'or\nIl fallait encor\nFou de volupté,\nAlors j'ai dompté"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "J'ai repris mon luth,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Et, suivant le but",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Je m'en vais cherchant",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Le secret du chant",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "J'ai repris mon luth,\nEt, suivant le but\nJe m'en vais cherchant\nLe secret du chant"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Œil épanoui,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Je peins ébloui",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Le ciel radieux,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "Et, mélodieux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Œil épanoui,\nJe peins ébloui\nLe ciel radieux,\nEt, mélodieux"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Près du fleuve grec",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Murmurant avec",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 19,
                    "line_text": "Fiers de leur candeur,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 20,
                    "line_text": "Je dis la splendeur",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Près du fleuve grec\nMurmurant avec\nFiers de leur candeur,\nJe dis la splendeur"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Mon vin triomphant,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Sais-tu quelle enfant",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Viens, et tu verras,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "Poëte, quel bras",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Mon vin triomphant,\nSais-tu quelle enfant\nViens, et tu verras,\nPoëte, quel bras"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "O chasseur altier,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Qui fuis le sentier",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Songeur qu'autrefois",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 28,
                    "line_text": "Rencontrait au bois",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "O chasseur altier,\nQui fuis le sentier\nSongeur qu'autrefois\nRencontrait au bois"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Comme toi, qui vins",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Si jeune aux divins",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 31,
                    "line_text": "Ami, j'ai toujours",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 32,
                    "line_text": "Voulu des amours",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Comme toi, qui vins\nSi jeune aux divins\nAmi, j'ai toujours\nVoulu des amours"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Ah ! quand Mai sourit",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Aux prés où fleurit",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Trouveurs de loisir,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 36,
                    "line_text": "Sachons y choisir",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Ah ! quand Mai sourit\nAux prés où fleurit\nTrouveurs de loisir,\nSachons y choisir"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Nymphe au regard bleu,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Si sa lèvre en feu",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Nos fronts sans témoins,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 40,
                    "line_text": "Qu'elle soit au moins",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Nymphe au regard bleu,\nSi sa lèvre en feu\nNos fronts sans témoins,\nQu'elle soit au moins"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Toi, pâle et rêvant,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Au bois que le vent",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 43,
                    "line_text": "Tu suis à dessein",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 44,
                    "line_text": "La guerrière au sein",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Toi, pâle et rêvant,\nAu bois que le vent\nTu suis à dessein\nLa guerrière au sein"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Moi, parmi nos jeux,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Mon plus orageux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Toujours s'en revient",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 48,
                    "line_text": "Vers celle qui tient",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Moi, parmi nos jeux,\nMon plus orageux\nToujours s'en revient\nVers celle qui tient"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Sans doute elle a pris",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "La foule en mépris,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Un peu trop souvent",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 52,
                    "line_text": "Sa crinière au vent.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Sans doute elle a pris\nLa foule en mépris,\nUn peu trop souvent\nSa crinière au vent."
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "J'aime sa pâleur,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Et sa bouche en fleur",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 55,
                    "line_text": "Son sang et sa chair",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 56,
                    "line_text": "Les voilà, mon cher",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "J'aime sa pâleur,\nEt sa bouche en fleur\nSon sang et sa chair\nLes voilà, mon cher"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "O sens embrasés !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "Maîtresse aux baisers",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 59,
                    "line_text": "Tendre et chère voix,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 60,
                    "line_text": "Ici tu la vois",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "O sens embrasés !\nMaîtresse aux baisers\nTendre et chère voix,\nIci tu la vois"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Dos flexible et nu !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Sourire ingénu",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 63,
                    "line_text": "L'or de ses cheveux",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 64,
                    "line_text": "M'enivre, et je veux,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Dos flexible et nu !\nSourire ingénu\nL'or de ses cheveux\nM'enivre, et je veux,"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Dans mon sang qui bout",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "Gardant jusqu'au bout",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 67,
                    "line_text": "Tout comme à présent,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 68,
                    "line_text": "Mourir en baisant",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Dans mon sang qui bout\nGardant jusqu'au bout\nTout comme à présent,\nMourir en baisant"
        }
    ],
    "corpus": "mel"
}