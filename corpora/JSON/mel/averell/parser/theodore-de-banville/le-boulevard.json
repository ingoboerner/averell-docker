                {
                    "line_number": 1,
                    "line_text": "Sur le grand boulevard,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Tout un peuple bavard,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sur le grand boulevard,\nTout un peuple bavard,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Comme il va, par troupeaux,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Oh ! qu'on voit de chapeaux",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Comme il va, par troupeaux,\nOh ! qu'on voit de chapeaux"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Obstinés et têtus,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Tous les souliers pointus",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Obstinés et têtus,\nTous les souliers pointus"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Et pour nous inspirer",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "De même on voit errer",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et pour nous inspirer\nDe même on voit errer"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Flambants et rayonnants,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Brillent de surprenants",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Flambants et rayonnants,\nBrillent de surprenants"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Plus loin le regard, las",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Trouve les blancs lilas,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Plus loin le regard, las\nTrouve les blancs lilas,"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Et parmi d'autres fleurs,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Que l'on admire, ailleurs,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et parmi d'autres fleurs,\nQue l'on admire, ailleurs,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Des hommes bruns, vantant",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Boivent, en se hâtant,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Des hommes bruns, vantant\nBoivent, en se hâtant,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Et la trombe de vent",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Tourbillonne devant",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et la trombe de vent\nTourbillonne devant"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Le monde insoucieux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Mais tout a coup des cieux",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le monde insoucieux\nMais tout a coup des cieux"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Chacun se hâte, et sous",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Des femmes à l'œil doux",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Chacun se hâte, et sous\nDes femmes à l'œil doux"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "L'eau qui tombe d'en haut,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
