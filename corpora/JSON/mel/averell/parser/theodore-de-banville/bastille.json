            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Dans sa gueule vorace",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Les tribuns, dont la race",
