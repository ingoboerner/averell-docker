                {
                    "line_number": 1,
                    "line_text": "Acteurs mélodieux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "De grâce, au nom des Dieux,",
