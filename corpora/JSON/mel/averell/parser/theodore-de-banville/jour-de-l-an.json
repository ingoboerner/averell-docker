                {
                    "line_number": 1,
                    "line_text": "Fillette rose et fier bandit",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Les petits enfants nous ont dit,",
