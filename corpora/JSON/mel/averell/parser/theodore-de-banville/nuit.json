{
    "poem_title": "Nuit",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1892",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Roch, le bon noctambule,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Amoureux comme feu",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Erre sous le ciel bleu.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Roch, le bon noctambule,\nAmoureux comme feu\nErre sous le ciel bleu."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Oubliant nos désastres,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Il voit les diamants",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 6,
                    "line_text": "Pleins d'éblouissements.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Oubliant nos désastres,\nIl voit les diamants\nPleins d'éblouissements."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Ivre du pur silence",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Où le rêve subtil",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 9,
                    "line_text": "Oh ! maintenant, dit-il,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ivre du pur silence\nOù le rêve subtil\nOh ! maintenant, dit-il,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Je renais, je respire !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Je me récite du",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Ce n'est pas défendu.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je renais, je respire !\nJe me récite du\nCe n'est pas défendu."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Oh ! quelle joie immense !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "La nuit, ce temps béni",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "Et le jour est fini.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Oh ! quelle joie immense !\nLa nuit, ce temps béni\nEt le jour est fini."
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Ayant quitté leurs bagues,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Par le sommeil touchés,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 18,
                    "line_text": "Figurants sont couchés.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ayant quitté leurs bagues,\nPar le sommeil touchés,\nFigurants sont couchés."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Ronflant en folles gammes,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Ces gens dorment avec",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 21,
                    "line_text": "A Paris comme au Pecq.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ronflant en folles gammes,\nCes gens dorment avec\nA Paris comme au Pecq."
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Et les filles de joie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Bataillon que la Faim",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "Se reposent enfin.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et les filles de joie,\nBataillon que la Faim\nSe reposent enfin."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "D'une main rude et forte",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "J'ai vu que l'on barrait",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 27,
                    "line_text": "Du fauve cabaret.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "D'une main rude et forte\nJ'ai vu que l'on barrait\nDu fauve cabaret."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Tout gorgés d'écrevisses,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 29,
                    "line_text": "Les fêteurs ont quitté",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 30,
                    "line_text": "Et leur iniquité.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tout gorgés d'écrevisses,\nLes fêteurs ont quitté\nEt leur iniquité."
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Je suis seul, ô mon rêve !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Mon regard triste et pur",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 33,
                    "line_text": "Jusqu'au limpide azur.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je suis seul, ô mon rêve !\nMon regard triste et pur\nJusqu'au limpide azur."
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "Et d'anges ou d'oiselles",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Dans l'éther bleu, je vois",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 36,
                    "line_text": "Et j'écoute des voix.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et d'anges ou d'oiselles\nDans l'éther bleu, je vois\nEt j'écoute des voix."
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Où sont les faux artistes",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Et sur les boulevarts",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 39,
                    "line_text": "Cortèges des bavards ?",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Où sont les faux artistes\nEt sur les boulevarts\nCortèges des bavards ?"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "Évanouis ! La terre,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "Dans un délicieux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 42,
                    "line_text": "Murmure avec les cieux.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Évanouis ! La terre,\nDans un délicieux\nMurmure avec les cieux."
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Certes, parfois la Vie",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Prépare d'affreux mets,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 45,
                    "line_text": "De nous les servir ; mais",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Certes, parfois la Vie\nPrépare d'affreux mets,\nDe nous les servir ; mais"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 46,
                    "line_text": "Elle n'est plus mauvaise,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Lorsque seul et sans bruit",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                },
                {
                    "line_number": 48,
                    "line_text": "La caressante Nuit.",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Elle n'est plus mauvaise,\nLorsque seul et sans bruit\nLa caressante Nuit."
        }
    ],
    "corpus": "mel"
}