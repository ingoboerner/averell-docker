{
    "poem_title": "ÉCRIT SUR UN EXEMPLAIRE\nDES ODELETTES",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1857",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Quand j'ai fait ceci,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Moi que nul souci",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "La fièvre de l'or",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Nous tenait encor :",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Quand j'ai fait ceci,\nMoi que nul souci\nLa fièvre de l'or\nNous tenait encor :"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Pendant ces moments,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Comme les romans",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 7,
                    "line_text": "Le joyeux About,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Elle avait pris tout",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Pendant ces moments,\nComme les romans\nLe joyeux About,\nElle avait pris tout"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Vous rappelez-vous",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Les efforts jaloux,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Les peurs, les succès ?",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Le combat eut ses",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Vous rappelez-vous\nLes efforts jaloux,\nLes peurs, les succès ?\nLe combat eut ses"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Oh ! Qu'il fut ardent,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Hélas ! Moi pendant",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Et son bruit d'enfer,",
                    "metrical_pattern": null,
                    "line_length": "2",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "J'essayais un air",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Oh ! Qu'il fut ardent,\nHélas ! Moi pendant\nEt son bruit d'enfer,\nJ'essayais un air"
        }
    ],
    "corpus": "mel"
}