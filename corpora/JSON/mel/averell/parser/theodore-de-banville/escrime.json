{
    "poem_title": "Escrime",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1883-1884",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Chez nous l'Éternel Féminin",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "A pris un essor léonin.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Les femmes les plus délicates",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Chez nous l'Éternel Féminin\nA pris un essor léonin.\nLes femmes les plus délicates"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "D'autres, ayant le charme empreint",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Sur leur front, dont nous n'avions craint",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Que les œillades assassines,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "D'autres, ayant le charme empreint\nSur leur front, dont nous n'avions craint\nQue les œillades assassines,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Celles-là, dont le vent mutin",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "A follement, dès le matin,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 9,
                    "line_text": "Baisé les boucles et les tresses,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Celles-là, dont le vent mutin\nA follement, dès le matin,\nBaisé les boucles et les tresses,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Celles-ci, cœurs inexpliqués,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Mettent en rhythmes compliqués",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Leurs mélodieuses tristesses",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Celles-ci, cœurs inexpliqués,\nMettent en rhythmes compliqués\nLeurs mélodieuses tristesses"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "D'autres par l'esprit le plus fin",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Nous ravissent. D'autres enfin,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Et certes ce n'est pas un crime",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "D'autres par l'esprit le plus fin\nNous ravissent. D'autres enfin,\nEt certes ce n'est pas un crime"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Elles en font même très bien.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Carolus Duran ne sait rien",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Vraiment que désormais ignore",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Elles en font même très bien.\nCarolus Duran ne sait rien\nVraiment que désormais ignore"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Ces tireurs, qu'Amour effleurait,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Tiennent maintenant le fleuret,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 21,
                    "line_text": "Enchaînant avec mille charmes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ces tireurs, qu'Amour effleurait,\nTiennent maintenant le fleuret,\nEnchaînant avec mille charmes"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Que n'as-tu pu voir, ô Balzac !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Leurs ripostes du tac au tac,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Leur jeu correct et leur mimique",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Que n'as-tu pu voir, ô Balzac !\nLeurs ripostes du tac au tac,\nLeur jeu correct et leur mimique"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Aussi bien que l'homme hideux,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Elles savent faire : Une ! Deux !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Quant à leurs attaques d'allonge,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Aussi bien que l'homme hideux,\nElles savent faire : Une ! Deux !\nQuant à leurs attaques d'allonge,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Qu'elles mènent agilement",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 29,
                    "line_text": "Les changements d'engagement !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Quand un homme est leur adversaire,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qu'elles mènent agilement\nLes changements d'engagement !\nQuand un homme est leur adversaire,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Car bien vite mécontenté,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Il est toujours au fond tenté",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 33,
                    "line_text": "De tomber aux pieds de ce sexe",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Car bien vite mécontenté,\nIl est toujours au fond tenté\nDe tomber aux pieds de ce sexe"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "Il se sent devenir poltron",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "A voir frémir sous le plastron,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Comme une cruelle épigramme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Il se sent devenir poltron\nA voir frémir sous le plastron,\nComme une cruelle épigramme,"
        }
    ],
    "corpus": "mel"
}