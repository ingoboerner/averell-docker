{
    "poem_title": "VARIATIONS LYRIQUES",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1857",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Le carnaval s'amuse !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Viens le chanter, ma muse,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Sur un rhythme gaillard",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le carnaval s'amuse !\nViens le chanter, ma muse,\nSur un rhythme gaillard"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Et d'abord, sur ta nuque,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "En dépit de l'eunuque,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Fais flotter tes cheveux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et d'abord, sur ta nuque,\nEn dépit de l'eunuque,\nFais flotter tes cheveux"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Chante ton dithyrambe",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "En laissant voir ta jambe",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 9,
                    "line_text": "Et ton sein arrosé",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Chante ton dithyrambe\nEn laissant voir ta jambe\nEt ton sein arrosé"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Laisse même, ô déesse,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Avec ta blonde tresse,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Le maillot des Keller",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Laisse même, ô déesse,\nAvec ta blonde tresse,\nLe maillot des Keller"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Puisque je congédie",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Les vers de tragédie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Laisse le décorum",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puisque je congédie\nLes vers de tragédie,\nLaisse le décorum"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "La tunique et les voiles",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Semés d'un ciel d'étoiles,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Et les manteaux épars",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La tunique et les voiles\nSemés d'un ciel d'étoiles,\nEt les manteaux épars"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Que ses vierges plaintives,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Catholiques ou juives,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 21,
                    "line_text": "Tiennent des sanhédrins",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Que ses vierges plaintives,\nCatholiques ou juives,\nTiennent des sanhédrins"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Mais toi, sans autre insigne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Que la feuille de vigne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Et les souples accords",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais toi, sans autre insigne\nQue la feuille de vigne\nEt les souples accords"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Laisse ton sein de neige",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Chanter tout le solfège",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "De ses accords pourprés,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Laisse ton sein de neige\nChanter tout le solfège\nDe ses accords pourprés,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Ou bien, mon adorée,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 29,
                    "line_text": "Prends la veste dorée",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Et le soulier verni",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ou bien, mon adorée,\nPrends la veste dorée\nEt le soulier verni"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Mets ta ceinture, et plaque",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Sur le velours d'un claque",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 33,
                    "line_text": "Les rubans querelleurs",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mets ta ceinture, et plaque\nSur le velours d'un claque\nLes rubans querelleurs"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "Fais, sur plus de richesses",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Que n'en ont les duchesses,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Coller jusqu'au talon",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Fais, sur plus de richesses\nQue n'en ont les duchesses,\nColler jusqu'au talon"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Dans tes lèvres écloses",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Mets les cris et les poses",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Et les folles ardeurs",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Dans tes lèvres écloses\nMets les cris et les poses\nEt les folles ardeurs"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "Puis, sans peur ni réserve,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "Réchauffant de ta verve",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Le mollet engourdi",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puis, sans peur ni réserve,\nRéchauffant de ta verve\nLe mollet engourdi"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Sur tes pas fiers et souples",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Traînant cent mille couples,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 45,
                    "line_text": "Montre leur jusqu'où va",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sur tes pas fiers et souples\nTraînant cent mille couples,\nMontre leur jusqu'où va"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 46,
                    "line_text": "Et, dans le bal féerique,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Hurle un rhythme lyrique",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Dont tu feras cadeau",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et, dans le bal féerique,\nHurle un rhythme lyrique\nDont tu feras cadeau"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Tapez, pierrots et masques,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Sur vos tambours de basques !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Faites de vos grelots",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tapez, pierrots et masques,\nSur vos tambours de basques !\nFaites de vos grelots"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 52,
                    "line_text": "Formidables orgies,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 53,
                    "line_text": "Suivez sous les bougies",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Les sax aux voix de fer",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Formidables orgies,\nSuivez sous les bougies\nLes sax aux voix de fer"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "Sous le gaz de Labeaume",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "Hurrah ! Suivez le heaume",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 57,
                    "line_text": "Et la cuirasse d'or",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sous le gaz de Labeaume\nHurrah ! Suivez le heaume\nEt la cuirasse d'or"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 58,
                    "line_text": "Et madame panache,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 59,
                    "line_text": "Dont le front se harnache",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 60,
                    "line_text": "De douze ou quinze bouts",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et madame panache,\nDont le front se harnache\nDe douze ou quinze bouts"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "Au son de la musette",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Suivez ange et frisette,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 63,
                    "line_text": "Et ce joli poupon,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Au son de la musette\nSuivez ange et frisette,\nEt ce joli poupon,"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 64,
                    "line_text": "Et Blanche aux belles formes,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 65,
                    "line_text": "Dont les cheveux énormes",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "Ont été peints, je crois,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et Blanche aux belles formes,\nDont les cheveux énormes\nOnt été peints, je crois,"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 67,
                    "line_text": "De même que la Loire",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 68,
                    "line_text": "Se promène avec gloire",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 69,
                    "line_text": "Dans son grand corridor",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "De même que la Loire\nSe promène avec gloire\nDans son grand corridor"
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 70,
                    "line_text": "Sa chevelure rousse",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 71,
                    "line_text": "Coule, orgueilleuse et douce ;",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 72,
                    "line_text": "Elle épouvanterait",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sa chevelure rousse\nCoule, orgueilleuse et douce ;\nElle épouvanterait"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 73,
                    "line_text": "Chantez, musique et danse !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 74,
                    "line_text": "Que le doux vin de France",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 75,
                    "line_text": "Tombe dans le cristal",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Chantez, musique et danse !\nQue le doux vin de France\nTombe dans le cristal"
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 76,
                    "line_text": "Pas de pudeur bégueule !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 77,
                    "line_text": "Amis ! La France seule",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 78,
                    "line_text": "Est l'aimable et divin",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pas de pudeur bégueule !\nAmis ! La France seule\nEst l'aimable et divin"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 79,
                    "line_text": "Laissons à l'Angleterre",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 80,
                    "line_text": "Ses brouillards et sa bière !",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 81,
                    "line_text": "Laissons-la dans le gin",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Laissons à l'Angleterre\nSes brouillards et sa bière !\nLaissons-la dans le gin"
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 82,
                    "line_text": "Que la pâle Ophélie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 83,
                    "line_text": "En sa mélancolie,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 84,
                    "line_text": "Cueille dans les roseaux",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Que la pâle Ophélie,\nEn sa mélancolie,\nCueille dans les roseaux"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 85,
                    "line_text": "Que, sensitive humaine,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
