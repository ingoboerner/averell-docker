        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Vous faisiez peu de façons,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Et d'aimables polissons",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
