            ],
            "stanza_text": "Les cieux resplendissants d'Étoiles\nAux radieux frissonnements,\nRessemblent à des flots dormants\nQue sillonnent de blanches voiles."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Quand l'azur déchire ses voiles,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Nous voyons les bleus firmaments,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "Les cieux resplendissants d'Étoiles",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Aux radieux frissonnements.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Quand l'azur déchire ses voiles,\nNous voyons les bleus firmaments,\nLes cieux resplendissants d'Étoiles\nAux radieux frissonnements."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Quel peintre mettra sur ses toiles,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "O Dieu ! ces clairs fourmillements,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Ces fournaises de diamants",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Qu'à mes yeux ravis tu dévoiles,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 13,
                    "line_text": "Les cieux resplendissants d'Étoiles ?",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Quel peintre mettra sur ses toiles,\nO Dieu ! ces clairs fourmillements,\nCes fournaises de diamants\nQu'à mes yeux ravis tu dévoiles,\nLes cieux resplendissants d'Étoiles ?"
        }
