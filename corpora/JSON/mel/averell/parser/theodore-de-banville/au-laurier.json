    "structure": "suite périodique",
    "year": "1892",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Si j'étais vraiment le bon ouvrier",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Que du noir oubli sa volonté sauve,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Ce que je voudrais, c'est toi, noir Laurier,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Si j'étais vraiment le bon ouvrier\nQue du noir oubli sa volonté sauve,\nCe que je voudrais, c'est toi, noir Laurier,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Car feuillage sombre, effroi des méchants,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Lorsque je te vois, mon âme savoure,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 6,
                    "line_text": "Devant tes rameaux, la gloire des chants",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Car feuillage sombre, effroi des méchants,\nLorsque je te vois, mon âme savoure,\nDevant tes rameaux, la gloire des chants"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Héros et rimeurs, sous les grands cieux clairs,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Nous sentons en nous le même délire",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 9,
                    "line_text": "Et la chaste Épée aux brillants éclairs",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Héros et rimeurs, sous les grands cieux clairs,\nNous sentons en nous le même délire\nEt la chaste Épée aux brillants éclairs"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Pour revivre un jour sur les blancs frontons,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Quand le clairon d'or enfle son haleine,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "C'est d'un cœur égal que nous combattons",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Pour revivre un jour sur les blancs frontons,\nQuand le clairon d'or enfle son haleine,\nC'est d'un cœur égal que nous combattons"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Henri Quatre, ainsi que François Premier,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Brûlé d'une ardeur jamais endormie,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "En quittant le casque au hardi cimier,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Henri Quatre, ainsi que François Premier,\nBrûlé d'une ardeur jamais endormie,\nEn quittant le casque au hardi cimier,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Et dans le passé farouche et saignant",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Quand mon souvenir enflammé recule,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 18,
                    "line_text": "Je revois Linos, chanteur, enseignant",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et dans le passé farouche et saignant\nQuand mon souvenir enflammé recule,\nJe revois Linos, chanteur, enseignant"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Eschyle, superbe entre les grands cœurs,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Pour qui les exploits sont des intermèdes,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 21,
                    "line_text": "Avant de rhythmer l'ode pour ses chœurs,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Eschyle, superbe entre les grands cœurs,\nPour qui les exploits sont des intermèdes,\nAvant de rhythmer l'ode pour ses chœurs,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Et le fauve Achille au casque mouvant,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Lorsque son armure était dégrafée,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "Charmait la cithare, et fut un savant",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et le fauve Achille au casque mouvant,\nLorsque son armure était dégrafée,\nCharmait la cithare, et fut un savant"
        }
    ],
    "corpus": "mel"
