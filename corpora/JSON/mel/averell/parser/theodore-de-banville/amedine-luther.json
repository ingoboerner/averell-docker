{
    "poem_title": "AMÉDINE LUTHER",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1867",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Adieu, bras de neige, adieu, front de rose !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Adieu, bras de neige, adieu, front de rose !"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 2,
                    "line_text": "Amédine, hélas ! Notre cher trésor !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Amédine, hélas ! Notre cher trésor !"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Elle était rieuse, elle était vermeille,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Elle était rieuse, elle était vermeille,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Ses cheveux tombaient en flots triomphants,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ses cheveux tombaient en flots triomphants,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Et resplendissaient, fiers de leur finesse,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et resplendissaient, fiers de leur finesse,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 6,
                    "line_text": "Ils prenaient dans l'ombre, et comme par jeu,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ils prenaient dans l'ombre, et comme par jeu,"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Et l'air se jouait parmi la dorure",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et l'air se jouait parmi la dorure"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 8,
                    "line_text": "Ô pâle ornement d'un front sidéral,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ô pâle ornement d'un front sidéral,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Nulle n'aura plus, nulle enfant au monde,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Nulle n'aura plus, nulle enfant au monde,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Qu'on voyait frémir autour de ton front !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Qu'on voyait frémir autour de ton front !"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Ces rayons riants qui dans les ravines",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ces rayons riants qui dans les ravines"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Lorsque tu courais, avec tes seize ans !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Lorsque tu courais, avec tes seize ans !"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Qu'ici-bas l'exil ne garde qu'une heure !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Qu'ici-bas l'exil ne garde qu'une heure !"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 14,
                    "line_text": "N'est-ce pas hier qu'en sa voix passait",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "N'est-ce pas hier qu'en sa voix passait"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Et qu'elle parut, foulant le théâtre",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et qu'elle parut, foulant le théâtre"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Si jeune, oh ! Si jeune, espoirs adorés !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Si jeune, oh ! Si jeune, espoirs adorés !"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Et sa voix naïve, et son front qui penche !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et sa voix naïve, et son front qui penche !"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 18,
                    "line_text": "Hélas ! Je la vois encor. Nous disions :",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Hélas ! Je la vois encor. Nous disions :"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "C'est elle ! Jamais lèvre plus choisie",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "C'est elle ! Jamais lèvre plus choisie"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 20,
                    "line_text": "Celle-ci n'est pas jeune pour un jour !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Celle-ci n'est pas jeune pour un jour !"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Pour jamais la grâce en fleur la décore",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Pour jamais la grâce en fleur la décore"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Et déjà, déjà, pauvre ange mortel,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et déjà, déjà, pauvre ange mortel,"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Dans l'immensité bleue aux sombres voiles",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Dans l'immensité bleue aux sombres voiles"
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 24,
                    "line_text": "Le lys est brisé. C'est fini. Plus rien",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Le lys est brisé. C'est fini. Plus rien"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Dont les cheveux blonds aux mourantes flammes",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Dont les cheveux blonds aux mourantes flammes"
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 26,
                    "line_text": "Mais, va, jeune grâce aux yeux si touchants !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais, va, jeune grâce aux yeux si touchants !"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Des rimeurs plaintifs qui savent encore",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Des rimeurs plaintifs qui savent encore"
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Ils diront comment tu fus notre sœur",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ils diront comment tu fus notre sœur"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Et comment ta voix eut l'attrait magique",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et comment ta voix eut l'attrait magique"
        },
        {
            "stanza_number": 30,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 30,
                    "line_text": "Amédine ! Aux champs tout la saluait,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Amédine ! Aux champs tout la saluait,"
        },
        {
            "stanza_number": 31,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Oh ! Rien qu'en disant ce nom d'Amédine,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Oh ! Rien qu'en disant ce nom d'Amédine,"
        },
        {
            "stanza_number": 32,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 32,
                    "line_text": "Et riante ; l'air baisait son bras nu ;",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et riante ; l'air baisait son bras nu ;"
        },
        {
            "stanza_number": 33,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Dans la forêt verte, où rit la pervenche,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Dans la forêt verte, où rit la pervenche,"
        },
        {
            "stanza_number": 34,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "Elle était la joie, elle était l'orgueil",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Elle était la joie, elle était l'orgueil"
        },
        {
            "stanza_number": 35,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Entoure à présent de crêpes funèbres !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Entoure à présent de crêpes funèbres !"
        },
        {
            "stanza_number": 36,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 36,
                    "line_text": "Pleurs désespérés, pleurs silencieux !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Pleurs désespérés, pleurs silencieux !"
        },
        {
            "stanza_number": 37,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Scintilleront, moi j'évoquerai celle",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Scintilleront, moi j'évoquerai celle"
        },
        {
            "stanza_number": 38,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 38,
                    "line_text": "Elle reviendra, mais, comme jadis,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Elle reviendra, mais, comme jadis,"
        },
        {
            "stanza_number": 39,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "Libre en sa Bretagne, errante et sans chaînes,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Libre en sa Bretagne, errante et sans chaînes,"
        },
        {
            "stanza_number": 40,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "Ou, comédienne aux riches habits,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ou, comédienne aux riches habits,"
        },
        {
            "stanza_number": 41,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Et des robes d'or, semant sa parole",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et des robes d'or, semant sa parole"
        },
        {
            "stanza_number": 42,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 42,
                    "line_text": "Et d'un pas léger grimpant le coteau",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et d'un pas léger grimpant le coteau"
        },
        {
            "stanza_number": 43,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Et plus tard, tous ceux dont la muse est reine,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et plus tard, tous ceux dont la muse est reine,"
        },
        {
            "stanza_number": 44,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 44,
                    "line_text": "Sur le front des fleurs met ses diamants,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Sur le front des fleurs met ses diamants,"
        },
        {
            "stanza_number": 45,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Écoutant avec le souffle des brises",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Écoutant avec le souffle des brises"
        },
        {
            "stanza_number": 46,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 46,
                    "line_text": "Reverront son pur visage, arrosé,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Reverront son pur visage, arrosé,"
        },
        {
            "stanza_number": 47,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "Et toi, lueur vive, aux reflets d'opale,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et toi, lueur vive, aux reflets d'opale,"
        },
        {
            "stanza_number": 48,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 48,
                    "line_text": "Qui baignais de feu son col et ses bras,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Qui baignais de feu son col et ses bras,"
        },
        {
            "stanza_number": 49,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Clair rayonnement, chevelure d'Ève,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Clair rayonnement, chevelure d'Ève,"
        },
        {
            "stanza_number": 50,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 50,
                    "line_text": "Amédine vit, ange au front doré !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Amédine vit, ange au front doré !"
        },
        {
            "stanza_number": 51,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Cherchant ses regards qui versaient les charmes,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Cherchant ses regards qui versaient les charmes,"
        }
    ],
    "corpus": "mel"
}