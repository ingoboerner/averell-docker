{
    "poem_title": "Élégie",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1843-1846",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tombez dans mon cœur, souvenirs confus,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tombez dans mon cœur, souvenirs confus,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 2,
                    "line_text": "Oh ! parlez-moi d'elle, antres et rochers,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Oh ! parlez-moi d'elle, antres et rochers,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Parlez, parlez d'elle, ô sentiers fleuris !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Parlez, parlez d'elle, ô sentiers fleuris !"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "O charmes amers ! dans ce frais décor",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "6"
                }
            ],
            "stanza_text": "O charmes amers ! dans ce frais décor"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "C'est elle, ô mon cœur ! sur ces gazons verts,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "C'est elle, ô mon cœur ! sur ces gazons verts,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 6,
                    "line_text": "Je vois s'envoler ses fins cheveux d'or",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "6"
                }
            ],
            "stanza_text": "Je vois s'envoler ses fins cheveux d'or"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Et notre amandier couvre son beau cou",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et notre amandier couvre son beau cou"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 8,
                    "line_text": "Sur mon bras frémit son bras ingénu,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Sur mon bras frémit son bras ingénu,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Le feuillage est noir, le ciel étoilé,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Le feuillage est noir, le ciel étoilé,"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "La belle-de-nuit s'ouvre toute en feu,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La belle-de-nuit s'ouvre toute en feu,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Écoutez, ma mie, au coin du vieux mur,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Écoutez, ma mie, au coin du vieux mur,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Chante ta chanson, ô doux rossignol !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Chante ta chanson, ô doux rossignol !"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Et que pour toi seul, à côté du lys,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et que pour toi seul, à côté du lys,"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 14,
                    "line_text": "Des yeux tant aimés tombe un divin pleur",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Des yeux tant aimés tombe un divin pleur"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "O larme d'amour, trésor sans pareil !",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "O larme d'amour, trésor sans pareil !"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Qui t'envoie, hélas ! charmant souvenir,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Qui t'envoie, hélas ! charmant souvenir,"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Hélas ! je suis seul dans ces bois épars",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Hélas ! je suis seul dans ces bois épars"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 18,
                    "line_text": "Une illusion, songe évanoui,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Une illusion, songe évanoui,"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Je fatigue seul le flot de cristal,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Je fatigue seul le flot de cristal,"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 20,
                    "line_text": "L'antre et la fontaine où croît le glaïeul,",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "L'antre et la fontaine où croît le glaïeul,"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "La forêt tremblante et l'azur du lac",
                    "metrical_pattern": null,
                    "line_length": "5+5",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La forêt tremblante et l'azur du lac"
        }
    ],
