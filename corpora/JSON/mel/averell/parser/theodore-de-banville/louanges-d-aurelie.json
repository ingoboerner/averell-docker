{
    "poem_title": "Louanges d'Aurélie",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1857",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Toi qui rêvas parmi les lys,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Avec le sylphe et les willis",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Et la rosée en diamants,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 4,
                    "line_text": "Un théâtre pour les amants",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Toi qui rêvas parmi les lys,\nAvec le sylphe et les willis\nEt la rosée en diamants,\nUn théâtre pour les amants"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Je sais, poëte du roi Lear,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Une femme qui fait pâlir",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 7,
                    "line_text": "Dont ta noble main couronna",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 8,
                    "line_text": "Juliette et Desdémona,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Je sais, poëte du roi Lear,\nUne femme qui fait pâlir\nDont ta noble main couronna\nJuliette et Desdémona,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Elle avait au front moins de fleurs,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Celle que, d'amour et de pleurs",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "La lune rêveuse, en songeant,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Couronnait de rayons d'argent",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Elle avait au front moins de fleurs,\nCelle que, d'amour et de pleurs\nLa lune rêveuse, en songeant,\nCouronnait de rayons d'argent"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Elle avait moins de doux regards,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Celle qui, les cheveux épars",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Blanche comme un camellia,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 16,
                    "line_text": "A sa servante Émilia",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Elle avait moins de doux regards,\nCelle qui, les cheveux épars\nBlanche comme un camellia,\nA sa servante Émilia"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Il est moins agréable au ciel,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Cet ange qu'un chant immortel",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 19,
                    "line_text": "Cet inestimable joyau",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 20,
                    "line_text": "Sur lequel pleure Olympio",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Il est moins agréable au ciel,\nCet ange qu'un chant immortel\nCet inestimable joyau\nSur lequel pleure Olympio"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Et toi, mon maître, ô fier Ronsard,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Enthousiaste du doux art,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Qui jadis nous émerveillais",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 24,
                    "line_text": "Sur les roses et les œillets",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et toi, mon maître, ô fier Ronsard,\nEnthousiaste du doux art,\nQui jadis nous émerveillais\nSur les roses et les œillets"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Celle que je chante en ces vers",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "T'eût donné, sous tes lauriers verts,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Qu'il n'en fallut pour mettre au jour",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 28,
                    "line_text": "Les cent filles de ton amour",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Celle que je chante en ces vers\nT'eût donné, sous tes lauriers verts,\nQu'il n'en fallut pour mettre au jour\nLes cent filles de ton amour"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Car cette maîtresse aux beaux yeux",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Dans un poëme harmonieux",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 31,
                    "line_text": "Ni dans ton marbre, ô Phidias,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 32,
                    "line_text": "Ni dans les grands yeux de Diaz",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Car cette maîtresse aux beaux yeux\nDans un poëme harmonieux\nNi dans ton marbre, ô Phidias,\nNi dans les grands yeux de Diaz"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "C'est une femme aux yeux plus doux,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Vivante et qui peut, comme nous,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "Mais qui sur son front sidéral",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 36,
                    "line_text": "Porte le rhythme et l'idéal",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "C'est une femme aux yeux plus doux,\nVivante et qui peut, comme nous,\nMais qui sur son front sidéral\nPorte le rhythme et l'idéal"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Ce n'est pas un rêve charmant",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Qu'il faudra pleurer en fermant",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Et cet ange aux ongles d'onyx,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 40,
                    "line_text": "Plus beau que Laure et Béatrix,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Ce n'est pas un rêve charmant\nQu'il faudra pleurer en fermant\nEt cet ange aux ongles d'onyx,\nPlus beau que Laure et Béatrix,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "On entend, parmi le satin,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Battre son cœur sous son beau sein",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 43,
                    "line_text": "Les rossignols, pleins de doux chants,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 44,
                    "line_text": "Peuvent écouter dans les champs",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "On entend, parmi le satin,\nBattre son cœur sous son beau sein\nLes rossignols, pleins de doux chants,\nPeuvent écouter dans les champs"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Et quand elle s'arrête au bois",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Pour écouter sourdre les voix",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "A travers les arbres du parc,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 48,
                    "line_text": "Les Naïades admirent l'arc",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et quand elle s'arrête au bois\nPour écouter sourdre les voix\nA travers les arbres du parc,\nLes Naïades admirent l'arc"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Le soir, à cette heure de feu",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Où se pâme sous le ciel bleu",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "La Nuit humide de parfums",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 52,
                    "line_text": "Se mire dans ses grands yeux bruns,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Le soir, à cette heure de feu\nOù se pâme sous le ciel bleu\nLa Nuit humide de parfums\nSe mire dans ses grands yeux bruns,"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Et les extases du soleil",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Emplissent les airs d'or vermeil",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 55,
                    "line_text": "Quand les beaux châles d'Orient",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 56,
                    "line_text": "Murmurent sur son cou riant",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et les extases du soleil\nEmplissent les airs d'or vermeil\nQuand les beaux châles d'Orient\nMurmurent sur son cou riant"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Car c'est pour orner ses beaux reins",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "Que le pays des Dieux sereins",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 59,
                    "line_text": "Semble dans un tissu changeant",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 60,
                    "line_text": "Tramer avec l'or et l'argent",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Car c'est pour orner ses beaux reins\nQue le pays des Dieux sereins\nSemble dans un tissu changeant\nTramer avec l'or et l'argent"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 61,
                    "line_text": "O beau songe ! sonnet vivant !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 62,
                    "line_text": "Calice entr'ouvert que le vent",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 63,
                    "line_text": "Sa main blanche comme le lait",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 64,
                    "line_text": "Passe à travers le bracelet",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "O beau songe ! sonnet vivant !\nCalice entr'ouvert que le vent\nSa main blanche comme le lait\nPasse à travers le bracelet"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 65,
                    "line_text": "Je vois sous les pâles duvets",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 66,
                    "line_text": "Ses veines couleur des bleuets",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 67,
                    "line_text": "Ses ongles dignes de Scyllis,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 68,
                    "line_text": "Ses bras aussi blancs que les lys,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Je vois sous les pâles duvets\nSes veines couleur des bleuets\nSes ongles dignes de Scyllis,\nSes bras aussi blancs que les lys,"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 69,
                    "line_text": "Et mon âme pleine et sans fond,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 70,
                    "line_text": "D'où parfois à mon œil profond",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 71,
                    "line_text": "Partout attirée à la fois,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 72,
                    "line_text": "Demeure tremblante et sans voix",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et mon âme pleine et sans fond,\nD'où parfois à mon œil profond\nPartout attirée à la fois,\nDemeure tremblante et sans voix"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 73,
                    "line_text": "Tels nous sentons, irrésolus,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 74,
                    "line_text": "De vivants désirs, qui n'ont plus",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 75,
                    "line_text": "Couler en nous comme des flots",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                },
                {
                    "line_number": 76,
                    "line_text": "Avec le rhythme et les sanglots",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Tels nous sentons, irrésolus,\nDe vivants désirs, qui n'ont plus\nCouler en nous comme des flots\nAvec le rhythme et les sanglots"
        }
    ],
    "corpus": "mel"
