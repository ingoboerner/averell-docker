                {
                    "line_number": 1,
                    "line_text": "Lorsque avec les sons",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Dont tu les complètes,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Tu fais des chansons",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "De mes odelettes,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 5,
                    "line_text": "Mille aspects divers",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 6,
                    "line_text": "De grâce physique",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 7,
                    "line_text": "Naissent dans mes vers",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 8,
                    "line_text": "Avec ta musique !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Lorsque avec les sons\nDont tu les complètes,\nTu fais des chansons\nDe mes odelettes,\nMille aspects divers\nDe grâce physique\nNaissent dans mes vers\nAvec ta musique !"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "A ta seule voix,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Tout en eux s'éveille",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Et vit à la fois.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "O rare merveille !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 13,
                    "line_text": "A ma vigne en fleur,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 14,
                    "line_text": "A ma moisson mûre,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 15,
                    "line_text": "Tu rends la couleur",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 16,
                    "line_text": "Avec le murmure !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "A ta seule voix,\nTout en eux s'éveille\nEt vit à la fois.\nO rare merveille !\nA ma vigne en fleur,\nA ma moisson mûre,\nTu rends la couleur\nAvec le murmure !"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Au ciel rougissant",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "De clartés sans voiles,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "La nuit en naissant",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Frissonne d'étoiles,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 21,
                    "line_text": "Et sous les berceaux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 22,
                    "line_text": "Où sa voix touchante",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 23,
                    "line_text": "Ravit les ruisseaux,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 24,
                    "line_text": "Le rossignol chante !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Au ciel rougissant\nDe clartés sans voiles,\nLa nuit en naissant\nFrissonne d'étoiles,\nEt sous les berceaux\nOù sa voix touchante\nRavit les ruisseaux,\nLe rossignol chante !"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "La biche qui court",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Parmi les charmilles",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 27,
                    "line_text": "S'arrête tout court,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Et des jeunes filles",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 29,
                    "line_text": "Sous tes feux tremblants,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 30,
                    "line_text": "O lune incertaine,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 31,
                    "line_text": "Lavent leurs pieds blancs",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 32,
                    "line_text": "Dans une fontaine.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "La biche qui court\nParmi les charmilles\nS'arrête tout court,\nEt des jeunes filles\nSous tes feux tremblants,\nO lune incertaine,\nLavent leurs pieds blancs\nDans une fontaine."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "C'est sous le bouleau,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Dont les feuilles sombres",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 35,
                    "line_text": "Découpent dans l'eau",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "De légères ombres,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 37,
                    "line_text": "Et lorsqu'un éclair",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 38,
                    "line_text": "Montre leurs visages,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 39,
                    "line_text": "On sent courir l'air",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 40,
                    "line_text": "Dans ces paysages !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "C'est sous le bouleau,\nDont les feuilles sombres\nDécoupent dans l'eau\nDe légères ombres,\nEt lorsqu'un éclair\nMontre leurs visages,\nOn sent courir l'air\nDans ces paysages !"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Derniers enchanteurs",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Des âmes en fête,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 43,
                    "line_text": "O divins chanteurs,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Qui sur notre tête",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 45,
                    "line_text": "Agitez encor",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 46,
                    "line_text": "D'une main hardie",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 47,
                    "line_text": "Les clochettes d'or",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 48,
                    "line_text": "De la mélodie !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Derniers enchanteurs\nDes âmes en fête,\nO divins chanteurs,\nQui sur notre tête\nAgitez encor\nD'une main hardie\nLes clochettes d'or\nDe la mélodie !"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Dans l'azur secret,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Un sylphe voltige",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 51,
                    "line_text": "Sur votre forêt",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 52,
                    "line_text": "Où tout est prestige.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 53,
                    "line_text": "Chaque art a le sien,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 54,
                    "line_text": "Mais rien ne s'achève,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 55,
                    "line_text": "O musicien,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 56,
                    "line_text": "Qu'avec votre rêve !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Dans l'azur secret,\nUn sylphe voltige\nSur votre forêt\nOù tout est prestige.\nChaque art a le sien,\nMais rien ne s'achève,\nO musicien,\nQu'avec votre rêve !"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Le monde amoureux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "De la Poésie",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 59,
                    "line_text": "Se sent plus heureux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 60,
                    "line_text": "Lorsqu'il s'extasie",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 61,
                    "line_text": "Aux accords si doux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 62,
                    "line_text": "Nés de ce délire,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 63,
                    "line_text": "Mais c'est toujours vous",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 64,
                    "line_text": "Qui tenez la lyre !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Le monde amoureux\nDe la Poésie\nSe sent plus heureux\nLorsqu'il s'extasie\nAux accords si doux\nNés de ce délire,\nMais c'est toujours vous\nQui tenez la lyre !"
        }
