{
    "poem_title": "Inviolata",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1860-1861",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Avec ces traits harmonieux, pareils",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Et ce teint rose et ces anneaux vermeils",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Avec ces traits harmonieux, pareils\nEt ce teint rose et ces anneaux vermeils"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Avec les noirs sourcils et les grands cils",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Se joue, orgueil de tes regards subtils,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Avec les noirs sourcils et les grands cils\nSe joue, orgueil de tes regards subtils,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Ta beauté, lys exalté, vêtement",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Garde, malgré l'épanouissement,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ta beauté, lys exalté, vêtement\nGarde, malgré l'épanouissement,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Telle Artémis éveille les chasseurs",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Et parmi nous tu n'as pas d'autres sœurs",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Telle Artémis éveille les chasseurs\nEt parmi nous tu n'as pas d'autres sœurs"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Pareille aux Dieux, dont le généreux flanc,",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Sentait courir sous la chair, non du sang,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pareille aux Dieux, dont le généreux flanc,\nSentait courir sous la chair, non du sang,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "On voit frémir un rayon embaumé",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Et l'on sent bien que ton corps est formé",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "On voit frémir un rayon embaumé\nEt l'on sent bien que ton corps est formé"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Comme Cypris, qui porte un ciel d'amour",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Et qui, malgré ses délires d'un jour,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Comme Cypris, qui porte un ciel d'amour\nEt qui, malgré ses délires d'un jour,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Cruelle et rose et répandant l'effroi,",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Tu sais que rien ne peut faner en toi",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Cruelle et rose et répandant l'effroi,\nTu sais que rien ne peut faner en toi"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Tu vois nos maux d'un œil indifférent,",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Sont invaincus plus que l'eau du torrent",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tu vois nos maux d'un œil indifférent,\nSont invaincus plus que l'eau du torrent"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Et tant d'amours, hélas ! faits pour flétrir",
                    "metrical_pattern": null,
                    "line_length": "4+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "O mon trésor, n'ont pas pu défleurir",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et tant d'amours, hélas ! faits pour flétrir\nO mon trésor, n'ont pas pu défleurir"
        }
    ],
    "corpus": "mel"
