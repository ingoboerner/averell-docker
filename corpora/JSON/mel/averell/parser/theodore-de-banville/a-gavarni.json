{
    "poem_title": "A Gavarni",
    "author": "Théodore de Banville",
    "structure": "suite périodique",
    "year": "1856",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "La Beauté, fatal aimant,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Est pareille au diamant",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 3,
                    "line_text": "Que la fange peut mouiller",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La Beauté, fatal aimant,\nEst pareille au diamant\nQue la fange peut mouiller"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Jusqu'au milieu du ruisseau,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "L'éclat pur de son berceau",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Garde un charme essentiel",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Jusqu'au milieu du ruisseau,\nL'éclat pur de son berceau\nGarde un charme essentiel"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Ainsi, leurs cheveux au vent,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Vois ces folles qui souvent",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 9,
                    "line_text": "Bercent le premier venu",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ainsi, leurs cheveux au vent,\nVois ces folles qui souvent\nBercent le premier venu"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Ces filles aux teints flétris,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Qui dévisagent Paris",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Avec leur regard moqueur,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ces filles aux teints flétris,\nQui dévisagent Paris\nAvec leur regard moqueur,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Leur sein insensible et froid",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Que mord le corset étroit,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "N'a jamais pendant un jour",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Leur sein insensible et froid\nQue mord le corset étroit,\nN'a jamais pendant un jour"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 16,
                    "line_text": "Idoles ivres d'encens,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 17,
                    "line_text": "Dont rien n'éveille les sens,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Elle n'ont jamais pleuré",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Idoles ivres d'encens,\nDont rien n'éveille les sens,\nElle n'ont jamais pleuré"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Plus pâles que nos Ennuis,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Ces spectres des folles nuits",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 21,
                    "line_text": "Ne mentent même pas bien,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Plus pâles que nos Ennuis,\nCes spectres des folles nuits\nNe mentent même pas bien,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 22,
                    "line_text": "Rien ! ni l'orgie et le bal",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 23,
                    "line_text": "Qui se tord en carnaval",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Sous les clairons furieux,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Rien ! ni l'orgie et le bal\nQui se tord en carnaval\nSous les clairons furieux,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Ni le Vin, or ruisselant,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Âme du raisin sanglant",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 27,
                    "line_text": "Qui met ses riches manteaux",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ni le Vin, or ruisselant,\nÂme du raisin sanglant\nQui met ses riches manteaux"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Ni la colère du Jeu,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 29,
                    "line_text": "Qui rend puissants comme un dieu",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Les combattants éblouis",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ni la colère du Jeu,\nQui rend puissants comme un dieu\nLes combattants éblouis"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Ni cette perle des mers",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Arrachée aux flots amers,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 33,
                    "line_text": "Ni Golconde et son trésor,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ni cette perle des mers\nArrachée aux flots amers,\nNi Golconde et son trésor,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 34,
                    "line_text": "Car l'Or sur notre chemin,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 35,
                    "line_text": "C'est l'Art sacré dont la main",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Embellit les horizons",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Car l'Or sur notre chemin,\nC'est l'Art sacré dont la main\nEmbellit les horizons"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "C'est la sereine fierté,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "C'est un jour de liberté",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 39,
                    "line_text": "Sous les ombrages fleuris",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "C'est la sereine fierté,\nC'est un jour de liberté\nSous les ombrages fleuris"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 40,
                    "line_text": "C'est l'Amitié, douce voix,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 41,
                    "line_text": "Qu'on peut encore une fois",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Accueillir et mieux choyer",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "C'est l'Amitié, douce voix,\nQu'on peut encore une fois\nAccueillir et mieux choyer"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Mais ce gouffre où tout se perd !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Mais elles ! L'or ne leur sert",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 45,
                    "line_text": "Qu'à se parer de chiffons",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais ce gouffre où tout se perd !\nMais elles ! L'or ne leur sert\nQu'à se parer de chiffons"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 46,
                    "line_text": "Pourquoi donc les chantons-nous,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 47,
                    "line_text": "Cœurs de l'Idéal jaloux,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Qui toujours au ciel obscur",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pourquoi donc les chantons-nous,\nCœurs de l'Idéal jaloux,\nQui toujours au ciel obscur"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Sur leurs têtes sans douceur",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Pourquoi, poëte et penseur,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 51,
                    "line_text": "Fais-tu jaillir un rayon",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Sur leurs têtes sans douceur\nPourquoi, poëte et penseur,\nFais-tu jaillir un rayon"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 52,
                    "line_text": "O philosophe subtil,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 53,
                    "line_text": "Dis-le-moi, que reste-t-il",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "A leur front désenchanté ?",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O philosophe subtil,\nDis-le-moi, que reste-t-il\nA leur front désenchanté ?"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "La Beauté, miroir secret,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "Où l'amour divin paraît",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 57,
                    "line_text": "Reflété comme en un ciel",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "La Beauté, miroir secret,\nOù l'amour divin paraît\nReflété comme en un ciel"
        }
    ],
    "corpus": "mel"
}