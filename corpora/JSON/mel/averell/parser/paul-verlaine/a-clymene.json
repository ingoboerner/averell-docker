                }
            ],
            "stanza_text": "Mystiques barcarolles,\nRomances sans paroles,\nChère, puisque tes yeux,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Puisque ta voix, étrange",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Vision qui dérange",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Et trouble l’horizon",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puisque ta voix, étrange\nVision qui dérange\nEt trouble l’horizon"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Puisque l’arôme insigne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "De ta pâleur de cygne",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 9,
                    "line_text": "Et puisque la candeur",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puisque l’arôme insigne\nDe ta pâleur de cygne\nEt puisque la candeur"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Ah ! puisque tout ton être,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Musique qui pénètre,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Nimbes d’anges défunts,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ah ! puisque tout ton être,\nMusique qui pénètre,\nNimbes d’anges défunts,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "A, sur d’almes cadences",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "En ses correspondances",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "a"
                },
                {
                    "line_number": 15,
                    "line_text": "Induit mon cœur subtil,",
                    "metrical_pattern": null,
                    "line_length": "6",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "A, sur d’almes cadences\nEn ses correspondances\nInduit mon cœur subtil,"
        }
