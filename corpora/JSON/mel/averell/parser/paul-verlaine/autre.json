                {
                    "line_number": 1,
                    "line_text": "CAR tu vis en toutes les femmes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Et toutes les femmes c’est toi.",
