                {
                    "line_number": 18,
                    "line_text": "Pour la procession si touchante",
                    "metrical_pattern": null,
                    "line_length": "6+3",
                    "rhyme": ""
                },
                {
                    "line_number": 19,
                    "line_text": "Qui a nom : du Voeu de Louis Treize :",
                    "metrical_pattern": null,
                    "line_length": "3+6",
                    "rhyme": ""
