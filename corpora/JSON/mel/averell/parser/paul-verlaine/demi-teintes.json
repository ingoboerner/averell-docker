            ],
            "stanza_text": "O la Dulcinée\nDe ce Toboso,\nToi qui m'es donnée,\nAinsi qu'un oiseau\nSur ma main distraite,\nPour sourire un peu\nOu pleurer au lieu,\nPardonne au poëte"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "L'air indifférent,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Bien qu'aimable en somme,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Que parfois il prend,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "L'inconscient homme",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 13,
                    "line_text": "Moins préoccupé",
                    "metrical_pattern": null,
