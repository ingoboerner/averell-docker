{
    "poem_title": "A LÉON CLADEL",
    "author": "Paul Verlaine",
    "structure": "sonnet",
    "year": "1890",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "TU fus excessif",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Et je t’en aimais",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "D’un amour plus vif,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Plus vif que jamais,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "TU fus excessif\nEt je t’en aimais\nD’un amour plus vif,\nPlus vif que jamais,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Depuis que la mort,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 6,
                    "line_text": "Cette vie en mieux,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                },
                {
                    "line_number": 7,
                    "line_text": "A brisé l’effort",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "c"
                },
                {
                    "line_number": 8,
                    "line_text": "De Toi vers les cieux,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "d"
                }
            ],
            "stanza_text": "Depuis que la mort,\nCette vie en mieux,\nA brisé l’effort\nDe Toi vers les cieux,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Vers des cieux voulus",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "e"
                },
                {
                    "line_number": 10,
                    "line_text": "Par ta volonté,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "f"
                },
                {
                    "line_number": 11,
                    "line_text": "Des cieux absolus,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "e"
                }
            ],
            "stanza_text": "Vers des cieux voulus\nPar ta volonté,\nDes cieux absolus,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 12,
                    "line_text": "Toi ressuscité",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "f"
                },
                {
                    "line_number": 13,
                    "line_text": "Aux fins, glorieux.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "g"
                },
                {
                    "line_number": 14,
                    "line_text": "D’une vie en mieux.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "g"
                }
            ],
            "stanza_text": "Toi ressuscité\nAux fins, glorieux.\nD’une vie en mieux."
        }
    ],
