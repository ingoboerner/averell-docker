            "stanza_type": null,
            "lines": [
                {
                    "line_number": 28,
                    "line_text": "Et, plus tard pucelles",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": ""
                },
                {
                    "line_number": 29,
                    "line_text": "Diront leurs hauts faits",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": ""
                },
                {
                    "line_number": 30,
                    "line_text": "En des vers mauvais.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": ""
                }
            ],
            "stanza_text": "Et, plus tard pucelles\nDiront leurs hauts faits\nEn des vers mauvais."
