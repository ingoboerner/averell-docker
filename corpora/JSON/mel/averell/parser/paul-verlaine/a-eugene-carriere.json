                },
                {
                    "line_number": 13,
                    "line_text": "Des pleurs, vrais au fond, d'un bonhomme",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "e"
                },
                {
                    "line_number": 14,
                    "line_text": "Un peu jadis et mal Socrate.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "d"
