{
    "poem_title": "COMPLAINTE DE LA LUNE EN PROVINCE",
    "author": "Jules Laforgue",
    "structure": "suite périodique",
    "year": "1885",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Ah ! La belle pleine Lune,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Grosse comme une fortune !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ah ! La belle pleine Lune,\nGrosse comme une fortune !"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "La retraite sonne au loin,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Un passant, monsieur l'adjoint ;",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La retraite sonne au loin,\nUn passant, monsieur l'adjoint ;"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Un clavecin joue en face,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Un chat traverse la place :",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Un clavecin joue en face,\nUn chat traverse la place :"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "La province qui s'endort !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Plaquant un dernier accord,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "La province qui s'endort !\nPlaquant un dernier accord,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Le piano clôt sa fenêtre.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Quelle heure peut-il bien être ?",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Le piano clôt sa fenêtre.\nQuelle heure peut-il bien être ?"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Calme lune, quel exil !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Faut-il dire : ainsi soit-il ?",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Calme lune, quel exil !\nFaut-il dire : ainsi soit-il ?"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Lune, ô dilettante Lune,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "À tous les climats commune,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Lune, ô dilettante Lune,\nÀ tous les climats commune,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Tu vis hier le Missouri,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Et les remparts de Paris,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tu vis hier le Missouri,\nEt les remparts de Paris,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Les fiords bleus de la Norvège,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Les pôles, les mers, que sais-je ?",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Les fiords bleus de la Norvège,\nLes pôles, les mers, que sais-je ?"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Lune heureuse ! Ainsi tu vois,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "À cette heure, le convoi",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Lune heureuse ! Ainsi tu vois,\nÀ cette heure, le convoi"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "De son voyage de noce !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Ils sont partis pour l'Écosse.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "De son voyage de noce !\nIls sont partis pour l'Écosse."
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Quel panneau, si, cet hiver,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Elle eût pris au mot mes vers !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Quel panneau, si, cet hiver,\nElle eût pris au mot mes vers !"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Lune, vagabonde Lune,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Faisons cause et mœurs communes ?",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Lune, vagabonde Lune,\nFaisons cause et mœurs communes ?"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Ô riches nuits ! Je me meurs,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "La province dans le cœur !",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ô riches nuits ! Je me meurs,\nLa province dans le cœur !"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Et la lune a, bonne vieille,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Du coton dans les oreilles.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et la lune a, bonne vieille,\nDu coton dans les oreilles."
        }
    ],
    "corpus": "mel"
}