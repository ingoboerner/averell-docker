{
    "poem_title": "COMPLAINTE DU TEMPS ET DE SA COMMÈRE L'ESPACE",
    "author": "Jules Laforgue",
    "structure": "distiques",
    "year": "1885",
    "manually_checked": false,
    "stanzas": [
        {
