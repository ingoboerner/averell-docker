        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "On porte secours au jeune homme.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Immobile comme un paquet,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "Il n’est pas mort, mais c’est tout comme ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
