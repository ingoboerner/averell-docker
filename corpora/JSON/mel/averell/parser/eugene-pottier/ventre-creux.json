            "lines": [
                {
                    "line_number": 1,
                    "line_text": "J'ai faim, disait Ventre creux",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Je suis las des fruits véreux",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "J'ai faim, disait Ventre creux\nJe suis las des fruits véreux"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Peuple, me dit en tout lieu",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "On ne fait bon pot-au-feu",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Peuple, me dit en tout lieu\nOn ne fait bon pot-au-feu"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Un gras marguillier sans fiel,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Me dit : Tu gagnes le ciel.",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Un gras marguillier sans fiel,\nMe dit : Tu gagnes le ciel."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Un meneur fort amical",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Prends un Bourgeois radical",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Un meneur fort amical\nPrends un Bourgeois radical"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Pour qui ces torches là-bas,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Quel est ce dieu ? — le bœuf gras !",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pour qui ces torches là-bas,\nQuel est ce dieu ? — le bœuf gras !"
        }
    ],
    "corpus": "mel"
}