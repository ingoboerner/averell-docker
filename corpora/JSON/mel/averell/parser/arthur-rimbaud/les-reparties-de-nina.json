{
    "poem_title": "Les reparties de Nina",
    "author": "Arthur Rimbaud",
    "structure": "suite périodique avec x",
    "year": "1869-1870",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Ta poitrine sur ma poitrine,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "0"
                },
                {
                    "line_number": 2,
                    "line_text": "Ayant de l’air plein la narine,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "1"
                }
            ],
            "stanza_text": "Ta poitrine sur ma poitrine,\nAyant de l’air plein la narine,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Du bon matin bleu qui vous baigne",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Quand tout le bois frissonnant saigne",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Du bon matin bleu qui vous baigne\nQuand tout le bois frissonnant saigne"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "De chaque branche, gouttes vertes,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "On sent dans les choses ouvertes",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "De chaque branche, gouttes vertes,\nOn sent dans les choses ouvertes"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Tu plongerais dans la luzerne",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Divine avec ce bleu qui cerne",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tu plongerais dans la luzerne\nDivine avec ce bleu qui cerne"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Amoureuse de la campagne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Comme une mousse de champagne,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Amoureuse de la campagne,\nComme une mousse de champagne,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Riant à moi, brutal d’ivresse,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Comme cela, − la belle tresse,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Riant à moi, brutal d’ivresse,\nComme cela, − la belle tresse,"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Ton goût de framboise et de fraise,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Riant au vent vif qui te baise",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ton goût de framboise et de fraise,\nRiant au vent vif qui te baise"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Au rose églantier qui t’embête",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Riant surtout, ô folle tête,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Au rose églantier qui t’embête\nRiant surtout, ô folle tête,"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Dix-sept ans ! Tu seras heureuse !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "La grande campagne amoureuse !",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Dix-sept ans ! Tu seras heureuse !\nLa grande campagne amoureuse !"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "− Ta poitrine sur ma poitrine,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Lents, nous gagnerions la ravine,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "− Ta poitrine sur ma poitrine,\nLents, nous gagnerions la ravine,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Puis, comme une petite morte,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Tu me dirais que je te porte,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Puis, comme une petite morte,\nTu me dirais que je te porte,"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "Je te porterais, palpitante,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "L’oiseau filerait son andante,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Je te porterais, palpitante,\nL’oiseau filerait son andante,"
        },
        {
            "stanza_number": 13,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Je te parlerais dans ta bouche :",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Ton corps, comme une enfant qu’on couche,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Je te parlerais dans ta bouche :\nTon corps, comme une enfant qu’on couche,"
        },
        {
            "stanza_number": 14,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 27,
                    "line_text": "Qui coule, bleu, sous ta peau blanche",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Te parlant bas la langue franche…",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Qui coule, bleu, sous ta peau blanche\nTe parlant bas la langue franche…"
        },
        {
            "stanza_number": 15,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Nos grands bois sentiraient la sève,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Sablerait d’or fin leur grand rêve",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Nos grands bois sentiraient la sève,\nSablerait d’or fin leur grand rêve"
        },
        {
            "stanza_number": 16,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 31,
                    "line_text": "Le soir ?… Nous reprendrons la route",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Flânant, comme un troupeau qui broute,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Le soir ?… Nous reprendrons la route\nFlânant, comme un troupeau qui broute,"
        },
        {
            "stanza_number": 17,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Les bons vergers à l’herbe bleue",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Comme on les sent toute une lieue,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Les bons vergers à l’herbe bleue\nComme on les sent toute une lieue,"
        },
        {
            "stanza_number": 18,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 35,
                    "line_text": "Nous regagnerions le village",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "Et ça sentira le laitage",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Nous regagnerions le village\nEt ça sentira le laitage"
        },
        {
            "stanza_number": 19,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Ça sentira l’étable pleine",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Pleine d’un rythme lent d’haleine,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ça sentira l’étable pleine\nPleine d’un rythme lent d’haleine,"
        },
        {
            "stanza_number": 20,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 39,
                    "line_text": "Blanchissant sous quelque lumière ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 40,
                    "line_text": "Une vache fienterait fière,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Blanchissant sous quelque lumière ;\nUne vache fienterait fière,"
        },
        {
            "stanza_number": 21,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "− Les lunettes de la grand’mère",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Dans son missel, le pot de bière",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "− Les lunettes de la grand’mère\nDans son missel, le pot de bière"
        },
        {
            "stanza_number": 22,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 43,
                    "line_text": "Moussant entre les larges pipes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Fument : dix, quinze, immenses lippes",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Moussant entre les larges pipes\nFument : dix, quinze, immenses lippes"
        },
        {
            "stanza_number": 23,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Happent le jambon aux fourchettes",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Le feu qui claire les couchettes,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Happent le jambon aux fourchettes\nLe feu qui claire les couchettes,"
        },
        {
            "stanza_number": 24,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 47,
                    "line_text": "Les fesses luisantes et grasses",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Qui fourre, à genoux, dans des tasses,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Les fesses luisantes et grasses\nQui fourre, à genoux, dans des tasses,"
        },
        {
            "stanza_number": 25,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 49,
                    "line_text": "Frolé par un mufle qui gronde",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 50,
                    "line_text": "Et pourlèche la face ronde",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Frolé par un mufle qui gronde\nEt pourlèche la face ronde"
        },
        {
            "stanza_number": 26,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 51,
                    "line_text": "Noire, rogue au bord de sa chaise,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 52,
                    "line_text": "Une vieille devant la braise",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Noire, rogue au bord de sa chaise,\nUne vieille devant la braise"
        },
        {
            "stanza_number": 27,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 53,
                    "line_text": "Que de choses nous verrions, chère,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 54,
                    "line_text": "Quand la flamme illumine, claire,",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Que de choses nous verrions, chère,\nQuand la flamme illumine, claire,"
        },
        {
            "stanza_number": 28,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 55,
                    "line_text": "− Et puis, fraîche et toute nichée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 56,
                    "line_text": "La maison, la vitre cachée",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "− Et puis, fraîche et toute nichée\nLa maison, la vitre cachée"
        },
        {
            "stanza_number": 29,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 57,
                    "line_text": "Tu viendras, tu viendras, je t’aime,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 58,
                    "line_text": "Tu viendras, n’est-ce pas ? et même…",
                    "metrical_pattern": null,
                    "line_length": "4",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tu viendras, tu viendras, je t’aime,\nTu viendras, n’est-ce pas ? et même…"
        },
        {
            "stanza_number": 30,
            "stanza_type": null,
            "lines": [],
            "stanza_text": ""
        }
    ],
