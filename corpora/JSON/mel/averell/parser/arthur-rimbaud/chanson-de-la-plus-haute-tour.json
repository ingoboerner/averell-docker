            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Je me suis dit : laisse,",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Et qu'on ne te voie :",
