                },
                {
                    "line_number": 10,
                    "line_text": "Qui fleurent de longs miels végétaux et rosés,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Et qu’interrompt parfois un sifflement, salives",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
