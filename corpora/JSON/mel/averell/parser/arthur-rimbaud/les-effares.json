{
    "poem_title": "Les Effarés",
    "author": "Arthur Rimbaud",
    "structure": "forme RegExp (appariements)",
    "year": "1869-1870",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Noirs dans la neige et dans la brume,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Au grand soupirail qui s’allume,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Noirs dans la neige et dans la brume,\nAu grand soupirail qui s’allume,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "A genoux, cinq petits, − misère ! −",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 4,
                    "line_text": "Regardent le boulanger faire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "A genoux, cinq petits, − misère ! −\nRegardent le boulanger faire"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Ils voient le fort bras blanc qui tourne",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "La pâte grise, et qui l’enfourne",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ils voient le fort bras blanc qui tourne\nLa pâte grise, et qui l’enfourne"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Ils écoutent le bon pain cuire.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 8,
                    "line_text": "Le boulanger au gras sourire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Ils écoutent le bon pain cuire.\nLe boulanger au gras sourire"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Ils sont blottis, pas un ne bouge,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Au souffle du soupirail rouge,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ils sont blottis, pas un ne bouge,\nAu souffle du soupirail rouge,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Et quand, pendant que minuit sonne,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 12,
                    "line_text": "Façonné, pétillant et jaune,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Et quand, pendant que minuit sonne,\nFaçonné, pétillant et jaune,"
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "Quand, sous les poutres enfumées,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Chantent les croûtes parfumées,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Quand, sous les poutres enfumées,\nChantent les croûtes parfumées,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 15,
                    "line_text": "Quand ce trou chaud souffle la vie ;",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 16,
                    "line_text": "Ils ont leur âme si ravie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Quand ce trou chaud souffle la vie ;\nIls ont leur âme si ravie"
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Ils se ressentent si bien vivre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Les pauvres petits pleins de givre !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ils se ressentent si bien vivre,\nLes pauvres petits pleins de givre !"
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 19,
                    "line_text": "Collant leurs petits museaux roses",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 20,
                    "line_text": "Au grillage, chantant des choses,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "Collant leurs petits museaux roses\nAu grillage, chantant des choses,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Mais bien bas, − comme une prière…",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Repliés vers cette lumière",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mais bien bas, − comme une prière…\nRepliés vers cette lumière"
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 23,
                    "line_text": "− Si fort, qu’ils crèvent leur culotte,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                },
                {
                    "line_number": 24,
                    "line_text": "− Et que leur lange blanc tremblotte",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "c"
                }
            ],
            "stanza_text": "− Si fort, qu’ils crèvent leur culotte,\n− Et que leur lange blanc tremblotte"
        }
    ],
    "corpus": "mel"
