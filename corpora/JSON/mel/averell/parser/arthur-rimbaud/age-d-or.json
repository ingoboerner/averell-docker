            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Et puis une Voix",
                    "metrical_pattern": null,
                    "line_length": "5",
                    "rhyme": ""
                },
                {
                    "line_number": 22,
                    "line_text": "— Est-elle angélique ! —",
                    "metrical_pattern": null,
                    "line_length": "5",
