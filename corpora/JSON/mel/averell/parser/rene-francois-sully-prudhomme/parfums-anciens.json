{
    "poem_title": "PARFUMS ANCIENS",
    "author": "René-François Sully Prudhomme",
    "structure": "suite périodique",
    "year": "1875",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "O senteur suave et modeste",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Qu'épanchait le front maternel,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Et dont le souvenir nous reste",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Comme un lointain parfum d'autel,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O senteur suave et modeste\nQu'épanchait le front maternel,\nEt dont le souvenir nous reste\nComme un lointain parfum d'autel,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Pure émanation divine",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Qui mêlais en moi ta douceur",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 7,
                    "line_text": "A la petite senteur fine",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Des longues tresses d'une sœur,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Pure émanation divine\nQui mêlais en moi ta douceur\nA la petite senteur fine\nDes longues tresses d'une sœur,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Chère odeur, tu t'en es allée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Où sont les parfums de jadis,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 11,
                    "line_text": "Où remonte l'âme exhalée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Des violettes et des lis.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Chère odeur, tu t'en es allée\nOù sont les parfums de jadis,\nOù remonte l'âme exhalée\nDes violettes et des lis."
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 13,
                    "line_text": "O fraîche senteur de la vie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 14,
                    "line_text": "Qu'au temps des premières amours",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 15,
                    "line_text": "Un baiser candide a ravie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 16,
                    "line_text": "Au plus délicat des velours,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "O fraîche senteur de la vie\nQu'au temps des premières amours\nUn baiser candide a ravie\nAu plus délicat des velours,"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 17,
                    "line_text": "Loin des lèvres décolorées",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 18,
                    "line_text": "Tu t'es enfuie aussi là-bas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 19,
                    "line_text": "Jusqu'où planent, évaporées,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 20,
                    "line_text": "Les jeunesses des vieux lilas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Loin des lèvres décolorées\nTu t'es enfuie aussi là-bas,\nJusqu'où planent, évaporées,\nLes jeunesses des vieux lilas,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 21,
                    "line_text": "Et le cœur, cloué dans l'abîme,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 22,
                    "line_text": "Ne peut suivre, à ta trace uni,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 23,
                    "line_text": "Le voyage épars et sublime",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 24,
                    "line_text": "Que tu poursuis dans l'infini.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et le cœur, cloué dans l'abîme,\nNe peut suivre, à ta trace uni,\nLe voyage épars et sublime\nQue tu poursuis dans l'infini."
        },
        {
            "stanza_number": 7,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 25,
                    "line_text": "Mais ô toi, l'homicide arome",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 26,
                    "line_text": "Dont en pleurant nous nous grisons,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 27,
                    "line_text": "Où notre cœur cherchait un baume",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 28,
                    "line_text": "Et n'aspira que des poisons,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Mais ô toi, l'homicide arome\nDont en pleurant nous nous grisons,\nOù notre cœur cherchait un baume\nEt n'aspira que des poisons,"
        },
        {
            "stanza_number": 8,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 29,
                    "line_text": "Ah ! toi seule, odeur trop aimée",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 30,
                    "line_text": "Des cheveux trop noirs et trop lourds,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 31,
                    "line_text": "Tu nous laisses, courte fumée,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 32,
                    "line_text": "Des vestiges brûlant toujours.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ah ! toi seule, odeur trop aimée\nDes cheveux trop noirs et trop lourds,\nTu nous laisses, courte fumée,\nDes vestiges brûlant toujours."
        },
        {
            "stanza_number": 9,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 33,
                    "line_text": "Dans les replis où tu te glisses",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 34,
                    "line_text": "Tu déposes un marc fatal,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 35,
                    "line_text": "Comme l'âcre odeur des épices",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 36,
                    "line_text": "S'incruste aux coins d'un vieux cristal.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Dans les replis où tu te glisses\nTu déposes un marc fatal,\nComme l'âcre odeur des épices\nS'incruste aux coins d'un vieux cristal."
        },
        {
            "stanza_number": 10,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 37,
                    "line_text": "Et tel, dans une eau fraîche et claire,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 38,
                    "line_text": "Le flacon, vainement plongé,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 39,
                    "line_text": "Garde l'âcreté séculaire",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 40,
                    "line_text": "De l'essence qui l'a rongé,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Et tel, dans une eau fraîche et claire,\nLe flacon, vainement plongé,\nGarde l'âcreté séculaire\nDe l'essence qui l'a rongé,"
        },
        {
            "stanza_number": 11,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 41,
                    "line_text": "Tel, dans la tendresse embaumante",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 42,
                    "line_text": "Que verse au cœur, pour l'assainir,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 43,
                    "line_text": "Une fidèle et chaste amante,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 44,
                    "line_text": "Sévit encor ton souvenir.",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tel, dans la tendresse embaumante\nQue verse au cœur, pour l'assainir,\nUne fidèle et chaste amante,\nSévit encor ton souvenir."
        },
        {
            "stanza_number": 12,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 45,
                    "line_text": "Ô parfum modeste et suave,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 46,
                    "line_text": "Épanché du front maternel,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 47,
                    "line_text": "Qui laves ce que rien ne lave,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 48,
                    "line_text": "Où donc es-tu, parfum d'autel !",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Ô parfum modeste et suave,\nÉpanché du front maternel,\nQui laves ce que rien ne lave,\nOù donc es-tu, parfum d'autel !"
        }
    ],
    "corpus": "mel"
}