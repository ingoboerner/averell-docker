            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Tu les feras pleurer, enfant belle et chérie,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Qui plus tard suspendront leur jeune rêverie",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
