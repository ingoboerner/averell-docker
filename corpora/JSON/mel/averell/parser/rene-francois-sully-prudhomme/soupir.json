{
    "poem_title": "SOUPIR",
    "author": "René-François Sully Prudhomme",
    "structure": "suite périodique",
    "year": "1867",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Ne jamais la voir ni l'entendre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Ne jamais tout haut la nommer,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 3,
                    "line_text": "Mais, fidèle, toujours l'attendre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ne jamais la voir ni l'entendre,\nNe jamais tout haut la nommer,\nMais, fidèle, toujours l'attendre,"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 4,
                    "line_text": "Ouvrir les bras et, las d'attendre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 5,
                    "line_text": "Sur le néant les refermer,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 6,
                    "line_text": "Mais encor, toujours les lui tendre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ouvrir les bras et, las d'attendre,\nSur le néant les refermer,\nMais encor, toujours les lui tendre,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Ah ! Ne pouvoir que les lui tendre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Et dans les pleurs se consumer,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 9,
                    "line_text": "Mais ces pleurs toujours les répandre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ah ! Ne pouvoir que les lui tendre,\nEt dans les pleurs se consumer,\nMais ces pleurs toujours les répandre,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 10,
                    "line_text": "Ne jamais la voir ni l'entendre,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                },
                {
                    "line_number": 11,
                    "line_text": "Ne jamais tout haut la nommer,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                },
                {
                    "line_number": 12,
                    "line_text": "Mais d'un amour toujours plus tendre",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ne jamais la voir ni l'entendre,\nNe jamais tout haut la nommer,\nMais d'un amour toujours plus tendre"
        }
