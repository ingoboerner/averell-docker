{
    "poem_title": "LA COLOMBE ET LE LIS",
    "author": "René-François Sully Prudhomme",
    "structure": "suite périodique",
    "year": "1867",
    "manually_checked": false,
    "stanzas": [
        {
            "stanza_number": 1,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Femme, cette colombe au col rose et mouvant,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "Ne l'avait pas sentie humecter si souvent",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Femme, cette colombe au col rose et mouvant,\nNe l'avait pas sentie humecter si souvent"
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Elle n'avait jamais reçu de toi tout bas",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Ni jamais de tes doigts, à l'heure des repas,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Elle n'avait jamais reçu de toi tout bas\nNi jamais de tes doigts, à l'heure des repas,"
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Elle n'avait jamais senti ton cœur frémir",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Ni ses plumes trembler sous ton jeune soupir,",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Elle n'avait jamais senti ton cœur frémir\nNi ses plumes trembler sous ton jeune soupir,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Tu la laissais languir captive dans l'osier,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "D'un sanglot suppliant elle enflait son gosier :",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Tu la laissais languir captive dans l'osier,\nD'un sanglot suppliant elle enflait son gosier :"
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Jamais les fleurs du vase où rêve le printemps",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Jamais, sur le lis pur et grave, si longtemps",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Jamais les fleurs du vase où rêve le printemps\nJamais, sur le lis pur et grave, si longtemps"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Quel ancien souvenir ou quel récent amour,",
                    "metrical_pattern": null,
                    "line_length": "6+6",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "A fait naître en ton cœur ce suprême retour",
                    "metrical_pattern": null,
                    "line_length": "8",
                    "rhyme": "b"
                }
            ],
            "stanza_text": "Quel ancien souvenir ou quel récent amour,\nA fait naître en ton cœur ce suprême retour"
        }
    ],
    "corpus": "mel"
}