            "lines": [
                {
                    "line_number": 1,
                    "line_text": "Mon cœur tarabiscoté",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 2,
                    "line_text": "A pris un point de côté.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Mon cœur tarabiscoté\nA pris un point de côté."
        },
        {
            "stanza_number": 2,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 3,
                    "line_text": "Tes effluves le font battre",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 4,
                    "line_text": "Comme trois. Que dis-je ? Quatre.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Tes effluves le font battre\nComme trois. Que dis-je ? Quatre."
        },
        {
            "stanza_number": 3,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 5,
                    "line_text": "Ce n'est point un cœur de rien,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 6,
                    "line_text": "Un noctambule vaurien,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Ce n'est point un cœur de rien,\nUn noctambule vaurien,"
        },
        {
            "stanza_number": 4,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 7,
                    "line_text": "Il ne fait de politesses",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 8,
                    "line_text": "Qu'aux baronnes, aux comtesses.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Il ne fait de politesses\nQu'aux baronnes, aux comtesses."
        },
        {
            "stanza_number": 5,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 9,
                    "line_text": "Et, ce bel entretenu,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 10,
                    "line_text": "Regarde, il est devenu,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Et, ce bel entretenu,\nRegarde, il est devenu,"
        },
        {
            "stanza_number": 6,
            "stanza_type": null,
            "lines": [
                {
                    "line_number": 11,
                    "line_text": "Grâce au sucre où tu t'enlises,",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                },
                {
                    "line_number": 12,
                    "line_text": "Confiture de Merises.",
                    "metrical_pattern": null,
                    "line_length": "7",
                    "rhyme": "a"
                }
            ],
            "stanza_text": "Grâce au sucre où tu t'enlises,\nConfiture de Merises."
        }
    ],
    "corpus": "mel"
}