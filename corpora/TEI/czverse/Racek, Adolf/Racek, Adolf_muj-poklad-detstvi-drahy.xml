 <text>
  <front>
   <head>Můj poklad dětství drahý,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0101010" line_length="7">Můj poklad dětství drahý, </l>
     <l n="2" met="100100" rhyme="1" line_length="6">oltářík s andílky,</l>
     <l n="3" met="1101000" line_length="7">dar hraček nejkrasší ty</l>
     <l n="4" met="110100" rhyme="1" line_length="6">dnů štědrých nadílky –</l>
     <l n="5" met="01001" line_length="5">tys vzpomínka má!</l>
    </lg>
    <lg n="2">
     <l n="6" met="0101000" line_length="7">Mou knihou obrázkovou</l>
     <l n="7" met="010000" rhyme="2" line_length="6">a divadélkem mým,</l>
     <l n="8" met="1001000" line_length="7">růžencem z amethystů,</l>
     <l n="9" met="100100" rhyme="2" line_length="6">jablíčkem voskovým –</l>
     <l n="10" met="01001" line_length="5">tys vzpomínka má!</l>
    </lg>
    <lg n="3">
     <l n="11" met="0101010" line_length="7">Mých vzácných květin sbírkou</l>
     <l n="12" met="010100" rhyme="3" line_length="6">a skříňka barviček,</l>
     <l n="13" met="1001010" line_length="7">obrázek svatý s krajkou,</l>
     <l n="14" met="100100" rhyme="3" line_length="6">říkanka sluníček –</l>
     <l n="15" met="01001" line_length="5">tys vzpomínka má!</l>
    </lg>
   </lg>
  </body>
 </text>
