 <text>
  <front>
   <head>Břečtanů oponou,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">Břečtanů oponou, </l>
     <l n="2" met="100100" rhyme="1" line_length="6">jasmínu záclonou</l>
     <l n="3" met="100100" rhyme="2" line_length="6">do husta zarůstá</l>
     <l n="4" met="1001" rhyme="2" line_length="4">zahrada má.</l>
    </lg>
    <lg n="2">
     <l n="5" met="100101" line_length="6">Stíny mých dum a snů</l>
     <l n="6" met="100100" rhyme="1" line_length="6">za mraznou, za svůdnou</l>
     <l n="7" met="100100" rhyme="1" line_length="6">ocúnu s korunou</l>
     <l n="8" met="1100" rhyme="1" line_length="4">jdou princeznou</l>
     <l n="9" met="1000" rhyme="1" line_length="4">zahradou mou.</l>
    </lg>
    <lg n="3">
     <l n="10" met="100101" rhyme="1" line_length="6">Jasanů sosen tmou</l>
     <l n="11" met="100100" rhyme="1" line_length="6">zahleděn za lunou</l>
     <l n="12" met="100100" rhyme="2" line_length="6">zamčená na ústa</l>
     <l n="13" met="100100" line_length="6">podobnou zaúpím</l>
     <l n="14" met="100001" rhyme="1" line_length="6">VIOLONCELA hrou</l>
     <l n="15" met="010100" rhyme="1" line_length="6">sám bloudě zahradou.</l>
    </lg>
   </lg>
  </body>
 </text>
