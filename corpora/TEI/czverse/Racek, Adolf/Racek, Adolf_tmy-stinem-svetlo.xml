  <front>
   <head>Tmy stínem světlo,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="11010" line_length="5">Tmy stínem světlo, </l>
     <l n="2" met="1101" rhyme="1" line_length="4">stín světla tma,</l>
     <l n="3" met="10010100" rhyme="1" line_length="8">plnosti dno je nicota,</l>
     <l n="4" met="010001" rhyme="2" line_length="6">a všehomíra líc</l>
     <l n="5" met="0100" rhyme="2" line_length="4">jest všehonic.</l>
    </lg>
   </lg>
  </body>
