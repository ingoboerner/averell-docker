 <text>
  <front>
   <head>Tys zakletý můj zámek,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0100010" line_length="7">Tys zakletý můj zámek, </l>
     <l n="2" met="100100" rhyme="1" line_length="6">větévka kouzelná,</l>
     <l n="3" met="1101000" line_length="7">má hora křišťálová,</l>
     <l n="4" met="010100" rhyme="1" line_length="6">tys bílá princezna –</l>
     <l n="5" met="01001" rhyme="1" line_length="5">tys pohádka má!</l>
    </lg>
    <lg n="2">
     <l n="6" met="0101000" line_length="7">Tys poklad tajuplný,</l>
     <l n="7" met="100100" rhyme="2" line_length="6">měsíčků ohníčkem,</l>
     <l n="8" met="0100010" line_length="7">tys čarovné mé slovo,</l>
     <l n="9" met="010100" rhyme="2" line_length="6">já zlatým rytířem –</l>
     <l n="10" met="01001" rhyme="1" line_length="5">tys pohádka má!</l>
    </lg>
    <lg n="3">
     <l n="11" met="0101010" line_length="7">Tys skrytý prsten v moři,</l>
     <l n="12" met="100100" rhyme="3" line_length="6">poslední perlička,</l>
     <l n="13" met="1001010" line_length="7">království, modrý ptáček</l>
     <l n="14" met="010100" rhyme="3" line_length="6">a živá vodička –</l>
     <l n="15" met="01001" rhyme="1" line_length="5">tys pohádka má!</l>
    </lg>
   </lg>
  </body>
 </text>
