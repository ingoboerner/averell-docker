  <front>
   <head>Genius Němce Wachsmanna.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101000" rhyme="1" line_length="8">Vnukl pánbůh Wachsmannovi</l>
     <l n="2" met="100100" rhyme="2" line_length="6">přešťastnou myšlenku:</l>
     <l n="3" met="10101000" rhyme="1" line_length="8">umřel, nedal Kolárovi</l>
     <l n="4" met="100010" rhyme="2" line_length="6">podepsat svou směnku.</l>
    </lg>
   </lg>
  </body>
