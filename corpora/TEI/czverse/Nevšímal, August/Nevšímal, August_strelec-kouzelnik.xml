  <front>
   <head>Střelec kouzelník.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="11001010" rhyme="1" line_length="8">Šel satyrik lidstva davem,</l>
     <l n="2" met="10101000" rhyme="1" line_length="8">střelil hlupce epigramem,</l>
     <l n="3" met="1000100" rhyme="2" line_length="7">udělal se doktorem,</l>
     <l n="4" met="1000100" rhyme="2" line_length="7">vzkřísil ho zas humorem.</l>
    </lg>
   </lg>
  </body>
