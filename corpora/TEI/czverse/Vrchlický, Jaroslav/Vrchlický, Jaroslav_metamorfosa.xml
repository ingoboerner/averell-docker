  <front>
   <head>METAMORFOSA.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" line_length="6">Lidmi jsme my pouze</l>
     <l n="2" met="10101" rhyme="1" line_length="5">večer, pokud kvas,</l>
     <l n="3" met="101010" line_length="6">ale šelmy dělá</l>
     <l n="4" met="10100" rhyme="1" line_length="5">jitro rázem z nás.</l>
    </lg>
   </lg>
  </body>
