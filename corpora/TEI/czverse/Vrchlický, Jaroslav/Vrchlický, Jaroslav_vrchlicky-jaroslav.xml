   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0101010" rhyme="1" line_length="7">Ne, Bože, více štěstí!</l>
     <l n="2" met="010101" rhyme="2" line_length="6">Jen klid, jen klid, jen klid!</l>
     <l n="3" met="1001010" rhyme="1" line_length="7">Železnou rozbils pěstí,</l>
     <l n="4" met="0101010" rhyme="1" line_length="7">co mohlo plát a kvésti,</l>
     <l n="5" met="010001" rhyme="2" line_length="6">mou myšlénku i cit.</l>
     <l n="6" met="0101010" rhyme="1" line_length="7">Já více nechci štěstí,</l>
     <l n="7" met="010101" rhyme="2" line_length="6">jen klid! jen klid! jen klid!</l>
    </lg>
   </lg>
