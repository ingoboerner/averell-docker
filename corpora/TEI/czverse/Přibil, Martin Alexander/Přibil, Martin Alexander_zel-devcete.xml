 <text>
  <front>
   <head>Žel děvčete.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0010" line_length="4">Kde jsou dnové,</l>
     <l n="2" met="1010" line_length="4">Zlatí dnové</l>
     <l n="3" met="1010100" rhyme="1" line_length="7">Věku mého mladého!</l>
     <l n="4" met="1010" line_length="4">Ach proč nebe</l>
     <l n="5" met="1010" line_length="4">Nechce děvám</l>
     <l n="6" met="1100100" rhyme="1" line_length="7">Přát osudu stálého!</l>
    </lg>
    <lg n="2">
     <l n="7" met="1000" line_length="4">Sotva se mi</l>
     <l n="8" met="1000" line_length="4">Šestnáckráte</l>
     <l n="9" met="1000100" rhyme="2" line_length="7">Háječek náš zelenal;</l>
     <l n="10" met="0010" line_length="4">Již ten osud,</l>
     <l n="11" met="1010" line_length="4">Zlobný osud</l>
     <l n="12" met="0100101" rhyme="2" line_length="7">Pryč odemne blahost hnal.</l>
    </lg>
    <lg n="3">
     <l n="13" met="0010" line_length="4">Když jsem ještě</l>
     <l n="14" met="1010" line_length="4">Malá byla,</l>
     <l n="15" met="1000100" rhyme="3" line_length="7">Nestála sem o lásku;</l>
     <l n="16" met="1010" line_length="4">Ale, hleďte!</l>
     <l n="17" met="0010" line_length="4">Tu mne všecko</l>
     <l n="18" met="1000100" rhyme="3" line_length="7">Líbalo, co o sázku.</l>
    </lg>
    <lg n="4">
     <l n="19" met="1010" line_length="4">Nyní cosi</l>
     <l n="20" met="1000" line_length="4">Takového</l>
     <l n="21" met="1010100" rhyme="4" line_length="7">Mneby více těšilo;</l>
     <l n="22" met="0010" line_length="4">Teď však od mne</l>
     <l n="23" met="1010" line_length="4">Všecko prchá,</l>
     <l n="24" met="1000100" rhyme="4" line_length="7">Jakby se to zděsilo.</l>
    </lg>
    <lg n="5">
     <l n="25" met="0100" line_length="4">Jen Vilímek</l>
     <l n="26" met="0100" line_length="4">Mně jediný</l>
     <l n="27" met="1001000" rhyme="5" line_length="7">Ještě se nevyhýbal.</l>
     <l n="28" met="1100" line_length="4">Ach, Vilímek</l>
     <l n="29" met="1010" line_length="4">Kdyby nebyl,</l>
     <l n="30" met="1010100" rhyme="5" line_length="7">Mneby žádný nelíbal!</l>
    </lg>
   </lg>
  </body>
 </text>
