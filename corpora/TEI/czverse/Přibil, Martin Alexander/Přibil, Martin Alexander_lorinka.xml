 <text>
  <front>
   <head>Lorinka.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0010" rhyme="1" line_length="4">Jak jsi hezká,</l>
     <l n="2" met="1010" rhyme="1" line_length="4">Děvo česká!</l>
     <l n="3" met="100" rhyme="2" line_length="3">Lorinko!</l>
    </lg>
    <lg n="2">
     <l n="4" met="0010" rhyme="3" line_length="4">Jak se stydím,</l>
     <l n="5" met="0010" rhyme="3" line_length="4">Když tě vidím</l>
     <l n="6" met="100" rhyme="2" line_length="3">Děvinko!</l>
    </lg>
    <lg n="3">
     <l n="7" met="1010" rhyme="4" line_length="4">Srdce bodáš,</l>
     <l n="8" met="1010" rhyme="4" line_length="4">Ohně podáš,</l>
     <l n="9" met="100" rhyme="2" line_length="3">Srdénko!</l>
    </lg>
    <lg n="4">
     <l n="10" met="0010" rhyme="5" line_length="4">Kyž je moje</l>
     <l n="11" met="1010" rhyme="5" line_length="4">Srdce tvoje,</l>
     <l n="12" met="100" rhyme="2" line_length="3">Milenko!</l>
    </lg>
    <lg n="5">
     <l n="13" met="1010" rhyme="6" line_length="4">Šťastným zazvu,</l>
     <l n="14" met="0010" rhyme="6" line_length="4">Až tě nazvu:</l>
     <l n="15" met="100" rhyme="2" line_length="3">Chotinko!</l>
    </lg>
   </lg>
  </body>
 </text>
