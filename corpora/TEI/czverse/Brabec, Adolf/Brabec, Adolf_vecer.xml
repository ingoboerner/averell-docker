  <front>
   <head>VEČER.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">Uhasl červánek,</l>
     <l n="2" met="10010" rhyme="2" line_length="5">pohasly stráně,</l>
     <l n="3" met="100100" rhyme="1" line_length="6">umlkl skřivánek,</l>
     <l n="4" met="10010" rhyme="2" line_length="5">zvědavé laně</l>
     <l n="5" met="10100" rhyme="3" line_length="5">z lesa rokytí</l>
     <l n="6" met="100100" rhyme="3" line_length="6">věncovím ovity</l>
     <l n="7" met="10010" line_length="5">na pastvu vyšly.</l>
    </lg>
   </lg>
  </body>
