 <text>
  <front>
   <head>Naše slečinky.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010010100" line_length="10">Naše slečinky, to jsou květinky:</l>
     <l n="2" met="100" rhyme="1" line_length="3">nesejou,</l>
     <l n="3" met="100" rhyme="1" line_length="3">nevějou,</l>
     <l n="4" met="100100" rhyme="2" line_length="6">leninu nepředou,</l>
     <l n="5" met="100100" rhyme="2" line_length="6">domácnost nevedou</l>
     <l n="6" met="01010" rhyme="3" line_length="5">a přece zdá se,</l>
     <l n="7" met="00100010" rhyme="4" line_length="8">že i při nejmenší péči</l>
     <l n="8" met="1000101010" rhyme="4" line_length="10">Šalamouna krále šatem předčí</l>
     <l n="9" met="101010" rhyme="3" line_length="6">ve vší jeho kráse.</l>
    </lg>
   </lg>
  </body>
 </text>
