 <text>
  <front>
   <head>Už zhasla, vím,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0101" rhyme="1" line_length="4">Už zhasla, vím, </l>
     <l n="2" met="01" rhyme="2" line_length="2">a zbyl</l>
     <l n="3" met="0101" rhyme="1" line_length="4">jen planý rým,</l>
     <l n="4" met="101" rhyme="2" line_length="3">blahých chvil</l>
     <l n="5" met="101" rhyme="1" line_length="3">čpavý dým.</l>
    </lg>
    <lg n="2">
     <l n="6" met="010" rhyme="3" line_length="3">A hoří</l>
     <l n="7" met="100" rhyme="4" line_length="3">odlesk jen</l>
     <l n="8" met="1001" rhyme="4" line_length="4">– vyznělý sten –</l>
     <l n="9" met="10" rhyme="3" line_length="2">v moři.</l>
    </lg>
    <lg n="3">
     <l n="10" met="101" rhyme="5" line_length="3">Žití kus</l>
     <l n="11" met="100" rhyme="6" line_length="3">pochován,</l>
     <l n="12" met="101" rhyme="5" line_length="3">zbyl jen hnus,</l>
    </lg>
    <lg n="4">
     <l n="13" met="101" rhyme="7" line_length="3">planý rým,</l>
     <l n="14" met="101" rhyme="6" line_length="3">starých ran</l>
    </lg>
    <lg n="5">
     <l n="15" met="101" rhyme="7" line_length="3">čpavý dým.</l>
    </lg>
   </lg>
  </body>
 </text>
