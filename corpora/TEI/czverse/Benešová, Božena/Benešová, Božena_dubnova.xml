 <text>
  <front>
   <head>DUBNOVÁ</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001010" rhyme="1" line_length="8">Zavoněla celá země</l>
     <l n="2" met="101" rhyme="2" line_length="3">v jarní dech,</l>
     <l n="3" met="10101010" rhyme="1" line_length="8">a jak vůně tajně, jemně</l>
     <l n="4" met="10101010" rhyme="1" line_length="8">dávná láska slétla ke mně</l>
     <l n="5" met="101" rhyme="2" line_length="3">v lehkých snech.</l>
    </lg>
    <lg n="2">
     <l n="6" met="10101010" rhyme="3" line_length="8">Prošla srdce tmavé skrýše</l>
     <l n="7" met="101" rhyme="2" line_length="3">v lehkých snech,</l>
     <l n="8" met="10000010" rhyme="3" line_length="8">zazpívala si tam tiše</l>
     <l n="9" met="10100010" rhyme="3" line_length="8">a zas táhla výš a výše</l>
     <l n="10" met="101" rhyme="2" line_length="3">v jarní dech.</l>
    </lg>
   </lg>
  </body>
 </text>
