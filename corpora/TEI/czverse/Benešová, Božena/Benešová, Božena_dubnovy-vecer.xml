 <text>
  <front>
   <head>DUBNOVÝ VEČER</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101000" rhyme="1" line_length="8">Hnědé hrudy, sametové,</l>
     <l n="2" met="10001010" rhyme="2" line_length="8">prosycené jarní vláhou</l>
     <l n="3" met="10001010" rhyme="3" line_length="8">naposledy blýskly kolem.</l>
     <l n="4" met="10101010" rhyme="1" line_length="8">Jedna hvězda nebem plove,</l>
     <l n="5" met="10001010" rhyme="4" line_length="8">za nízkým se mrakem skrývá,</l>
     <l n="6" met="10110010" rhyme="3" line_length="8">a již jde po světě holém</l>
     <l n="7" met="10101010" rhyme="2" line_length="8">noc svou tajnou, temnou drahou,</l>
     <l n="8" met="10001000" rhyme="4" line_length="8">úskočná a zádumčivá.</l>
    </lg>
   </lg>
  </body>
 </text>
