  <front>
   <head>123. Na otce hloupé dcery.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010100" rhyme="1" line_length="7">Snadli dcera loudavá</l>
     <l n="2" met="100001" rhyme="2" line_length="6">Za muže již se vdá?</l>
     <l n="3" met="1010100" rhyme="1" line_length="7">Posud neví – pěnkava</l>
     <l n="4" met="001010" rhyme="2" line_length="6">Kam svým nosem sedá.</l>
    </lg>
   </lg>
  </body>
