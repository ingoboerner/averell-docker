   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101010" rhyme="1" line_length="6">O ty malý štěpe!</l>
     <l n="2" met="100010" rhyme="1" line_length="6">Neníli ti lépe</l>
     <l n="3" met="00101" rhyme="2" line_length="5">Že jsy nízký strom?</l>
     <l n="4" met="00101" rhyme="2" line_length="5">Neb tě mine hrom.</l>
