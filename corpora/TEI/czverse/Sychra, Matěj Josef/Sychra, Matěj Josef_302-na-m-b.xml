   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001010" rhyme="1" line_length="8">Vede se ti jako psovi</l>
     <l n="2" met="10101000" rhyme="1" line_length="8">Podlé bajky Ezopovy.</l>
     <l n="3" met="0110100" rhyme="2" line_length="7">Cos měl, tím jsy pohrdal,</l>
     <l n="4" met="1010100" rhyme="2" line_length="7">Kozý bobky vydělal.</l>
    </lg>
   </lg>
