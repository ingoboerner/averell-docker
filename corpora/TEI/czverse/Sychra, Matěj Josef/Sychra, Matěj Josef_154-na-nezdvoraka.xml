   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="001000" rhyme="1" line_length="6">Tak jsy přívětivý</l>
     <l n="2" met="0010" rhyme="2" line_length="4">Když jsy spitý:</l>
     <l n="3" met="010010" rhyme="1" line_length="6">Jak za ňadry živý</l>
     <l n="4" met="1010" rhyme="2" line_length="4">Ježek skrytý.</l>
