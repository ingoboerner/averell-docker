   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Dovede tvá čubka</l>
     <l n="2" met="1000100" rhyme="2" line_length="7">Na tebe se doptati;</l>
     <l n="3" met="011010" rhyme="1" line_length="6">Neb ví holka klubka</l>
     <l n="4" met="1000100" rhyme="2" line_length="7">Po niti kde hledati.</l>
    </lg>
