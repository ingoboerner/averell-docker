   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="001010" rhyme="1" line_length="6">Kdo se dobře učí,</l>
     <l n="2" met="00100" rhyme="2" line_length="5">Buď náš kamarád!</l>
     <l n="3" met="101010" rhyme="1" line_length="6">Koho knížka mučí,</l>
     <l n="4" met="10101" rhyme="2" line_length="5">Nesmí s námi hrát!</l>
