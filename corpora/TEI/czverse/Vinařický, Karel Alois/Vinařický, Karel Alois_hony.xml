 <text>
  <front>
   <head>Hony.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10010" line_length="5">Do pole, háje,</l>
     <l n="2" met="10100" line_length="5">Muži, jonáci;</l>
     <l n="3" met="100100" line_length="6">Honiti naše je</l>
     <l n="4" met="1000" line_length="4">Vyražení!</l>
    </lg>
    <lg n="2">
     <l n="5" met="100100" line_length="6">Lada i paseky,</l>
     <l n="6" met="10010" line_length="5">Pole a louky</l>
     <l n="7" met="10010" line_length="5">Ku honu ráno</l>
     <l n="8" met="1010" line_length="4">Mile zovou.</l>
    </lg>
    <lg n="3">
     <l n="9" met="10010" line_length="5">Do pole, háje,</l>
     <l n="10" met="10100" line_length="5">Muži, jonáci;</l>
     <l n="11" met="100100" line_length="6">Honiti naše je</l>
     <l n="12" met="1000" line_length="4">Vyražení!</l>
    </lg>
    <lg n="4">
     <l n="13" met="100100" line_length="6">U lesa tenata</l>
     <l n="14" met="10000" line_length="5">Pozaložíme,</l>
     <l n="15" met="01000" line_length="5">A uvidíme,</l>
     <l n="16" met="0010" line_length="4">Co se lapí.</l>
    </lg>
    <lg n="5">
     <l n="17" met="10010" line_length="5">Do pole, háje,</l>
     <l n="18" met="10100" line_length="5">Muži, jonáci;</l>
     <l n="19" met="100100" line_length="6">Honiti naše je</l>
     <l n="20" met="1000" line_length="4">Vyražení!</l>
    </lg>
    <lg n="6">
     <l n="21" met="100100" line_length="6">Na laně, jeleny</l>
     <l n="22" met="01000" line_length="5">Si počekáme;</l>
     <l n="23" met="10100" line_length="5">Běhy zajíci</l>
     <l n="24" met="1000" line_length="4">Nepomohou.</l>
    </lg>
    <lg n="7">
     <l n="25" met="10010" line_length="5">Do pole, háje,</l>
     <l n="26" met="10100" line_length="5">Muži, jonáci;</l>
     <l n="27" met="100100" line_length="6">Honiti naše je</l>
     <l n="28" met="1000" line_length="4">Vyražení!</l>
    </lg>
    <lg n="8">
     <l n="29" met="100100" line_length="6">U meze na luhu</l>
     <l n="30" met="01000" line_length="5">Se vyčekáme,</l>
     <l n="31" met="01000" line_length="5">A pojecháme</l>
     <l n="32" met="1010" line_length="4">Zase domů.</l>
    </lg>
    <lg n="9">
     <l n="33" met="10010" line_length="5">Do pole, háje,</l>
     <l n="34" met="10100" line_length="5">Muži, jonáci;</l>
     <l n="35" met="100100" line_length="6">Honiti naše je</l>
     <l n="36" met="1000" line_length="4">Vyražení!</l>
    </lg>
   </lg>
  </body>
 </text>
