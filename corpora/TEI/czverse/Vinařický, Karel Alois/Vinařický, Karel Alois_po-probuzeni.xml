 <text>
  <front>
   <head>Po probuzení.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101" rhyme="1" line_length="3">Bílý den!</l>
     <l n="2" met="100" rhyme="1" line_length="3">Z peřin ven!</l>
     <l n="3" met="10010" rhyme="2" line_length="5">Nebeské oko,</l>
     <l n="4" met="10100" rhyme="3" line_length="5">Zlaté sluníčko</l>
     <l n="5" met="10100" rhyme="3" line_length="5">Bude brzíčko</l>
     <l n="6" met="10100" rhyme="2" line_length="5">Státi vysoko.</l>
     <l n="7" met="1000" rhyme="4" line_length="4">Z postele ven!</l>
     <l n="8" met="1001" rhyme="4" line_length="4">K práci je den!</l>
    </lg>
   </lg>
  </body>
