   <head>Dvoucepní.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1111" rhyme="1" line_length="4">Klop snop! tep, klep!</l>
     <l n="2" met="1111" rhyme="1" line_length="4">Mlat, plat! cep, chléb!</l>
     <l n="3" met="1100" line_length="4">Spát, hrát! cuch, dluch!</l>
     <l n="4" met="111" rhyme="1" line_length="3">Mlat, plat, chléb!</l>
    </lg>
   </lg>
