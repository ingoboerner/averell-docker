 <text>
  <front>
   <head>Ze všeho jediná</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" line_length="6">Ze všeho jediná </l>
     <l n="2" met="100100" rhyme="1" line_length="6">zbyla’s mně, matičko,</l>
     <l n="3" met="100100" line_length="6">vychladlé zemi jak</l>
     <l n="4" met="100100" rhyme="1" line_length="6">podzimní sluníčko.</l>
    </lg>
    <lg n="2">
     <l n="5" met="100100" line_length="6">Podzimní sluníčko</l>
     <l n="6" met="100100" rhyme="2" line_length="6">neslní, nepálí,</l>
     <l n="7" met="100100" line_length="6">předce se zatřesem,</l>
     <l n="8" met="000100" rhyme="2" line_length="6">když se nám zakalí.</l>
    </lg>
   </lg>
  </body>
