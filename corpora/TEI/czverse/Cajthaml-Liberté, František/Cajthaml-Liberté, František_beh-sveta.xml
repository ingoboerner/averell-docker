   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Poctivec se bojí</l>
     <l n="2" met="101010" rhyme="2" line_length="6">vstoupit v štěstí chrámy –</l>
     <l n="3" met="001010" rhyme="1" line_length="6">kol nich stohy stojí</l>
     <l n="4" met="100010" rhyme="2" line_length="6">plevele a slámy.</l>
    </lg>
   </lg>
