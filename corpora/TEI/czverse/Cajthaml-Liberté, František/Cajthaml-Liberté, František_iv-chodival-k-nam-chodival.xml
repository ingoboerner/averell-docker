   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1000100" line_length="7">Chodíval k nám, chodíval </l>
     <l n="2" met="101000" rhyme="1" line_length="6">ptáček chocholatý,</l>
     <l n="3" met="0101000" line_length="7">a div si pro „petrolej“</l>
     <l n="4" met="100010" rhyme="1" line_length="6">nepolámal hnáty.</l>
