   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101010" rhyme="1" line_length="6">Skromnost pěstuj! Nyní</l>
     <l n="2" met="10001" rhyme="2" line_length="5">krásná je to ctnost:</l>
     <l n="3" met="100110" rhyme="1" line_length="6">maso když rvou jiní,</l>
     <l n="4" met="10001" rhyme="2" line_length="5">ohlodávej kost.</l>
