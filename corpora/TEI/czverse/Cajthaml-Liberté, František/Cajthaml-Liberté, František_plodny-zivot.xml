   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="11110" line_length="5">Půl sta let živ byl</l>
     <l n="2" met="10010" line_length="5">soukromník Zíval,</l>
     <l n="3" met="11001" line_length="5">spal, jedl a pil,</l>
     <l n="4" met="10010" line_length="5">kupony stříhal.</l>
