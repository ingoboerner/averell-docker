     <l n="1" met="111" rhyme="1" line_length="3">Bác! bác! bum!</l>
     <l n="2" met="101000" rhyme="2" line_length="6">Blesku Bělohorský!</l>
     <l n="3" met="00100" rhyme="1" line_length="5">to je unikum,</l>
     <l n="4" met="010010" rhyme="2" line_length="6">jak František Dvorský</l>
     <l n="5" met="10100" rhyme="1" line_length="5">hřímá v publikum!</l>
