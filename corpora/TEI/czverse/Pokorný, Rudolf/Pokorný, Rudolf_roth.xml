   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="01000100" line_length="8">Zde vizte mne jen, přátelé,</l>
     <l n="2" met="01000100" rhyme="1" line_length="8">jsem Staročech já pálený,</l>
     <l n="3" met="01010000" line_length="8">však mezi Staročechy jsem</l>
     <l n="4" met="00100100" rhyme="1" line_length="8">já sám jediný červený.</l>
    </lg>
   </lg>
