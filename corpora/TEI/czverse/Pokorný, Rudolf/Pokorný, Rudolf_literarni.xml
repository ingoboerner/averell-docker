   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101000" line_length="8">Aby naše písemnictví</l>
     <l n="2" met="1000100" rhyme="1" line_length="7">nechodilo po hlavě,</l>
     <l n="3" met="10001010" line_length="8">skritisují křížem krážem</l>
     <l n="4" met="1000100" rhyme="1" line_length="7">v taneční je zábavě!</l>
    </lg>
   </lg>
