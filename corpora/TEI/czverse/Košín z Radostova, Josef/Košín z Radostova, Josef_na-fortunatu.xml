   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101000" rhyme="1" line_length="6">Slovo nejluznější</l>
     <l n="2" met="100010" rhyme="2" line_length="6">pro tebe je „láska“,</l>
     <l n="3" met="101000" rhyme="1" line_length="6">ze všech nejstrašnější</l>
     <l n="4" met="101010" rhyme="2" line_length="6">ale slovo „vráska“.</l>
