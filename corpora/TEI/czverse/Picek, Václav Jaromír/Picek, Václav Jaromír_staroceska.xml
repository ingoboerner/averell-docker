 <text>
  <front>
   <head>STAROČESKÁ.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">Bývali Čechové</l>
     <l n="2" met="10100" line_length="5">Statní jonáci,</l>
     <l n="3" met="100100" rhyme="1" line_length="6">Bývali rekové,</l>
     <l n="4" met="1001" rhyme="2" line_length="4">Muži co květ;</l>
     <l n="5" met="100100" rhyme="3" line_length="6">Bývali, bývali,</l>
     <l n="6" met="100100" rhyme="3" line_length="6">Hájili, zpívali,</l>
     <l n="7" met="0011" rhyme="2" line_length="4">Tak je znal svět.</l>
    </lg>
    <lg n="2">
     <l n="8" met="100100" rhyme="1" line_length="6">Časové, Čechové,</l>
     <l n="9" met="00100" line_length="5">Jak se měníte!</l>
     <l n="10" met="100100" rhyme="1" line_length="6">Buďte zas rekové,</l>
     <l n="11" met="0011" rhyme="2" line_length="4">Jak vás znal svět,</l>
     <l n="12" met="100100" rhyme="4" line_length="6">Rekové v smýšlení,</l>
     <l n="13" met="100100" rhyme="4" line_length="6">V lásce a dychtění</l>
     <l n="14" met="1001" rhyme="2" line_length="4">Pro vlasti květ.</l>
    </lg>
   </lg>
  </body>
 </text>
