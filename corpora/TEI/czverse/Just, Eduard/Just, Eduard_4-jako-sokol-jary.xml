   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101010" line_length="6">Jako Sokol jarý </l>
     <l n="2" met="1000" line_length="4">přicházím k Vám</l>
     <l n="3" met="010010" line_length="6">a k novému roku</l>
     <l n="4" met="1000" line_length="4">provolávám</l>
     <l n="5" met="10010" line_length="5">upřímné „Na zdar!“</l>
