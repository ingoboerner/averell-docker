   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001010" line_length="8">Ráda bych Ti pěkně přála, </l>
     <l n="2" met="1000100" line_length="7">poněvadž však neumím,</l>
     <l n="3" met="10001000" line_length="8">raději se Otčenášek</l>
     <l n="4" met="1010100" line_length="7">za Tvé blaho pomodlím.</l>
