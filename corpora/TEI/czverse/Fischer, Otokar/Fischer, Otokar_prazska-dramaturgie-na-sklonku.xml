  <front>
   <head>PRAŽSKÁ DRAMATURGIE NA SKLONKU SAISONY 1921/1922</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0010001" rhyme="1" line_length="7">Já sic opustil svou loď,</l>
     <l n="2" met="1000001" rhyme="1" line_length="7">nechal jsem tam však svou choť.</l>
     <l n="3" met="1010100" rhyme="2" line_length="7">Zdenou řídím posádku</l>
     <l n="4" met="1000100" rhyme="2" line_length="7">bez drátu jak na drátku.</l>
    </lg>
    <lg n="2">
     <l n="5" met="0010001" rhyme="1" line_length="7">Já sic opustil svou loď,</l>
     <l n="6" met="1000001" rhyme="1" line_length="7">nechal jsem tam však svou choť.</l>
     <l n="7" met="1010100" rhyme="2" line_length="7">Zdenou řídím posádku</l>
     <l n="8" met="1000100" rhyme="2" line_length="7">bez drátu jak na drátku.</l>
    </lg>
   </lg>
  </body>
