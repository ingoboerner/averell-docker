   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101010100" line_length="9">Sláva slavným! Od všech budiž ctín</l>
     <l n="2" met="101000" rhyme="1" line_length="6">farář v Miletíně!</l>
     <l n="3" met="10101000100" line_length="11">Nemá nad něj milejšího Hospodin</l>
     <l n="4" met="101000" rhyme="1" line_length="6">ani hospodyně.</l>
    </lg>
   </lg>
