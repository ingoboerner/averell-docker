 <text>
  <front>
   <head>OST UND WEST</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10100" rhyme="1" line_length="5">Museum nestavte</l>
     <l n="2" met="101000" rhyme="2" line_length="6">dole u Vltavy,</l>
     <l n="3" met="1010" rhyme="3" line_length="4">přijde z jara</l>
     <l n="4" met="1010" rhyme="3" line_length="4">voda jará,</l>
     <l n="5" met="100010" rhyme="2" line_length="6">do Němec ho splaví.</l>
    </lg>
    <lg n="2">
     <l n="6" met="100010" rhyme="1" line_length="6">Raději ho nechte</l>
     <l n="7" met="100010" rhyme="4" line_length="6">na Hradčanech státi,</l>
     <l n="8" met="1010" rhyme="5" line_length="4">bude na něj</l>
     <l n="9" met="1010" rhyme="5" line_length="4">vítr zdravej</l>
     <l n="10" met="100010" rhyme="4" line_length="6">od severa váti.</l>
    </lg>
   </lg>
  </body>
 </text>
