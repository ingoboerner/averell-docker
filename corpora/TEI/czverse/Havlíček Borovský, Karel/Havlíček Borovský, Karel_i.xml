 <text>
  <front>
   <head>I.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1000" rhyme="1" line_length="4">Jeláčku náš,</l>
     <l n="2" met="1010" rhyme="1" line_length="4">copak děláš?</l>
     <l n="3" met="101000" rhyme="2" line_length="6">Sněm nám rozehnali,</l>
     <l n="4" met="100100" rhyme="2" line_length="6">Slovany prodali,</l>
     <l n="5" met="1010" rhyme="1" line_length="4">[:ty nic nedbáš:].</l>
    </lg>
    <lg n="2">
     <l n="6" met="1000" rhyme="3" line_length="4">Jeláčku můj,</l>
     <l n="7" met="1001" rhyme="3" line_length="4">v slovu mi stůj,</l>
     <l n="8" met="000100" rhyme="4" line_length="6">co jsi mi sliboval,</l>
     <l n="9" met="001000" rhyme="4" line_length="6">když jsi vyrukoval,</l>
     <l n="10" met="0100" rhyme="3" line_length="4">[:že budeš můj:].</l>
    </lg>
    <lg n="3">
     <l n="11" met="1001" rhyme="5" line_length="4">Slavie má,</l>
     <l n="12" met="1010" rhyme="5" line_length="4">není možná,</l>
     <l n="13" met="100100" rhyme="5" line_length="6">počkej až do jara,</l>
     <l n="14" met="010100" rhyme="5" line_length="6">až bude fiala,</l>
     <l n="15" met="1010" rhyme="5" line_length="4">[:bude jiná:]!</l>
    </lg>
   </lg>
  </body>
 </text>
