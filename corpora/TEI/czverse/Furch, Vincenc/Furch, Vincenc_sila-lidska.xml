  <front>
   <head>Síla lidská</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010" line_length="4">Síla lidská </l>
     <l n="2" met="1000" rhyme="1" line_length="4">Nevydatná,</l>
     <l n="3" met="1000" line_length="4">Obmezená,</l>
     <l n="4" met="1000" rhyme="1" line_length="4">Máloplatná.</l>
     <l n="5" met="1010" line_length="4">Ostrým pluhem</l>
     <l n="6" met="1010" rhyme="2" line_length="4">Osud oře,</l>
     <l n="7" met="1010" line_length="4">V brázdy seje</l>
     <l n="8" met="1010" rhyme="2" line_length="4">Štěstí, hoře.</l>
    </lg>
   </lg>
  </body>
