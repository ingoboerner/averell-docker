 <text>
  <front>
   <head>LXXXVI. Dumám;</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10" line_length="2">Dumám; </l>
     <l n="2" met="01010010" rhyme="1" line_length="8">mou hlavu pokryla plíseň,</l>
     <l n="3" met="10010" rhyme="2" line_length="5">kukačka kuká,</l>
     <l n="4" met="10010010010" rhyme="1" line_length="11">na mysli tane mi slovenská píseň</l>
     <l n="5" met="1000" rhyme="2" line_length="4">prostozvuká:</l>
    </lg>
    <lg n="2">
     <l n="6" met="00101010" rhyme="3" line_length="8">„Já som bača tuze starý,</l>
     <l n="7" met="1000100" rhyme="3" line_length="7">dožijem-li do jary,</l>
     <l n="8" met="100010010" line_length="9">bude-li mi kukačka kukat</l>
     <l n="9" met="1010100" rhyme="3" line_length="7">na tom našem chotary?“</l>
    </lg>
    <lg n="3">
     <l n="10" met="10010" rhyme="2" line_length="5">Kukačka kuká,</l>
     <l n="11" met="10" rhyme="4" line_length="2">dumám,</l>
     <l n="12" met="100" rhyme="4" line_length="3">poslouchám:</l>
     <l n="13" met="10010" rhyme="2" line_length="5">srdce mi puká.</l>
    </lg>
   </lg>
  </body>
 </text>
