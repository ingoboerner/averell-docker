 <text>
  <front>
   <head>Čest.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010" rhyme="1" line_length="4">Poďte, dítky!</l>
     <l n="2" met="1010" rhyme="1" line_length="4">Sbírat kvítky</l>
     <l n="3" met="10100" rhyme="2" line_length="5">Poďte do pole!</l>
     <l n="4" met="1010" rhyme="3" line_length="4">Budem hráti,</l>
     <l n="5" met="1010" rhyme="3" line_length="4">Ke hře zváti</l>
     <l n="6" met="10100" rhyme="2" line_length="5">Pilné pachole.</l>
    </lg>
    <lg n="2">
     <l n="7" met="1010" rhyme="4" line_length="4">Horem dolem</l>
     <l n="8" met="1010" rhyme="4" line_length="4">Širým polem</l>
     <l n="9" met="10100" rhyme="5" line_length="5">V spolku běhati,</l>
     <l n="10" met="1010" rhyme="6" line_length="4">V drsné stráni,</l>
     <l n="11" met="1010" rhyme="6" line_length="4">V květné pláni</l>
     <l n="12" met="10100" rhyme="5" line_length="5">Bylin hledati.</l>
    </lg>
    <lg n="3">
     <l n="13" met="0010" rhyme="7" line_length="4">Pak se zvednem,</l>
     <l n="14" met="1010" rhyme="7" line_length="4">Místo vzhlédnem,</l>
     <l n="15" met="10100" rhyme="8" line_length="5">Oukol změříme;</l>
     <l n="16" met="1010" rhyme="9" line_length="4">Podlé řady</l>
     <l n="17" met="1010" rhyme="9" line_length="4">Vůkol sady</l>
     <l n="18" met="10100" rhyme="8" line_length="5">Sobě vštěpíme.</l>
    </lg>
    <lg n="4">
     <l n="19" met="0010" rhyme="10" line_length="4">Kdo své pole</l>
     <l n="20" met="1010" rhyme="10" line_length="4">V celém kole</l>
     <l n="21" met="10100" rhyme="11" line_length="5">Nejlíp vypraví,</l>
     <l n="22" met="1010" rhyme="12" line_length="4">Toho stále</l>
     <l n="23" met="1010" rhyme="12" line_length="4">Jako krále</l>
     <l n="24" met="10100" rhyme="11" line_length="5">Každý pozdraví.</l>
    </lg>
    <lg n="5">
     <l n="25" met="1000" rhyme="13" line_length="4">Předevšemi</l>
     <l n="26" met="1010" rhyme="13" line_length="4">V naší zemi</l>
     <l n="27" met="10001" rhyme="14" line_length="5">Moudrému buď čest;</l>
     <l n="28" met="1010" rhyme="15" line_length="4">Rozum zdravý</l>
     <l n="29" met="1010" rhyme="15" line_length="4">Rádce pravý</l>
     <l n="30" met="10100" rhyme="14" line_length="5">Pocty hoden jest.</l>
    </lg>
   </lg>
  </body>
 </text>
