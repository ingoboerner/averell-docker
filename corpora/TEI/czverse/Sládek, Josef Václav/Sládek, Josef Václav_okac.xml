 <text>
  <front>
   <head>OKÁČ.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0010100" rhyme="1" line_length="7">JÁ jsem děvče okaté,</l>
     <l n="2" met="1001100" rhyme="1" line_length="7">šatečky mám strakaté,</l>
     <l n="3" met="1000100" rhyme="2" line_length="7">vypadám v nich celičká</l>
     <l n="4" met="0010100" rhyme="2" line_length="7">jak ta lesní pěnička.</l>
    </lg>
    <lg n="2">
     <l n="5" met="0010100" rhyme="1" line_length="7">Já jsem děvče okaté,</l>
     <l n="6" met="1001100" rhyme="1" line_length="7">tvářičky mám buclaté,</l>
     <l n="7" met="1010101" rhyme="3" line_length="7">na nich jako růže květ,</l>
     <l n="8" met="1010101" rhyme="3" line_length="7">a mne těší celý svět!</l>
    </lg>
   </lg>
  </body>
 </text>
