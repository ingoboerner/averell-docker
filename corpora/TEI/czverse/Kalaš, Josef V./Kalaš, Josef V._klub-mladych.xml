 <text>
  <front>
   <head>Klub mladých.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10100" rhyme="1" line_length="5">Mozek veliký,</l>
     <l n="2" met="10100" rhyme="1" line_length="5">ostré jazyky,</l>
     <l n="3" met="1010" rhyme="2" line_length="4">tělo chabé,</l>
     <l n="4" met="1010" rhyme="2" line_length="4">ruce slabé,</l>
     <l n="5" met="1010" rhyme="3" line_length="4">nohy žádné,</l>
     <l n="6" met="1010" rhyme="3" line_length="4">snadno padne.</l>
    </lg>
    <lg n="2">
     <l n="7" met="10101010" line_length="8">Proto rychle pomoc léky,</l>
     <l n="8" met="101010" rhyme="4" line_length="6">tělu více vůle,</l>
     <l n="9" met="10101000" line_length="8">hlavu v řece vymáchejte</l>
     <l n="10" met="010010" rhyme="4" line_length="6">a vyhráno z půle.</l>
    </lg>
   </lg>
  </body>
 </text>
