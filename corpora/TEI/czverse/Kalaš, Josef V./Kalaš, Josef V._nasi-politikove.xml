   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">Okovu, rumpálu,</l>
     <l n="2" met="1001" rhyme="2" line_length="4">roveň váš mrav:</l>
     <l n="3" met="100100" rhyme="1" line_length="6">nahoru pomalu...</l>
     <l n="4" met="1010" rhyme="2" line_length="4">dolů střemhlav.</l>
