   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010100010" rhyme="1" line_length="10">Tento skvostně vytesaný kámen </l>
     <l n="2" met="10100010" rhyme="2" line_length="8">Kryje pozůstatky pána</l>
     <l n="3" met="10001000" rhyme="2" line_length="8">Nebožtíka Tulipána;</l>
     <l n="4" met="0010100010" rhyme="1" line_length="10">Buď mu lehký odpočinek. Amen.</l>
    </lg>
   </lg>
