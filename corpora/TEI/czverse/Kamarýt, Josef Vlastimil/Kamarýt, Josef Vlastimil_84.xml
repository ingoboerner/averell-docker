  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">K práci buď ochotný,</l>
     <l n="2" met="100100" rhyme="1" line_length="6">řádný a čistotný;</l>
     <l n="3" met="101000" rhyme="2" line_length="6">vlídný ke každému,</l>
     <l n="4" met="101100" rhyme="2" line_length="6">nemluv lži žádnému;</l>
     <l n="5" met="100100" rhyme="3" line_length="6">tichý co ovčička,</l>
     <l n="6" met="100100" rhyme="3" line_length="6">dobrá buď dušička.</l>
    </lg>
   </lg>
  </body>
