   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010" line_length="4">Mladou ještě</l>
     <l n="2" met="10001000" rhyme="1" line_length="8">pro milence pozemského,</l>
     <l n="3" met="1010" line_length="4">a jižť hodnou</l>
     <l n="4" met="1000" rhyme="1" line_length="4">nebeského</l>
     <l n="5" met="100" line_length="3">sobě Ji</l>
     <l n="6" met="1000100" line_length="7">zasnoubili anjelé.</l>
