   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1000100" rhyme="1" line_length="7">Nakaženost vnitřností</l>
     <l n="2" met="10001000" rhyme="2" line_length="8">smradem ust se vyzrazuje:</l>
     <l n="3" met="0010100" rhyme="1" line_length="7">tak též vnitřní mrzkosti</l>
     <l n="4" met="10011000" rhyme="2" line_length="8">hanebná řeč vyjevuje.</l>
    </lg>
   </lg>
