    <lg n="1">
     <l n="1" met="10101" rhyme="1" line_length="5">Mnohý v ustech med,</l>
     <l n="2" met="10101" rhyme="1" line_length="5">v srdci chová jed;</l>
     <l n="3" met="100010" rhyme="2" line_length="6">v oči se ti klaní,</l>
     <l n="4" met="100010" rhyme="2" line_length="6">po straně tě haní.</l>
    </lg>
