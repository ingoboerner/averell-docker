  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001" rhyme="1" line_length="5">Nestydatá řeč</l>
     <l n="2" met="010010" rhyme="2" line_length="6">jest pro milou mládež</l>
     <l n="3" met="101010" rhyme="2" line_length="6">ctnosti její krádež;</l>
     <l n="4" met="101010" line_length="6">v její outlé srdce</l>
     <l n="5" met="10001" rhyme="1" line_length="5">vražedlný meč.</l>
    </lg>
   </lg>
  </body>
