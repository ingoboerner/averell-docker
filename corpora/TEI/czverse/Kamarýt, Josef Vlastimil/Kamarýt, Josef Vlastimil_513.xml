   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101000" line_length="6">Žák, jenž na otázku</l>
     <l n="2" met="100010" rhyme="1" line_length="6">odpovědi neví,</l>
     <l n="3" met="100110" line_length="6">ve škole má vážnost –</l>
     <l n="4" met="010010" rhyme="1" line_length="6">co na sejpce plevy.</l>
    </lg>
   </lg>
