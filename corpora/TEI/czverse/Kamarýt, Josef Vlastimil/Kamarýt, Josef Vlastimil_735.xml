   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="011001010" rhyme="1" line_length="9">Vše Bůh k prospěchu tvému strojí,</l>
     <l n="2" met="010101010" rhyme="1" line_length="9">on kárá, ale zase hojí:</l>
     <l n="3" met="11000100" rhyme="2" line_length="8">nes v naději své břemeno,</l>
     <l n="4" met="01010100" rhyme="2" line_length="8">a pros – ním bude složeno.</l>
    </lg>
   </lg>
