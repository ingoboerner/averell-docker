   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010100" rhyme="1" line_length="7">Zákon Páně světlo tvé,</l>
     <l n="2" met="1010100" rhyme="1" line_length="7">řiď ním pilně kroky své;</l>
     <l n="3" met="10001010" rhyme="2" line_length="8">přivede tě světlo jeho</l>
     <l n="4" met="10001000" rhyme="2" line_length="8">do království blaženého.</l>
    </lg>
   </lg>
