   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="010010" line_length="6">Kdo ve věcech Božích</l>
     <l n="2" met="10001000" rhyme="1" line_length="8">mudrovat se opováží,</l>
     <l n="3" met="101010" line_length="6">jistě v samý zmatek,</l>
     <l n="4" met="10001010" rhyme="1" line_length="8">v potupu sám sebe sráží.</l>
    </lg>
   </lg>
