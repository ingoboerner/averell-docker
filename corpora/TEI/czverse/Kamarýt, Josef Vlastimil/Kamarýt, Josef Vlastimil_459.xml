   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010100" rhyme="1" line_length="9">Rceli kdo, že Boha miluje,</l>
     <l n="2" met="1001000" rhyme="2" line_length="7">bratra pak nenávidí,</l>
     <l n="3" met="010010100" rhyme="1" line_length="9">ten se slovem Božím žertuje,</l>
     <l n="4" met="1010010" rhyme="2" line_length="7">a jen sebe sám šidí.</l>
    </lg>
   </lg>
