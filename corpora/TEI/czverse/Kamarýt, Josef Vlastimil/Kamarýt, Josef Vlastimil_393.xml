   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1001010010" line_length="10">Na zemi této vzácné je léto,</l>
     <l n="2" met="101" rhyme="1" line_length="3">vzácný den;</l>
     <l n="3" met="1011010010" line_length="10">rok i den hyne, mladost i mine,</l>
     <l n="4" met="101" rhyme="1" line_length="3">jako sen.</l>
    </lg>
   </lg>
