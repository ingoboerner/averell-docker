   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1110" rhyme="1" line_length="4">Meč má chránit,</l>
     <l n="2" met="1010" rhyme="1" line_length="4">neli ranit.</l>
     <l n="3" met="001010100" rhyme="2" line_length="9">To si každý mocný pamatuj,</l>
     <l n="4" met="010010100" rhyme="2" line_length="9">a nižšího sebe nesužuj.</l>
    </lg>
   </lg>
