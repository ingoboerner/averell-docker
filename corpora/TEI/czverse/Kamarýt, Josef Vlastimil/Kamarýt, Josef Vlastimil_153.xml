   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Jestliže tvůj bližní</l>
     <l n="2" met="111010" rhyme="1" line_length="6">hlad má, nebo žížní:</l>
     <l n="3" met="1000100" line_length="7">nezavírej ruky své,</l>
     <l n="4" met="0110100" line_length="7">co máš, uděl ochotně.</l>
    </lg>
   </lg>
