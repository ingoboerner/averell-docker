   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="01001010" line_length="8">Když od přátel příliš mnoho</l>
     <l n="2" met="1000" rhyme="1" line_length="4">pohledáváš,</l>
     <l n="3" met="00001000" line_length="8">sám jim „Mne se odřekněte!“</l>
     <l n="4" met="1010" rhyme="1" line_length="4">radu dáváš.</l>
    </lg>
   </lg>
