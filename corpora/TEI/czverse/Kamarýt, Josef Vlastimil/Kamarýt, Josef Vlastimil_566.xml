   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001010" rhyme="1" line_length="8">Blaze nám, když zásluh máme,</l>
     <l n="2" met="0010100" rhyme="2" line_length="7">neb až jednou zemřeme,</l>
     <l n="3" met="0010100" rhyme="2" line_length="7">ty jen s sebou vezmeme,</l>
     <l n="4" met="10001000" rhyme="1" line_length="8">ostatní zde zanecháme.</l>
    </lg>
   </lg>
