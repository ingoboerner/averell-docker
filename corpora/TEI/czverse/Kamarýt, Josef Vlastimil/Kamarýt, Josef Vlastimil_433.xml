  <front>
   <head>433.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101000" rhyme="1" line_length="8">Komu v jasné spanilosti</l>
     <l n="2" met="1010100" rhyme="2" line_length="7">růže v tváři rozkvetá,</l>
     <l n="3" met="00101000" rhyme="1" line_length="8">kdo své chyby, nepravosti</l>
     <l n="4" met="1010100" rhyme="2" line_length="7">snažně z srdce vymetá,</l>
     <l n="5" met="10101010" rhyme="3" line_length="8">komu z očí v jasném zření</l>
     <l n="6" met="1000100" rhyme="4" line_length="7">veselost se usmívá:</l>
     <l n="7" met="10001000" rhyme="3" line_length="8">Nevinnosti k oslavení</l>
     <l n="8" met="1000100" rhyme="4" line_length="7">chválopíseň zazpívá.</l>
    </lg>
   </lg>
  </body>
