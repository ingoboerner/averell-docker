   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="010010" line_length="6">Co za dobré uznáš,</l>
     <l n="2" met="10001" rhyme="1" line_length="5">do toho se dej;</l>
     <l n="3" met="001010" line_length="6">co jsi jednou začal,</l>
     <l n="4" met="00101" rhyme="1" line_length="5">v tom i stálost měj.</l>
    </lg>
