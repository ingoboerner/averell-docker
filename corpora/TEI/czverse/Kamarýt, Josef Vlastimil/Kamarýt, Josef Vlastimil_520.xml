   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101000" rhyme="1" line_length="6">Křivé přísahání,</l>
     <l n="2" met="101000" rhyme="1" line_length="6">na cti utrhání,</l>
     <l n="3" met="101000" rhyme="1" line_length="6">bližních okrádání,</l>
     <l n="4" met="101000" rhyme="1" line_length="6">život bez pokání –</l>
     <l n="5" met="100100" rhyme="1" line_length="6">do pekla zahání.</l>
    </lg>
   </lg>
  </body>
