   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1000101" rhyme="1" line_length="7">O člověče! každý den</l>
     <l n="2" met="1010100" rhyme="2" line_length="7">pošli k nebi prosby tvé;</l>
     <l n="3" met="0010100" rhyme="1" line_length="7">ne však ruce k Bohu jen,</l>
     <l n="4" met="1010100" rhyme="2" line_length="7">spolu srdce zdvihni své.</l>
    </lg>
   </lg>
