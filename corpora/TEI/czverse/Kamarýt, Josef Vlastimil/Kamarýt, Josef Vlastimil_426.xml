   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010100" rhyme="1" line_length="7">Není ještě ztraceno,</l>
     <l n="2" met="0010100" rhyme="1" line_length="7">co být může vráceno.</l>
     <l n="3" met="10001" rhyme="2" line_length="5">Ztracený však čas –</l>
     <l n="4" met="10000" rhyme="2" line_length="5">nevrátí se zas.</l>
    </lg>
   </lg>
