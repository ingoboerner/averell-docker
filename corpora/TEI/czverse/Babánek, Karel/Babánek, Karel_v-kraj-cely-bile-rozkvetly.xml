 <text>
  <front>
   <head>V. Kraj celý bíle rozkvetlý.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="11010100" line_length="8">Kraj celý bíle rozkvetlý. </l>
     <l n="2" met="0101010" line_length="7">Mou duši sladce spíjí</l>
     <l n="3" met="11010" line_length="5">květ bílý, bílý.</l>
    </lg>
    <lg n="2">
     <l n="4" met="01010100" line_length="8">Jak tiché chvíle sváteční</l>
     <l n="5" met="1001010" line_length="7">do duše moji svítí</l>
     <l n="6" met="11010" line_length="5">květ bílý, bílý.</l>
    </lg>
    <lg n="3">
     <l n="7" met="10010100" line_length="8">Pod jarním nebem nezměrným</l>
     <l n="8" met="1001010" line_length="7">rozkvetla duše moje</l>
     <l n="9" met="11010" line_length="5">v květ bílý, bílý.</l>
    </lg>
   </lg>
  </body>
 </text>
