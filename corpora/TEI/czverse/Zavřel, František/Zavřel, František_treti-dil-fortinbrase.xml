     <l n="1" met="1011000" rhyme="1" line_length="7">Třetí díl Fortinbrase </l>
     <l n="2" met="100010" rhyme="2" line_length="6">nevyjde, mon ami.</l>
     <l n="3" met="100" rhyme="2" line_length="3">Perlami</l>
