   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1000100" rhyme="1" line_length="7">Usměrněný opičák</l>
     <l n="2" met="1000100" rhyme="2" line_length="7">zadarmo nic nekoná.</l>
     <l n="3" met="10000" rhyme="2" line_length="5">Z Napoleona</l>
     <l n="4" met="1010100" rhyme="1" line_length="7">stal se v Praze levičák!</l>
