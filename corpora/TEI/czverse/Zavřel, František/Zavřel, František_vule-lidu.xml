    <lg n="1">
     <l n="1" met="101010" rhyme="1" line_length="6">Kukly všechny pukly</l>
     <l n="2" met="100010" rhyme="2" line_length="6">obnaženy po týl:</l>
     <l n="3" met="100010" rhyme="1" line_length="6">z revoluční kukly</l>
     <l n="4" met="101010" rhyme="2" line_length="6">vylét schlíplý motýl.</l>
