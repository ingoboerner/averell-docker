  <front>
   <head>ROZKAZ</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1001000" rhyme="1" line_length="7">Ve jménu humanity,</l>
     <l n="2" met="100100" rhyme="2" line_length="6">Husa a svědomí</l>
     <l n="3" met="01010" rhyme="1" line_length="5">buď na kříž vbitý</l>
     <l n="4" met="01010000" rhyme="2" line_length="8">kdo není (přeskočilo mi!)</l>
     <l n="5" met="0100" rhyme="2" line_length="4">tak pitomý</l>
     <l n="6" met="100" rhyme="2" line_length="3">jako my!</l>
    </lg>
   </lg>
  </body>
