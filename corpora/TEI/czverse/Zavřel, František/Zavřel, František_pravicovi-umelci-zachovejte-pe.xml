  <front>
   <head>PRAVICOVÍ UMĚLCI, ZACHOVEJTE PEVNOU PÁTEŘ!</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1000100" rhyme="1" line_length="7">Milovaná pravice</l>
     <l n="2" met="100010100" rhyme="1" line_length="9">obsadila všechny posice</l>
     <l n="3" met="10001010" rhyme="2" line_length="8">od Visly až k vlnám Ebra.</l>
     <l n="4" met="10011000" rhyme="3" line_length="8">Hojnosti roh vybrakován,</l>
     <l n="5" met="1010001000" rhyme="3" line_length="10">šťasten venkovan i nevenkovan.</l>
     <l n="6" met="100010" rhyme="2" line_length="6">Dramatik ať žebrá!</l>
    </lg>
   </lg>
