   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101000" line_length="8">Ještě Polska nezhynula,</l>
     <l n="2" met="101010" rhyme="1" line_length="6">ale jistě zhyne,</l>
     <l n="3" met="10001010" line_length="8">nenajde-li místo šlechty</l>
     <l n="4" met="100010" rhyme="1" line_length="6">spasitele jiné.</l>
