  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101010" rhyme="1" line_length="8">Nehty kouše z hloupé nudy</l>
     <l n="2" met="100010" rhyme="2" line_length="6">bohatý a zívá –</l>
     <l n="3" met="10111010" rhyme="1" line_length="8">třeba hlad měl, přece chudý</l>
     <l n="4" met="101010" rhyme="2" line_length="6">zpívá, vesel bývá.</l>
    </lg>
    <lg n="2">
     <l n="5" met="10001000" rhyme="3" line_length="8">Bohatí se nasytili,</l>
     <l n="6" met="101010" rhyme="4" line_length="6">něco přece zbude:</l>
     <l n="7" met="10101000" rhyme="3" line_length="8">na kosť, kterou zahodili,</l>
     <l n="8" met="101010" rhyme="4" line_length="6">flétnu chudý hude.</l>
    </lg>
   </lg>
  </body>
