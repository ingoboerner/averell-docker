 <text>
  <front>
   <head>Proroctví.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001000" rhyme="1" line_length="8">Vyletěla zezulenka</l>
     <l n="2" met="10" rhyme="2" line_length="2">z bora –</l>
     <l n="3" met="10001000" rhyme="1" line_length="8">Bude-li ta Majdalenka</l>
     <l n="4" met="10" rhyme="3" line_length="2">moja?</l>
    </lg>
    <lg n="2">
     <l n="5" met="10001000" rhyme="1" line_length="8">Zamýšlí se zezulenka,</l>
     <l n="6" met="10" rhyme="4" line_length="2">dumá,</l>
     <l n="7" met="10001000" line_length="8">za uchem se pazúrečkem</l>
     <l n="8" met="10" rhyme="4" line_length="2">drbá.</l>
    </lg>
    <lg n="3">
     <l n="9" met="10101000" rhyme="1" line_length="8">Aj, co kuká zezulenka</l>
     <l n="10" met="10" rhyme="3" line_length="2">moja?</l>
     <l n="11" met="10101000" rhyme="1" line_length="8">„Bude, bude Majdalenka</l>
     <l n="12" met="10" rhyme="3" line_length="2">tvoja!“</l>
    </lg>
   </lg>
  </body>
