  <front>
   <head>Přijmi, bratře, dnes</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10100" rhyme="1" line_length="5">Přijmi, bratře, dnes </l>
     <l n="2" met="10101" rhyme="1" line_length="5">srdce mého ples</l>
     <l n="3" met="100010" rhyme="2" line_length="6">za bratrské přání –</l>
     <l n="4" met="101010" rhyme="3" line_length="6">aby dobré nebe</l>
     <l n="5" met="100010" rhyme="3" line_length="6">blažilo vždy Tebe</l>
     <l n="6" met="101000" rhyme="2" line_length="6">v květu, v radování.</l>
    </lg>
   </lg>
  </body>
