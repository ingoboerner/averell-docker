  <front>
   <head>Anjelíček strážce</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Anjelíček strážce </l>
     <l n="2" met="100010" rhyme="2" line_length="6">šepce mi to přání:</l>
     <l n="3" met="100010" rhyme="1" line_length="6">na osudu vážce</l>
     <l n="4" met="100010" rhyme="2" line_length="6">štěstí ať se sklání</l>
     <l n="5" met="101000" rhyme="2" line_length="6">k Tobě do skonání.</l>
    </lg>
   </lg>
  </body>
