   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="001010" line_length="6">Jakž Ti nemám přáti </l>
     <l n="2" met="100100" rhyme="1" line_length="6">štěstí, já chudinec?</l>
     <l n="3" met="000100" line_length="6">Vždyť jsi můj největší</l>
     <l n="4" met="101000" rhyme="1" line_length="6">v světě dobrodinec!</l>
