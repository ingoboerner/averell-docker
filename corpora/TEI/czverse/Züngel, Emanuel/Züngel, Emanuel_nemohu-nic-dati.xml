 <text>
  <front>
   <head>Nemohu nic dáti</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Nemohu nic dáti </l>
     <l n="2" met="100010" rhyme="2" line_length="6">vázaného Tobě,</l>
     <l n="3" met="101010" rhyme="1" line_length="6">mohu jenom přáti,</l>
     <l n="4" met="101010" rhyme="2" line_length="6">čeho žádáš sobě.</l>
     <l n="5" met="100100" rhyme="3" line_length="6">Srdce mé tlukoucí</l>
     <l n="6" met="101010" rhyme="3" line_length="6">chová lásku vroucí,</l>
     <l n="7" met="101000" rhyme="4" line_length="6">stále vypravuje,</l>
     <l n="8" met="010100" rhyme="4" line_length="6">jak Tebe miluje.</l>
     <l n="9" met="010010" rhyme="5" line_length="6">To srdéčko moje,</l>
     <l n="10" met="001010" rhyme="5" line_length="6">ač je dávno Tvoje,</l>
     <l n="11" met="100010" rhyme="6" line_length="6">daruji Ti k svátku</l>
     <l n="12" met="100100" rhyme="6" line_length="6">na věčnou památku.</l>
    </lg>
   </lg>
  </body>
 </text>
