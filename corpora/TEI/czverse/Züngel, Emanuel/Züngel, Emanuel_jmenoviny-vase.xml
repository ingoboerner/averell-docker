 <text>
  <front>
   <head>Jmenoviny Vaše,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" line_length="6">Jmenoviny Vaše, </l>
     <l n="2" met="010100" rhyme="1" line_length="6">má drahá kmotřičko,</l>
     <l n="3" met="100010" line_length="6">pobízí dnes k přání</l>
     <l n="4" met="010100" rhyme="1" line_length="6">mé útlé srdíčko.</l>
     <l n="5" met="1000100" line_length="7">Nemohu však ničeho</l>
     <l n="6" met="100010" rhyme="2" line_length="6">lepšího Vám přáti,</l>
     <l n="7" met="001110" line_length="6">než by Pán Bůh ráčil</l>
     <l n="8" met="001010" rhyme="2" line_length="6">Vám svou milost dáti</l>
     <l n="9" met="101010" line_length="6">a Vás v zdraví, v blahu</l>
     <l n="10" met="101000" rhyme="2" line_length="6">dlouho zachovati.</l>
    </lg>
   </lg>
  </body>
 </text>
