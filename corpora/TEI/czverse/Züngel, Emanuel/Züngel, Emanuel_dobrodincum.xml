    <lg n="1">
     <l n="1" met="00101000" rhyme="1" line_length="8">Co jste roku minulého</l>
     <l n="2" met="10000100" rhyme="1" line_length="8">učinili mi dobrého,</l>
     <l n="3" met="1001101" rhyme="2" line_length="7">za to Vám Bůh radost dej,</l>
     <l n="4" met="1010100" rhyme="2" line_length="7">ve všech pracích požehnej!</l>
