   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10100" rhyme="1" line_length="5">Drahá matičko, </l>
     <l n="2" met="10100" rhyme="1" line_length="5">slyš mé přáníčko,</l>
     <l n="3" met="10100" line_length="5">dej Ti Pámbiček</l>
     <l n="4" met="100100" rhyme="1" line_length="6">na zemi nebíčko.</l>
