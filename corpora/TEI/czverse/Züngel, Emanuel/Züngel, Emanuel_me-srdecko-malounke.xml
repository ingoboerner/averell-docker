   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0100100" rhyme="1" line_length="7">Mé srdečko malounké </l>
     <l n="2" met="1000100" rhyme="1" line_length="7">přeje Vám vše hezounké,</l>
     <l n="3" met="1001000" rhyme="2" line_length="7">zdraví a spokojenost,</l>
     <l n="4" met="1000100" rhyme="2" line_length="7">všelikou i blaženost.</l>
