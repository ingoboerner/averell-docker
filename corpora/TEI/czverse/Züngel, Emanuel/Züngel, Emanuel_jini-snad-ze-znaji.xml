  <front>
   <head>Jiní snad že znají</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Jiní snad že znají </l>
     <l n="2" met="101000" rhyme="2" line_length="6">krásněj blahopřáti,</l>
     <l n="3" met="000010" rhyme="1" line_length="6">co však zde se tají,</l>
     <l n="4" met="100010" rhyme="2" line_length="6">nemohou Ti dáti:</l>
     <l n="5" met="001010" rhyme="3" line_length="6">jest to láska vroucí,</l>
     <l n="6" met="101000" rhyme="3" line_length="6">vděčnost nehynoucí.</l>
    </lg>
   </lg>
  </body>
