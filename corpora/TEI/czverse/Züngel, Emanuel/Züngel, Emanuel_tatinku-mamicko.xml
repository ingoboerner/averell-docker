 <text>
  <front>
   <head>Tatínku, mamičko,</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">Tatínku, mamičko, </l>
     <l n="2" met="100100" rhyme="1" line_length="6">dědečku, babičko,</l>
     <l n="3" met="100100" rhyme="1" line_length="6">bratříčku, sestřičko,</l>
     <l n="4" met="00010" rhyme="2" line_length="5">kde jste kdo, slyšte,</l>
     <l n="5" met="10010" rhyme="2" line_length="5">v paměť si vpište,</l>
     <l n="6" met="00110" rhyme="3" line_length="5">co Vám chci přáti</l>
     <l n="7" met="100100" rhyme="4" line_length="6">ze srdce plného:</l>
     <l n="8" met="110100" rhyme="4" line_length="6">rač všeho dobrého</l>
     <l n="9" met="11010" rhyme="3" line_length="5">Pán Bůh Vám dáti!</l>
    </lg>
   </lg>
  </body>
 </text>
