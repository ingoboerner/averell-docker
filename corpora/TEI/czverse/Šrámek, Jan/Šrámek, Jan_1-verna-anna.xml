     <l n="1" met="1000100" line_length="7">Michalovic Anninka</l>
     <l n="2" met="1000100" rhyme="1" line_length="7">věrností se chlubila;</l>
     <l n="3" met="10010110" line_length="8">právem to, neb již sto mužům</l>
     <l n="4" met="1010100" rhyme="1" line_length="7">věčnou věrnosť slíbila.</l>
