  <front>
   <head>SUSPIRIUM.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10010" line_length="5">Charakter z juchty</l>
     <l n="2" met="10110" line_length="5">dej mi, ó Pane,</l>
     <l n="3" met="100100" line_length="6">spadne-li do louže,</l>
     <l n="4" met="000100" line_length="6">nic se mu nestane.</l>
    </lg>
   </lg>
  </body>
