  <front>
   <head>MOUDROST OTCOVSKÁ.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="11000" line_length="5">Ber po hodinách,</l>
     <l n="2" met="010001" rhyme="1" line_length="6">však po minutách plať,</l>
     <l n="3" met="0100001" line_length="7">vždy ourodu svou tak sej,</l>
     <l n="4" met="011101" rhyme="1" line_length="6">by čert měl jenom nať.</l>
    </lg>
   </lg>
  </body>
