  <front>
   <head>RADA OTCOVSKÁ.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="110010" rhyme="1" line_length="6">Chceš vykonat, Náci,</l>
     <l n="2" met="100010" rhyme="1" line_length="6">nesmrtelnou práci?</l>
     <l n="3" met="100010" rhyme="2" line_length="6">Neorej a nesej,</l>
     <l n="4" met="101010" rhyme="2" line_length="6">piš jen samou essai</l>
     <l n="5" met="010010" rhyme="1" line_length="6">a vykonáš, Náci,</l>
     <l n="6" met="100010" rhyme="1" line_length="6">nesmrtelnou práci.</l>
    </lg>
   </lg>
  </body>
