  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10010100" rhyme="1" line_length="8">Frázička krásná velice,</l>
     <l n="2" met="011000" rhyme="2" line_length="6">co chce, zbagatelí,</l>
     <l n="3" met="100100100" rhyme="1" line_length="9">hodí se ke každé kritice</l>
     <l n="4" met="010010" rhyme="2" line_length="6">jak jelito k zelí.</l>
