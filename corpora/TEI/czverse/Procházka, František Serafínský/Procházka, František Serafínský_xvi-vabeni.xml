  <front>
   <head>XVI. VÁBENÍ.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010" line_length="4">Pojď k nám, Janku,</l>
     <l n="2" met="1000" rhyme="1" line_length="4">na česnečku,</l>
     <l n="3" met="1000" line_length="4">přivítám tě</l>
     <l n="4" met="1000" rhyme="1" line_length="4">ve věnečku.</l>
     <l n="5" met="1000100" rhyme="2" line_length="7">Ve věnečku leluje,</l>
     <l n="6" met="0010100" rhyme="2" line_length="7">tak se ona jmenuje. –</l>
    </lg>
    <lg n="2">
     <l n="7" met="1010" line_length="4">Vlezu radši</l>
     <l n="8" met="1000" rhyme="3" line_length="4">do kožucha,</l>
     <l n="9" met="1000" line_length="4">do česnečky</l>
     <l n="10" met="1010" rhyme="3" line_length="4">spadla mucha,</l>
     <l n="11" met="1010100" rhyme="2" line_length="7">a ta tvoje leluje</l>
     <l n="12" met="1000100" rhyme="2" line_length="7">po sedničce hopcuje.</l>
    </lg>
   </lg>
  </body>
 </text>
