 <text>
  <front>
   <head>XIV. DRAHÝ ZPĚVÁK.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100100" rhyme="1" line_length="6">Za jednu hubičku</l>
     <l n="2" met="101100" rhyme="1" line_length="6">Chtěla mít písničku:</l>
     <l n="3" met="0101010" rhyme="2" line_length="7">Toť nejde, moje milá!</l>
     <l n="4" met="000100" rhyme="3" line_length="6">Což pak by kytička</l>
     <l n="5" met="100100" rhyme="3" line_length="6">Z jednoho kvítečka</l>
     <l n="6" met="1001000" rhyme="2" line_length="7">Nanic se uvíjela!</l>
    </lg>
    <lg n="2">
     <l n="7" met="101100" line_length="6">Kolik jde kvítečků</l>
     <l n="8" met="100100" rhyme="1" line_length="6">Na pěknou kytičku,</l>
     <l n="9" met="100100" line_length="6">Tolik též hubiček</l>
     <l n="10" met="100100" rhyme="1" line_length="6">Na hezkou písničku.</l>
    </lg>
   </lg>
  </body>
 </text>
