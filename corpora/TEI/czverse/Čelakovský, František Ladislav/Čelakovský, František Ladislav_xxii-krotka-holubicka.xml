  <front>
   <head>XXII. KROTKÁ HOLUBIČKA.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="100010" rhyme="1" line_length="6">Holubička s dubu</l>
     <l n="2" met="100100" rhyme="1" line_length="6">Letěla k holubu:</l>
     <l n="3" met="101000" rhyme="2" line_length="6">Milý nad rybníčky</l>
     <l n="4" met="100100" rhyme="2" line_length="6">Pase tam koníčky,</l>
     <l n="5" met="101010" line_length="6">A já za ním půjdu.</l>
    </lg>
    <lg n="2">
     <l n="6" met="100010" rhyme="3" line_length="6">Holubička krotká</l>
     <l n="7" met="100010" rhyme="3" line_length="6">S holoubkem se zobká:</l>
     <l n="8" met="010100" rhyme="4" line_length="6">Proč bysme taky tak</l>
     <l n="9" met="100010" rhyme="4" line_length="6">Neměli se zobkat,</l>
     <l n="10" met="001010" line_length="6">Když jsme mladí obá.</l>
    </lg>
   </lg>
  </body>
 </text>
