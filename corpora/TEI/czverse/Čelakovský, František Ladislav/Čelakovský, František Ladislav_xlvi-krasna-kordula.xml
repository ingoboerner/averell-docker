     <l n="10" met="101101101" rhyme="1" line_length="9">Ona má, ona má, ona má</l>
     <l n="11" met="1000100" rhyme="3" line_length="7">Kudrnatou hlavičku</l>
     <l n="12" met="101000" rhyme="3" line_length="6">Jako kapustičku;</l>
     <l n="13" met="100100" rhyme="2" line_length="6">Pystíčky odulé</l>
     <l n="14" met="101100" rhyme="2" line_length="6">Jako dvě cibule:</l>
