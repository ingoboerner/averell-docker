     <l n="94" met="101101101" rhyme="1" line_length="9">Ona má, ona má, ona má</l>
     <l n="95" met="100100" rhyme="28" line_length="6">Nožičky ze strunek,</l>
     <l n="96" met="100111" rhyme="28" line_length="6">Dělají klep, klep, klep;</l>
     <l n="97" met="100100" rhyme="29" line_length="6">Hedbávné ručičky,</l>
     <l n="98" met="101000" rhyme="29" line_length="6">Na nich bradavičky;</l>
