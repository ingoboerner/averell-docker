     <l n="75" met="101101101" rhyme="1" line_length="9">Ona má, ona má, ona má</l>
     <l n="76" met="100100" rhyme="21" line_length="6">Hedbávné ručičky,</l>
     <l n="77" met="101000" rhyme="21" line_length="6">Na nich bradavičky;</l>
     <l n="78" met="101000" rhyme="22" line_length="6">Nehty podobné ku</l>
     <l n="79" met="100100" rhyme="22" line_length="6">Pazourkům z česneku;</l>
