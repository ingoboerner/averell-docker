   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10101010" line_length="8">„Množit lid náš českou školou</l>
     <l n="2" met="10010010" rhyme="1" line_length="8">povinnost každého táty...“</l>
     <l n="3" met="10101000" line_length="8">Při tom národ decimují</l>
     <l n="4" met="10001000" rhyme="1" line_length="8">gumovými inseráty.</l>
    </lg>
   </lg>
