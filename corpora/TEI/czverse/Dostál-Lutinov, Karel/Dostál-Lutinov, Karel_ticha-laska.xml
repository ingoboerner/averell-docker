   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010100" rhyme="1" line_length="7">Letí, letí motýli,</l>
     <l n="2" met="0010100" rhyme="1" line_length="7">jen se k sobě nachýlí,</l>
     <l n="3" met="0010100" rhyme="2" line_length="7">jen se křídly doteknou,</l>
     <l n="4" met="1010100" rhyme="2" line_length="7">ani slova neřeknou...</l>
