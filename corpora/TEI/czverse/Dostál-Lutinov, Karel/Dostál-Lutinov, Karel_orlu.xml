  <front>
   <head>ORLU.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="101" rhyme="1" line_length="3">Svaly tuž,</l>
     <l n="2" met="101" rhyme="1" line_length="3">slzy suš,</l>
     <l n="3" met="101" rhyme="2" line_length="3">křivdy ruš,</l>
     <l n="4" met="101" rhyme="1" line_length="3">Bohu služ,</l>
     <l n="5" met="001" rhyme="2" line_length="3">k svým se druž –</l>
     <l n="6" met="001" rhyme="1" line_length="3">pak jsi muž!</l>
    </lg>
   </lg>
  </body>
