  <front>
   <head>Píseň parníka.</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="111" line_length="3">Hu! hu! hu!</l>
     <l n="2" met="001" rhyme="1" line_length="3">Už je - du!</l>
     <l n="3" met="1000" line_length="4">Na - jed’ jsem se</l>
     <l n="4" met="1010" rhyme="1" line_length="4">Oh - ně, vo - dy,</l>
     <l n="5" met="0000" line_length="4">To jsou by - ly</l>
     <l n="6" met="1000" rhyme="1" line_length="4">Not - né ho - dy,</l>
     <l n="7" met="111" line_length="3">Hu! hu! hu!</l>
     <l n="8" met="001" rhyme="1" line_length="3">Už je - du!</l>
    </lg>
   </lg>
