   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="11010100" rhyme="1" line_length="8">Res victrix diis placuit,</l>
     <l n="2" met="110100" line_length="6">sed victa Catoni –</l>
     <l n="3" met="100100101" rhyme="1" line_length="9">korunu dostal tvůj čistý štít</l>
     <l n="4" met="110100" line_length="6">sto korun zákony.</l>
