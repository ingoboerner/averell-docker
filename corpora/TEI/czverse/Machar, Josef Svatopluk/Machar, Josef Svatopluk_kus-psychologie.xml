   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="110101010" rhyme="1" line_length="9">Ó Vašku, jenž tu sloužíš krotce</l>
     <l n="2" met="01010101" rhyme="2" line_length="8">jsa místo mzdy jak žito bit,</l>
     <l n="3" met="100101010" rhyme="1" line_length="9">k dobrému srdci pana otce</l>
     <l n="4" met="01010100" rhyme="2" line_length="8">jak István uč se promluvit!</l>
    </lg>
   </lg>
