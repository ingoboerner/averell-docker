  <front>
   <head>ANGLICKÝ MINISTERSKÝ PŘEDSEDA CHVÁLÍ ČESKOSLOVENSKO</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="110100100" rhyme="1" line_length="9">Mr. Baldwin pochválil Čechii</l>
     <l n="2" met="0101" rhyme="2" line_length="4">a jeho hlas</l>
     <l n="3" met="1000" rhyme="2" line_length="4">rozleh se v nás</l>
     <l n="4" met="1000010" rhyme="3" line_length="7">sebevědomým echem –</l>
     <l n="5" met="100010100" rhyme="1" line_length="9">přál bych nám být raděj Anglií</l>
     <l n="6" met="01010010" rhyme="3" line_length="8">a Baldwin aby byl Čechem!</l>
    </lg>
   </lg>
  </body>
