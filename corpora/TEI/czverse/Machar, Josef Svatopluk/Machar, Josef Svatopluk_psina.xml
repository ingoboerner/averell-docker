 <text>
  <front>
   <head>„PSINA“</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="10001000" rhyme="1" line_length="8">Po ideích obrozenských,</l>
     <l n="2" met="10001000" rhyme="1" line_length="8">po obzorech náboženských,</l>
     <l n="3" met="1010100" rhyme="2" line_length="7">heslech vážných, perutných,</l>
     <l n="4" met="1010100" rhyme="2" line_length="7">po všech vzletech mohutných –</l>
    </lg>
    <lg n="2">
     <l n="5" met="1010100" rhyme="3" line_length="7">žije v době poslední</l>
     <l n="6" met="10101010" line_length="8">tohle krásné nové slovo</l>
     <l n="7" met="10101010" line_length="8">jako program, jako smysl</l>
     <l n="8" met="1010101" rhyme="3" line_length="7">našich slavných českých dní:</l>
     <l n="9" met="10" line_length="2">„psina“.</l>
    </lg>
   </lg>
  </body>
 </text>
