   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010" rhyme="1" line_length="4">Dobrý Švehlo,</l>
     <l n="2" met="1010" rhyme="1" line_length="4">tohle šlehlo!</l>
     <l n="3" met="1010000" rhyme="2" line_length="7">Žádné ceremonie –</l>
     <l n="4" met="10101010" rhyme="3" line_length="8">prostý rolník pohřben bude</l>
     <l n="5" met="10001010" rhyme="3" line_length="8">v hostivařské líše chudé,</l>
     <l n="6" met="0010000" rhyme="2" line_length="7">to je demokracie.</l>
    </lg>
   </lg>
