   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="0101010" line_length="7">Co divu, že’s ty radou</l>
     <l n="2" met="0100001" rhyme="1" line_length="7">A komorníkem se stal!</l>
     <l n="3" met="010001010" line_length="9">Vždyť Kaligula hřebce svého</l>
     <l n="4" met="100100" rhyme="1" line_length="6">Konsulem udělal.</l>
