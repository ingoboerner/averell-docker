 <text>
  <front>
   <head>Baba letí...</head>
  </front>
  <body>
   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" met="1010" rhyme="1" line_length="4">Baba letí</l>
     <l n="2" met="1000" rhyme="1" line_length="4">na koštěti</l>
     <l n="3" met="100010" rhyme="2" line_length="6">do nočního šera, –</l>
     <l n="4" met="10101" rhyme="3" line_length="5">babo, jenom leť!</l>
     <l n="5" met="1010" rhyme="4" line_length="4">Doma zbyla</l>
     <l n="6" met="1000" rhyme="4" line_length="4">roztomilá</l>
     <l n="7" met="1010" rhyme="2" line_length="4">tvoje dcera –</l>
     <l n="8" met="00100" rhyme="3" line_length="5">tak je dobře teď!</l>
    </lg>
   </lg>
  </body>
