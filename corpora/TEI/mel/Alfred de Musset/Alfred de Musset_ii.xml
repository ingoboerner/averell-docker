   <lg xmlns="http://www.tei-c.org/ns/1.0" type="poem">
    <lg n="1">
     <l n="1" rhyme="a" line_length="7">Si la flèche envenimée</l>
     <l n="2" rhyme="b" line_length="7">Ne peut sortir de mon flanc,</l>
     <l n="3" rhyme="a" line_length="7">La main de ma bien-aimée</l>
     <l n="4" rhyme="b" line_length="7">Peut en essuyer le sang !</l>
    </lg>
   </lg>
